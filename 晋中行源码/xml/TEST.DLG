<TRANSACTION>
<VARIABLE>
  <ITEM NAME="#TMP11" TYPE="%-1s" FUNCTIONS="s()" />  <!--保存交易信息-->
  <ITEM LINE="1"  COL="28" TYPE="%-18s" FUNCTIONS="T(授          权)"/>
  <ITEM LINE="4"  COL="20" TYPE="%-8s" FUNCTIONS="T(流水号:)"/>
  <ITEM LINE="4"  COL="28" NAME="#WSSRNO" INPUT="TEXT"  TYPE="%-7s" FUNCTIONS="T($FD14,2,7)"/> 
    
  <ITEM LINE="7"  COL="20" TYPE="%-8s" FUNCTIONS="T(授权号:)"/> 
  <ITEM LINE="7"  COL="28" INPUT="SELECT" NAME="#AUTHNUM" TYPE="%-7s" FUNCTIONS="T(0000000)" >  
  <SELECT>
	<OPTION VALUE="0000000" TITLE="0.AAAA"/>
	<OPTION VALUE="1111111" TITLE="1.BBBB"/>
  </SELECT>
  <ONLOSTFOCUS>"{
	enable(#WSSRNO,true);
	showmsg(#AUTHNUM);
	}"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#AUTHNUMFLG" TYPE="%-1s" FUNCTIONS="T(#AUTHNUM)" />  
  <ITEM NAME="#GO1" TYPE="%-1s" FUNCTIONS="T(0)j(#AUTHNUMFLG=0,T{1})j(#AUTHNUMFLGgreat0,T{1})G(0,#AUTHNAME)"/>
  <ITEM NAME="#TMP1" TYPE="%-3s" FUNCTIONS="T(@@@)" />  
  <ITEM NAME="#TMP2" TYPE="%-10s" FUNCTIONS="T(#TMP1+#AUTHNUM)" />  
  <ITEM NAME="#TMP3" TYPE="%-10s" FUNCTIONS="T($FD9=#TMP2)" />  
  <ITEM NAME="#TMP4" TYPE="%-1s" FUNCTIONS="T(9)" />  
  <ITEM NAME="#TMP5" TYPE="%-8s" FUNCTIONS="T(#TMP4+#WSSRNO)" />  
  <ITEM NAME="#TMP6" TYPE="%-8s" FUNCTIONS="T($FD14=#TMP5)" />  
  <ITEM NAME="#GO2" TYPE="%-1s" FUNCTIONS="T(0)G(0,#OK)"/>
        
  <ITEM LINE="10"  COL="20" TYPE="%-8s" FUNCTIONS="T(授权人:)"/>
  <ITEM LINE="10"  COL="28" NAME="#AUTHNAME" INPUT="TEXT"  TYPE="%-4s"  /> 
      
  <ITEM LINE="10"  COL="35" TYPE="%-8s" FUNCTIONS="T(口  令:)"/>
  <ITEM LINE="10"  COL="43" NAME="#AUTHPWD" INPUT="TEXT"  TYPE="%-6s" FUNCTIONS="i()" />  
  <ITEM NAME="#TMP7" TYPE="%-10s" FUNCTIONS="T(#AUTHNAME+#AUTHPWD)" />  
  <ITEM NAME="#TMP8" TYPE="%-10s" FUNCTIONS="T($FD9=#TMP7)" />  
  <ITEM NAME="#TMP9" TYPE="%-8s" FUNCTIONS="T(#TMP4+#WSSRNO)" />  
  <ITEM NAME="#TMP10" TYPE="%-8s" FUNCTIONS="T($FD14=#TMP9)" />  
  <ITEM NAME="#OK" LINE="15"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
</TRANSACTION>
