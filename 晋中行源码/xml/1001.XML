	 <OPTION VALUE="RPT555"      TITLE="支行资金头寸表"/>
	 <OPTION VALUE="RPT928"      TITLE="活期动户余额表"/> 
	 <OPTION VALUE="RPT931"      TITLE="内部户动户余额表"/> 
	 <OPTION VALUE="RPT930"      TITLE="贷款余额表"/> 
	 <OPTION VALUE="gD0296"      TITLE="准备金透支罚息传票"/> 
	 <OPTION VALUE="BR0"        TITLE="代发工资成功明细"/> 
	 <OPTION VALUE="BR1"        TITLE="代发工资失败明细"/> 
	 <OPTION VALUE="BRDWHZ"     TITLE="代发工资传票"/> 
	 <OPTION VALUE="gD811"      TITLE="一户通结息传票"/> 
	 <!--OPTION VALUE="RPT986"      TITLE="已到期定期户明细表" /> 
	 <OPTION VALUE="RPT926"      TITLE="同城结算轧差表" /--> 
	 <OPTION VALUE="RPT026"      TITLE="贷款正常转逾期清单"/>
	 <OPTION VALUE="RPT028"      TITLE="贷款正常转逾期传票"/>
	 <OPTION VALUE="RPT027"      TITLE="贷款正常转逾期扣款清单"/>
	 <OPTION VALUE="RPT065"      TITLE="贷款到期扣款传票"/>
	 <OPTION VALUE="RPT040"      TITLE="欠息贷款户明细表" /> 
	 <OPTION VALUE="RPT101"      TITLE="不欠本金但欠利息的贷款户" /> 
	 <OPTION VALUE="RPT046"      TITLE="贷款欠息明细"/>
	 <OPTION VALUE="RPT101"      TITLE="无本金但欠息利息户明细"/>
	 <OPTION VALUE="RPT056"      TITLE="承兑汇票扣款回单"/>
	 <OPTION VALUE="RPT985"      TITLE="承兑汇票到期清单"/>
	 <OPTION VALUE="RPT055"      TITLE="承兑汇票清单"/>
	 <OPTION VALUE="RPT083"      TITLE="承兑多余保证金清单"/>
   </SELECT> 
   <ONLOSTFOCUS> 
   "{ 
   			if(#RPTNAME=='BR0' or #RPTNAME=='BR1' or #RPTNAME=='BRDWHZ')
        {
        	show(#DWID,true);
        	show(#DWLABEL,true);
        }
        else
        {
        	show(#DWID,false);
        	show(#DWLABEL,false);        
        }
       // goitem(#RPTTYPE); 
   }" 
   </ONLOSTFOCUS> 
  </ITEM> 
  <ITEM LINE="14"  COL="35" NAME="#DWID"  TYPE="%4s" DEVICE="KEYBOARD" INPUT="TEXT" VISABLE="FALSE" FUNCTIONS=""/> 
 <!-- 
