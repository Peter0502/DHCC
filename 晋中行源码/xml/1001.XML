   <SELECT LINE="14" COL="52"> 
     <OPTION VALUE="0  使用本机终端打印机      " TITLE="0.使用本机终端打印机"/> 
     <OPTION VALUE="1  使用中心机房高速行打    " TITLE="1.使用中心机房高速行打"/> 
   </SELECT> 
  </ITEM> 
  --> 
  <ITEM NAME="#RPTTYPE"  TYPE="%-1s" FUNCTIONS="T(0)"> 
    <ONLOSTFOCUS> 
    "{ 
       if($KINBR=='19999' or $KINBR=='10001'){ 
         show(#SHOWKINBR,true); 
    show(#KINBR,true); 
       } 
       else{ 
         #KINBR=$KINBR; 
       } 
    }" 
    </ONLOSTFOCUS> 
  </ITEM> 
