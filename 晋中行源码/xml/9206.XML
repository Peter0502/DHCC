<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="9206 放款计划表查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD16='8206';
	$FD29=#BDATE;
	$FD38=#ACNO;
	$FD44=#EDATE;
	$FD67=#PSTS;
	$FD100=itoa(#LAMT*100,'%016.0f')+itoa(#HAMT*100,'%016.0f');
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9206                        放款计划表查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8206)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <BLOCK name="COMM" LINE="0" COL="7">
  <ITEM LINE="6"  COL="10" TYPE="%-10s" FUNCTIONS="T(是否放款!)"/>
  
  <ITEM LINE="8"  COL="10" TYPE="%-10s" FUNCTIONS="T(账    号:)"/>
  
  <ITEM LINE="10"  COL="10" TYPE="%-10s" FUNCTIONS="T(发放日期:)"/>
  <ITEM LINE="10"  COL="29" TYPE="%-2s" FUNCTIONS="T(→)"/>
  
  <ITEM LINE="12"  COL="10" TYPE="%-10s" FUNCTIONS="T(金    额:)"/>
  <ITEM LINE="12"  COL="43" TYPE="%-2s" FUNCTIONS="T(→)"/>
  
  <!--输入变量-->
  <ITEM NAME="#PSTS" LINE="6" COL="20" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="T(0)">
  <SELECT>
		<OPTION VALUE="0" TITLE="未放"/>
		<OPTION VALUE="1" TITLE="已放"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#ACNO" LINE="8" COL="20" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="T()">
  <ONLOSTFOCUS>
 "{		commsend_001();
 		$FD16='9958';
		$FD38=#ACNO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#ACNO=$FD38;
		show(#ACNO,true);
 }"
 </ONLOSTFOCUS>
  </ITEM>
  
  <ITEM NAME="#BDATE" LINE="10" COL="20" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#EDATE" LINE="10" COL="34" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  
  <ITEM NAME="#LAMT" LINE="12" COL="20" TYPE="%17.2f" INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#HAMT" LINE="12" COL="45" TYPE="%17.2f" INPUT="TEXT" FUNCTIONS=""/>
  </BLOCK>
  
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
   
  
  
  
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
