<ITEM LINE="21" COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
	<ONCLICK>
  		"{
  dim @tota as string;
  
  commsend_001();
	$FD16='5580';
  $FD34=#OPACKMSGNO;//报文编号
  $FD44=#OENDATE;//交易日期
  $FD55=#OPACKNO;//包流水号
	@tota=calltips();
