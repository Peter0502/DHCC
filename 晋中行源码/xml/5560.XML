<ITEM LINE="9"  COL="15" TYPE="%-15s" FUNCTIONS="T(交 易  日 期:)"/>
<ITEM LINE="11" COL="15" TYPE="%-15s" FUNCTIONS="T(交易 流 水号:)"/>

<!--输入变量-->
