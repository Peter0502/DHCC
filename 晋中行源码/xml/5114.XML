<?xml version="1.0" encoding="GBK" ?>

<TRANSACTION RIGHTS="11110000" TITLE="5114 银行汇票解挂"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->

<COMMSEND>
"{
	commsend_001();
	$FD21    = #CUR_NO;
	$FD89    = #NOTE_TYPE;
	$FD21    = #PO_NO;
	//$FD39    = itoa(val(#PO_BAL),'%15.0f');
	$FD39    = itoa(#PO_BAL*100,'%15.0f');
	showmsg('$FD39====='+$FD39);
	$FD123   = space(18)+#PAY_BR_NO;
	$FD16    = '5120';
	$MSGTYPE = '03001';
}"
</COMMSEND>
<COMMRECV>
</COMMRECV>
<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-49s" FUNCTIONS=
    "T(5114                           银行汇票解挂)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>

  <ITEM NAME="#MSGTYPE"     TYPE="-5s"    FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM NAME="#TXNO"        TYPE="-4s"    FUNCTIONS="T($TXNO=5120)"/>

  <ITEM NAME="#NOTE_TYPE"   TYPE="%-3s"   FUNCTIONS="T()"/><!---凭证种类--->

  <ITEM LINE="8"   COL="15" TYPE="%-14s"  FUNCTIONS="T(挂  失  日 期: )"/>
  <ITEM LINE="10"  COL="15" TYPE="%-14s"  FUNCTIONS="T(汇   票    号: )"/>
  <ITEM LINE="12"  COL="15" TYPE="%-14s"  FUNCTIONS="T(签  发  行 号: )"/>
  <ITEM LINE="14"  COL="15" TYPE="%-14s"  FUNCTIONS="T(票  面 金  额: )"/>
 
  <ITEM LINE="8"   COL="29" NAME="#LOSTDATE"  TYPE="%8d"     INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM LINE="10"  COL="29" NAME="#PO_NO"     TYPE="%16d"    INPUT="TEXT" FUNCTIONS="V(NB)"/>
  <ITEM LINE="12"  COL="29" NAME="#PAY_BR_NO" TYPE="%-12s"   INPUT="TEXT" FUNCTIONS="V(NB)"/>
  <ITEM LINE="14"  COL="29" NAME="#PO_BAL"    TYPE="%16.2f"  INPUT="TEXT" FUNCTIONS="V(NB)"/><!---金额--->
  <ITEM NAME="#CUR_NO"      TYPE="%-3s"       FUNCTIONS="T(RMB)"/><!---币种--->
  <ITEM NAME="#XAVBAL"      TYPE="%-20s"      FUNCTIONS="X(#PO_BAL)">
  <ONLOSTFOCUS>
  "{
	call('remember -add '+$HDATE+' '+#LOSTDATE,1);

  }"
  </ONLOSTFOCUS>
  </ITEM>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD"   TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />
  </VARIABLE>

<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE>v67611#STAT#WSSRNO
</PACKAGE>
</RESPONSE>

<OUTPUT>
<!--PACKAGE>
002$9999=9L490000D专用凭证N205$KINBR$HDATE#BRSNO$HDATE#BRSNO#BRNAME$TLRNO#BRNAME
#JYNO#JYNO#DJNO#DJNO#VOCNUM#VOCNUM#JDATE#JDATE#XTXAMT#XTXAMT#BRSNO#BRSNO
</PACKAGE-->

<PACKAGE> 
000$9999=9L490000A显示内容N505#PONO#PAYBRNO#XAVBAL#LOSTDATE#RSTAT#WSSRNO
</PACKAGE>
<!--PACKAGE> 
000$9999=9L490000B显示内容N205$KINBR#VOCNUM#BRSNO$HDATE#VOCNUM#GDATE#PACTYPE$TLRNO
</PACKAGE-->
<PACKAGE> 
000$9999=9L490000A汇票解卦内容N205#PONO#PAYBRNO#XAVBAL#LOSTDATE#RSTAT#WSSRNO
</PACKAGE>

