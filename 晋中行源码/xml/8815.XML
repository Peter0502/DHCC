
<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="8815   账单介质设定"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE UPLOOP="TRUE">
		<ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
		"T(8815                             账单介质设定)"/>
		<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
		"T(─────────────────────────────────────)"/>
		<ITEM NAME="#TXNO"    TYPE="%-4s"  FUNCTIONS="T($TXNO=8815)"/>
		<ITEM NAME="#MSGTYPE" TYPE="%-5s"  FUNCTIONS="T($MSGTYPE=03001)"/>
		<ITEM NAME="#TXCD"    TYPE="%4s"   FUNCTIONS="T(8815)"/>      <!---交易代码--->
   <ITEM LINE="7"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#CRD_ID_NO" FUNCTIONS="T(卡        号:)"/>
  <ITEM LINE="7"  COL="25" NAME="#CRD_NO"  INPUT="TEXT" TYPE="%-19s"  DEVICE="KEYBOARD" FUNCTIONS="">
		<ONLOSTFOCUS>"{
			dim @l as number;
			@l = len(#CRD_NO);
		
				if (@l == 19)
				{
					commsend_001();
					//callserver();
					if ($FD12 != '0000')
					{
						showmsg(geterror());
					//	return(false);
					}
				}
				else
				{
					showmsg('卡号位数错误！');
					//return(false);
				}
			
		}"</ONLOSTFOCUS>
	</ITEM>
	  <ITEM LINE="9"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#LBL_ID_TYPE" FUNCTIONS="T(账单介质类型:)"/>
<ITEM LINE="9" COL="25" NAME="#STMT_MEDIA_TYPE" TYPE="%-1s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(1)">
		<SELECT>
			<OPTION VALUE="P" TITLE = "0.纸质账单"/>
			<OPTION VALUE="E" TITLE = "1.电子账单"/>
			<OPTION VALUE="B" TITLE = "1.both"/>
		</SELECT>
	</ITEM>
	<ITEM LINE="11"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#STMT_EMAIL" FUNCTIONS="T(电 子  邮 箱:)"/>
		<ITEM LINE="11"  COL="25" NAME="#EMAIL"  INPUT="TEXT" TYPE="%-19s"  DEVICE="KEYBOARD" FUNCTIONS=""/>
		<!--输出-->
		<!--ITEM NAME="#NEXTPAGE_FLG" TYPE="%-10s"  FUNCTIONS=""/-->
