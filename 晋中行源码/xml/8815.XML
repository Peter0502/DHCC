		<ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
			<ONCLICK>
					"{
							dim @tagvalue as string;
							initfd();
							commsend_001();
							$FD77='12021';
							$FD30=#CRD_NO;
						  $FD67=#STMT_MEDIA_TYPE;
							$FD119=#EMAIL;
							//callagn_credit();
							if($FD12 != '0000')
							{		
								showmsg(geterror());
							//	return(false);
							}
 runoutput();
               close();
              }"
        </ONCLICK>
  </ITEM>
</VARIABLE>
