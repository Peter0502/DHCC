         print( 8, 20,'卡        号:[19]',$FD30);
         print( 9,20,'账单介质类型:[40]',$FD67);
         print( 10, 20,'电 子  邮 箱:[40]',$FD119);
        
        }"
        </PRINT>
</RESPONSE>

