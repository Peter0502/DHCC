<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1831         商业银行取对账数据"
	xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
	"{
		refresh();
		initfd();
		commsend_001();
	}"
</COMMSEND>
<COMMRECV>
	"{
		commrecv_001();
	}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE=" 3" COL=" 3"  TYPE="%-70s"  FUNCTIONS=  	"T(1831                       商业银行取对账数据)"/>
	<ITEM LINE=" 4" COL=" 3"  TYPE="%-74s"  FUNCTIONS="T(--------------------------------------------------------------------------)"/>
	<ITEM NAME="#TXCODE" TYPE="%-30s" FUNCTIONS="T(1831)"/>
	<ITEM NAME="#TXNAME"      TYPE="%-30s"  FUNCTIONS="T(商业银行取对账数据)"/>
	<ITEM LINE=" 6" COL=" 3"  TYPE="%-14s"  FUNCTIONS="T(工作    场次：)"/>
	<ITEM LINE=" 6" COL="17"  TYPE="%-8s"   NAME="#WorkRound"    INPUT="TEXT"   FUNCTIONS="T()V(NB)"/>
	<ITEM LINE=" 6" COL="37"  TYPE="%-14s"  FUNCTIONS="T(工作    日期：)"/>
	<ITEM LINE=" 6" COL="51"  TYPE="%-8s"   NAME="#WorkDate"     INPUT="TEXT"   FUNCTIONS="T()V(NB)D()"/>
	<ITEM LINE=" 8" COL=" 3"  TYPE="%-14s"  FUNCTIONS="T(清算    场次：)"/>
	<ITEM LINE=" 8" COL="17"  TYPE="%-8s"   NAME="#ClearRound"   INPUT="TEXT"   FUNCTIONS="T()V(NB)"/>
	<ITEM LINE=" 8" COL="37"  TYPE="%-14s"  FUNCTIONS="T(清算    日期：)"/>
	<ITEM LINE=" 8" COL="51"  TYPE="%-8s"   NAME="#ClearDate"    INPUT="TEXT"   FUNCTIONS="T()V(NB)D()"/>
	<!--ITEM LINE="10" COL=" 3"  TYPE="%-14s"  FUNCTIONS="T(交换    场次：)"/>
	<ITEM LINE="10" COL="17"  TYPE="%-8s"   NAME="#ExchgRound"   INPUT="TEXT"   FUNCTIONS="T()V(NB)"/>
	<ITEM LINE="10" COL="37"  TYPE="%-14s"  FUNCTIONS="T(交换    日期：)"/>
	<ITEM LINE="10" COL="51"  TYPE="%-8s"   NAME="#ExchgDate"    INPUT="TEXT"   FUNCTIONS="T()V(NB)D()"/-->
	<ITEM LINE="12" COL=" 3"  TYPE="%-14s"  FUNCTIONS="T(数据文件类型：)"/>
	<ITEM LINE="12" COL="17"  TYPE="%-1s"   NAME="#DataType"     INPUT="SELECT" FUNCTIONS="T(0)V(NB)">
		<SELECT>
			<OPTION VALUE="0" TITLE="取以下三类数据"/>  
			<OPTION VALUE="1" TITLE="取对账明细数据"/>
			<OPTION VALUE="2" TITLE="取对账差额数据"/>
			<OPTION VALUE="3" TITLE="取手续费数据"/>							
		</SELECT>		
	</ITEM>	

	<ITEM LINE="20"  COL="59" TYPE="%-6s" FUNCTIONS="T(确定)" INPUT="BUTTON" NAME="#OK" DEVICE="KEYBOARD" >
	<ONCLICK>
 		"{
 				
				initfd();
				commsend_001();	
				$FD16 = '1505';
				$FD18 = 'pbp';
				$FD28 = 'sxps';
				$FD52 = #WorkRound;
				$FD44 = #WorkDate;
				$FD45 = #ClearDate;
				$FD54 = #ClearRound;
				$FD55 = #ClearRound;
				$FD46 = #ClearDate;
				$FD23 = #DataType;
					
			 	callagnpbp();			
				if($FD12!='0000')
				{
					showmsg(geterror());
					return(false);
				}
				
				if(delspace($FD4) == '')
				{
					//生成核心流水-start
					dim @bak_fd81 as string;
					@bak_fd81=$FD81;
					$FD81=#TXNAME;
					gethxtraceno();
					$FD81=@bak_fd81;
					//生成核心流水-end
				}
				runoutput();
				rerun();
				refresh();
 		}"
 	</ONCLICK>
 	</ITEM>     
</VARIABLE>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="按任意键退出">
NOTHING
</PACKAGE>
	<PRINT>
		"{			
			PrintTxHead();
				
			print(10,25,'对账处理完成');
				
			PrintTxTail();
    		}"
   	</PRINT>
</RESPONSE>
</TRANSACTION>
	
