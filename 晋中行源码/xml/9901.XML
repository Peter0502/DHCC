  <ITEM LINE="11"  COL="5" TYPE="%-10s" FUNCTIONS="T(交易日期:)"/>
  <ITEM LINE="11"  COL="25" TYPE="%-2s" FUNCTIONS="T(→)"/>
  <ITEM LINE="12"  COL="5" TYPE="%-10s" FUNCTIONS="T(开销标志!)"/>   
  
 <!--输入变量-->
  <ITEM NAME="#ACNO" LINE="6" COL="15" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="">
 <ONLOSTFOCUS>
 "{		commsend_001();
 		$FD16='9958';
		$FD38=#ACNO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#ACNO=$FD38;
		show(#ACNO,true);
 }"
 </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#ACSN" LINE="6" COL="48" TYPE="%-3s" INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#NAME" LINE="7" COL="15" TYPE="%-50s" INPUT="TEXT" FUNCTIONS=""/>

  <ITEM NAME="#CTYPE" LINE="8" COL="15" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="T(1)">
  <SELECT>
		<OPTION VALUE="1" TITLE="个人"/>
		<OPTION VALUE="2" TITLE="公司"/>
		<OPTION VALUE="3" TITLE="机构"/>
		<OPTION VALUE="4" TITLE="同业"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#LAMT" LINE="9" COL="15" TYPE="%17.2f" INPUT="TEXT" FUNCTIONS=""/> 
  <ITEM NAME="#HAMT" LINE="9" COL="40" TYPE="%17.2f" INPUT="TEXT" FUNCTIONS=""/>
  
  <ITEM NAME="#OPNO" LINE="10" COL="15" TYPE="%-5s" INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#TEL"  LINE="10" COL="48" TYPE="%-7s" INPUT="TEXT" FUNCTIONS=""/>
  
  <ITEM NAME="#LDATE" LINE="11" COL="15" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#HDATE" LINE="11" COL="28" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  
  <ITEM NAME="#STS" LINE="12" COL="15" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="0" TITLE="申请"/>
		<OPTION VALUE="1" TITLE="开户"/>
		<OPTION VALUE="2" TITLE="销户"/>
	</SELECT>
  </ITEM>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
  </VARIABLE>
