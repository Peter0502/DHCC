<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9901 开销户登记簿查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD16='8901';
	$FD26=#NAME;
	$FD29=#LDATE;
	$FD38=#ACNO;
	$FD44=#HDATE;
	$FD51=#ACSN;
	$FD64=#TEL;
	$FD67=#STS;
	$FD70=#CTYPE;
	$FD91=#OPNO;
	$FD100=itoa(#LAMT*100,'%016.0f')+itoa(#HAMT*100,'%016.0f');
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9901                        开销户登记簿查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=9901)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="6"  COL="5" TYPE="%-10s" FUNCTIONS="T(客户账号:)"/>
  <ITEM LINE="6"  COL="38" TYPE="%-10s" FUNCTIONS="T(账户序号:)"/>
  
  <ITEM LINE="7"  COL="5" TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="8"  COL="5" TYPE="%-10s" FUNCTIONS="T(客户类型!)"/>
  
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(金    额:)"/>
