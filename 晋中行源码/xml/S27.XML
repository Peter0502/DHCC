<?xml version="1.0" encoding="GBK" ?>



<TRANSACTION RIGHTS="00110000" TITLE="L27 支行提入票据登记"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-49s" FUNCTIONS=
    "T(S27                           支行提入票据登记)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="6"  COL="8" TYPE="%-12s" FUNCTIONS="T(票据  笔数: )"/>
  <ITEM LINE="6"  COL="24" TYPE="%-12s" FUNCTIONS="T(取消用)"/>
  <ITEM LINE="6"  COL="38" TYPE="%-12s" FUNCTIONS="T(记帐 方式!)"/>
  <ITEM LINE="6"  COL="56" TYPE="%-12s" FUNCTIONS="T(记帐科目:)"/>
  <ITEM LINE="8"  COL="8" TYPE="%-12s" FUNCTIONS="T(收款 帐号: )"/>
  <ITEM LINE="8" COL="38" TYPE="%-12s" FUNCTIONS="T(交易 金额: )"/>
  <ITEM LINE="10" COL="8" TYPE="%-12s" FUNCTIONS="T(收款人名称: )"/>
  <ITEM LINE="12" COL="8" TYPE="%-12s" FUNCTIONS="T(凭证 种类:!)"/>
  <ITEM LINE="12" COL="38" TYPE="%-12s" FUNCTIONS="T(凭证 号码: )"/>

  <ITEM LINE="14" COL="8" TYPE="%-12s" FUNCTIONS="T(提出行行号:)"/>
  <ITEM LINE="14" COL="38" TYPE="%-12s" FUNCTIONS="T(付款人账号:)"/>

  <ITEM LINE="16" COL="8" TYPE="%-12s" FUNCTIONS="T(付款人名称: )"/>
<!--
  <ITEM LINE="16" COL="8" TYPE="%-12s" FUNCTIONS="T(对方机构号: )"/>
  <ITEM LINE="16" COL="38" TYPE="%-12s" FUNCTIONS="T(复  核 员:)"/>
-->
  <ITEM LINE="18" COL="8" TYPE="%-12s" FUNCTIONS="T(对方机构名:)"/>


  <ITEM LINE="6"  COL="19" NAME="#OACT" TYPE="%-4s" DEVICE="KEYBOARD" FUNCTIONS="j($HCODE=1,T{#OACT}T{0000})">
  <HELP LINES="17" COLS="14" LINE="5" COL="63">
  请您在这个栏位输入帐号的开户局号, 开户局号是18位帐号的前七位。</HELP>
  </ITEM>
  <ITEM LINE="6"  COL="48" NAME="#TXFLAG" TYPE="%-1s" DEVICE="KEYBOARD" FUNCTIONS="V(1|2|3)">
	<SELECT LINE="5" COL="59">
	<OPTION VALUE="1" TITLE="1 帐号对转"/>
	<OPTION VALUE="2" TITLE="2 科目对转"/>
	<OPTION VALUE="3" TITLE="3 现金"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#YYY"     TYPE="%-4s"  FUNCTIONS="T($KINBR,4,4)"/>
  <ITEM NAME="#TEMP"     TYPE="%-1s"  FUNCTIONS="T(#TXFLAG)G(1,#NEWACNO)"/>
  <ITEM NAME="#TEMP2"     TYPE="%-1s"  FUNCTIONS="T(#TXFLAG)G(3,#MONEY)"/>
  <ITEM LINE="6"  COL="66" NAME="#OACNO" TYPE="%-7s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM NAME="#TEMP3"     TYPE="%-1s"  FUNCTIONS="T(#TXFLAG)G(2,#MONEY)"/>

  <ITEM LINE="08" COL="19" NAME="#NEWACNO" TYPE="%-18s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)H(#NEWACNO)"/>  
  <ITEM LINE="08"  COL="48" NAME="#MONEY" TYPE="%17.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>

  <ITEM LINE="10"  COL="19" NAME="#PAYNAME" TYPE="%-40s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="12"  COL="19" NAME="#VOCTYPE" TYPE="%2d" DEVICE="KEYBOARD" FUNCTIONS="">
    <SELECT LINE="5" COL="41">
        <OPTION VALUE="99 旧凭证        " TITLE="99旧凭证"/>
        <OPTION VALUE="01 储蓄存折      " TITLE="01储蓄存折      "/>
        <OPTION VALUE="02 储蓄存单      " TITLE="02储蓄存单      "/>
        <OPTION VALUE="22 现金支票      " TITLE="22现金支票      "/>
        <OPTION VALUE="21 转帐支票      " TITLE="21转帐支票      "/>
        <OPTION VALUE="37 单位对公活折  " TITLE="37单位对公活折  "/>
        <OPTION VALUE="29 定期存款证实书" TITLE="29定期存款证实书"/>
        <OPTION VALUE="38 贴现凭证      " TITLE="38贴现凭证      "/>
        <OPTION VALUE="40 支款凭条      " TITLE="40支款凭条      "/>
        <OPTION VALUE="41 现金缴款单    " TITLE="41现金缴款单    "/>
        <OPTION VALUE="42 转帐借方凭证  " TITLE="42转帐借方凭证  "/>
        <OPTION VALUE="43 转帐贷方凭证  " TITLE="43转帐贷方凭证  "/>
        <OPTION VALUE="44 现金付出凭证  " TITLE="44现金付出凭证  "/>
        <OPTION VALUE="45 现金收入凭证  " TITLE="45现金收入凭证  "/>
        <OPTION VALUE="46 特种转帐凭证  " TITLE="46特种转帐凭证  "/>
        <OPTION VALUE="47 普通凭证      " TITLE="47普通凭证      "/>
        <OPTION VALUE="48 专用凭证      " TITLE="48专用凭证      "/>
        <OPTION VALUE="49 借款票据      " TITLE="49借款票据      "/>
        <OPTION VALUE="64 收费凭证      " TITLE="64收费凭证      "/>
      </SELECT>
   </ITEM>

   <ITEM LINE="12"  COL="48" NAME="#VOCNUM"   TYPE="%12d"  DEVICE="KEYBOARD" FUNCTIONS=""/>

  <ITEM LINE="14"  COL="19" NAME="#CHANGENO"  TYPE="%-4s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>
  <ITEM LINE="14"  COL="48" NAME="#OACTNO"  TYPE="%-32s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>

  <ITEM LINE="16"  COL="19" NAME="#CASHNAME" TYPE="%-40s" DEVICE="KEYBOARD" FUNCTIONS=""/>
   <ITEM NAME="#TOBRNO"   TYPE="%-10d"   FUNCTIONS=""/>
   <ITEM NAME="#CHECKNO" TYPE="%-2s"  FUNCTIONS="T()"/>
<!--
   <ITEM LINE="16"  COL="19" NAME="#TOBRNO"   TYPE="%-10d"   DEVICE="KEYBOARD" FUNCTIONS=""/>
   <ITEM LINE="16"  COL="48" NAME="#CHECKNO" TYPE="%-2s"  DEVICE="KEYBOARD" FUNCTIONS="T()"/>
-->
   <ITEM LINE="18"  COL="19" NAME="#PAYBRNAME"  TYPE="%-40s" DEVICE="KEYBOARD" FUNCTIONS=""/>



  <ITEM NAME="#HEJFNO"  TYPE="%-6s"  />        <!--冲正日志档序号-->
  <ITEM NAME="#FFF"     TYPE="%-8s"  FUNCTIONS="T($HDATE)"/>
  <ITEM NAME="#OPNBR"     TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#HTRMSEQ" TYPE="%-2s"  FUNCTIONS="T($HTRMSEQ)"/>              <!--冲正终端机号-->
  <ITEM NAME="#HCODE"   TYPE="%-1s"  FUNCTIONS="T($HCODE)"/>                <!--取消记号-->
  <ITEM NAME="#TRMSEQ"  TYPE="%-2s"  FUNCTIONS="T($TRMSEQ)"/>               <!--柜台机序号-->
  <ITEM NAME="#ACTNO"   TYPE="%-18s"  FUNCTIONS="T(#NEWACNO)"/>               <!--柜台机序号-->
  <ITEM NAME="#EJFNO"   TYPE="%-6s"  FUNCTIONS="T($TXTNO)"/>                <!--日志序号-->
  <ITEM NAME="#TASKID"  TYPE="%-2s"  FUNCTIONS="T(UC)"/>                    <!--终端程序类别-->
  <ITEM NAME="#TMTYPE"  TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--终端种类-->
  <ITEM NAME="#APTYPE"  TYPE="%-1s"  FUNCTIONS="T(a)"/>                     <!--业务类别-->
  <ITEM NAME="#TXNO"    TYPE="%-2s"  FUNCTIONS="T(99)"/>                    <!--交易代号-->
  <ITEM NAME="#PTYPE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--处理型态-->
  <ITEM NAME="#MULTTX"  TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--连动记号-->
  <ITEM NAME="#CRDB"    TYPE="%-1s"  FUNCTIONS="T(1)"/>                     <!--贷别-->
  <ITEM NAME="#TRNMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--训练记号-->
  <ITEM NAME="#SBTMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--代登记号-->
  <ITEM NAME="#TLRNO"   TYPE="%-2s"  FUNCTIONS="T($TLRNO)"/>                <!--操作员号-->
  <ITEM NAME="#CURCD"   TYPE="%-3s"  FUNCTIONS="T(RMB) "/>                  <!--币别-->
  <ITEM NAME="#NBCD"    TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--无折记号-->
  <ITEM NAME="#XCRT"    TYPE="%-8s"  FUNCTIONS="T($XCRT) "/>                <!--外汇汇率-->
  <ITEM NAME="#REASON"  TYPE="%-6s"  FUNCTIONS="T(0)"/>                     <!--主管许可理由-->
  <ITEM NAME="#SUPIN1"  TYPE="%-1s"  FUNCTIONS="T(1)"/>                     <!--主管许可记号2-->
  <ITEM NAME="#SUPIN2"  TYPE="%-1s"  FUNCTIONS="T(0)"/>                     <!--主管许可记号1-->
  <ITEM NAME="#DSCPT"   TYPE="%-3s"  FUNCTIONS="j(#TXFLAG=3,T{001}T{002})"/>                   <!--摘要-->
  <ITEM NAME="#TXTYPE"  TYPE="%-2s"  FUNCTIONS="a( )"/>                     <!--帐务别-->
  <ITEM NAME="#KINBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>


  <ITEM NAME="#TXAMT"   TYPE="%-12s" FUNCTIONS="T(#MONEY,5,12) "/>         <!--金额-->
  <ITEM NAME="#HNAME" TYPE="%-4s" FUNCTIONS="j(#HCODE=0,T{交易}T{取消})"/>

  <ITEM NAME="#OPAYACTNO"  TYPE="%-18s"  />   
  <ITEM NAME="#OPAYNAME"  TYPE="%-40s"  />   
  <ITEM NAME="#OTXAMT"  TYPE="%-16s"  />   
  <ITEM NAME="#OAVBAL"  TYPE="%-16s"  />   
  <ITEM NAME="#OPAYBRNO"  TYPE="%-7s"  />   
  <ITEM NAME="#OPAYBRNAME"  TYPE="%-40s"  />   
  <ITEM NAME="#OPAYCNT"  TYPE="%-4s"  />   
  <ITEM NAME="#OCHANGE"  TYPE="%-4s"  />   
  <ITEM NAME="#BRSNO"  TYPE="%-6s"  />   
  <ITEM NAME="#PZDACNO"  TYPE="%-18s"  />   
  <ITEM NAME="#PZCACNO"  TYPE="%-18s"  />   

  <ITEM NAME="#SERVER"  TYPE="%-1s"     FUNCTIONS="T($ORKINBR=#OPNBR)T($TTSKID=UP)"/>
</VARIABLE>

<MONEY VARIABLE="#MONEY"/>

<REQUEST><PACKAGE>
 $APCTLHD$HTRMSEQ$HEJFNO#OPNBR#KINBR#TRMSEQ#EJFNO#TASKID#TMTYPE
 #APTYPE#TXNO#PTYPE#DSCPT$HCODE#ACTNO#TXTYPE#MULTTX#CRDB$SUPCD1$SUPCD2#REASON
 #TLRNO#NBCD#TRNMOD#SBTMOD#CURCD#XCRT#TXAMT
 #OACT#NEWACNO#PAYNAME#MONEY#VOCTYPE#VOCNUM#CHANGENO#OACTNO#TOBRNO#PAYBRNAME#CHECKNO#CASHNAME#OACNO#TXFLAG
<!--
$MONEY13$MONEY12$MONEY11$MONEY10$MONEY09$MONEY08$MONEY07$MONEY06$MONEY05$MONEY04$MONEY03$MONEY02$MONEY01
-->
</PACKAGE></REQUEST>

<RESPONSE>
<PACKAGE> a99101#OPAYACTNO#OTXAMT#OPAYBRNO#OPAYBRNAME#OCHANGE#OPAYCNT</PACKAGE>
</RESPONSE>

<OUTPUT>
<PACKAGE> 
000$9999=9C140000A   N505#OPAYACTNO#OTXAMT#OPAYBRNO#OPAYBRNAME#OCHANGE#OPAYCNT
</PACKAGE>
