<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="2107 客户工作信息录入"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD22=#WORKTYPE;
	$FD26=#DWNAME;
	$FD28=#CUSTNO;
	$FD41=itoa(#MONEY*100,'%016.0f');
	$FD44=#RZDATE;
	$FD45=#LZDATE;
	$FD66=#KHTYPE;
	$FD67=#STAT;
	$FD68=#WORK;
	$FD69=#JOBTYPE;

}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	#KHTYPE=$FD66;
	#ORDERNO=$FD35;
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(2107                         客户工作信息录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1107)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="7" COL="15" TYPE="%-10s" FUNCTIONS="T(客 户 号:)"/>
  <ITEM NAME="#SHOW" LINE="7" COL="45" TYPE="%-10s" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(工作序号:)"/>
  <ITEM LINE="9"  COL="15" TYPE="%-10s" FUNCTIONS="T(工作状态:)"/>
  <ITEM LINE="10"  COL="15" TYPE="%-10s" FUNCTIONS="T(行业代码:)"/>
  <ITEM LINE="11"  COL="15" TYPE="%-10s" FUNCTIONS="T(职业代码:)"/>
  <ITEM LINE="12"  COL="15" TYPE="%-10s" FUNCTIONS="T(职位代码:)"/>
  <ITEM LINE="13" COL="15" TYPE="%-10s" FUNCTIONS="T(单位名称:)"/>
  <ITEM LINE="15" COL="15" TYPE="%-10s" FUNCTIONS="T(入职日期:)"/>
  <ITEM LINE="16" COL="15" TYPE="%-10s" FUNCTIONS="T(离职日期:)"/>
  <ITEM LINE="17" COL="15" TYPE="%-10s" FUNCTIONS="T(年薪收入:)"/>
  <ITEM NAME="#CUSTNO" 	INPUT="TEXT" LINE="7" COL="25" TYPE="%-8s" 	FUNCTIONS="T()V(NB)">
        <ONLOSTFOCUS>
	"{
		dim @txno_bak as string;
		@txno_bak=$TXNO;
		$TXNO='9942';
		$FD28=#CUSTNO;
		commsend_001();
		$TXNO=@txno_bak;
		callserver();
		#KHTYPE=$FD66;
		//showmsg(#KHTYPE);	
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
			}
	}"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#ORDERNO" INPUT="TEXT" 	LINE="7" COL="55" TYPE="%-4s"  VISABLE="FALSE" FUNCTIONS=""/>
  <ITEM NAME="#STAT" 	INPUT="SELECT" 	LINE="9" COL="25" TYPE="%-1s" 	FUNCTIONS="T(0)V(NB)">
    	<SELECT>
		<OPTION VALUE="0" TITLE="1.当前工作"/>
		<OPTION VALUE="1" TITLE="1.以前工作"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#WORKTYPE" INPUT="SELECT" LINE="10" COL="25" TYPE="%-2s" 	FUNCTIONS="T(10)V(NB)">
  	<SELECT>
		<OPTION VALUE="1"  TITLE="1.农业"/>
		<OPTION VALUE="10" TITLE="1.城市建设"/>
		<OPTION VALUE="11" TITLE="1.旅游宾馆和餐饮业"/>
		<OPTION VALUE="12" TITLE="1.房地产"/>
		<OPTION VALUE="13" TITLE="1.科研设计"/>
		<OPTION VALUE="14" TITLE="1.文化艺术"/>
		<OPTION VALUE="15" TITLE="1.财政、金融和保险业"/>
		<OPTION VALUE="16" TITLE="1.新闻、出版和媒体"/>
		<OPTION VALUE="17" TITLE="1.公用服务业务"/>
		<OPTION VALUE="18" TITLE="1.社会团体"/>
		<OPTION VALUE="19" TITLE="1.工艺美术和礼品业"/>
		<OPTION VALUE="2"  TITLE="1.工业"/>
		<OPTION VALUE="20" TITLE="1.物资经营和回收"/>
		<OPTION VALUE="21" TITLE="1.教育事业"/>
		<OPTION VALUE="22"  TITLE="1.商业"/>
		<OPTION VALUE="23" TITLE="1.信息产业"/>
		<OPTION VALUE="24" TITLE="1.部队"/>
		<OPTION VALUE="25"  TITLE="1.其他"/>
		<OPTION VALUE="3" TITLE="1.服务业"/>
		<OPTION VALUE="4" TITLE="1.邮电通讯"/>
		<OPTION VALUE="5"  TITLE="1.社区服务"/>
		<OPTION VALUE="6" TITLE="1.零售业"/>
		<OPTION VALUE="7" TITLE="1.交通运输"/>
		<OPTION VALUE="8" TITLE="1.建筑业"/>
		<OPTION VALUE="9" TITLE="1.医疗卫生"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#S_WORKTYPE" TYPE="%-30s" FUNCTIONS="S(#WORKTYPE,#WORKTYPE)"/>
  <ITEM NAME="#WORK" 	INPUT="SELECT" LINE="11" COL="25" TYPE="%-1s" 	FUNCTIONS="T(1)V(NB)">
  	<SELECT>
		<OPTION VALUE="1" TITLE="1.单位、机构负责人"/>
		<OPTION VALUE="2" TITLE="1.专业技术人员"/>
		<OPTION VALUE="3" TITLE="1.办事人员和有关人员"/>
		<OPTION VALUE="4" TITLE="1.商业、服务业人员"/>
		<OPTION VALUE="5" TITLE="1.农林牧渔业生产人员"/>
		<OPTION VALUE="6" TITLE="1.生产运输设备操作人员"/>
		<OPTION VALUE="7" TITLE="1.军官"/>
		<OPTION VALUE="8" TITLE="1.其他"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#S_WORK" TYPE="%-30s" FUNCTIONS="S(#WORK,#WORK)"/>
 <ITEM NAME="#JOBTYPE" INPUT="SELECT" LINE="12" COL="25" TYPE="%-1s" 	FUNCTIONS="T(1)V(NB)">
  	<SELECT>
		<OPTION VALUE="1" TITLE="1.初级"/>
		<OPTION VALUE="2" TITLE="1.中级"/>
		<OPTION VALUE="3" TITLE="1.高级"/>
		<OPTION VALUE="4" TITLE="1.其他"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#DWNAME" INPUT="TEXT" LINE="13" COL="25" TYPE="%-50s" FUNCTIONS="T()V(NB)"/>
  <ITEM NAME="#GO1"	TYPE="%1d" 	FUNCTIONS="G(#STAT=1,#LZDATE)"/>
  <ITEM NAME="#RZDATE" INPUT="TEXT" LINE="15" COL="25" TYPE="%-8s" FUNCTIONS="D()"/>
  <ITEM NAME="#GO2"	TYPE="%1d" 	FUNCTIONS="G(#STAT=0,#MONEY)"/>
  <ITEM NAME="#LZDATE" INPUT="TEXT" LINE="16" COL="25" TYPE="%-8s" FUNCTIONS="D()"/>
  <ITEM NAME="#MONEY" INPUT="TEXT" LINE="17" COL="25" TYPE="%16.2f" FUNCTIONS="T()"/>
  <!--输出变量--->
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM NAME="#KHTYPE" TYPE="%-1s" FUNCTIONS="T()"/>
<!--	
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">-->

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
    	<ONCLICK>
    	"{
    		dim @tota as string;
    		dim @ch as string;
				commsend_001();
				$FD22=#WORKTYPE;
				$FD26=#DWNAME;
				$FD28=#CUSTNO;
				$FD41=itoa(#MONEY*100,'%016.0f');
				$FD44=#RZDATE;
				$FD45=#LZDATE;
				$FD66=#KHTYPE;
				$FD67=#STAT;
				$FD68=#WORK;
				$FD69=#JOBTYPE;
    		@tota=callserver();
    		#ORDERNO=$FD35;
       		if($FD12!='0000'){
				showmsg(geterror());
					return(false);
				}
				else{
				showall();
				show(#SHOW,true);
				//showmsg(#ORDERNO);
				show(#ORDERNO,true);
				runoutput();
    		@ch=showmsg('下一步执行 客户生活信息录入，Y-继续;N-退出：');
					if(@ch == 'Y' or @ch == 'y'){	
						open('2108.XML', '#CUSTNO='+#CUSTNO+';');
						return(true);
					}
					else{
						return(true);
					}
					}
    		}"
	</ONCLICK>    		
  </ITEM>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="按任意键继续">
	NOTHING
	</PACKAGE>
	<PRINT>"{
		print(8,30,        '录入成功！')
		print(9,10, '行    业:[30]',#S_WORKTYPE);
		print(10,10,'职    业:[30]',#S_WORK);
		print(11,10,'单位名称:[50]',#DWNAME);
		print(12,10,'客 户 号:[8]',#CUSTNO);
		}"
	</PRINT>
</RESPONSE>
<RESPONSE>
	<PACKAGE DEVICE="PRINTER" LINESPACE="24" DESC="请打印业务专用凭证">
	NOTHING
	</PACKAGE>
	<PRINT>
	"{
	  $KINDNO='00';
    dim @txtime  as string;
    @txtime=substr($SYSTIME,0,2)+':'+substr($SYSTIME,2,2)+':'+substr($SYSTIME,4,2);
	  print( 5, 20,'机构名称:[30]',$BRNAME);
    print( 5, 2,'代码:2107');
	  print( 5, 13,'交易:客户工作信息录入');
	  print( 8, 20,'客 户 号:[8]',#CUSTNO);
	  print( 9, 20,'行    业:[30]',#S_WORKTYPE);
	  print(10, 20,'职    业:[30]',#S_WORK);
	  print(10, 20,'单位名称:[50]',#DWNAME);
	  
	  print( 24,20,'备注:');
	  print( 24,27,'交易日期:[8] [8]',$HDATE,@txtime);
	  print( 24,5,'柜员:[4]',$FD7);
	  print( 24,5,'流水号:[6]',$FD4);
	}"
	</PRINT>
</RESPONSE>
</TRANSACTION>




