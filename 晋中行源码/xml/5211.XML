<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5211 网点提出手工录入"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	
    	$FD16='4211';	
      $FD25=getitems('#PAYER_NAME');     	
      $FD30=getitems('#ACTNO24');      	
      $FD31=getitems('#PAYEE_AC_NO')+space(6);  
      $FD39=getitems('#TXAMT');
      
     	$FD67=getitems('#BILL_TYPE');     
     	$FD68='2';   
	    
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';
	  // parm_seqn=1 贷方	
	  //  	setitems('#',$FD120);  
	  // parm_seqn=2	
	  //  	setitems('#',$FD121); 

}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5211                          网点提出手工录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4211)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  
  <ITEM NAME="#SPACE6"   TYPE="%-6s" FUNCTIONS=""/>  
  <ITEM NAME="#ACTNO24"   TYPE="%-24s" FUNCTIONS=""/>

  <ITEM LINE="6"  COL="5" TYPE="%-10s" FUNCTIONS="T(票据种类:!)"/>
  <ITEM NAME="#BILL_TYPE" LINE="6"  COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T(1)"  >
	<SELECT>
		<OPTION VALUE="1" TITLE="0.同城"/>
		<OPTION VALUE="2" TITLE="1.市国库税票"/>		
		<OPTION VALUE="3" TITLE="2.县国库税票"/>
		<OPTION VALUE="4" TITLE="3.大同城进帐单"/>
		<OPTION VALUE="5" TITLE="4.委托收款"/>
		<OPTION VALUE="6" TITLE="5.其他"/>								
	</SELECT>
  </ITEM>   
 
 
   <ITEM LINE="8"  COL="5" TYPE="%-10s" FUNCTIONS="T(提出类型:!)"/>
  <ITEM NAME="#TX_TYPE" LINE="8"  COL="15" TYPE="%-1s"   INPUT="SELECT" FUNCTIONS="T(2)" ENABLE="FALSE">
	<SELECT>
	
		<OPTION VALUE="1" TITLE="0.提出借方"/>
		<OPTION VALUE="2" TITLE="1.提出贷方"/>	
	</SELECT>
  </ITEM>  
 
  <ITEM LINE="10"  COL="5" TYPE="%-12s" FUNCTIONS="T(付款人账号:)"/>  
  <ITEM NAME="#PAYER_AC_NO" INPUT="TEXT" LINE="10"  COL="17" TYPE="%-18s"  FUNCTIONS="V(NB)" >
  <ONLOSTFOCUS>"{
    	dim @baktxno as string;
    	@baktxno=$TXNO;
    	$TXNO='9748';
    	

      $FD30=getitems('#PAYER_AC_NO');  
     	$FD68='2';    	
    	  	
	$FD2=$OPNBR;
	$FD3=$KINBR;
	$FD5=$HDATE;
	$FD7=$TLRNO;
	$FD10=$TTYNAME;
	$FD16='9748';
	$FD79=space(6);
	
    	callserver();
    	$TXNO=@baktxno;
    	if($FD12!='0000'){
    		showmsg(geterror());
    		return(false);
    	}

    	setitems('#ACTNO24',$FD30);  
    	setitems('#PAYER_NAME',$FD25);    
    	
    	showall();  	
 	 
  }"
  </ONLOSTFOCUS>
  </ITEM> 
 
  <ITEM LINE="12"  COL="5" TYPE="%-12s" FUNCTIONS="T(收款人账号:)"/>  
  <ITEM NAME="#PAYEE_AC_NO" INPUT="TEXT" LINE="12"  COL="17" TYPE="%-18s"  FUNCTIONS="V(NB)" />
 

   <ITEM LINE="14"  COL="5" TYPE="%-10s" FUNCTIONS="T(金    额:)"/>   
   <ITEM LINE="14"  COL="17" NAME="#TXAMT" INPUT="TEXT" TYPE="%17.2f" FUNCTIONS="0.00"/>
    
  <ITEM LINE="16"  COL="5" TYPE="%-12s" FUNCTIONS="T(付款人姓名:)"/>  
  <ITEM  LINE="16"  COL="17"  NAME="#PAYER_NAME" INPUT="TEXT" TYPE="%-50s" ENABLE="FALSE" FUNCTIONS="" />


  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

    
  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    
    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="网点提出手工录入成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'网 点 提 出 手 工 录 入 成 功 ！');

				
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>


