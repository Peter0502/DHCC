  <ITEM LINE="6"  COL="0"  TYPE="%-10s" NAME="#L3" FUNCTIONS="T(余    额:)"/>
  <ITEM LINE="7"  COL="0"  TYPE="%-10s" NAME="#L4" FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="9"  COL="0"  TYPE="%-10s" NAME="#A1" VISABLE="FALSE" FUNCTIONS="T(凭证种类:!)"/>
  <ITEM LINE="9"  COL="45" TYPE="%-10s" NAME="#A2" VISABLE="FALSE" FUNCTIONS="T(摘要选项:!)"/>
  <ITEM LINE="11" COL="0"  TYPE="%-10s" NAME="#L9" FUNCTIONS="T(转出金额:)"/>

  <ITEM NAME="#CUSTNAME" LINE="4"  COL="10" TYPE="%-50s" FUNCTIONS=""/>

  <ITEM NAME="#BIZHONG"  LINE="5"  COL="10" TYPE="%-2s" INPUT="SELECT"  ENABLE="FALSE" FUNCTIONS="">
	<SELECT>
		<OPTION VALUE="01" TITLE="0.人民币"/>
	</SELECT>
  </ITEM>

  <ITEM NAME="#LAVBAL" LINE="6"  COL="10" TYPE="%-16s" FUNCTIONS="X(#AVBAL)"/>
  <ITEM NAME="#AVBAL" TYPE="%17.2f" FUNCTIONS="J(#LAVBAL)"/>

  <ITEM NAME="#PRODNAME" LINE="7"  COL="10" TYPE="%-30s" FUNCTIONS=""/>

  <ITEM NAME="#VOCTYPE_A1"  LINE="9"  COL="10" TYPE="%-3s" INPUT="SELECT" VISABLE="FALSE" FUNCTIONS="">
