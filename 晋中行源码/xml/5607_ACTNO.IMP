		<OPTION VALUE="01" TITLE="0.备用金"/>
	</SELECT>
  <ONLOSTFOCUS>"{
		#FD101_6 = getseltitle(#DSCPT_A2,#DSCPT_A2);
  }"
  </ONLOSTFOCUS>
  </ITEM>

