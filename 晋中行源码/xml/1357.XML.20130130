    "{
         dim @acno as  string;
         
         initfd();
         commsend_001();
         $FD16='1537';
         $FD30=#CARDNO;
         @acno=callserver();
         #NAME=delspace($FD25);
         #LXDZ=delspace($FD82);
         #YDDH=delspace($FD83);
         #BAL =itoa(val($FD43,0.01),'%-17.2f');
         #BYJE=itoa(val($FD42,0.01),'%-17.2f');
         #LCJE=itoa(val($FD41,0.01),'%-17.2f');
         #XXZT=delspace($FD69);
         //showmsg(#XXZT);
         if(#XXZT == '1')
         {
           #XXZT='签约';
         }
         else if(#XXZT=='2')
         {
           #XXZT='临时解约';
         }
         else if(#XXZT=='*')
         {
           #XXZT='解约';
         }
         else if(#XXZT=='P')
         {
           #XXZT='临时解约';
         }
         else
         {
           showmsg('此账户未签约..');
           return(false);
         }
         show(#NAME1,true);
         show(#NAME,true);
         show(#BAL1,true);
         show(#BAL,true);
         show(#BYJE1,true);
         show(#BYJE,true);
         show(#LCJE1,true);
         show(#LCJE,true);
         show(#YDDH1,true);
         show(#YDDH,true);
         show(#LXDZ1,true);
         show(#LXDZ,true);
         show(#XXZT1,true);
         show(#XXZT,true);
     }"
    </ONLOSTFOCUS>
