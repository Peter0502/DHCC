<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11000000" TITLE="3401  转存单交易"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(                      转存单交易 -凭证信息录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  
  <ITEM LINE="12" COL="12" TYPE="%-9s" FUNCTIONS="T(介质种类:)"/>
  <ITEM LINE="13" COL="12" TYPE="%-9s" FUNCTIONS="T(凭证号码:)"/>
  
  <ITEM NAME="#BEGIN" TYPE="%1s" FUNCTIONS="S(#MEDIUMTYPE,0008)"/> 
  <ITEM NAME="#MEDIUMTYPE"   INPUT="SELECT"  LINE="12" COL="21" TYPE="%4s" FUNCTIONS="T(0008)V(NB)">
      <SELECT>
		<OPTION VALUE="0008" TITLE="0.单位定期存单"/>
     </SELECT>
	 <ONLOSTFOCUS>
	 "{
		initfd();
		commsend_001();
		$FD16 = '9739';
		$FD116 = #MEDIUMTYPE;
		callserver();
		if($FD12!='0000')
		{
			showmsg(geterror());
			return (false);
		}
	 }"
	 </ONLOSTFOCUS>
  </ITEM>

  <ITEM NAME="#CREDENCENO"   INPUT="TEXT"  LINE="13" COL="21" TYPE="%16d" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
		$BUFFER = '';
		$BUFFER = #MEDIUMTYPE+space(121)+#CREDENCENO+space(1);
		$FD116 = $BUFFER;
		commsend_001();
		$FD16 = '9782';
		callserver();
		if($FD12=='0000')
		{
			showmsg(geterror());
			return (false);
		}
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#CONFIRM"  LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
<ONCLICK>"{
	close('#CONFIRM','');	
}"
</ONCLICK>
</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
