		showblock('BLOCK_TX',false,true);
  }"
  </ONLOSTFOCUS>
  </ITEM>
	
  <ITEM LINE="06"  COL="17" NAME="#NOTPAY_DATE"   TYPE="%-8s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)D()"/>
  <ITEM LINE="07"  COL="17" NAME="#AC_BR_NO"   TYPE="%-12s"  DEVICE="KEYBOARD" FUNCTIONS=""> <!--接受行,原发起行--->
<ONLOSTFOCUS>
"{

		showblock('BLOCK_LB',false,true);
		showblock('BLOCK_TX',false,true);

		initfd();
		commsend_001();
		commsend_123();
		commsend_hv();
	    $FD16 ='9565';
		callserver();
		#T_QR_TX_NUM = #QR_TX_NUM;
		#T_NOTPAY_DATE      = #NOTPAY_DATE     ;
		#T_AC_BR_NO         = #AC_BR_NO        ;
		if($FD12!='0000' and $FD12!='NROW')
		{
   			showblock('BLOCK_LB',false,true);
   			showblock('BLOCK_TX',false,true);
			return (false);
			showmsg(geterror());
		}
		else if($FD12=='NROW')
		{
			showmsg('查复涉及到的支付交易并不存在!');
			#T_NOTPAY_CONTENT = '你行查询的这笔业务，我行并不存在。';
			commrecv_123();
			show(#NOTPAY_CONTENT,true);
			enable(#NOTPAY_CONTENT,false);
   			showblock('BLOCK_LB',false,true);
   			showblock('BLOCK_TX',false,true);
		}
		else if($FD12=='0000')
		{
   				showblock('BLOCK_LB',true,true);
   				showblock('BLOCK_TX',true,true);

				commrecv_123();
				#TX_AMT = val($FD39)/100;
				#O_WT_DAY = $FD45;

				$FD16 = '9553';
				$FD32 = #AC_BR_NO;
				callserver()
				if($FD12!='0000')
				{
					showmsg(geterror());
					return (false);
				}

				#BR_NAME = substr($FD96,0,60);
				show(#BR_NAME  ,true);
				show(#PAY_AC_NO,true);
				show(#PAY_NAME,true);
				show(#CASH_AC_NO,true);
				show(#CASH_NAME,true);
				show(#TX_AMT,true);
				show(#AC_BR_NO,true);
				show(#OORDERNO,true);
				show(#O_WT_DAY,true);
				show(#NOTPAY_CONTENT,true);
				#O_CMTNO = #CMTNO;//得到原支付交易的cmtno
				#QR_TX_NUM = #T_QR_TX_NUM;
				#NOTPAY_DATE      = #T_NOTPAY_DATE     ;
				#AC_BR_NO         = #T_AC_BR_NO        ;

				enable(#BR_NAME		,false);
				enable(#PAY_AC_NO	,false);
				enable(#PAY_NAME	,false);
				enable(#CASH_AC_NO	,false);
				enable(#CASH_NAME	,false);
				enable(#TX_AMT		,false);
				enable(#OORDERNO	,false);
				enable(#O_WT_DAY	,false);
				enable(#T_OR_BR_NO	,false);

				enable(#NOTPAY_CONTENT,false);
			}

				
		}"
	</ONLOSTFOCUS>
