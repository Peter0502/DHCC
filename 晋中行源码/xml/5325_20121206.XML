  <ITEM LINE="08"  COL="17" LINES="3" COLS="60"   NAME="#T_NOTPAY_CONTENT"   TYPE="%-255s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
	  <ONLOSTFOCUS>
  "{
	if (strfldcnt(#T_NOTPAY_CONTENT,':') != 1 or strfldcnt(#T_NOTPAY_CONTENT,'{') != 1 or strfldcnt(#T_NOTPAY_CONTENT,'}') != 1)
	{
	  showmsg('输入内容中不能含有半角:{}字符,请修改为全角其它字符');
	return(false);
	}
  }"
  </ONLOSTFOCUS>
