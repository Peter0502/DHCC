        <OPTION VALUE="2" TITLE="2.对公"/>
    </SELECT>
   </ITEM>
  <ITEM NAME="#FXDJ"  ENABLE="FALSE"  INPUT="SELECT"   LINE="6"   COL="45" TYPE="%-1s" FUNCTIONS="">
