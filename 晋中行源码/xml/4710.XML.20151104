 <?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00100000" TITLE="4710 理财产品状态调整"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD16='2224';
    $FD65='0001';
    $FD63=#CPDM;
    $FD67=#CZLX;
    $FD46=#CPQXR;//20151103
    $FD44=#MJQSR;//20151103
    $FD45=#MJJSR;//20151103
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(4710                         理财产品状态调整)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=2224)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="5"  COL="5"  TYPE="%-12s"  FUNCTIONS="T(操作类型:)"/> 
  <ITEM NAME="#CZLX"  INPUT="SELECT"   LINE="5"   COL="17" TYPE="%-20s" FUNCTIONS="">
   <SELECT>
   	<OPTION VALUE="3" TITLE="暂停发售"/>
   	<OPTION VALUE="4" TITLE="恢复发售"/>
   	<OPTION VALUE="5" TITLE="产品成立"/>
   	<OPTION VALUE="7" TITLE="开放式产品阶段开放"/><!--20151102LJ-->
   </SELECT>
   <ONLOSTFOCUS>
  "{
	  dim @tota as string;
	  initfd();
	  commsend_001();
	  $FD16='8018';
	  $FD65='0005';
	  switch(#CZLX)
	  {
	    case '1':
	    case '2':
	      $FD67 = 'A'; break;
	    case '3':
	    case '5':
	      $FD67 = '0'; break;
	    case '4':
	      $FD67 = 'B'; break;
	    case '7':
	      $FD67 = 'E'; break;//20151102LJ
	  }
	  showmsg('$FD67='+$FD67);
	  
	  @tota=callserver();
	  if($FD12 != '0000')
	  {
	  	showmsg(geterror());
	  	return(false);
	  }
	  setsel(#CPDM,@tota,'o5t60o1v20o2');
	  //LJstart20151103
	  if(#CZLX == '7')
	  {
	  	enable(#CPQXR,true);
	  	enable(#MJQSR,true);
	  	enable(#MJJSR,true);
	  	show(#JDQXR,true);
	  	show(#QXR,false);
	  	show(#JDQSR,true);
	  	show(#QSR,false);
	  	show(#JDJSR,true);
	  	show(#JSR,false);
	  }
	  else
	  {
	  	show(#JDQXR,false);
	  	show(#QXR,true);
	  	show(#JDQSR,false);
	  	show(#QSR,true);
	  	show(#JDJSR,false);
	  	show(#JSR,true);
	  	enable(#CPQXR,false);
	  	enable(#MJQSR,false);
	  	enable(#MJJSR,false);
	  }
	  //LJend
	  show(#CPDM,true);
  }"
  </ONLOSTFOCUS> 
  </ITEM> 
  <ITEM NAME="#SCZLX" TYPE="%-20s"  FUNCTIONS="S(#CZLX,#CZLX)"/>
  <BLOCK NAME="COMMON" LINE="6" COL="3">
  <!--显示变量-->
  <ITEM LINE="0"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(产品代码:)"/>
  <ITEM LINE="1"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="2"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(产品类型:)"/>
  <ITEM LINE="2"  COL="35" TYPE="%-10s"  FUNCTIONS="T(收益类型:)"/>
  <ITEM LINE="3"  COL="5"  TYPE="%-12s"  NAME="#QSR"    FUNCTIONS="T(募集起始日:)"/>
  <ITEM LINE="3"  COL="5"  TYPE="%-16s"  NAME="#JDQSR"  FUNCTIONS="T(阶段募集起始日:)"  VISABLE="FALSE"/><!--LJ-->
  <ITEM LINE="3"  COL="35" TYPE="%-12s"  NAME="#JSR"    FUNCTIONS="T(募集结束日:)"/>
  <ITEM LINE="3"  COL="35" TYPE="%-16s"  NAME="#JDJSR"  FUNCTIONS="T(阶段募集结束日:)"  VISABLE="FALSE"/><!--LJ-->
  <ITEM LINE="4"  COL="5"  TYPE="%-12s"  NAME="#QXR"    FUNCTIONS="T(产品起息日:)" "/>
  <ITEM LINE="4"  COL="5"  TYPE="%-16s"  NAME="#JDQXR"  FUNCTIONS="T(产品阶段起息日:)"  VISABLE="FALSE"/><!--LJ-->
  <ITEM LINE="4"  COL="35" TYPE="%-12s"  FUNCTIONS="T(产品到期日:)"/>
  <ITEM LINE="5"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(期    限:)"/>
  <ITEM LINE="5"  COL="35" TYPE="%-10s"  FUNCTIONS="T(币    种:)"/>
  <ITEM LINE="6"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(客户类型:)"/>
  <ITEM LINE="6"  COL="35" TYPE="%-10s"  FUNCTIONS="T(风险等级:)"/>
  <ITEM LINE="7"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(投资方向:)"/>
  <ITEM LINE="7"  COL="35" TYPE="%-12s"  FUNCTIONS="T(预期收益率:)"/>
  <ITEM LINE="8"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(手续费率:)"/>
  <ITEM LINE="8"  COL="35" TYPE="%-10s"  FUNCTIONS="T(托管费率:)"/>
  <ITEM LINE="9"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(发售总额:)"/>
 <!-- <ITEM LINE="9"  COL="35" TYPE="%-10s"  FUNCTIONS="T(单份金额:)"/>-->
  <ITEM LINE="10" COL="5"  TYPE="%-12s"  FUNCTIONS="T(总行剩余额:)"/>
  <ITEM LINE="10" COL="35" TYPE="%-12s"  FUNCTIONS="T(机构剩余额:)"/>
  <ITEM LINE="11" COL="5"  TYPE="%-12s"  FUNCTIONS="T(个人最低额:)"/>
  <ITEM LINE="11" COL="35" TYPE="%-12s"  FUNCTIONS="T(对公最低额:)"/>
  <ITEM LINE="12" COL="5"  TYPE="%-10s"  FUNCTIONS="T(递增金额:)"/>
  <ITEM LINE="12" COL="35" TYPE="%-10s"  FUNCTIONS="T(允许赎回:)"/>
  <ITEM LINE="13" COL="5"  TYPE="%-10s"  FUNCTIONS="T(允许撤销:)"/>
  <ITEM LINE="13" COL="35" TYPE="%-10s"  FUNCTIONS="T(管理费率:)"/>
  
  <ITEM NAME="#ZDMJ"  TYPE="%-16.2f" FUNCTIONS="T()"/><!--最低募集金额-->
  <ITEM NAME="#YJMJ"  TYPE="%-16.2f" FUNCTIONS="T()"/> <!--科目已经募集金额-->
  <!--输入产品代码-->
  <ITEM  LINE="0"   COL="15" TYPE="%-60s"  NAME="#CPDM" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="V(NB)">
    <ONLOSTFOCUS>
	  "{
	  	    dim @tota as string;
	        initfd();
	        commsend_001();
	  	    $FD16='8018';
	  	    $FD62=#CPDM;
	  	    $FD65='0001';	  	
	  	    @tota=callserver();
	  	    if($FD12 != '0000')
	  	    {
	  		     showmsg(geterror());
	  		     return(false);
	  	    }
	        #CPMC = delspace($FD25);
	        #CPLX =  $FD67;
	        #SYLX =  $FD68;
	        #MJQSR=  $FD44;
	        #MJJSR=  $FD45;
	        #CPQXR=  $FD46;
	        #CPDQR=  $FD47;
	        #QX   =  itoa(atoi($FD28)) + $FD22;
	        #BZ   =  $FD21;
	        #KHLX =  $FD70;
	        #FXDJ =  $FD69;
	        #TZFX =  $FD72;
	        #YQSYL=  val($FD84,0.000001);
	        #SXFL =  val($FD85,0.000001);
	        #TGFL =  val(substr($FD100,64,8),0.000001);
	        #FSZE =  val($FD39,0.01);
	        //#DFJE =  val($FD40,0.01);
	        #ZHSYE=  val($FD39,0.01) - val($FD43,0.01);
	        #JGSYE=  val(substr($FD100,0,16),0.01);
	        #GRZDE=  val($FD41,0.01);
	        #DGZDE=  val($FD42,0.01);
	        #DZJE =  val(substr($FD100,16,16),0.01);
	        #YXSH =  $FD71;
          #YXCX =  $FD93;
	        #GLFL =  val($FD97,0.000001);	
	        #ZDMJ =  val(substr($FD100,32,16),0.01);
	        #YJMJ =  val($FD43,0.01);
	        showmsg('#TXNO'+#TXNO);
	       if(#CZLX=='5')
	       {
		
		     if (#YJMJ&lt;#ZDMJ)
	       {
	        showmsg('认购总额小于募集最低限额，改产品不可成立!');
          return(false);
	        }
	        }
	  }"
    </ONLOSTFOCUS>
  </ITEM>
  <!--回显变量-->
  <ITEM NAME="#CPMC"  INPUT="LABEL"   LINE="1"   COL="15" TYPE="%-60s" FUNCTIONS=""/>
  
  <ITEM NAME="#CPLX"  ENABLE="FALSE"  INPUT="SELECT"   LINE="2"   COL="15" TYPE="%-1s" FUNCTIONS="">
  <SELECT>
        <OPTION VALUE="1" TITLE="1.封闭式"/>
        <OPTION VALUE="2" TITLE="2.开放式"/>
    </SELECT>
   </ITEM>
  <ITEM NAME="#SYLX"  ENABLE="FALSE"  INPUT="SELECT"   LINE="2"   COL="45" TYPE="%-1s" FUNCTIONS="">
  <SELECT>
        <OPTION VALUE="1" TITLE="1.保本固定收益"/>
        <OPTION VALUE="2" TITLE="2.保本浮动收益"/>
        <OPTION VALUE="3" TITLE="3.非保本浮动"/>
    </SELECT>
   </ITEM>
  <ITEM NAME="#MJQSR" ENABLE="FALSE"  INPUT="TEXT"   LINE="3"   COL="15" TYPE="%-8s" FUNCTIONS=""/><!--LJ-->
  <ITEM NAME="#MJJSR" ENABLE="FALSE"  INPUT="TEXT"   LINE="3"   COL="45" TYPE="%-8s" FUNCTIONS=""/><!--LJ-->
  <ITEM NAME="#CPQXR" ENABLE="FALSE"  INPUT="TEXT"   LINE="4"   COL="15" TYPE="%-8s" FUNCTIONS=""/><!--LJ-->
  <ITEM NAME="#CPDQR" INPUT="LABEL"   LINE="4"   COL="45" TYPE="%-8s" FUNCTIONS=""/>
  <ITEM NAME="#QX"    INPUT="LABEL"   LINE="5"   COL="15" TYPE="%-10s" FUNCTIONS=""/>
  <ITEM NAME="#BZ"    ENABLE="FALSE"  INPUT="SELECT"   LINE="5"   COL="45" TYPE="%-2s" FUNCTIONS="">
  <SELECT>
        <OPTION VALUE="01" TITLE="1.人民币"/>
    </SELECT>
   </ITEM>
  <ITEM NAME="#KHLX"  ENABLE="FALSE"  INPUT="SELECT"   LINE="6"   COL="15" TYPE="%-1s" FUNCTIONS="">
  <SELECT>
        <OPTION VALUE="0" TITLE="0.个人单位皆可"/>
