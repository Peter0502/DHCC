        <OPTION VALUE="3" TITLE="3.平衡型"/>
        <OPTION VALUE="4" TITLE="4.进取型"/>
        <OPTION VALUE="5" TITLE="5.激进型"/>
    </SELECT>
   </ITEM>
  <ITEM NAME="#TZFX"  ENABLE="FALSE"  INPUT="SELECT"   LINE="7"   COL="15" TYPE="%-1s" FUNCTIONS="">
