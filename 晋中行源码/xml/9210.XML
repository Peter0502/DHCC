<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="9210 贷款罚息信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
		commsend_001();
        $FD31 = #LNACTNO;
        $FD39 = space(16);
        $FD44 = #QYDATE1;
        $FD45 = #QYDATE2;
        $FD46 = #ZYDATE1;
        $FD47 = #ZYDATE2;
        $FD67 = #FXFLAG;
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(9210                        贷款罚息信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8210)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="6"  COL="34"  TYPE="%-14s" FUNCTIONS="T([查询条件])"/>
  <ITEM LINE="7"  COL="5"  TYPE="%-74s" FUNCTIONS="T(----------------------------------------------------------------------)"/>
  
  <BLOCK NAME="COMM" LINE="0" COL="15">
  <ITEM LINE="9"  COL="10"  TYPE="%-10s" FUNCTIONS="T(罚息状态:!)"/>
  <ITEM LINE="11" COL="10"  TYPE="%-10s" FUNCTIONS="T(贷款帐号:)"/>
  <ITEM LINE="13" COL="10"  TYPE="%-10s" FUNCTIONS="T(启用日期:)"/>
  <ITEM LINE="13" COL="30"  TYPE="%-3s"  FUNCTIONS="T(-->)"/>
  <ITEM LINE="15" COL="10"  TYPE="%-10s" FUNCTIONS="T(止用日期:)"/>
  <ITEM LINE="15" COL="30"  TYPE="%-3s"  FUNCTIONS="T(-->)"/>


  <ITEM LINE="9"   COL="20" TYPE="%-1s"  NAME="#FXFLAG"    DEVICE="KEYBOARD" FUNCTIONS="T(0)">
    <SELECT LINE="0" COL="0">
        <OPTION VALUE="0" TITLE="启用"/>
        <OPTION VALUE="1" TITLE="止用"/>
    </SELECT>
  </ITEM>

  <ITEM LINE="11"  COL="20" TYPE="%-24s"  NAME="#LNACTNO"   DEVICE="KEYBOARD" FUNCTIONS=""/>

  <ITEM LINE="13"  COL="20" TYPE="%-8s"  NAME="#QYDATE1"    DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="13"  COL="35" TYPE="%-8s"  NAME="#QYDATE2"    DEVICE="KEYBOARD" FUNCTIONS=""/>

  <ITEM LINE="15"  COL="20" TYPE="%-8s"  NAME="#ZYDATE1"    DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="15"  COL="35" TYPE="%-8s"  NAME="#ZYDATE2"    DEVICE="KEYBOARD" FUNCTIONS=""/>
  
  </BLOCK>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>

<OUTPUT>
<PACKAGE>
000$9999=9P130000A显示内容N505
</PACKAGE>

<FORMAT NAME="P130000A">
"03               交易成功!
"
</FORMAT>
</OUTPUT>
</TRANSACTION>
