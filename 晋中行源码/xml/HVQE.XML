<?xml version="1.0" encoding="GBK" ?>

<TRANSACTION RIGHTS="11111111" TITLE="HVQE     质押融资扣款明细"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVQE                        质押融资扣款明细)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="6"  COL="3"  TYPE="%-10s"  FUNCTIONS="T(交易日期:)"/>
  <ITEM LINE="6"  COL="25" TYPE="%-10s"  FUNCTIONS="T(交易序号:)"/>
  <ITEM LINE="8"  COL="3" TYPE="%-10s"  FUNCTIONS="T(交易状态:)"/>
  <ITEM LINE="10"  COL="3"  TYPE="%-12s" FUNCTIONS="T(借记行号:)"/>
  <ITEM LINE="12"  COL="3"  TYPE="%-14s" FUNCTIONS="T(贷记行号:)"/>
  <ITEM LINE="14" COL="3" TYPE="%-10s" FUNCTIONS="T(交易金额:)"/>
  <ITEM LINE="16" COL="3" TYPE="%-10s" FUNCTIONS="T(扣款本金:)"/>
  <ITEM LINE="18" COL="3" TYPE="%-10s" FUNCTIONS="T(扣款利息:)"/>
  <ITEM LINE="20" COL="3" TYPE="%-16s" FUNCTIONS="T(备    注:)"/>
   <!--
  <ITEM LINE="12" COL="7"  TYPE="%-12s" FUNCTIONS="T(货币符号:!)"/>
  <ITEM LINE="12" COL="33" TYPE="%-11s" FUNCTIONS="T(交易金额:)"/>
  -->
  <ITEM LINE="6"    COL="13" NAME="#HDATE"     TYPE="%-8s"    FUNCTIONS="V(NB)"/>
  <ITEM LINE="6"    COL="35" NAME="#ORDERNO"   TYPE="%-6s"    FUNCTIONS="V(NB)"/>
  <ITEM LINE="8"    COL="13" NAME="#STAT"     TYPE="%-8s"     FUNCTIONS="V(NB)"/>
  <ITEM LINE="10"   COL="13" NAME="#DRBRNO"   TYPE="%-12s"    FUNCTIONS="V(NB)"/>
  <ITEM LINE="12"   COL="13" NAME="#CRBRNO"   TYPE="%-12s"    FUNCTIONS="V(NB)"/>
  <ITEM NAME="#TXAMT"    TYPE="%16.2f"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="14"   COL="13" NAME="#LTXAMT"   TYPE="%-17s"    FUNCTIONS="X(#TXAMT)"/>
  
  <ITEM NAME="#CORPUS"   TYPE="%16.2f"   FUNCTIONS="V(NB)"/>  
  <ITEM LINE="16"   COL="13" NAME="#LCORPUS"   TYPE="%-17s"    FUNCTIONS="X(#CORPUS)"/>
  <ITEM NAME="#INTR"     TYPE="%16.2f"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="18"   COL="13" NAME="#LINTR"    TYPE="%-17s"    FUNCTIONS="X(#INTR)"/>   
  <ITEM LINE="20"   COL="13" NAME="#REASON"   TYPE="%-60s"    FUNCTIONS="V(NB)"/>
 
  <ITEM LINE="21"  COL="65" NAME="#CLOSE"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
  }"
  </ONCLICK>
  </ITEM>
   
</VARIABLE>
</TRANSACTION>
