    dim @acno  as string;
    dim @seqn  as string;
    dim @name  as string;
    dim @amt   as string;
    dim @voctype as string;
    dim @vocnum  as string;
    dim @brf   as string;
    @cnt=9;
    print( 5,10,'机构名称:[30]',$BRNAME);
    print( 5, 2,'交易代码: 4115');
    print( 5,2,'交易名称:二级账户批量扣款');

    print( 7,5,'    账  号          子户序号  子  户  名  称           交易金额        备       注');
