  <PACKAGE DEVICE="PRINTER" DESC="请打印业务凭证" LINESPACE="24" CHECK="#CLLX=2">
      NOTHING
  </PACKAGE>
  <PRINT>"{
    dim @file  as file;
    dim @txtime as string;
    @txtime=substr($SYSTIME,0,2)+':'+substr($SYSTIME,2,2)+':'+substr($SYSTIME,4,2);
    @file=openfile('../tmp/'+$KINBR+$TTYNAME,'r');
