<TRANSACTION RIGHTS="11110100" TITLE="5344 行名行号查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD44=$HDATE;
    $FD3 =$KINBR;
    $FD7=$TLRNO;

    $FD81 = #BAKCODE;
    $FD82 = #LNAME;
    $FD22 = #OBRNOFLAG;
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(5344                    行名行号查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5884)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="8"  COL="05" TYPE="%-10s" FUNCTIONS="T(行    号:)"/>
  <ITEM LINE="10" COL="05" TYPE="%-10s" FUNCTIONS="T(行    名:)"/>
  <ITEM LINE="12" COL="05" TYPE="%-10s" FUNCTIONS="T(参与类别:)"/>


  <ITEM LINE="8"  COL="15" NAME="#BAKCODE"  TYPE="%-12s" DEVICE="KEYBOARD" FUNCTIONS="T()"/>
  <ITEM LINE="10" COL="15" NAME="#LNAME"   TYPE="%-60s" DEVICE="KEYBOARD" FUNCTIONS="T()">
  <ONLOSTFOCUS>
  "{
	showmsg('支持多个条件查询 请用空格或者TAB键分割条件  如:山西 长治     商业  ');
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="12" COL="15" NAME="#OBRNOFLAG" TYPE="%-2s"    DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(09)">
    <SELECT LINE="12" COL="15" >
            <OPTION VALUE="09"  TITLE="0.全部"/>
            <OPTION VALUE="01"  TITLE="1.直接参与人行"/>
            <OPTION VALUE="02"  TITLE="2.直接参与国库"/>
            <OPTION VALUE="03"  TITLE="3.EIS转换中心"/>
            <OPTION VALUE="04"  TITLE="4.直接参与商业银行"/>
            <OPTION VALUE="05"  TITLE="5.开户特许直接参与者"/>
            <OPTION VALUE="06"  TITLE="6.开户特许间接参与者"/>
            <OPTION VALUE="07"  TITLE="7.间接参与者"/>
            <OPTION VALUE="08"  TITLE="8.未开户特许直接参与者(债券)"/>
    </SELECT>
   </ITEM>


  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />

  </VARIABLE>
  <REQUEST>
</REQUEST>

<RESPONSE>
    <PACKAGE>
    NOTHING
    </PACKAGE>
    <PRINT>"{
        }"
    </PRINT>
</RESPONSE>

<OUTPUT>
</OUTPUT>
</TRANSACTION>

