<ITEM LINE="1"   COL="12" NAME="#ACTNO3"   TYPE="%-32s"  DEVICE="KEYBOARD" VISABLE="TRUE" FUNCTIONS="V(NB)">
<ONLOSTFOCUS>
	"{
    #ACTNO_2=#ACTNO3;
		if(substr(#ACTNO_2,0,6)=='621223' or substr(#ACTNO_2,0,6)=='621780')
		{
			showmsg('卡介质不允许手输卡号，请重新选择读取方式！');
    	return(false);
		}
		if(len(delspace(#ACTNO_2))==13 and (substr(#ACTNO_2,0,1)=='1'))
		{
			showmsg('存折介质不允许手输号码，请重新选择读取方式！');
    	return(false);
		}
	}"
	</ONLOSTFOCUS>
</ITEM>

