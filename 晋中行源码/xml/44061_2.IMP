	@tota = callserver();
	$TXNO=@baktxno;
	if($FD12!='0000'){
		showmsg(geterror());
		return(false);
	}
	setitems('#FD102_1#FD102_2#FD102_3#FD102_4#FD102_5#FD102_6#FD102_7#FD102_8#FD102_9#FD102_10#FD102_11#FD102_12#FD102_13#FD102_14#FD102_15#FD102_16#FD102_17#FD102_18#FD102_19#FD102_20#FD102_21#FD102_22#FD102_23#FD102_24#FD102_25#FD102_26#FD102_27#FD102_28#FD102_29#FD102_30#FD102_31#FD102_32#FD102_33#FD102_34',$FD102);
  //showmsg('#FD102_34='+#FD102_34);
	#CD102=$FD102;
	#CUSTNAME_2 = #FD102_17;
	#BIZHONG_2 = #FD102_19;
	#LAVBAL_2 = itoa(val(#FD102_18,0.01),'%.2f');
	#PRODNAME_2 = #FD102_31;
		#MIMA = #FD102_6;
	if('Y' != #MIMA){
	#MIMA='N';
		enable(#DRAW_PWD, false);
	}

enable(#CITYPE, false);
enable(#CINO, false);
//showmsg('账户类型:=['+#FD102_22+'], #KHJE=['+itoa(#KHJE, '%17.2f')+']');
#XZCAVBAL_2 = #KHJE;
enable(#XZCAVBAL_2, false);
if('Y' == #FD102_9)
{
//	showmsg('证件支取');
	#CITYPE=#FD102_16;
	enable(#CITYPE, false);
	#CINO=#FD102_10;
	enable(#CINO, true);
}
  //  if(delspace(#FD102_4) == '')    //当凭证号为空时,即输入是对公账号
  show(#CPDATE, false);
  show(#DSCPT, false);
enable(#VOCTYPE_2, false);
enable(#VOCNUM_2, false);
  if('1' != #FD102_22)
    {
        while(substr(@tota, @i, 4)=='102L'){
             @i = @i + 31;
        }
        @list2=substr(@tota, 0 , @i+1);
        setsel(#DSCPT,@list2,'o5t20o1v3o2');

        @list1=substr(@tota, @i, len(@tota)-@i);
        setsel(#VOCTYPE_2,@list1,'o5t20o1v3o2');

		enable(#VOCTYPE_2, true);
		enable(#VOCNUM_2, true);
		show(#L10,true);
		show(#L11,true);
		show(#L12,true);
		show(#CPDATE,true);
		show(#MIMA,true);
		show(#DSCPT,true);

	}
	else
	{
	  ;
		#MMFLAG_2  = #FD102_6;
		#ZJFLAG_2  = #FD102_9;
		// showmsg(@tota);
    setsel(#VOCTYPE_2,@tota,'o5t20o1v3o2');
    //showmsg(#VOCTYPE_2);
		#VOCTYPE_2 = #FD102_3;
		#VOCNUM_2  = #FD102_4;
		#FD102_21='   ';
		show(#L7,true);
		show(#L8,true);

		show(#MMFLAG_2,true);
		show(#ZJFLAG_2,true);
	}
   #CPH=$FD24;
   if(#CPH=='142' or #CPH=='144')
  {
  	  dim @tota1 as string;
			dim @dkid as string;
			initfd();
			commsend_001();
			$FD16='9598';
			$FD123 = 'select ac_id from mdm_ac_rel where ac_no='+chr(39)+#DKZH+chr(39)+'';
			@tota1 = callserver();
			if($FD12!='0000')
			{
				showmsg(geterror());
				return(false);
			}
			if(@tota1!='')
			{
				@dkid = strfld(@tota1,'|',0);
				dim @tota2 as string;
			  initfd();
			  commsend_001();
			  $FD16='9598';
			  $FD123 = 'select pact_no from ln_mst where ac_id='+chr(39)+@dkid+chr(39)+''; 
			  @tota2 = callserver();
			  if($FD12!='0000')
			  {
			  	showmsg(geterror());
			  	return(false);
			  }              
			  if(@tota2!='') 	
			  {
			    #JJH = strfld(@tota2,'|',0); 
			    dim @tota3 as string;
			    initfd();
			    commsend_001();
			    $FD16='9598';
			    $FD123 = 'select * from ln_auth where type='+chr(39)+'13'+chr(39)+' and sts='+chr(39)+'X'+chr(39)+' and pact_no='+chr(39)+#JJH+chr(39)+' and pay_ac_no='+chr(39)+ #ACTNO_2+chr(39)+'';
			    @tota3 = callserver();
			    if($FD12!='0000')
			    {
			    	showmsg(geterror());
			    	return(false);
			    }              
			    if(@tota3=='') 	
			    {  
			    	showmsg('担保保证金取款需要审批');
		        return(false);
			    } 
			  }
			 } 	
  }
	showblock('ZZH_PGE2',true,true);
	
  }"
  </ONLOSTFOCUS>
