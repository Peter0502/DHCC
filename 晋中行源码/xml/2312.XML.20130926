<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="2312 机构客户信息维护"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	commsend_001();
	$FD28=#KHH;
	$FD62=#JGDMZH;
	$FD33=#SYFRZH;
	$FD81=#FRDBMC;
	$FD82=#FRSFZH;
	$FD58=#QYDKKH;
	$FD71=#SYZXS;
	$FD67=#DWXZ;
	$FD41=getitems('#QYKBZJ');
	$FD64=#XYD;
	$FD83=#JBHKHH;
	$FD25=#SSSJDW;
	$FD26=#SSZGDW;
	$FD27=#ZJDBDW;
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<FIRSTWORK>
"
{
  #PIC='';
  #PIC1='';
}
"
</FIRSTWORK>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(2312                        机构客户信息维护)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1305)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
 <!--  输入变量  -->
 
  <ITEM LINE="5" COL="12" TYPE="%-10s" FUNCTIONS="T(客 户 号:)"/>
  <ITEM NAME="#KHH" INPUT="TEXT" LINE="5" COL="22" TYPE="%8s" FUNCTIONS="">
  <ONLOSTFOCUS>
 "{	initfd();
 		dim @tota as string;	
		commsend_001();
 		$FD16='9780';
		$FD28=#KHH;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#JGDMZH=delspace($FD62);
		#SYFRZH=delspace($FD33);
		#FRDBMC=delspace($FD81);
		#FRSFZH=delspace($FD82);
		#QYDKKH=delspace($FD58);
		#SYZXS=delspace($FD71);
		#DWXZ=delspace($FD67);
		setitems('#QYKBZJ',$FD41);
		#XYD=delspace($FD64);
		#JBHKHH=delspace($FD83);
		#SSSJDW=delspace($FD25);
		#SSZGDW=delspace($FD26);
		#ZJDBDW=delspace($FD27);
		show(#JGDMZH,true);
		enable(#JGDMZH,false);
		show(#SYFRZH,true);
		enable(#SYFRZH,false);
		show(#FRDBMC,true);
		show(#FRSFZH,true);
		show(#QYDKKH,true);
		show(#SYZXS,true);
		show(#DWXZ,true);
		show(#QYKBZJ,true);
		show(#XYD,true);
		show(#JBHKHH,true);
		show(#SSSJDW,true);
		show(#SSZGDW,true);
		show(#ZJDBDW,true);
 }"
 </ONLOSTFOCUS>
 </ITEM>
 
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(--------------------------------------------------------------------------)"/>
  <ITEM LINE="8"  COL="4"  TYPE="%-14s" FUNCTIONS="T(机构代码证号:)"/>
  <!--added by liuyong 2009-9-1 显示图片信息 start-->
 <ITEM LINE="8" COL="43" TYPE="%-10s" FUNCTIONS="T(照    片:)"/>
 <ITEM NAME="#SETPIC" TYPE="%10s" FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
     dim @txnobak as string;
     dim @tatl as string;
     @txnobak = $TXNO;
     $TXNO = '9666';
     $FD28 = #KHH;
     $FD72 = '1';//2代表印件
     commsend_001();
     @tatl = callserver();
     if($FD12 !='0000')
     {
         
     }
     $TXNO = @txnobak;
     setpicture(#PIC,1,1);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <!--added by liuyong 2009-9-1 显示图片信息 end-->
  <ITEM LINE="9"  COL="4"  TYPE="%-14s" FUNCTIONS="T(事业法人证号:)"/>
