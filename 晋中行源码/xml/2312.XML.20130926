  <ITEM LINE="11" COL="4"  TYPE="%-14s" FUNCTIONS="T(法人身份证号:)"/>
  <ITEM LINE="12" COL="4"  TYPE="%-14s" FUNCTIONS="T(企业贷款卡号:)"/>
  <ITEM LINE="13" COL="4"  TYPE="%-14s" FUNCTIONS="T(所有制  形式:!)"/>
