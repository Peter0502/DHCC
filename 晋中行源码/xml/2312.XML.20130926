  <ITEM NAME="#JBHKHH" INPUT="TEXT" LINE="15" COL="18" TYPE="%-50s" FUNCTIONS=""/>
  <ITEM NAME="#SSSJDW" INPUT="TEXT" LINE="16" COL="18" TYPE="%-50s" FUNCTIONS=""/>
  <ITEM NAME="#SSZGDW" INPUT="TEXT" LINE="17" COL="18" TYPE="%-50s" FUNCTIONS=""/>
