		<OPTION VALUE="1" TITLE="全额补款"/>
		<OPTION VALUE="2" TITLE="差额补款"/>
		<OPTION VALUE="3" TITLE="自收自支"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#QYKBZJ" INPUT="TEXT" LINE="14" COL="18" TYPE="%17.2f" FUNCTIONS=""/>
