  <ITEM LINE="14" COL="4"  TYPE="%-14s" FUNCTIONS="T(企业开办资金:)"/>
  <ITEM LINE="14" COL="43" TYPE="%-10s" FUNCTIONS="T(信 誉 度:!)"/>
  <ITEM LINE="15" COL="4"  TYPE="%-14s" FUNCTIONS="T(基本户开户行:)"/>
