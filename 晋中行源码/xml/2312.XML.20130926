		<OPTION VALUE="1" TITLE="国有"/>
		<OPTION VALUE="2" TITLE="民办"/>
		<OPTION VALUE="3" TITLE="其他"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#DWXZ" INPUT="SELECT" LINE="13" COL="54" TYPE="%-1s" FUNCTIONS="">
