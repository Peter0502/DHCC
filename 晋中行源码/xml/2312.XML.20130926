  <ITEM NAME="#QYDKKH" INPUT="TEXT" LINE="12" COL="18" TYPE="%-16s" FUNCTIONS=""/>
  <!--added by liuyong 2009-9-15 上传图片信息 start-->
  <ITEM LINE="12" COL="54" TYPE="%-10s" LINES="220" COLS="120" FUNCTIONS="A(2)" NAME="#PIC1" VISABLE="FALSE"/>
  <ITEM NAME="#SETPICCONTENT1" FUNCTIONS="" TYPE="%-10s">
  <ONLOSTFOCUS>
  "{
     setvfile(true,true);
     addvfile(#PIC1);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#TRANPIC1" FUNCTIONS="" TYPE="%-10s">
  <ONLOSTFOCUS>
  "{
     if(#PIC1=='')
     {
       $FD70='0';
     }
     else
     {
       $FD70='1';
     }
     dim @txnobak as string;
     @txnobak = $TXNO;
     $TXNO = '9667';
     $FD28 = #KHH;
     $FD62 = '';
     $FD72 = '2';
     commsend_001();
     callserver();

     if($FD12 !='0000')
     {
         showmsg(geterror());
