  <ITEM LINE="9" COL="15" TYPE="%-10s" FUNCTIONS="T(程序:)"/>


  <ITEM NAME="#NAME" 		INPUT="TEXT" LINE="9" COL="25" 	TYPE="%-24s" FUNCTIONS="T()V(NB)">
      <ONLOSTFOCUS>
	"{
		dim @txno_bak as string;
		@txno_bak=$TXNO;
