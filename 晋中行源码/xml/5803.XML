<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="5802 保证金利率查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD31 = #RATEACCT;
	$FD32 = #SAFEMONEYACCT;
	$FD33 = #PROTOCOL;
	$FD67 = #SAFEMONEYTYPE;
	$FD44 = #DEPOSITDATE;
	
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(5803                        保证金利率查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4372)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM  LINE="8" COL="12" TYPE="%-10s" FUNCTIONS="T(查询条件)"/>
  <ITEM LINE="9"  COL="12"  TYPE="%-60s" FUNCTIONS=
   "T(--------------------------------------)"/>
  
  <!--输入变量-->
  <ITEM LINE="10" COL="12" TYPE="%-11s" FUNCTIONS="T(保证金类型:)"/>
  <ITEM LINE="10" COL="42" TYPE="%-13s" FUNCTIONS="T(承兑协议编号:)"/>
  <ITEM LINE="12" COL="12" TYPE="%-11s" FUNCTIONS="T(保证金帐号:)"/>
  <ITEM LINE="14" COL="12" TYPE="%-9s" FUNCTIONS="T(收息帐号:)"/>
  <ITEM LINE="16" COL="12" TYPE="%-15s" FUNCTIONS="T(保证金存款日期:)"/>
  
  
  <ITEM NAME="#SAFEMONEYTYPE" INPUT="SELECT" LINE="10" COL="23" TYPE="%-1s" FUNCTIONS="">
