<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9203 贷款放/还款计划查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD38=#ACTNO;
	$FD70=#STAT;
	$FD51=space(3,'0');

}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
    
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(9203                      贷款放/还款计划查询)"/>    
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8203)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->

  <ITEM LINE="9" 	COL="25" TYPE="%-15s"  	FUNCTIONS="T(放款/还款状态:)"/>
  <ITEM LINE="11" COL="25" TYPE="%-15s" 	FUNCTIONS="T(账         号:)"/>


  <ITEM NAME="#STAT" 		INPUT="SELECT" LINE="9" COL="40" 	TYPE="%-1s" FUNCTIONS="">
    	<SELECT>
  				<OPTION VALUE="0" TITLE="1.未还"/>
  				<OPTION VALUE="1" TITLE="1.已还"/>
  				<OPTION VALUE="2" TITLE="1.未放"/>
  				<OPTION VALUE="3" TITLE="1.已放"/>
  	</SELECT>
 </ITEM>
  <ITEM NAME="#ACTNO" 	INPUT="TEXT" LINE="11" COL="40" TYPE="%-19s" FUNCTIONS="V(NB)"/>
  <ONLOSTFOCUS>
  "{
  	dim @txno as string;
		@txno=$TXNO;
		$TXNO='6700';
		$FD30=#ACTNO;
		$FD67='3';
    callserver();
    $TXNO=@txno;    
    if($FD12!='0000')
       {showmsg(geterror());
        return(false);
    }
    if($KINBR!=$FD91)
    {
        showmsg('只能查询本机构帐户!');
        return(false);
    } 
  "}
  </ONLOSTFOCUS>
  <!--输出变量--->
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">


</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE>
	NOTHING
	</PACKAGE>
</RESPONSE>

</TRANSACTION>


