<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="5701 银行承兑汇票查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
		initfd();
		commsend_001();
        $FD12 = '4338';
        $FD29 = #QFDATE2;
        $FD39 = space(16);
        $FD40 = itoa(val(#XCPAVBAL1,100),'%016d');  
        $FD41 = itoa(val(#XCPAVBAL2,100),'%016d');  
        $FD44 = #QFDATE1;
        $FD45 = #DQDATE1;
        $FD46 = #DQDATE2;
        $FD59 = #HPNUM;
        $FD67 = #HPFLAG;
        commsend_001();
        callserver();
        $FD16 = '4338';
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
   "T(5701                        银行承兑汇票查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4338)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="6"  COL="10"  TYPE="%-14s" FUNCTIONS="T([查询条件])"/>
  <ITEM LINE="7"  COL="10"  TYPE="%-60s" FUNCTIONS="T(------------------------------------------------------------)"/>
  <ITEM LINE="9"  COL="10"  TYPE="%-14s" FUNCTIONS="T(汇票号码:)"/>
  <ITEM LINE="10" COL="10"  TYPE="%-10s" FUNCTIONS="T(出票金额:)"/>
  <ITEM LINE="10" COL="43"  TYPE="%-3s"  FUNCTIONS="T(-->)"/>
  <ITEM LINE="11" COL="10"  TYPE="%-10s" FUNCTIONS="T(签发日期:)"/>
  <ITEM LINE="11" COL="30"  TYPE="%-3s"  FUNCTIONS="T(-->)"/>
  <ITEM LINE="12" COL="10"  TYPE="%-10s" FUNCTIONS="T(到期日期:)"/>
  <ITEM LINE="12" COL="30"  TYPE="%-3s"  FUNCTIONS="T(-->)"/>
  <ITEM LINE="13" COL="10"  TYPE="%-10s" FUNCTIONS="T(汇票状态:!)"/>


  <ITEM LINE="9"  COL="20" TYPE="%-16s"  NAME="#HPNUM"    DEVICE="KEYBOARD" FUNCTIONS=""/>

  <ITEM LINE="10"  COL="20" TYPE="%16.2f" NAME="#CPAVBAL1"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)J(#XCPAVBAL1)"/>
  <ITEM TYPE="%-16s" NAME="#XCPAVBAL1" FUNCTIONS="X(#CPAVBAL1)"/>
  <ITEM LINE="10"  COL="47" TYPE="%16.2f" NAME="#CPAVBAL2"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)J(#XCPAVBAL2)"/>
  <ITEM TYPE="%-16s" NAME="#XCPAVBAL2" FUNCTIONS="X(#CPAVBAL2)"/>

  <ITEM LINE="11"  COL="20" TYPE="%-8s"  NAME="#QFDATE1"    DEVICE="KEYBOARD" FUNCTIONS="D()"/>
  <ITEM LINE="11"  COL="35" TYPE="%-8s"  NAME="#QFDATE2"    DEVICE="KEYBOARD" FUNCTIONS="D()"/>

  <ITEM LINE="12"  COL="20" TYPE="%-8s"  NAME="#DQDATE1"    DEVICE="KEYBOARD" FUNCTIONS="D()"/>
  <ITEM LINE="12"  COL="35" TYPE="%-8s"  NAME="#DQDATE2"    DEVICE="KEYBOARD" FUNCTIONS="D()"/>

  <ITEM LINE="13"  COL="20" TYPE="%-1s"  NAME="#HPFLAG"    DEVICE="KEYBOARD" FUNCTIONS="T( )">
    <SELECT LINE="0" COL="0">
        <OPTION VALUE="1" TITLE="签发"/>
        <OPTION VALUE="2" TITLE="核销"/>
        <OPTION VALUE="3" TITLE="退票"/>
        <OPTION VALUE="4" TITLE="托收"/>
    </SELECT>
  </ITEM>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>

<OUTPUT>
<PACKAGE>
000$9999=9P130000A显示内容N505
</PACKAGE>

<FORMAT NAME="P130000A">
"03               交易成功!
"
</FORMAT>
</OUTPUT>
</TRANSACTION>
