<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000010" TITLE="M31 指纹设备维护"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE UPLOOP="FALSE">
  <ITEM LINE="3"  COL="3" TYPE="%-50s" FUNCTIONS=
    "T(1198                       操作员指纹设备维护)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="10"  COL="15" TYPE="%-10s" FUNCTIONS="T(操作员号: )"/>
  <ITEM LINE="12"  COL="15" TYPE="%-10s" FUNCTIONS="T(使用状态: )"/>
  <ITEM LINE="10"  COL="25" NAME="#TLRNO1" TYPE="%4d" DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>"{
	dim @tota as string;
	#STAT=getconfig(#TLRNO1+'.FINGER');
	if(#STAT!='1'){
		#STAT='0';
	}
	show(#STAT,true);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="12"  COL="25" NAME="#STAT" TYPE="%1d" INPUT="SELECT" FUNCTIONS="V(NB)">
  <SELECT>
	<OPTION VALUE="0" TITLE="0.无指纹设备"/>
	<OPTION VALUE="1" TITLE="1.有指纹设备"/>
  </SELECT>
  </ITEM>

  <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
  <ONCLICK>"{
	setconfig(#TLRNO1+'.FINGER',#STAT);
	runoutput();
	rerun();
  }"
  </ONCLICK>
  </VARIABLE>

  <REQUEST><PACKAGE>
  </PACKAGE></REQUEST>

<RESPONSE>
	<PACKAGE DEVICE="SCRREN" DESC="任意键继续">
	NOTHING
	</PACKAGE>
	<PRINT>"{ 
		print(3,25,'柜员指纹状态');
		print(7,20,'柜员号:[4  ]',#TLRNO1);
		if(#STAT=='0'){
			print(9,20,'状  态:不使用指纹设备');
		}else{
			print(9,20,'状  态:使用指纹设备');
		}
    }"</PRINT>
</RESPONSE>
</TRANSACTION>
