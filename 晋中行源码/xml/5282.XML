<?xml version="1.0" encoding="GBK" ?> 
<TRANSACTION RIGHTS="00000000" TITLE="5282  提入票据查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
  "{ commsend_001();
     $FD44=#TXDATE;
     $FD39=#TXAMT;
     $FD67=#NOSTAT;
     $FD68=#TRMODE;
     $FD91=#BRNO;
   }" 
</COMMSEND>
<COMMRECV>
  "{ commrecv_001();
   }"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
	 "T(5282                          提入票据查询)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
	 "T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4282)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="6" COL="33" TYPE="%-12s" FUNCTIONS="T(【查询条件】)"/>
  <!--输入变量-->
  <ITEM LINE="7" COL="3" TYPE="%-74s" FUNCTIONS=
	"T(--------------------------------------------------------------------------)"/>
	<ITEM LINE="9" COL="12" TYPE="%-10s" FUNCTIONS="T(传递方式!)"/>
	<ITEM LINE="11" COL="12" TYPE="%-10s" FUNCTIONS="T(票据状态!)"/>
	<ITEM LINE="13" COL="12" TYPE="%-10s" FUNCTIONS="T(机构编码:)"/>
	<ITEM LINE="15" COL="12" TYPE="%-10s" FUNCTIONS="T(交易日期: )"/>
  <ITEM LINE="17" COL="12" TYPE="%-10s" FUNCTIONS="T(金    额: )"/>
  <ITEM NAME="#TRMODE" INPUT="SELECT" LINE="9" COL="22" TYPE="%-1s" FUNCTIONS="T()">
	  <SELECT>
      <OPTION VALUE="1" TITLE="0. 手工传递票据"/>
      <OPTION VALUE="2" TITLE="1. 自动传递票据"/> 
	  </SELECT>
  </ITEM>
  <ITEM NAME="#NOSTAT" INPUT="SELECT" LINE="11" COL="22" TYPE="%-1s" FUNCTIONS="T()">
	  <SELECT>
      <OPTION VALUE="1" TITLE="0. 未打印"/>
      <OPTION VALUE="2" TITLE="1. 已经打印"/>
      <OPTION VALUE="3" TITLE="2. 未核销"/>
      <OPTION VALUE="4" TITLE="3. 已核销"/>
      <OPTION VALUE="5" TITLE="4. 已记帐"/>
      <OPTION VALUE="6" TITLE="5. 已退票"/>
	  </SELECT>
  </ITEM>
  <ITEM NAME="#BRNO" INPUT="TEXT" LINE="13" COL="22" TYPE="%-5s" FUNCTIONS="T()"/>
  <ITEM NAME="#TXDATE" INPUT="TEXT" LINE="15" COL="22" TYPE="%-8s" FUNCTIONS="T()D()"/>
  <ITEM NAME="#TXAMT" INPUT="TEXT" LINE="17" COL="22" TYPE="%16.2f" FUNCTIONS="T()X()"/>
  <!--------------有些变量没有用,只是为了公共脚本执行时不报错------------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="20" COL="60"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="显示内容">
      NOTHING
</PACKAGE>
</RESPONSE>
</TRANSACTION>
  
