<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111111000000000000000000000" TITLE="5552 节点代码查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
  commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(5552                          节点代码查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=5572)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="10" COL="25" TYPE="%-10s"  FUNCTIONS="T(节点代码:)"/>
  <ITEM LINE="12" COL="25" TYPE="%-10s" FUNCTIONS="T(节点名称:)"/>
  <ITEM LINE="14" COL="25" TYPE="%-10s"  FUNCTIONS="T(节点类型:)"/>
 

  <!--输入变量-->
  <ITEM NAME="#ORGCODE"   LINE="10" COL="36" TYPE="%-12s"  INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#ORGNAME"   LINE="12" COL="36" TYPE="%-30s" INPUT="TEXT" FUNCTIONS="T()"/>


  <ITEM NAME="#ORGTYPE" LINE="14" COL="36" TYPE="%-1s" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="">
  <SELECT LINE="14" COL="46">
     <OPTION VALUE="0" TITLE="中心"/>
     <OPTION VALUE="1" TITLE="征收机关"/>
     <OPTION VALUE="2" TITLE="商业银行"/>
  </SELECT>
  </ITEM>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  	<ONCLICK>
  		"{
        
        dim @tota as string;
        dim @selid as number;
        initfd();
        commsend_001();
        
	      $FD16='5572';
	      $FD58=#ORGCODE;
	      $FD25=#ORGNAME;
	      $FD67=#ORGTYPE;
	      @tota=calltips();
	      if($FD12!='0000'){
	           showmsg(geterror());
	           return(false);
	         }
	     if(autolist(@tota)){
            @selid=list_work();
            if(@selid &gt;= 0){
                 showmsg(list_gettext(@selid,1));
            }
       }
	     
	     //savefiletxt('../tmp/'+$KINBR+$TTYNAME,@tota);
	     //call('fview ../tmp/'+$KINBR+$TTYNAME);
	}"</ONCLICK>
</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
</RESPONSE>
</TRANSACTION>
