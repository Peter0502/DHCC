<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5252 中心提入通存"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	
    	$FD16='4252';	
 	    $MSGTYPE='03001';
     	$FD11='0';
     	
      $FD30=getitems('#ACTNO24');          
      $FD39=getitems('#TXAMT');  
      
     	// $FD67=getseltitle(#BRF,#BILL_TYPE);      	      

     	$FD81=getseltitle(#BRF,#BRF); 
     	$FD83=getitems('#PAYEE_BR_NAME'); 
     	
     	// showmsg($FD81);   
     	   	
     	$FD89=getitems('#BRF');       	
     	$FD90=getitems('#BILL_TYPE');   
	
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';

}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5252                          中心提入通存)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4252)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  

  <ITEM NAME="#ACTNO24"   TYPE="%-24s" FUNCTIONS=""/>
 
  <ITEM LINE="6"  COL="5" TYPE="%-12s" FUNCTIONS="T(收款人账号:)"/>  
  <ITEM NAME="#PAYEE_AC_NO" INPUT="TEXT" LINE="6"  COL="17" TYPE="%-18s"  FUNCTIONS="V(NB)" >
  <ONLOSTFOCUS>"{
    	dim @baktxno as string;  
    	dim @tota as string;
    	@baktxno=$TXNO;



     	$FD2=$OPNBR;
     	$FD3=$KINBR;
     	$FD5=$HDATE;
     	$FD7=$TLRNO;
     	$FD10=$TTYNAME;
     	$FD16='9753';
     	$FD79=#PASSWD; 

 			@tota=callserver();
       if($FD12!='0000'){
           showmsg(geterror());
           return(false);
       }
       
      setsel(#BRF,substr(@tota,0,21*62),'o5t10o1v3o2');     
 	
 	
 	  $MSGTYPE='03001';
    $FD11='0';
     	
     	
      $FD30=getitems('#PAYEE_AC_NO');  
      $FD31=getitems('#PAYEE_AC_NO');   
         	  	
    	$FD2=$OPNBR;
    	$FD3=$KINBR;
    	$FD5=$HDATE;
    	$FD7=$TLRNO;
    	$FD10=$TTYNAME;
    	$FD16='9776';  
    	$FD79=#PASSWD;
    	
    	callserver();
    	
    	$TXNO=@baktxno;
    	if($FD12!='0000'){
    		showmsg(geterror());
    		return(false);
    	}

    	setitems('#ACTNO24',$FD30);  
    	setitems('#PAYEE_NAME',$FD25);    
    	setitems('#PAYEE_BR_NAME',$FD83);  
    	    	
    	showblock('ACTINFO',true,true);	 	
 	 
  }"
  </ONLOSTFOCUS>
  </ITEM> 

  <BLOCK NAME="ACTINFO" LINE="7" COL="0" VISABLE="FALSE" >  
  
  <ITEM LINE="0"  COL="5" TYPE="%-12s" FUNCTIONS="T(户      名:)"/>  
  <ITEM LINE="0"  COL="17"  NAME="#PAYEE_NAME" INPUT="TEXT" TYPE="%-50s" ENABLE="FALSE" FUNCTIONS="V(NB)"/>

  <ITEM LINE="1"  COL="5" TYPE="%-12s" FUNCTIONS="T(开户行行名:)"/>  
  <ITEM  LINE="1"  COL="17"  NAME="#PAYEE_BR_NAME" INPUT="TEXT" TYPE="%-50s" ENABLE="FALSE" FUNCTIONS=""/>
     
  </BLOCK>       


  <ITEM LINE="10"  COL="5" TYPE="%-12s" FUNCTIONS="T(金      额:)"/>   
   <ITEM LINE="10"  COL="17" NAME="#TXAMT" INPUT="TEXT" TYPE="%17.2f" FUNCTIONS="0.00"/>   
 
   <ITEM LINE="12"  COL="5" TYPE="%-10s" FUNCTIONS="T(凭证种类:!)"/>
  <ITEM NAME="#BILL_TYPE" LINE="12"  COL="15" TYPE="%-3s"   INPUT="SELECT" FUNCTIONS="T(120)" >
	<SELECT>
	
		<OPTION VALUE="120" TITLE="0.进帐单"/>
		<OPTION VALUE="131" TITLE="1.电子记账凭证"/>	
	</SELECT>
  </ITEM>  
  
   <ITEM LINE="14"  COL="5" TYPE="%-10s" FUNCTIONS="T(摘    要:!)"/>
  <ITEM NAME="#BRF" LINE="14"  COL="15" TYPE="%-3s"   INPUT="SELECT" FUNCTIONS="T(001)" >
	<SELECT>
	
		<OPTION VALUE="001" TITLE="0.货款    "/>
	
					
		
	</SELECT>
  </ITEM>    
 
    
  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="中心提入通存成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'中 心 提 入 通 存 成 功 ！');

				
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>





