<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6806 行内贴现审核"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	$MSGTYPE='03001';
	dim @baktxno as string;
	@baktxno=$TXNO;
	$TXNO='9986';
	$FD12='4916';
	$FD25=#NAME;
    $FD45=#MDATE;
    $FD48=#WDAYS;
    $FD60=#PNNO;
    $FD62=#PANO;
    $FD67=#ACTYPE;
    $FD68=#LIND;
    $FD83=#ACNO;
    $FD84=getitems('#PINT');
    $FD90=#NOTYPE;
    $FD100=getitems('#TINT#AMT');
	commsend_001();
	callserver();
	if($FD12!='0000')
		 {
			showmsg(geterror());
			return(false);
		 }
	$FD16='4916';
	$MSGTYPE='03001';
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
			showmsg('交易成功!');
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(6806                        行内贴现审核)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=6806)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="5"  COL="5" TYPE="%-14s" FUNCTIONS="T(贴现协议编号:)"/>
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(-----------------------------【回显信息】----------------------------------)"/>
  <ITEM LINE="7"  COL="5" TYPE="%-10s" FUNCTIONS="T(转账类型!)"/>
  <ITEM LINE="7"  COL="28" TYPE="%-16s" FUNCTIONS="T(贴现申请人账号:)"/>

  <ITEM LINE="8"  COL="5" TYPE="%-16s" FUNCTIONS="T(贴现申请人户名:)"/>
  
  
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴 现 率:)"/>
  <ITEM LINE="9"  COL="28" TYPE="%-12s" FUNCTIONS="T(异地同城标!)"/>
  <ITEM LINE="9"  COL="52" TYPE="%-10s" FUNCTIONS="T(在途天数:)"/>
  
  <ITEM LINE="10"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴现利息:)"/>
  
  <ITEM LINE="15"  COL="5" TYPE="%-10s" FUNCTIONS="T(票据类型!)"/>
  <ITEM LINE="15"  COL="50" TYPE="%-10s" FUNCTIONS="T(票 据 号:)"/>
  <ITEM LINE="16"  COL="5" TYPE="%-10s" FUNCTIONS="T(汇票金额:)"/>
  <ITEM LINE="16"  COL="50" TYPE="%-12s" FUNCTIONS="T(汇票到期日:)"/>
 
  
  <!--输入变量-->
  <ITEM NAME="#PANO" LINE="5" COL="20" TYPE="%-16s" INPUT="TEXT" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
 "{
    commsend_001();
 		$FD16='9860';
		$FD62=#PANO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#NAME=$FD25;
		#MDATE=$FD45;
		#WDAYS=$FD48;
		#PNNO=$FD60;
		#ACTYPE=$FD67;
		#LIND=$FD68;
		#ACNO=$FD83;
		setitems('#PINT',$FD84);
		#NOTYPE=$FD90;
		setitems('#TINT#AMT',$FD100);
		show(#ACTYPE,true);
		show(#ACNO,true);
		show(#NAME,true);
		show(#PINT,true);
		show(#LIND,true);
		show(#WDAYS,true);
		show(#TINT,true);
		show(#NOTYPE,true);
		show(#PNNO,true);
		show(#AMT,true);
		show(#MDATE,true);
 }"
 </ONLOSTFOCUS>
 </ITEM>
  <ITEM NAME="#ACTYPE" LINE="7" COL="15" TYPE="%-1s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="1" TITLE="本行"/>
		<OPTION VALUE="2" TITLE="同城"/>
		<OPTION VALUE="3" TITLE="大额"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#ACNO" LINE="7" COL="44" TYPE="%-32s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>

  <ITEM NAME="#NAME" LINE="8" COL="21" TYPE="%-50s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#PINT" LINE="9" COL="15" TYPE="%9.6f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM LINE="9"  COL="25" TYPE="%-2s" FUNCTIONS="T(‰)"/>
  <ITEM NAME="#LIND" LINE="9" COL="40" TYPE="%-1s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="0" TITLE="本地"/>
		<OPTION VALUE="1" TITLE="异地"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#WDAYS" LINE="9" COL="62" TYPE="%4d" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#TINT" LINE="10" COL="15" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#NOTYPE" LINE="15" COL="15" TYPE="%-3s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="111" TITLE="银行承兑汇票"/>
		<OPTION VALUE="112" TITLE="商业承兑汇票"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#PNNO" LINE="15" COL="60" TYPE="%-16s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#AMT" LINE="16" COL="15" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#MDATE" LINE="16" COL="60" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>

  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
  <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
  <ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  </VARIABLE>

<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE DEVICE="SCREEN"  DESC="行内贴现审核">
NOTHING
</PACKAGE>
 <PRINT>"{
	  print( 7, 30,'审核成功');
	  print( 8, 20,'贴现 协议 编号 : [16]',#PANO);
   }"
</PRINT>
</RESPONSE>

<RESPONSE>
<PACKAGE DEVICE="PRINTER"  LINESPACE="38" DESC="打印凭条">
NOTHING
</PACKAGE>
 <PRINT>"{
      print( 5, 16,'[4]     [2]     [2]',#YEAR,#MON,#DAY);
	  print( 7,  8,'[5]',$FD2);
	  print( 7, 24,'4916 行内贴现审核');
	  print( 7, 46,'[4]',$FD7);
	  print( 7, 62,'[6]',$FD4);
      print( 8,  2,'贴现 协议 编号 : [16]',#PANO);
	  print( 9,  2,'审核成功');
  }"
 </PRINT>
</RESPONSE>

</TRANSACTION>
