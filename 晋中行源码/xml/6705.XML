<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00110000" TITLE="6705    再贷款信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
    $FD29=#ENDDATE;
    $FD38=#ACTNO;
    $FD40=getitems('#AMT');
    $FD44=#REGDATE;
    $FD60=#PROTID;
    $FD65=#TRLNO;
    $FD67=#RELOADTYPE;
    $FD70=#STATE;
    $FD84=getitems('#RATE');
    $FD91=#BRNOID;
    commsend_001();
   $FD16 = '4805';
    
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3"  TYPE="%-52s" FUNCTIONS=
    "T(6705                         再贷款信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=4805)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="6"  COL="10"  TYPE="%-12s" FUNCTIONS=
   "T(【查询条件】)"/>
   <ITEM LINE="7"  COL="10"  TYPE="%-60s" FUNCTIONS=
   "T(------------------------------------------------------------)"/>
   <ITEM LINE="9"     COL="10"  TYPE="%-12s" FUNCTIONS="T(状      态:!)"/>
   <ITEM LINE="10"    COL="10"  TYPE="%-12s" FUNCTIONS="T(协议  编号:)"/>
   <ITEM LINE="11"    COL="10"  TYPE="%-12s" FUNCTIONS="T(贷款  金额:)"/>
 
   <ITEM LINE="12"   COL="10"  TYPE="%-12s" FUNCTIONS="T(再贷款账号:)"/>
   <ITEM LINE="13"   COL="10"  TYPE="%-12s" FUNCTIONS="T(再贷款类型:!)"/>
   <ITEM LINE="13"   COL="44"  TYPE="%-10s" FUNCTIONS="T(利    率:)"/>
   <ITEM LINE="14"   COL="10"  TYPE="%-12s" FUNCTIONS="T(机构  编码:)"/>
   <ITEM LINE="14"   COL="44"  TYPE="%-10s" FUNCTIONS="T(操 作 员:)"/>
   <ITEM LINE="15"   COL="10"  TYPE="%-12s" FUNCTIONS="T(登记  日期:)"/>
  
   <ITEM LINE="15"   COL="44"  TYPE="%-10s" FUNCTIONS="T(到期日期:)"/>
  
   <ITEM LINE="9"    COL="22" NAME="#STATE" TYPE="%1s" INPUT="SELECT"  FUNCTIONS="">
    <SELECT>
			<OPTION VALUE ="0" TITLE= "0.录 入" />
			<OPTION VALUE ="1" TITLE= "1.已 记 帐" />
			<OPTION VALUE ="2" TITLE= "2.全部还款" />
		</SELECT>
	 </ITEM>
   <ITEM LINE="10"   COL="22" NAME="#PROTID"      TYPE="%-16s"   INPUT="TEXT"    FUNCTIONS=""/> 
   <ITEM LINE="11"   COL="22" NAME="#AMT"         TYPE="%17.2f"  INPUT="TEXT"    FUNCTIONS="J(#LANT)"/>
   <ITEM NAME="#LAMT" TYPE="%-16s" FUNCTIONS="X(#AMT)"/>
   <ITEM LINE="12"   COL="22" NAME="#ACTNO"        TYPE="%-20s"   INPUT="TEXT"    FUNCTIONS=""/> 
   <ITEM LINE="13"   COL="22" NAME="#RELOADTYPE"   TYPE="%1s"     INPUT="SELECT"  FUNCTIONS="">
    <SELECT>
			<OPTION VALUE ="1" TITLE="1.短期再贷款" />
			<OPTION VALUE ="2" TITLE="2.发展再贷款" />
		</SELECT>
	 </ITEM>
   <ITEM LINE="13" COL="54" NAME="#RATE"       TYPE="%9.6f"  INPUT="TEXT"    FUNCTIONS="T(0)"/> 
   <ITEM LINE="14" COL="22" NAME="#BRNOID"     TYPE="%-5s"   INPUT="TEXT"    FUNCTIONS=""/> 
   <ITEM LINE="14" COL="54" NAME="#TRLNO"      TYPE="%-7s"   INPUT="TEXT"    FUNCTIONS=""/>
   <ITEM LINE="15" COL="22" NAME="#REGDATE"    TYPE="%8s"    INPUT="TEXT"    FUNCTIONS="T(0)"/>
   <ITEM LINE="15" COL="54" NAME="#ENDDATE"    TYPE="%8s"    INPUT="TEXT"    FUNCTIONS="T(0)"/>
    
	 <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
	
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
	<ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
	<ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
	<ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
</VARIABLE>
<REQUEST>
</REQUEST>
