<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-50s" FUNCTIONS=
	"T(5637                       财政集中支付垫款户查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5627)"/>
  <ITEM NAME="#TXNAME" TYPE="%-12s" FUNCTIONS="T(财政集中支付垫款户查询)"/><!---交易名称--->
  <ITEM NAME="#TXNUMBER" TYPE="%-4s" FUNCTIONS="T(5627)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <BLOCK LINE="10" COL="10" >

  <ITEM LINE="5" COL="8" TYPE="%-14s" FUNCTIONS="T(交易机构代码: )"/>
  <ITEM LINE="5" COL="22" TYPE="%-5s" NAME="#TX_BR_NO" INPUT="TEXT" FUNCTIONS="T($KINBR)"/>
  
  <ITEM LINE="7" COL="8" TYPE="%-14s"  FUNCTIONS="T(财 政  编 码: )"/>
  <ITEM LINE="7" COL="22" TYPE="%-16s" NAME="#FIN_CODE" INPUT="TEXT" FUNCTIONS=""/>
  
  <ITEM LINE="9" COL="8" TYPE="%-14s" FUNCTIONS="T(支 付  类 型: )"/>
  <ITEM LINE="9" COL="22" TYPE="%-2s" NAME="#PAY_TYPE" INPUT="SELECT" FUNCTIONS="">
    <SELECT>
      <OPTION VALUE="1"  TITLE="直接支付" />
      <OPTION VALUE="2"  TITLE="授权支付" />
    </SELECT>
   </ITEM>
  
  </BLOCK>
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
    "{
        dim @tota as string; 
        dim @filename as string; 
        initfd();
	      commsend_001();
	      $FD16=#TXNO;
	      $FD21=#PAY_TYPE;
	      $FD64=#TX_BR_NO;
	      $FD59=#FIN_CODE;
	     
	      @tota=calltips();
        if( $FD12!='0000')
        {
            showmsg(geterror());
  		      return(false);
        }
       // showmsg('查询成功');
         if(autolist(@tota))
        {
            list_work();
        }
        
        //@filename='../tmp/'+$KINBR+$TTYNAME+#TXNO;
        //savefiletxt(@filename,@tota); 
        //call('fview '+@filename);  

    }"
  </ONCLICK> 
   </ITEM> 
</VARIABLE>
