	  <ONLOSTFOCUS>
		 "{
		   //showmsg($HDATE);
		   show(#HEDUI1,false);
   		 show(#HEDUI,false);
			 initfd();
			 commsend_002();
			 $FD16 = '8305';    //银行向财政读取执收单位单笔开票信息
			 //$FD86 = #YHDM;     //银行代码
