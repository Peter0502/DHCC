<RESPONSE>
  <PACKAGE DEVICE="SCREEN"  DESC="银行向财政提交单笔缴款成功">
       NOTHING
  </PACKAGE>
  <PRINT>
   "{
      dim @txtime as string;
      @txtime = substr($SYSTIME,0,2)+':'+substr($SYSTIME,2,2)+':'+substr($SYSTIME,4,2);
      print( 2,30,'银行向财政提交单笔缴款信息');
      print( 4,7,'财政代码:[20]   银行代码:[20]',#CZDM,#YHDM);   
      print( 5,7,'单位代码:[20]   开 票 ID:[20]',#DWDM,#KPID);        
      print( 6,7,'开票日期:[20]   金    额:[20]',#KPDATE,delspace(itoa(val(itoa(#AMT,'%-17.2f')),'%-016.0f')));  
      print( 7,7,'缴款流水号:[20] 付款人名称:[30]',#JJLSHAO,#FKRNAME);  
      print( 8,7,'付款人银行:[20] 付款人账号:[20]',#FKRBANK,#FKRACCNO);   
      print( 9,7,'付款行行号:[20] 收款人名称:[30]',#FKHNO,#SKRNAME);  
      print(10,7,'收款人银行:[20] 收款人账号:[20]',#SKRBANK,#SKRACCNO);    
      print(11,7,'网点代码:[20]   缴款机构:[20]',#WDDM,$BRNAME);                
      if(#JKFS == '1')
      {
        print(12,7,'现转标志：现金  交易金额(￥): [20]',itoa(#TXAMT,'%-17.2f'));
        print(13,7,'凭证类型: [20]   凭证号码: ',#VOCTYPE1); 
        print(14,7,'平台流水号:[13]  核心流水号:[13]',$FD52,$FD4);    
        print(15,7,'交易日期:[8] [8]  柜员:[4]',$HDATE,@txtime,$FD7); 
      } 
    }"
  </PRINT>
</RESPONSE>

