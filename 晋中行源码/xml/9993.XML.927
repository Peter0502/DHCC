			 callagn_new();
			 if($FD12 != '0000')
			 {
				 showmsg(geterror());
				 return(false);
			 }
			 else
			 {			
				#CZDM=$FD86;        //财政代码
				#YHDM=$FD87;        //银行代码
				#DWDM=$FD31;        //单位代码
				#KPID=$FD32;        //开票ID
