			<OPTION VALUE ="0" TITLE="0.信息正确" />
			<OPTION VALUE ="1" TITLE="1.信息有误" />
	  </SELECT> 
		<ONLOSTFOCUS>
		 "{		   
		   if(#HEDUI=='0') 
		   {
		   	loadpage('PAGE3');
			show(#HEDUI1,false);
   			show(#HEDUI,false);
		   }
		   else
		   {
		   	 goitem(#JJLSHAO,true);
		   	 enable(#YHDM,true);
		   }
		  }"
		</ONLOSTFOCUS>
