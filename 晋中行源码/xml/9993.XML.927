 </PAGE>
 <PAGE NAME="PAGE3">
	<ITEM LINE="07"   COL="11"  TYPE="%-10s"  FUNCTIONS="T(帐    号:)"/>
	<ITEM LINE="08"   COL="10"  TYPE="%-16s"  FUNCTIONS="T(【产品信息】)"/>
