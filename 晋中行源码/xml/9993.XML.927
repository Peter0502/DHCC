	<ITEM LINE="7"    COL="21"  NAME="#ACTNO" TYPE="%-19s" DEVICE="KEYBOARD"  FUNCTIONS="T()V(NB)">
    <ONLOSTFOCUS>
     "{
		dim @s as string;
		dim @tota as string;
		dim @list1 as string;
		dim @list2 as string;
		dim @i as number;
		initfd();
		$TXNO='9954';
		$FD101=#ACTNO;
		commsend_002();
		@tota=callserver();
		if($FD12!='0000')
		{
			showmsg(geterror());
			return(false);
		}
		setitems('#ACTNO24#ACTSEQ#TXAMT#VOCTYPE#VOCNUM#DESC10#NBFLAG#CUSTNAME#AVBAL#CURCD#CASHFLAG#DSCPT#ACTTYPE#ACTSEQ_2#PRODNAME',$FD101);
		show(#LNAME,true);
		show(#LCURCD,true);
		show(#LAVBAL,true);
		show(#LPRODNAME,true);
		show(#LVOCTYPE,true);
		show(#LCASHFLAG,true); //取消转帐
		show(#LTXAMT,true);
		show(#VOCTYPE,true);
		delspace(#CUSTNAME);
		show(#CUSTNAME,true);
		show(#CURCD,true);
		show(#XAVBAL,true);
		show(#PRODNAME,true);
		show(#VOCTYPE,true); 
		show(#CASHFLAG,true); //取消转帐
		show(#LVOCDSCPT,true);
		show(#VOCDSCPT,true);
		#TXAMT=#AMT;
		show(#TXAMT,true);
		while(substr(@tota, @i, 4)=='101C')
		{
			@i = @i + 30;
		}
		@list1=substr(@tota, 0 , @i+1);
		@list2=substr(@tota, @i, len(@tota)-@i);
		setsel(#VOCDSCPT,@list1,'o5t20o1v2o2');
		setsel(#VOCTYPE,@list2,'o5t20o1v3o2');
		$TXNO='2205';
		//#VOCDSCPT='01';
		//show(#VOCDSCPT,true);
		showblock('ACTINFO',true,true); //改为BLOCK形式	
		//show(#LVOCDSCPT,true);
		//show(#VOCDSCPT,true);
			}"
		</ONLOSTFOCUS>
	</ITEM>
  <BLOCK NAME="ACTINFO"  VISABLE="FALSE">
		<ITEM LINE="10"  COL="11" NAME="#LNAME"      INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
		<ITEM LINE="11"  COL="11" NAME="#LCURCD"     INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(币    种:)"/>
		<ITEM LINE="12"  COL="11" NAME="#LAVBAL"     INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(余    额:)"/>
		<ITEM LINE="13"  COL="11" NAME="#LPRODNAME"  INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(产品名称:)"/>		  
		<ITEM LINE="15"  COL="11" NAME="#LVOCTYPE"   INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(凭证类型:)"/>
		<ITEM LINE="16"  COL="11" NAME="#LCASHFLAG"  INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(现转标志:)"/>
		<ITEM LINE="17"  COL="11" NAME="#LTXAMT"     INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(发 生 额:)"/>
		<ITEM LINE="18"  COL="11" NAME="#LVOCDSCPT"  INPUT="LABEL"  TYPE="%-10s" FUNCTIONS="T(摘    要:)"/>
	  <ITEM LINE="10"  COL="23" NAME="#CUSTNAME"   TYPE="%-60s"   FUNCTIONS=""/>
		<ITEM LINE="11"  COL="21" NAME="#CURCD"      TYPE="%-2s"    FUNCTIONS="">
			<SELECT>
				 <OPTION VALUE="01" TITLE="人民币"/>
			</SELECT>
	  </ITEM>
		<ITEM NAME="#AVBAL" TYPE="%17.2f"  FUNCTIONS="J(#XAVBAL)"/>
		<ITEM LINE="12" COL="21" NAME="#XAVBAL"   TYPE="%-16s"  INPUT="LABEL"  FUNCTIONS="X(#AVBAL)"/>
		<ITEM LINE="13" COL="21" NAME="#PRODNAME" TYPE="%-30s"  FUNCTIONS=""/>
		<ITEM LINE="15" COL="21" NAME="#VOCTYPE"  TYPE="%-3s"   INPUT="SELECT"    DEVICE="KEYBOARD" FUNCTIONS="">
			<SELECT>
				 <OPTION VALUE="119" TITLE="现金交款单"/>
			</SELECT>
		</ITEM>
		<ITEM NAME="#VOCTYPE1"    TYPE="%-20s"     FUNCTIONS="S(#VOCDSCPT,#VOCDSCPT)">
		<ITEM LINE="16" COL="21" NAME="#CASHFLAG" TYPE="%-1s" INPUT="SELECT"   FUNCTIONS="">
		  <SELECT>
				<OPTION VALUE="1" TITLE="现金"/>
			</SELECT>
		</ITEM>
		<ITEM LINE="17"  COL="21" NAME="#TXAMT" TYPE="%17.2f" INPUT="TEXT"  DEVICE="KEYBOARD" FUNCTIONS="V(0000000000000001,9999999999999999)"/>
		<ITEM NAME="#XTXAMT" TYPE="%-16s" FUNCTIONS="X(#TXAMT)"/>
		<ITEM LINE="18" COL="21" NAME="#VOCDSCPT" TYPE="%-2s" INPUT="SELECT"   FUNCTIONS="T()"/>
		<ITEM NAME="#SRESUME"    TYPE="%-20s"     FUNCTIONS="S(#VOCDSCPT,#VOCDSCPT)">
		  <ONLOSTFOCUS>
		   "{
		     if(#VOCDSCPT != '31')
		     {
		         #ZRESUME = #SRESUME;
		         show(#ZRESUME,true);
		         enable(#ZRESUME,false);
		     }
		     else
		     {
		         #ZRESUME = '';
		         //show(#ZRESUME,true);
		         enable(#ZRESUME,true);
		     } 
		    }"
		  </ONLOSTFOCUS>
		</ITEM>
		<ITEM NAME="#ZRESUME"   LINE="18" COL="24" TYPE="%-20s"  INPUT="TEXT"    FUNCTIONS="V(NB)"/>
		<ITEM LINE="20"  COL="11"  NAME="#HEDUI2"  TYPE ="%-10s" FUNCTIONS = "T(是否缴款:!)"/>	 
    <ITEM LINE="20"  COL="21"  NAME="#HEDUI3"  TYPE="%-1s"   INPUT="SELECT"   FUNCTIONS="T(0)V(NB)" >
	    <SELECT>
			  <OPTION VALUE ="0" TITLE="0.是" />
			  <OPTION VALUE ="1" TITLE="1.否" />
	    </SELECT>
	    <ONLOSTFOCUS>
        "{
          if(#HEDUI3=='0')
          {
            showmsg('开始缴款！');
          }
          else
          {
            showmsg('退出缴款！');
            loadpage('DEFAULT');
            goitem(#JJLSHAO,true);
            enable(#YHDM,true);
          }
	      }"
		  </ONLOSTFOCUS>
    </ITEM> 
	</BLOCK>
	<ITEM NAME="#ACTNO24"   TYPE="%-24s" FUNCTIONS="T(#ACTNO)"/>
	<ITEM NAME="#ACTSEQ"    TYPE="%-4s"  FUNCTIONS=""/>
	<ITEM NAME="#VOCNUM"    TYPE="%-16s" FUNCTIONS=""/>
	<ITEM NAME="#NBFLAG"    TYPE="%-1s"  FUNCTIONS=""/>
	<ITEM NAME="#DESC10"    TYPE="%-10s" FUNCTIONS=""/>
	<ITEM NAME="#DSCPT"     TYPE="%-3s"  FUNCTIONS=""/>
	<ITEM NAME="#ACTTYPE"   TYPE="%-1s"  FUNCTIONS=""/>
	<ITEM NAME="#ACTSEQ_2"  TYPE="%-4s"  FUNCTIONS=""/>	
 </PAGE>
 <ITEM  NAME="#BTN" LINE="21"  COL="63" TYPE="%-6s"    FUNCTIONS="T(确  定)" INPUT="BUTTON">
	<ONCLICK>
	"{	
	         //	initfd();
	         //	  commsend_002();
	         //    commsend_101();
	         //	  $TXNO='2205';
	         //	  callserver();
           //  if($FD12!='0000')
           //    {
	         //     showmsg(geterror());
	         //     return(false);
           //   }
           //   else
           //   {
           //    showmsg('$FD101='+$FD101);
           //     showmsg('核心缴款成功!');
           //   }
		
		initfd();
		dim @txtime as string;
		@txtime = substr($SYSTIME,0,2)+':'+substr($SYSTIME,2,2)+':'+substr($SYSTIME,4,2);
		commsend_002();
		commsend_101();
		//showmsg('$FD101='+$FD101);
		$FD16='8306';
		$FD86=#CZDM;         //财政代码
		$FD87=#YHDM;        //银行代码
		$FD31=#DWDM;        //单位代码
		$FD32=#KPID;        //开票ID
		$FD30=#JJLSHAO;     //缴款流水号	
		$FD38=#KPDATE;      //开票日期						
		$FD39=delspace(itoa(val(itoa(#AMT,'%-17.2f')),'%-016.0f'));   //交易金额
		//showmsg('$FD39='+$FD39);
		$FD25=#FKRNAME;     //付款人名称
		$FD27=#FKRBANK;     //付款人银行
		$FD81=#FKRACCNO;    //付款人账号
		$FD62=#FKHNO;       //付款行行号
		$FD26=#SKRNAME;     //收款人名称
		$FD76=#SKRBANK;     //收款人银行
		$FD83=#SKRACCNO;    //收款人账号
		$FD54=#WDDM;         //网点代码
		$FD55=#GYNAME;       //银行人员 实际没用上
		//showmsg($HDATE);
		$FD37=substr(#SKDATE,0,4)+'-'+substr(#SKDATE,4,2)+'-'+substr(#SKDATE,6,2)+' '+@txtime;       //收款时间
		callagn_new();
		if($FD12!='0000')
		{
		  showmsg(geterror());
		  return(false);
		}
		else
		{
		showmsg('财政缴款成功!');
		}
		runoutput();
		show(#BTN,false);
		show(#REPRT,true);
		show(#END,true);
	 }"
	</ONCLICK>
 </ITEM>
 <ITEM LINE="21" NAME="#REPRT" COL="41" TYPE="%-16s" VISABLE="false" FUNCTIONS="T(是否需要补打)" INPUT="BUTTON">
	 <ONCLICK>
		"{
			runoutput();
		}"
	 </ONCLICK>
 </ITEM>
 <ITEM LINE="21" NAME="#END" COL="59" TYPE="%-6s" VISABLE="false" FUNCTIONS="T(结  束)" INPUT="BUTTON">
	 <ONCLICK>
		"{
			closeall();
			return(true);	
		}"
	 </ONCLICK>
 </ITEM>
