	  	 {
	  		 showmsg('密码长度必须为六位!');
	  		 return(false);
	  	 }
	  	 dim @tota as string;
	  	 initfd();
	     commsend_001();
       $FD16='9317';
       $FD30=#ZH;
       $FD79=#ZHMM;
       @tota=callserver();
