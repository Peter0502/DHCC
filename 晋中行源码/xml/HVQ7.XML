<?xml version="1.0" encoding="GBK" ?>

<TRANSACTION RIGHTS="11111111" TITLE="     申请撤销明细"
xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<VARIABLE>

<ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
"T(HVQ7                          申请退回明细)"/>
<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
"T(─────────────────────────────────────)"/>
<ITEM LINE="06"  COL="8"  TYPE="%-12s"  FUNCTIONS="T(交易日期:)"/>
<ITEM LINE="07"  COL="8"  TYPE="%-12s"  FUNCTIONS="T(申请序号:)"/>
<ITEM LINE="08"  COL="8"  TYPE="%-12s"  FUNCTIONS="T(申请行号:)"/>
<ITEM LINE="09"  COL="8"  TYPE="%-12s"  FUNCTIONS="T(交易状态:)"/>


<ITEM LINE="10" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原报文编号:)"/>
<ITEM LINE="11" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原交易金额:)"/>
<ITEM LINE="12" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原委托日期:)"/>
<ITEM LINE="13" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原支付序号:)"/>
<ITEM LINE="14" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原发起行号:)"/>
<ITEM LINE="15" COL="8"  TYPE="%-12s"  FUNCTIONS="T(原发起行号:)"/>

<ITEM LINE="6"   COL="20" NAME="#TX_DATE"       TYPE="%-8s"   FUNCTIONS="V(NB)"/>
<ITEM LINE="7"   COL="20" NAME="#ORDERNO"       TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,393,8)"/>
<ITEM LINE="8"   COL="20" NAME="#ORBRNO"        TYPE="%-12s"   FUNCTIONS="V(NB)T($BUFFER,19,12)"/>
<ITEM NAME="#TMP_TXSTAT" TYPE="%-1s" FUNCTIONS="T($BUFFER,801,1)">
<ONLOSTFOCUS>
"{
	HV_SET_STAT();
}"
</ONLOSTFOCUS>
</ITEM>
<ITEM LINE="9"   COL="20" NAME="#TXSTAT"    TYPE="%-8s"   FUNCTIONS=""/>


<ITEM NAME="#CMTNO" TYPE="%-3s" FUNCTIONS="T($BUFFER,1,3)">
<ONLOSTFOCUS>
"{
	HV_SET_CMTNAME();
}"
</ONLOSTFOCUS>
</ITEM> 
<ITEM LINE="10"   COL="20" NAME="#CMTNO_DES"   TYPE="%-30s"   FUNCTIONS=""/>

<ITEM NAME="#TMP_TXAMT"	TYPE="%-16s"	FUNCTIONS=""/>
<ITEM NAME="#HTXAMT"		TYPE="%16.2f"   FUNCTIONS="">
<ONLOSTFOCUS>
"{
	#HTXAMT	= atoi(#TMP_TXAMT)/100;
}"
</ONLOSTFOCUS>
</ITEM>
<ITEM LINE="11"   COL="20" NAME="#LHTXAMT"   TYPE="%-21s"  FUNCTIONS="X(#HTXAMT)"/>
<ITEM LINE="12" COL="20" NAME="#OWTDATE"      TYPE="%-8s"   FUNCTIONS=""/>
<ITEM LINE="13" COL="20" NAME="#OORDERNO"     TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,473,8)"/>
<ITEM LINE="14"  COL="20" NAME="#OORBRNO"     TYPE="%-12s" FUNCTIONS="T(#ORBRNO)"/>
<ITEM LINE="15"  COL="20" NAME="#OACBRNO"     TYPE="%-12s" FUNCTIONS="T($BUFFER,211,12)"/>


   <ITEM NAME="#INITLIST" TYPE="%-1s" FUNCTIONS="">
  <ONLOSTFOCUS>"{
	 dim @reason as string;
	 @reason=substr($BUFFER,539,255);
	 @reason = delspace(@reason);
	 if(#INITLIST=='')
	 {
		 list_init(6,30,11,47);
		 list_loadtext('原申请附言',@reason);
		 #INITLIST='1';
		 list_show();
	 }
         else
	 {
		 list_work();
	 }
	 if(#TMP_TXSTAT=='C')
		show(#RESPONSE,true);
	else 
		show(#RESPONSE,false);
  }"
  </ONLOSTFOCUS>
  </ITEM>

 <ITEM LINE="21"  COL="55" NAME="#RESPONSE"    INPUT="BUTTON"  TYPE="%-8s" FUNCTIONS="T(退回应答)">
  <ONCLICK>"{
 if(#TMP_TXSTAT=='C'){
		dim @orderno as string;
		@orderno = substr(#ORDERNO,6,3);
                open('5120.XML','#NOTPAY_DATE='+#TX_DATE+';#O_NOTPAY_ORDERNO='+substr(#ORDERNO,5,3)+';#AC_BR_NO='+#ORBRNO+';#CMTNO=313;');
        }else{
                showmsg('此种状态交易无法做退回应答');
        }

  }"
  </ONCLICK>
  </ITEM>

 <ITEM LINE="21"  COL="65" NAME="#OK"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确定)">
  <ONCLICK>"{
	close();

  }"
  </ONCLICK>
  </ITEM>


</VARIABLE>
</TRANSACTION>
