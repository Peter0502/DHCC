<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111111000000000000000000000" TITLE="5553 国库代码查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
  commsend_001();
	
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(5553                      国库代码查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=5573)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="10" COL="25" TYPE="%-10s"  FUNCTIONS="T(国库代码:)"/>
  <ITEM LINE="12" COL="25" TYPE="%-10s" FUNCTIONS="T(国库名称:)"/>
  <ITEM LINE="14" COL="25" TYPE="%-10s"  FUNCTIONS="T(国库级次:)"/>

  <!--输入变量-->
  <ITEM NAME="#ORGCODE" LINE="10" COL="36" TYPE="%-10s" INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#ORGNAME" LINE="12" COL="36" TYPE="%-30s" INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#ORGTYPE" LINE="14" COL="36" TYPE="%-1s" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="">
      <SELECT>
           <OPTION VALUE ="0" TITLE="0.数据中心" />
           <OPTION VALUE ="1" TITLE="0.总库" />
           <OPTION VALUE ="2" TITLE="0.省库" />
           <OPTION VALUE ="3" TITLE="0.中心支库" />
           <OPTION VALUE ="4" TITLE="0.县(区)支库" />
           <OPTION VALUE ="5" TITLE="0.乡库" />
      </SELECT>
  </ITEM>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  	<ONCLICK>
  		"{
  dim @tota as string;
  dim @selid as number;
  
  commsend_001();
	$FD16='5573';
  $FD11=#ORGTYPE;
	$FD26=#ORGCODE;
	$FD25=#ORGNAME;
	@tota=calltips();
	if($FD12!='0000'){
	  showmsg(geterror());
	  return(false);
	}
	if(autolist(@tota)){
            @selid=list_work();
            if(@selid &gt;= 0){
                 showmsg(list_gettext(@selid,1));
            }
       }
	//runoutput();
	//rerun();
  		}"</ONCLICK>
  		</ITEM>

</VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
</RESPONSE>


</TRANSACTION>
