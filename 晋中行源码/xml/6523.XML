<TRANSACTION RIGHTS="00100000" TITLE="6523 普通借记回执业务录入)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(6523                        普通借记回执业务录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM LINE="7"   COL="25" TYPE="%-10s" FUNCTIONS="T(原发起行:)"/>
  <ITEM LINE="9"  COL="25" TYPE="%-10s" FUNCTIONS="T(来包日期:)"/>
  <ITEM LINE="11"  COL="25" TYPE="%-10s" FUNCTIONS="T(委托日期:)"/>
  <ITEM LINE="13"  COL="25" TYPE="%-10s" FUNCTIONS="T(支付序号:)"/>
  <ITEM LINE="15"  COL="25" TYPE="%-10s" FUNCTIONS="T(处理方式:)"/>

  <ITEM NAME="#OPNBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#ACTTYPE" TYPE="%-1s"/>

  <!--支行往帐支付录入输入项输入定义-->
  <ITEM NAME="#HDATE" TYPE="%-8s"  FUNCTIONS="T($HDATE)"/>
  <ITEM LINE="7"   COL="35" NAME="#ORBRNO" TYPE="%12s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T()">
  <ONLOSTFOCUS>
  "{
  		dim @a as string;  		
  		@a = checkno('D32',#ORBRNO);
  		if(@a!=''){
  			//showmsg(@a);
  			showhelp(3,70,18,3,'大额行号信息','行 名:'+substr(@a,0,60));
  		}else{
  			showmsg('无此支付行号!');
  			return(false);
  		}
  	}"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#PKGID"  TYPE="%8d" DEVICE="KEYBOARD" FUNCTIONS="T()"/>
  <ITEM LINE="9"  COL="35" NAME="#PKGDAY" TYPE="%8s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)D()"/>
  <ITEM LINE="11"  COL="35" NAME="#WTDAY"  TYPE="%8s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)D()"/>
  <ITEM LINE="13"  COL="35" NAME="#ORDERNOHV"  TYPE="%8d" DEVICE="KEYBOARD"  FUNCTIONS="T()V(NB)"/>
  <ITEM LINE="15" COL="35" NAME="#OPTYPE" TYPE="%-1s" DEVICE="KEYBOARD" FUNCTIONS="T(0)">
     <SELECT LINE="15" COL="25">
         <OPTION VALUE="0"      TITLE="0.录入"/>
         <OPTION VALUE="3"      TITLE="1.修改"/>
     </SELECT>
  </ITEM>
  <ITEM NAME="#PKGNO" TYPE="%-3s"  FUNCTIONS="T(008)"/>
  <ITEM NAME="#OPCD" TYPE="%-2s"  FUNCTIONS="j(#OPTYPE=0,T{10}T{11})"/>
  
  <!--输入项定义到这里结束-->
  <!--系统变量定义 -->
  <ITEM NAME="#HTRMSEQ" TYPE="%-2s"  FUNCTIONS="T($HTRMSEQ)"/>
  <ITEM NAME="#HEJFNO"  TYPE="%-6s"  FUNCTIONS="T($HEJFNO)"/>
  <ITEM NAME="#KINBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#KINDNO"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#ACTNO"   TYPE="%-18s" FUNCTIONS="T(#PAYACTNO,1,18)"/>
  <ITEM NAME="#TRMSEQ"  TYPE="%-2s"  FUNCTIONS="T($TRMSEQ)"/>
  <ITEM NAME="#EJFNO"   TYPE="%-6s"  FUNCTIONS="T($TXTNO)"/>
  <ITEM NAME="#TASKID"  TYPE="%-2s"  FUNCTIONS="T(UB)"/>
  <ITEM NAME="#TMTYPE"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#APTYPE"  TYPE="%-1s"  FUNCTIONS="T(l)"/>
  <ITEM NAME="#TXNO"    TYPE="%-2s"  FUNCTIONS="T(23)"/>
  <ITEM NAME="#PTYPE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#DSCPT"   TYPE="%-3s"  FUNCTIONS="T(001)"/>
  <ITEM NAME="#HCODE"   TYPE="%-1s"  FUNCTIONS="T($HCODE)"/>
  <ITEM NAME="#RCODE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TXAMT"   TYPE="%13.2f" FUNCTIONS="T(#HTXAMT,4,12)"/>
  <ITEM NAME="#TXTYPE"  TYPE="%-2s"   FUNCTIONS="T(#ZCTYPE)"/>
  <ITEM NAME="#MULTTX"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CRDB"    TYPE="%-1s"  FUNCTIONS="T(1)"/>
  <ITEM NAME="#SUPIN1"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SUPIN2"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#REASON"  TYPE="%-6s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TLRNO"   TYPE="%6d"  FUNCTIONS="T($TLRNO)"/>
  <ITEM NAME="#TRNMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SBTMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CURCD"   TYPE="%-3s"  FUNCTIONS="T(000)"/>
  <ITEM NAME="#XCRT"    TYPE="%-8s"  FUNCTIONS="T($XCRT)"/>
  <ITEM NAME="#NBCD"    TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#EDATE"   TYPE="%-8s"  FUNCTIONS=""/>
  <ITEM NAME="#ATYPENO" TYPE="%-12s" FUNCTIONS=""/>
  <!-- 系统变量定义到此结束 -->
  <!-- 返回变量定义 -->
  <ITEM NAME="#BRSRNO"   TYPE="%-6s" />
  <ITEM NAME="#MSG"      TYPE="%-4s"  FUNCTIONS="j($HCODE=1,T{取消}T{})"/>
  <ITEM NAME="#HEAD"     TYPE="%-1s"  FUNCTIONS="T($CCHED=1)"/>
  <ITEM NAME="#TXCNT"    TYPE="%2d"   FUNCTIONS="T($OCCURS=#TXCNT)J(#TOTCNT)" />
  <ITEM NAME="#TOTCNT"   TYPE="%5d"   FUNCTIONS="E(#TOTCNT+#TXCNT)J(#TOTCNTC)"/>
  <ITEM NAME="#TOTCNTC"  TYPE="%5d"   FUNCTIONS="T($TOTCNT=#TOTCNT)"/>
  <ITEM NAME="#SPACE"    TYPE="%-1s"  FUNCTIONS="T( )"/>
  <ITEM NAME="#SERVER"   TYPE="%-1s"  FUNCTIONS="ST($ORKINBR=#OPNBR)T($TTSKID=UP)"/>

<ITEM LINE="21"  COL="65"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" >
<ONCLICK>"{
  	//showmsg(#OPTYPE);
  	//close();
  }"
  </ONCLICK>
</ITEM>
</VARIABLE>
<!--发送信息包-->
<REQUEST><PACKAGE SERVICE="dh65230">
 $APCTLHD
 #HTRMSEQ$HEJFNO#OPNBR#KINBR#TRMSEQ#EJFNO#TASKID#TMTYPE
 #APTYPE#TXNO#PTYPE#DSCPT$HCODE#ACTNO#TXTYPE#MULTTX#CRDB$SUPCD1$SUPCD2#REASON
 #TLRNO#NBCD#TRNMOD#SBTMOD#CURCD#XCRT#TXAMT
 #OPTYPE#ORBRNO#PKGID#PKGDAY#WTDAY#ORDERNOHV#PKGNO#OPCD
</PACKAGE></REQUEST>
</TRANSACTION>
