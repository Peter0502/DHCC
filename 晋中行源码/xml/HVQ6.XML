<?xml version="1.0" encoding="GBK" ?>



<TRANSACTION RIGHTS="11111111" TITLE="      来帐退汇明细)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVQ6                     来帐退汇明细)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="5"  COL="03"  TYPE="%-14s"  FUNCTIONS="T(委 托  日 期:)"/>              
  <ITEM LINE="5"  COL="27"  TYPE="%-12s"  FUNCTIONS="T(交 易序 号:)"/>                 
  <ITEM LINE="5"  COL="46"  TYPE="%-16s"  FUNCTIONS="T(交  易  状  态:)"/>           
                                                                                     
  <ITEM LINE="7"  COL="03"  TYPE="%-15s"  FUNCTIONS="T(原报 文 编号:)"/>
  <ITEM LINE="7"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(原交易序号:)"/>
  <ITEM LINE="7"  COL="54"  TYPE="%-16s"  FUNCTIONS="T(原 委 托 日 期:)"/> 
                                                       
  <ITEM LINE="9"  COL="3"   TYPE="%-20s"  FUNCTIONS="T(原付款人开户行行号:)"/>
  <ITEM LINE="9"  COL="46"  TYPE="%-16s"  FUNCTIONS="T(原支付交易序号:)"/> 
   
  <ITEM LINE="11" COL="03"  TYPE="%-14s"  FUNCTIONS="T(支付交易序号:)"   />
  <ITEM LINE="11" COL="27"  TYPE="%-6s"   FUNCTIONS="T(币种:)" />
  <ITEM LINE="11" COL="46"  TYPE="%-11s"  FUNCTIONS="T(汇款金额:)"  />
  <ITEM LINE="13" COL="3"   TYPE="%-14s"  FUNCTIONS="T(原支付行行号:)"/>
  <ITEM LINE="13" COL="31"  TYPE="%-14s"  FUNCTIONS="T(原付款人帐号:)"/>
  <ITEM LINE="15" COL="3"   TYPE="%-14s"  FUNCTIONS="T(原付款人名称:)"/>
  <ITEM LINE="17" COL="3"   TYPE="%-14s"  FUNCTIONS="T(原接收行行号:)"/>
  <ITEM LINE="17" COL="31"  TYPE="%-14s"  FUNCTIONS="T(原收款人帐号:)"/>
  <ITEM LINE="19" COL="3"   TYPE="%-14s"  FUNCTIONS="T(原收款人名称:)"/>
 
  <ITEM LINE="5"   COL="17" NAME="#HDATE"     TYPE="%-8s"    FUNCTIONS="V(NB)T($BUFFER,61,8)"/>
  <ITEM LINE="5"   COL="39" NAME="#ORDERNO"   TYPE="%-6s"    FUNCTIONS="V(NB)T($BUFFER,69,6)"/>
  <ITEM LINE="5"   COL="61" NAME="#TXSTAT"    TYPE="%-8s"    FUNCTIONS="V(NB)T($BUFFER,75,8)"/>
  
  <ITEM LINE="21"   COL="3" NAME="#CMTNO"     TYPE="%-3s"   INPUT="SELECT" VISABLE="FALSE"  FUNCTIONS="V(NB)T($BUFFER,83,3)">
   <SELECT LINE="21" COL="3">
  	<OPTION VALUE="100 大额汇兑" TITLE="0.大额汇兑(100)"/>
  	<OPTION VALUE="101 委托收款" TITLE="1.委托收款(101)"/>
  	<OPTION VALUE="102 托收承付" TITLE="2.托收承付(102)"/>
  	<OPTION VALUE="105 同业拆借" TITLE="3.同业拆借(105)"/>
  </SELECT>
  </ITEM>
  <ITEM LINE="7"   COL="17" NAME="#LCMTNO"         TYPE="%-12s" FUNCTIONS="S(#CMTNO,#CMTNO)"/>
  <ITEM LINE="7"   COL="47" NAME="#IOORDERNO"      TYPE="%-6s"  FUNCTIONS="V(NB)T($BUFFER,86,6)"/>
  <ITEM LINE="7"   COL="69" NAME="#IOHDATE"        TYPE="%-8s"  FUNCTIONS="V(NB)T($BUFFER,92,8)"/>
  <ITEM LINE="9"   COL="23" NAME="#IOPAYNO"        TYPE="%-12s" FUNCTIONS="V(NB)T($BUFFER,100,12)"/>
  <ITEM LINE="9"   COL="61" NAME="#IOORDERNOHV"    TYPE="%-8s"  FUNCTIONS="V(NB)T($BUFFER,112,8)"/>
  
  <ITEM LINE="11"   COL="17" NAME="#ORDERNOHV"     TYPE="%8d"   FUNCTIONS="V(NB)T($BUFFER,120,8)"/>
  <ITEM LINE="21"  COL="3" NAME="#COUR"           TYPE="%-3s"  INPUT="SELECT" VISABLE="FALSE" FUNCTIONS="V(NB)T($BUFFER,128,3)">
      <SELECT LINE="21" COL="3" >
           <OPTION VALUE="RMB 人民币" TITLE="001.人民币"/>
      </SELECT>
   </ITEM>
 <ITEM LINE="11"  COL="33" NAME="#LCOUR"    TYPE="%-8s"   FUNCTIONS="S(#COUR,#COUR)"/>
 <ITEM NAME="#HTXAMT"   TYPE="%16.2f" FUNCTIONS="T($BUFFER,131,15)"/>
 <ITEM LINE="11"  COL="55" NAME="#LHTXAMT"  TYPE="%-17s"   FUNCTIONS="X(#HTXAMT)"/>
 <ITEM LINE="13"  COL="17" NAME="#PAYBRNO"  TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,146,12)"/>

 <ITEM LINE="13"  COL="45" NAME="#PAYACTNO" TYPE="%-32s"  FUNCTIONS="T($BUFFER,158,32)"/>
 <ITEM LINE="15"  COL="17" NAME="#PAYNAME"   TYPE="%-60s" FUNCTIONS="V(NB)T($BUFFER,190,60)"/>
 <ITEM LINE="17"  COL="17" NAME="#CASHBRNO"  TYPE="%-12s" FUNCTIONS="V(NB)T($BUFFER,250,12)"/>
 <ITEM LINE="17"  COL="45" NAME="#CASHACTNO" TYPE="%-32s" FUNCTIONS="V(NB)T($BUFFER,262,32)"/>
 <ITEM LINE="19"  COL="17" NAME="#CASHNAME"  TYPE="%-60s" FUNCTIONS="V(NB)T($BUFFER,294,60)"/>
  
  <ITEM LINE="21"  COL="35" NAME="#CLOSE"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="45" NAME="#QUERY"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(查 询)">
  <ONCLICK>"{
  	if(substr(#TXSTAT,0,4)=='来账' or substr(#TXSTAT,4,4)=='不符' or #TXSTAT=='全国押错' or #TXSTAT=='地方押错' or #TXSTAT=='正常接收'){
  		open('6601.XML','#CBRNO='+#CASHBRNO+';'+
	  					'#OCORU='+#COUR+';'+
	  					'#OCMTNO=108;'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#PAYBRNO+';'+
	  					'#OACBRNO='+#CASHBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		open('6601.XML','#CBRNO='+#PAYBRNO+';'+
	  					'#OCORU='+#COUR+';'+
	  					'#OCMTNO=108;'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#PAYBRNO+';'+
	  					'#OACBRNO='+#CASHBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="55" NAME="#HVPUT"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请撤销)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账排队' or #TXSTAT=='发    送'){
  		open('6604.XML','#OLDDATE='+#HDATE+';'+
	  					'#OLDORD='+#ORDERNO+';'+
	  					'#OLDTYPE=108;'+
	  					'#CORU='+#COUR+';'+
	  					'#MONEY='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请撤销');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
   <ITEM LINE="21"  COL="67" NAME="#HVUNTREAD"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请退回)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账清算' or substr(#TXSTAT,0,4)=='退回'){
  		open('6605.XML','#OLDDATE='+#HDATE+';'+
	  					'#RECBRNO='+#PAYBRNO+';'+
	  					'#OLDORD='+#ORDERNO+';'+
	  					'#OLDTYPE=108;'+
	  					'#CORU='+#COUR+';'+
	  					'#MONEY='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请退回');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
</TRANSACTION>
