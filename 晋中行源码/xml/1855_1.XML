  <ITEM LINE="16"  COL="47"  TYPE="%-18s"  NAME="#CreTp"       INPUT="LABEL" FUNCTIONS=""/>    
  <ITEM LINE="17"  COL="3"  type="%-12s"  FUNCTIONS="T(凭证  号码:)"/>
  <ITEM LINE="17"  COL="15"  TYPE="%-18s"  NAME="#CreNo"	INPUT="LABEL"/>  
  <ITEM LINE="17"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(借贷  标志:)"/>
  <ITEM LINE="17"  COL="47"  TYPE="%-18s"  NAME="#CdFlag"       INPUT="LABEL" FUNCTIONS=""/>    
  <ITEM LINE="18"  COL="3"  TYPE="%-12s"  FUNCTIONS="T(优先  级别:)"/>
  <ITEM LINE="18"  COL="15"  TYPE="%-18s"  NAME="#TxPri"	INPUT="LABEL"/>  
  
  <ITEM LINE="20"  COL="60"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON"> 
	<ONCLICK>
		"{
			initfd();
			commsend_001();
			//生成核心流水-start
			dim @bak_fd81 as string;
			@bak_fd81=$FD81;
			$FD81=#TXNAME;
			gethxtraceno();
			$FD81=@bak_fd81;
			//生成核心流水-end
			runoutput();
			close();
		}"
	</ONCLICK>
  </ITEM>
</VARIABLE>
	
<RESPONSE>
<PACKAGE DEVICE="SCREEN" LINESPACE="24" DESC="按任意键退出" >
NOTHING
</PACKAGE>
		<PRINT>
		"{	
		    
		    		PrintTxHead();
		 						
				print( 3,3, '报文  类型: [20]  交易  日期:[20]', #MesTp, #TxDt);
				print( 4,3, '报文标识号: [20]  往来  标志:[20]', #MsgId, #SrFlag);
				print( 5,3, '委托  日期: [20]  交易  序号:[20]', #EntDate, #OrderNo);
				print( 6,3, '业务  类型: [20]  业务  种类:[20]', #TxTp, #BusKind);
				print( 7,3, '发起行行号: [20]  接收行行号:[20]', #OutBankNo, #InBankNo);
				print( 8,3, '发起清算行: [20]  接收清算行:[20]', #Instgclr, #Instdclr);
				print( 9,3, '付款人账号: [20]  收款人账号:[20]', #Dbtrid, #Cdtrid);
				print( 10,3, '货币  符号: [20]  交易  金额:[20]', #Cur, #Amt);
				print(11,3, '手续费标志: [20]  手续费金额:[20]', #FeeWay, #FeeAmt);	
				print(12,3, '交易  状态: [20]  账务  状态:[20]', #MsgSts, #TxSts);
				print(13,3, '挂账  状态: [20]  凭证  类型:[20]', #HgSts, #CreTp);
				print(14,3, '凭证  号码: [20]  借贷  标志:[20]', #CreNo, #CdFlag);
				print(15,3, '优先  级别:[20]',#TxPri);
				
				PrintTxTail();
		}"
	</PRINT>
</RESPONSE>
