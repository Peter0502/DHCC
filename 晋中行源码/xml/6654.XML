<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="6654 行名行号查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
  
<COMMSEND>"{
	initfd();
	commsend_001();
	$FD67=#OBRATTR;
	$FD58=#BRNO;	
	$FD26=#BRNAME;
}"
</COMMSEND>
<COMMRECV>"{
  commrecv_001();
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
	 "T(6654                          行名行号查询)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
	 "T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=6654)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="9"   COL="15"  TYPE="%-10s" FUNCTIONS="T(行    号:)" />
  <ITEM LINE="11"  COL="15"  TYPE="%-10s" FUNCTIONS="T(行    名:)" />
  <ITEM LINE="13"  COL="15"  TYPE="%-10s" FUNCTIONS="T(参与类别:!)" />
  <ITEM LINE="9"   COL="25"  NAME="#BRNO"    TYPE="%-12s"  DEVICE="KEYBOARD"  INPUT="TEXT"  FUNCTIONS=""/> 
  <ITEM LINE="11"  COL="25"  NAME="#BRNAME"  TYPE="%-50s" DEVICE="KEYBOARD"  INPUT="TEXT"  FUNCTIONS=""/> 
  <ITEM LINE="13"  COL="25"  NAME="#OBRATTR" TYPE="%1s"  DEVICE="KEYBOARD"  INPUT="SELECT"  FUNCTIONS="T(0)" >
		<SELECT>
			<OPTION VALUE ="0" TITLE="0.全部" />
			<OPTION VALUE ="1" TITLE="0.参与大额支付" />
			<OPTION VALUE ="2" TITLE="0.参与电子联行" />
		</SELECT>
  </ITEM>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
	<ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="行名行号查询" >
   CKPT|@brno|@qsbrno|@brname|@citycode|@categ|@obrattr|@brsts|
</PACKAGE>
<PRINT>
"{
   print(2,10,'*****************************************');
   print(3,18,'大额支付系统行名行号查询');
   print(4,10,'*****************************************');
   print(6,15,'行      号: [12]',delspace(@brno));
   print(7,15,'清算行行号: [12]',@qsbrno);
   print(7,15,'机构  全称: [50]',@brname);
   print(9,15,'城市  代码: [4]',@citycode);
   print(10,15,'类      别: [40]',@categ);
   print(11,15,'参与  类别: [40]',@obrattr);
   print(12,15,'当前  状态: [40]',@brsts);
 }"
</PRINT>
</RESPONSE>
</TRANSACTION>
