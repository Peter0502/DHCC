   "{ 
        dim @prtcmd as string; 
        @prtcmd='fview -p rlp'+$KINBR+$NETPRT+' '; 
      //  @prtcmd='fview '; 
	    call('extend_tar.sh '+#RPTDATE); 
	    if(#KINBR=='99999'){ 
