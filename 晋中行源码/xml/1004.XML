<ITEM LINE="12" COL="35" NAME="#KINBR" TYPE="%-5s" DEVICE="KEYBOARD" INPUT="TEXT"  MESSAGE="" VISABLE="FALSE"  FUNCTIONS="T($KINBR,1,5)">  
