<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5253 网点提入退票"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	
    	$FD16='4253';	
 	    $MSGTYPE='03001';
     	$FD11='0';
     	
      $FD25=getitems('#PAYER_NAME'); 
      $FD26=getitems('#PAYEE_NAME');                
      $FD27=getitems('#PAYER_BR_NAME');      	
      $FD30=getitems('#PAYER_AC_NO')+space(6);          
      $FD31=getitems('#PAYEE_AC_NO')+space(6);  
      $FD39=getitems('#TXAMT'); 
      
     	$FD68=getitems('#BILL_TYPE');       
            
     	$FD81=getseltitle(#BRF,#BRF);  

     	
     	$FD82=getitems('#REMARK'); 	      
     	$FD83=getitems('#PAYEE_BR_NAME'); 
     	
     	$FD89=#BRF; 

}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';

}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5253                          网点提入退票)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4253)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  

  <ITEM NAME="#ACTNO24"   TYPE="%-24s" FUNCTIONS=""/>
 
  <ITEM LINE="6"  COL="5" TYPE="%-14s" FUNCTIONS="T(原付款人账号:)"/>  
  <ITEM NAME="#PAYER_AC_NO" INPUT="TEXT" LINE="6"  COL="19" TYPE="%-18s"  FUNCTIONS="V(NB)" >
  <ONLOSTFOCUS>"{
    	dim @baktxno as string;  
    	dim @tota as string;
    	@baktxno=$TXNO;

     	$FD2=$OPNBR;
     	$FD3=$KINBR;
     	$FD5=$HDATE;
     	$FD7=$TLRNO;
     	$FD10=$TTYNAME;
     	$FD16='9753';
     	$FD79=#PASSWD; 

 			@tota=callserver();
       if($FD12!='0000'){
           showmsg(geterror());
           return(false);
       }
       
      setsel(#BRF,substr(@tota,0,21*62),'o5t10o1v3o2');     

 	 
  }"
  </ONLOSTFOCUS>
  </ITEM> 

  <ITEM LINE="7"  COL="5" TYPE="%-14s" FUNCTIONS="T(原付款人名称:)"/>  
  <ITEM  LINE="7"  COL="19"  NAME="#PAYER_NAME" INPUT="TEXT" TYPE="%-50s"  FUNCTIONS="V(NB)"/>

  <ITEM LINE="8"  COL="5" TYPE="%-14s" FUNCTIONS="T(原付款人行名:)"/>  
  <ITEM  LINE="8"  COL="19"  NAME="#PAYER_BR_NAME" INPUT="TEXT" TYPE="%-50s"  FUNCTIONS="V(NB)"/>
 

  <ITEM LINE="9"  COL="5" TYPE="%-14s" FUNCTIONS="T(原收款人账号:)"/>  
  <ITEM NAME="#PAYEE_AC_NO" INPUT="TEXT" LINE="9"  COL="19" TYPE="%-18s"  FUNCTIONS="V(NB)" />
   
  <ITEM LINE="10"  COL="5" TYPE="%-14s" FUNCTIONS="T(原收款人名称:)"/>  
  <ITEM LINE="10"  COL="19"  NAME="#PAYEE_NAME" INPUT="TEXT" TYPE="%-50s"  FUNCTIONS="V(NB)"/>

  
  <ITEM LINE="11"  COL="5" TYPE="%-14s" FUNCTIONS="T(原收款人行名:)"/>  
  <ITEM  LINE="11"  COL="19"  NAME="#PAYEE_BR_NAME" INPUT="TEXT" TYPE="%-50s"  FUNCTIONS="V(NB)"/>
         


  <ITEM LINE="13"  COL="5" TYPE="%-10s" FUNCTIONS="T(金    额:)"/>   
   <ITEM LINE="13"  COL="15" NAME="#TXAMT" INPUT="TEXT" TYPE="%17.2f" FUNCTIONS="0.00"/>   
 
   <ITEM LINE="14"  COL="5" TYPE="%-10s" FUNCTIONS="T(提入类型:!)"/>
  <ITEM NAME="#BILL_TYPE" LINE="14"  COL="15" TYPE="%-1s"   INPUT="SELECT" FUNCTIONS="T(1)" >
	<SELECT>
	
		<OPTION VALUE="1" TITLE="0.提入借方"/>
		<OPTION VALUE="2" TITLE="1.提入贷方"/>	
	</SELECT>
  </ITEM>  
  
   <ITEM LINE="15"  COL="5" TYPE="%-10s" FUNCTIONS="T(摘    要:!)"/>
  <ITEM NAME="#BRF" LINE="15"  COL="15" TYPE="%-3s"   INPUT="SELECT" FUNCTIONS="T(001)" >
	<SELECT>	
		<OPTION VALUE="001" TITLE="0.货款    "/>

	</SELECT>
  </ITEM>    
   <ITEM LINE="16"  COL="5" TYPE="%-10s" FUNCTIONS="T(备    注:)"/>  
  <ITEM  LINE="16"  COL="17"  NAME="#REMARK" INPUT="TEXT" TYPE="%-50s"  FUNCTIONS=""/>

    
  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="网点提入退票成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'网 点 提 入 退 票 成 功 ！');

				
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>






