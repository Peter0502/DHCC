  <ITEM   NAME="#LW_IND"       TYPE="%-1s"  FUNCTIONS="T(1)"/>         <!---来往标识 1往,2来--->
  <ITEM   NAME="#OTYPE"        TYPE="%-1s"  FUNCTIONS="T(2)"/>         <!---大小额标志 1大额，2小额--->

  <ITEM LINE="6"   COL="20" NAME="#AC_BR_NO"    TYPE="%-12s"  DEVICE="KEYBOARD"   FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
        dim @baktxno1 as string;
        @baktxno1='';
        $FD96='';
        @baktxno1=$TXNO;
        $FD32=#AC_BR_NO;
        $TXNO='9553';
        commsend_001();
        callserver();
        $TXNO=@baktxno1;
        if($FD12!='0000'){
            showmsg(geterror());
            return(false);
        }
        showhelp(3,70,8,3,'支付系统行名信息',substr($FD96,0,60));

   }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="7"   COL="20" NAME="#O_CMTNO"    TYPE="%-3s"   DEVICE="KEYBOARD" INPUT="SELECT"   FUNCTIONS="T(001)V(001,011)">
   <SELECT LINE="7" COL="21" >
           <OPTION VALUE="001 普通贷记业务"      TITLE="001.普通贷记业务"/>
           <OPTION VALUE="008 普通借记业务"      TITLE="002.普通借记回执业务"/>
           <OPTION VALUE="005 定期贷记业务"      TITLE="005.定期贷记业务"/>
           <OPTION VALUE="011 定期借记业务"      TITLE="006.定期借记回执业务"/>
           <OPTION VALUE="007 贷记退汇业务"      TITLE="007.贷记退汇业务"/>
   </SELECT>
   </ITEM>

   <ITEM LINE="8"   COL="20" NAME="#O_WT_DAY"    TYPE="%-8s"   DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>
   <ITEM LINE="9"   COL="20" NAME="#OORDERNO"   TYPE="%8d"    DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>
   <ITEM LINE="11"  COL="15" NAME="#NOTE"       TYPE="%-60s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
    if (strfldcnt(#NOTE,':') != 1 or strfldcnt(#NOTE,'{') != 1 or strfldcnt(#NOTE,'}') != 1)
    {
        showmsg('输入内容中不能含有半角:{}字符,请修改为全角其它字符');
        return(false);
    }
    delspace(#NOTE);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#NOTPAY_CONTENT" TYPE="%-255s" FUNCTIONS="T(#NOTE)"/>


  <!-----占位字段--------------->
  <ITEM NAME="#PAY_QS_NO"     TYPE="%-12s"      FUNCTIONS=""/><!--(原)发起清算行行号-->
  <ITEM NAME="#OR_BR_NO"      TYPE="%-12s"      FUNCTIONS=""/><!--(原)发起行号,申请撤销行号 退回行号-->
