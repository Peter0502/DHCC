<TRANSACTION RIGHTS="11110000" TITLE="6532 小额业务撤销申请报文录入/发送"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD44 = #WT_DAY;
	$FD45 = #O_WT_DAY;
    $FD3 =$KINBR;
    $FD7=$TLRNO;
    commsend_123();
}"
</COMMSEND>
<COMMRECV>"{

	commrecv_001();
	commrecv_123();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-70s" FUNCTIONS=
    "T(5320                    小额业务撤销申请报文录入/发送)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5860)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="6"  COL="05" TYPE="%-14s" FUNCTIONS="T(接 收  行 号:)"/>
  <ITEM LINE="7"  COL="05" TYPE="%-14s" FUNCTIONS="T(原包 类型 号:)"/>
  <ITEM LINE="8"  COL="05" TYPE="%-14s" FUNCTIONS="T(原包委托日期:)"/>
  <ITEM LINE="9"  COL="05" TYPE="%-14s" FUNCTIONS="T(原 包  序 号:)"/>
  <ITEM LINE="11" COL="05" TYPE="%-10s" FUNCTIONS="T(附    言:)"/>

  <ITEM   NAME="#NOTPAY_DATE"  TYPE="%8d"   FUNCTIONS="T($HDATE)D()"/>    <!----查询日期--->
  <ITEM   NAME="#CMTNO"        TYPE="%-3s"  FUNCTIONS="T(321)"/>       <!----CMTNO--->
