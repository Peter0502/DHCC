  <ITEM LINE="10"  COL="5"    TYPE="%-73s" FUNCTIONS="T(-----------------------------------------------------------------------)"/>
  <ITEM LINE="12"  COL="5"  NAME="#LNAME"      TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="13"  COL="5"  NAME="#LCURCD"     TYPE="%-10s" FUNCTIONS="T(币    种:)"/>
  <ITEM LINE="14"  COL="5"  NAME="#LAVBAL"     TYPE="%-10s" FUNCTIONS="T(余    额:)"/>
  <ITEM LINE="15"  COL="5"  NAME="#LPRODNAME"  TYPE="%-10s" FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="16"  COL="5"  NAME="#LVOCTYPE"   TYPE="%-10s" FUNCTIONS="T(凭证类型:!)"/>
