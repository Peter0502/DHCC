 <ITEM LINE="08" COL="15" NAME="#ACTNO1" TYPE="%-19s" DEVICE="KEYBOARD"  FUNCTIONS="V(NB)">
   <ONLOSTFOCUS>
   "{
	dim @s as string;
	dim @tota as string;
	dim @list1 as string;
	dim @list2 as string;
	dim @i as number;
    // dim @baktxno as string;
    // @baktxno=$TXNO;
    // $TXNO='9730';

    initfd();
	$TXNO='9730';
	$FD101=#ACTNO1;
	commsend_001();
	@tota=callserver();
	if($FD12!='0000'){
		showmsg('帐号输入错误!');
	//	showmsg(geterror());
		return(false);
	}

	showblock('ACTINFO1',true,true);
    setitems('#202ACTNO#202ACSEQ#202AMT#VOCTYPE1#202VOCNUM#202DSCPT#202NBFLAG#CUSTNAME1#AVBAL1#CURCD1#202CASHFLAG#VOCDSCPT1#202ACTYPE#202ACSEQ2#PRODNAME1',$FD101);

	show(#CUSTNAME1,true);
	show(#CURCD1,true);
	show(#XAVBAL1,true);
	show(#PRODNAME1,true);
	show(#TXAMT2,true);

	while(substr(@tota, @i, 4)=='101C'){
		 @i = @i + 30;
	}
	 @list1=substr(@tota, 0 , @i+1);
	 @list2=substr(@tota, @i, len(@tota)-@i);
	 setsel(#VOCDSCPT1,@list1,'o5t20o1v2o2');
	 setsel(#VOCTYPE1,@list2,'o5t20o1v3o2');
  }"
  </ONLOSTFOCUS>
  </ITEM>
<ITEM LINE="12" COL="15" NAME="#CUSTNAME1" INPUT="LABEL"   TYPE="%-50s"  FUNCTIONS=""/>
<ITEM LINE="13" COL="15" NAME="#CURCD1"  TYPE="%-2s" INPUT="SELECT" FUNCTIONS="" ENABLE="FALSE">
    <SELECT>
		<OPTION VALUE="01" TITLE="人民币"/>
	</SELECT>
  </ITEM>
<ITEM NAME="#AVBAL1" TYPE="%17.2f"  FUNCTIONS="J(#XAVBAL1)"/>
<ITEM LINE="14" COL="15" NAME="#XAVBAL1"  TYPE="%-16s"  INPUT="LABEL" ENABLE="TRUE" FUNCTIONS="X(#AVBAL1)"/>
<ITEM LINE="15" COL="15" NAME="#PRODNAME1" TYPE="%-30s" INPUT="TEXT"  FUNCTIONS=""/>
<ITEM LINE="16" COL="15" NAME="#VOCTYPE1" TYPE="%-3s" INPUT="SELECT"  ENABLE="TRUE" FUNCTIONS="">
    <SELECT>
		<OPTION VALUE="001" TITLE="进帐单"/>
		<OPTION VALUE="002" TITLE="其它凭证"/>
	</SELECT>
  </ITEM>



<ITEM LINE="17" COL="15" NAME="#VOCDSCPT1" TYPE="%-2s" INPUT="SELECT" ENABLE="TRUE" DEVICE="KEYBOARD" FUNCTIONS="">
    <SELECT>
		<OPTION VALUE="01" TITLE="备用金"/>
	</SELECT>
  </ITEM>

 <!--
 <ITEM LINE="18"  COL="15" NAME="#TXAMT1" TYPE="%17.2f" INPUT="TEXT" ENABLE="TRUE" DEVICE="KEYBOARD" FUNCTIONS="J(#UTXAMT1)"/>
 -->
  <ITEM NAME="#TXAMT1" TYPE="%17.2f" ENABLE="FALSE" FUNCTIONS="J(#UTXAMT1)"/>
  <ITEM NAME="#UTXAMT1" TYPE="%-16s" FUNCTIONS="X(#TXAMT1)"/>

  <ITEM LINE="18"  COL="15" NAME="#TXAMT2" TYPE="%-16s" FUNCTIONS="T(#UTXAMT)"/>

 
  <ITEM  NAME="#CASHFLAG1" TYPE="%-1s"  FUNCTIONS="">
     <SELECT>
		   <OPTION VALUE="1" TITLE="现金"/>
	       <OPTION VALUE="2" TITLE="转帐"/>
		   <OPTION VALUE="3" TITLE="同城"/>
	       <OPTION VALUE="4" TITLE="大额"/>
	 </SELECT>
 </ITEM>


