<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5213 中心提出（核销）"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	
    	$FD16='4213';	
         	
      $FD39=getitems('#TXAMT');
         	
      $FD48=getitems('#BAT_NO');     	
    	
      $FD91=getitems('#BR_NO');  
 
     	
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';

}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5213                          中心提出（核销）)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4213)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
 


  <ITEM LINE="9"  COL="30" TYPE="%-10s" FUNCTIONS="T(网 点 号:)"/>  
  <ITEM NAME="#BR_NO" INPUT="TEXT" LINE="9"  COL="40" TYPE="%5s"  FUNCTIONS="V(NB)" />
   
 
  <ITEM LINE="11"  COL="30" TYPE="%-10s" FUNCTIONS="T(批 次 号:)"/>  
  <ITEM  LINE="11"  COL="40"  NAME="#BAT_NO" INPUT="TEXT" TYPE="%4s"  FUNCTIONS="" />
    

   <ITEM LINE="13"  COL="30" TYPE="%-10s" FUNCTIONS="T(金    额:)" />   

  <ITEM LINE="13"  COL="40" NAME="#TXAMT" INPUT="TEXT" TYPE="%17.2f" FUNCTIONS="0.00" />

  

  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="中心提出（核销）成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'中 心 提 出（ 核 销 ） 成 功 ！');

				
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>



