<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9104                        定期账户明细查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8104)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <BLOCK NAME="COMMON" LINE="7" COL="25">
  <ITEM LINE="0" COL="0" TYPE="%-8s" FUNCTIONS="T(帐    号)"/>
  <ITEM LINE="2" COL="0" TYPE="%-8s" FUNCTIONS="T(帐户序号)"/>
  <ITEM LINE="4" COL="0" TYPE="%-9s" FUNCTIONS="T(借贷标志!)"/>
  <ITEM LINE="6" COL="0" TYPE="%-9s" FUNCTIONS="T(现转标志!)"/>
  <ITEM LINE="8" COL="0" TYPE="%-10s" FUNCTIONS="T(开户机构号)"/>
  <ITEM LINE="10" COL="0" TYPE="%-8s" FUNCTIONS="T(交易日期)"/>
  <ITEM LINE="10" COL="17" TYPE="%-3s" FUNCTIONS="T( → )"/>
  <ITEM NAME="#ZH" INPUT="TEXT" LINE="0" COL="9" TYPE="%-20s" FUNCTIONS="T($ZH)V(NB)">
  <ONLOSTFOCUS>
  "{ dim @txno as string;
     @txno=$TXNO;
     $TXNO='9958';
     commsend_001();
     $FD38=#ZH;
     callserver();
     $TXNO=@txno;
     
     if($FD12!='0000')
       {showmsg(geterror());
         return(false);
         }
   if(#ZH != '')
     {
         initfd();
         @txno=$TXNO;
         $TXNO='6700';
         commsend_001();
         $FD30=#ZH;
         $FD67='2';
         callserver();
         $TXNO=@txno;     
         if($FD12!='0000')
            {showmsg(geterror());
             return(false);
         }
         if($KINBR!=$FD91)
         {
             showmsg('只能查询本机构帐户!');
             return(false);
         } 
     }  
        }"
  </ONLOSTFOCUS>       
   </ITEM>  
  <ITEM NAME="#ZHXH" INPUT="TEXT" LINE="2" COL="9" TYPE="%3s" FUNCTIONS="T()"/>
    
  <ITEM NAME="#ZJBZ" INPUT="SELECT" LINE="4" COL="10" TYPE="%-3s" FUNCTIONS="">
	<SELECT>
		<OPTION VALUE="0" TITLE="0.借"/>
		<OPTION VALUE="1" TITLE="1.贷"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#XZBZ" INPUT="SELECT" LINE="6" COL="10" TYPE="%-3s" FUNCTIONS="">
	<SELECT>
		<OPTION VALUE="1" TITLE="1.现金"/>
		<OPTION VALUE="2" TITLE="2.转帐"/>
		<OPTION VALUE="3" TITLE="3.同城"/>
		<OPTION VALUE="4" TITLE="3.大额"/>
			</SELECT>
  </ITEM>
  <ITEM NAME="#KHJG" INPUT="LABEL" LINE="8" COL="11" TYPE="%5s" FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#JYRQ" INPUT="TEXT" LINE="10" COL="9" TYPE="%-8s" FUNCTIONS="T()"/>
  <ITEM NAME="#JYRQQ" INPUT="TEXT" LINE="10" COL="22" TYPE="%-8s" FUNCTIONS="T()"/>
  </BLOCK>
  <!--输出变量--->
  <ITEM NAME="#TXCNT"   TYPE="%2d"  FUNCTIONS="T($OCCURS=#TXCNT)J(#TOTCNT)" />
  <ITEM NAME="#TOTCNT"  TYPE="%5d"  FUNCTIONS="E(#TOTCNT+#TXCNT)J(#TOTCNTC)"/>
  <ITEM NAME="#TOTCNTC" TYPE="%5d"  FUNCTIONS="T($TOTCNT=#TOTCNT)"/>
  <ITEM NAME="#O_TXDAY" TYPE="%-8s" FUNCTIONS=""/>
  <ITEM NAME="#O_PBDSC" TYPE="%-3s" FUNCTIONS="J(#OL_PBDSC)"/>
  <ITEM NAME="#OL_PBDSC" TYPE="%-2s" FUNCTIONS="T(  )j(#O_PBDSC=001,T{存})j(#O_PBDSC=002,T{取})"/>
  <ITEM NAME="#O_TXAMT" TYPE="%16.2f" FUNCTIONS="J(#OL_TXAMT)"/>
  <ITEM NAME="#OL_TXAMT" TYPE="%-16s" FUNCTIONS="X(#O_TXAMT)"/>
  <ITEM NAME="#O_CRDB" TYPE="%-1s" FUNCTIONS="J(#OL_SIGN)"/>
  <ITEM NAME="#OL_SIGN" TYPE="%-1s" FUNCTIONS="T( )j(#O_CRDB=1,T{-})"/>
  <ITEM NAME="#O_AVBAL" TYPE="%16.2f" FUNCTIONS="J(#OL_AVBAL)"/>
  <ITEM NAME="#OL_AVBAL" TYPE="%-16s" FUNCTIONS="X(#O_AVBAL)"/>
  <ITEM NAME="#O_TLRNO" TYPE="%-4s" FUNCTIONS=""/>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
	<ONCLICK>"{
		dim @tota as string;
		dim @selid as number;
		commsend_001();
		$FD16='8104';
		$FD38=#ZH;
	  $FD51=#ZHXH;
	  $FD70=#ZJBZ;
	  $FD66=#XZBZ;
	  $FD2=#KHJG;
	  $FD29=#JYRQ;
	  $FD44=#JYRQQ;
		@tota=callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		if(autolist(@tota)){
			@selid=list_work();
			if(@selid &gt;= 0){
			//	showmsg(list_gettext(@selid,1));
			}
		}
	}"
 	</ONCLICK>
  </ITEM>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
