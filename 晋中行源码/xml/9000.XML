<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="L10 房屋维修开户测试"
    xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
    commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS="T(9000                        房屋维修开户测试)"/>
  <ITEM LINE="4"  COL="3" TYPE="%-74s" FUNCTIONS="T(------------------------------------------------------------------------------------------)"/>
  <!--<ITEM  NAME="#tx_txm" 	TYPE="%16s"/>
  <ITEM  NAME="#acct_no" 	TYPE="%-32s"/>
  <ITEM  NAME="#amt"    	TYPE="%16.2f"/>
  <ITEM  NAME="#br_no"   	TYPE="%-5s"/>-->
  
  <ITEM LINE="05" COL="03" TYPE="%-9s"  FUNCTIONS="T(业务类型:)"/>
  <ITEM LINE="05" COL="12" NAME="#tx_code" TYPE="%4s"  INPUT="SELECT" DEVICE="KAYBOARD" FUNCTIONS="T(E100)V(NB)">
	<SELECT>
		<OPTION VALUE="J001" TITLE="开户  "/>
		<OPTION VALUE="J002" TITLE="修改"/>
		<OPTION VALUE="J011" TITLE="批量开户" />
	</SELECT>
  </ITEM>
  
  <ITEM LINE="06" COL="03" TYPE="%-9s" FUNCTIONS="T(条形码:)"/>
  <ITEM LINE="06" COL="12" NAME="#tx_txm"  TYPE="%-16s"  INPUT="TEXT"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="07" COL="03" TYPE="%-9s" FUNCTIONS="T(帐号:)"/>
  <ITEM LINE="07" COL="12" NAME="#acct_no" TYPE="%-32s" INPUT="TEXT"  FUNCTIONS="T()V(NB)"/>
  
  <ITEM LINE="08" COL="03" TYPE="%-9s" FUNCTIONS="T(开户金额:)"/>
  <ITEM LINE="08" COL="12" NAME="#amt"  TYPE="%16.2f"  INPUT="TEXT"  FUNCTIONS="V(NB)T()"/>
  
  <ITEM LINE="09" COL="03" TYPE="%-9s" FUNCTIONS="T(银行代码:)"/>
  <ITEM LINE="09" COL="12" NAME="#br_no" TYPE="%-32s" INPUT="TEXT"  FUNCTIONS="T()V(NB)">
  	
  	<ONLOSTFOCUS>
  	"{
			initfd();
			commsend_001();
			$FD5=   $HDATE;
			$FD16 = #tx_code;
			$FD24 = 'fwx';
			$FD30= 	#acct_no;
			$FD39 = itoa(#amt,'%-17.2f');
			$FD40 = #tx_txm;
			$FD41 = #br_no;
			showmsg($HDATE);
			showmsg(#tx_code);
			showmsg(#acct_no);
			showmsg($FD24);
			showmsg($FD39);
			showmsg($FD40);
			showmsg($FD41);
			callagn();
			if($FD12!='0000')
			{
				showmsg(geterror());
				showmsg('房屋维修基金开户出错!');
				return(false);
			}
			showmsg('交易成功!');
			refresh();
  	  }"
  	</ONLOSTFOCUS>
  	
  </ITEM>
</VARIABLE>

<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="显示内容">
	NOTHING
	</PACKAGE>
</RESPONSE>
</TRANSACTION>


