<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6810  回购式转贴现转入记账"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    initfd();
    $MSGTYPE='03001';
	dim @baktxno as string;
	@baktxno=$TXNO;
	$TXNO='9986';
	$FD12='4924';
	$FD25=#NAME;
    $FD44=#MDATE;
    $FD46=#RDATE;
    $FD62=#PANO;
    $FD68=#LIND;
    $FD70="0001";
    $FD84=getitems('#PINT');
    $FD100=getitems('#TINT#AMT');
	commsend_001();
    callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
	$FD16 = '4924';
  	$MSGTYPE='03001';
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	#ACNO=$FD33;
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-74s" FUNCTIONS=
    "T(6810                      回购式转贴现转入记账)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4924)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <!--ITEM LINE="6"  COL="5" TYPE="%-10s" FUNCTIONS="T(转账类型:!)"/-->
  <ITEM LINE="7"  COL="5" TYPE="%-16s" FUNCTIONS="T(转贴现协议编号:)"/>
  
  <ITEM LINE="9"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(-----------------------------【回显信息】----------------------------------)"/>
  
  <ITEM LINE="11"  COL="5"  TYPE="%-12s" FUNCTIONS="T(申请人名称:)"/>
  <ITEM LINE="12"  COL="5"  TYPE="%-14s" FUNCTIONS="T(异地同城标志:!)"/>
  <ITEM LINE="13"  COL="5"  TYPE="%-12s" FUNCTIONS="T(转贴现利率:)"/>
  <ITEM LINE="13"  COL="27" TYPE="%-2s"  FUNCTIONS="T(‰)"/>
  <ITEM LINE="13"  COL="36" TYPE="%-14s" FUNCTIONS="T(汇总票据金额:)"/>
  <ITEM LINE="14"  COL="5"  TYPE="%-12s" FUNCTIONS="T(转贴现日期:)"/>
  <ITEM LINE="14"  COL="35" TYPE="%-10s" FUNCTIONS="T(回购日期:)"/>
  <ITEM LINE="15"  COL="5"  TYPE="%-12s" FUNCTIONS="T(转贴现利息:)"/>
  <ITEM LINE="17"  COL="5"  TYPE="%-12s" FUNCTIONS="T(转贴现账号:)"/>
  
  <ITEM NAME="#NACNO" LINE="17"  COL="5" TYPE="%-12s" INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T(转贴现账号:)"/>
  
  <!--输入变量-->
  <!--
  	<ITEM NAME="#ACTYPE" LINE="6" COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="1" TITLE="同城"/>
	
	<OPTION VALUE="2" TITLE="大额"/>
	
	</SELECT>
  </ITEM>
	<ITEM NAME="#FLAGACTYPE" TYPE="%4s" FUNCTIONS="S(#ACTYPE,#ACTYPE)"/>
	-->
  <ITEM NAME="#PANO" LINE="7" COL="20" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
 "{
    	commsend_001();
 		$FD16='9860';
		$FD62=#PANO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#NAME=$FD25;
		#MDATE=$FD44;
		#RDATE=$FD46;
		#LIND=$FD68;
		setitems('#PINT',$FD84);
		setitems('#TINT#AMT',$FD100);
		show(#NAME,true);
		show(#PINT,true);
		show(#LIND,true);
		show(#TINT,true);
		show(#AMT,true);
		show(#MDATE,true);
		show(#RDATE,true);
 }"
 </ONLOSTFOCUS>
 </ITEM>
  
  <ITEM NAME="#NAME" LINE="11" COL="17" TYPE="%-50s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#LIND" LINE="12" COL="19" TYPE="%-1s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T()">
  	<SELECT>
		<OPTION VALUE="0" TITLE="本地"/>
		<OPTION VALUE="1" TITLE="异地"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#PINT" LINE="13" COL="17" TYPE="%9.6f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#AMT" LINE="13" COL="51" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#MDATE" LINE="14" COL="17" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#RDATE" LINE="14" COL="50" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#TINT" LINE="15" COL="17" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>

  
  <ITEM NAME="#ACNO" LINE="17" COL="17" TYPE="%-24s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
  <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
  <ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

  </VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="回购式转贴现转入记账!">
NOTHING
</PACKAGE>
<PRINT>"{
	 print( 5, 30,'回购式转贴现转入记帐成功');
	 print( 7, 20,'转账类型: [4]',#FLAGACTYPE);
	 print( 8, 20,'转贴现协议编号: [20]',#PANO);
     print( 9, 20,'转贴现账号: [24]',#ACNO);
 }"
  </PRINT>
 </RESPONSE>

<RESPONSE>
<PACKAGE DEVICE="PRINTER"  LINESPACE="38" DESC="打印凭条">
  NOTHING
</PACKAGE>
 <PRINT>"{
	print( 5, 16,'[4]     [2]     [2]',#YEAR,#MON,#DAY);
    print( 7,  8,'[5]',$FD2);
    print( 7, 24,'回购式转贴现转入记账');
    print( 7, 46,'[4]',$FD7);
	print( 7, 62,'[6]',$FD4);
	print( 8, 20,'转账类型: [4]',#FLAGACTYPE);
    print( 9, 20,'转贴现协议编号: [20]',#PANO);
    print(10, 20,'转贴现账号: [24]',#ACNO);
    print(11, 20,'记帐成功');
}"
</PRINT>
</RESPONSE>
</TRANSACTION>
