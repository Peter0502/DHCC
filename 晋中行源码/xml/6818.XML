  <ITEM LINE="5"  COL="5" TYPE="%-12s" FUNCTIONS="T(【查询条件】)"/>
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(---------------------------------------------------------------------------)"/>


  <ITEM LINE="8"  COL="5" TYPE="%-10s" FUNCTIONS="T(状    态!)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-16s" FUNCTIONS="T(转贴现协议编号:)"/>
 
  <ITEM LINE="11"  COL="5" TYPE="%-16s" FUNCTIONS="T(贴现申请人户名:)"/>
  
  <ITEM LINE="12"  COL="5" TYPE="%-10s" FUNCTIONS="T(转账类型!)"/>
  <ITEM LINE="12"  COL="40" TYPE="%-14s" FUNCTIONS="T(异地同城标志!)"/>
  
  <ITEM LINE="13"  COL="5" TYPE="%-10s" FUNCTIONS="T(在途天数:)"/>
