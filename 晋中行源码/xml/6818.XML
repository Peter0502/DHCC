  <ITEM LINE="16"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴 现 日:)"/>
  <ITEM LINE="16"  COL="40" TYPE="%-12s" FUNCTIONS="T(汇票到期日:)"/>
  
  

  <!--输入变量-->
  
  <ITEM NAME="#STS" LINE="8" COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="*" TITLE="销户"/>
		<OPTION VALUE="1" TITLE="录入"/>
		<OPTION VALUE="3" TITLE="正常"/>
	</SELECT>
  </ITEM>
  
 <ITEM NAME="#PANO" LINE="9" COL="21" TYPE="%-20s"  INPUT="TEXT" FUNCTIONS="T()"/>

  <ITEM NAME="#NAME" LINE="11" COL="21" TYPE="%-50s"  INPUT="TEXT" FUNCTIONS="T()"/>
 
  <ITEM NAME="#ACTYPE" LINE="12" COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="1" TITLE="本行"/>
		<OPTION VALUE="2" TITLE="同城"/>
		<!---
		<OPTION VALUE="3" TITLE="大额"/>
		-->
	</SELECT>
  </ITEM>
  <ITEM NAME="#LIND" LINE="12" COL="54" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="0" TITLE="本地"/>
		<OPTION VALUE="1" TITLE="异地"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#WDAYS" LINE="13" COL="15" TYPE="%3d"  INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#PINT" LINE="13" COL="50" TYPE="%9.6f"  INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#NOTYPE" LINE="14" COL="15" TYPE="%-3s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="111" TITLE="银行承兑汇票"/>
		<OPTION VALUE="112" TITLE="商业承兑汇票"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#PNNO" LINE="14" COL="50" TYPE="%-16s"  INPUT="TEXT" FUNCTIONS="T()"/>
 
   <ITEM NAME="#LAMT" LINE="15" COL="15" TYPE="%17.2f"  INPUT="TEXT" FUNCTIONS="J(#LLAMT)"/>
   <ITEM NAME="#LLAMT" TYPE="%-16s" FUNCTIONS="X(#LAMT)"/>
   <ITEM NAME="#HAMT" LINE="15" COL="38" TYPE="%17.2f"  INPUT="TEXT" FUNCTIONS="J(#LHAMT)"/>
   <ITEM NAME="#LHAMT" TYPE="%-16s" FUNCTIONS="X(#HAMT)"/>
  <ITEM NAME="#DATE" LINE="16" COL="15" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#MDATE" LINE="16" COL="52" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
      <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
	  <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
	  <ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
  </VARIABLE>
<REQUEST>
</REQUEST>
