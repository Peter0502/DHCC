<COMMSEND>"{
        initfd();
	$FD24=#NOTYPE;
    $FD26=#NAME;
    $FD29=#DATE;
    $FD38=#PANO;
    $FD40=getitems('#LAMT');
    $FD41=getitems('#HAMT');
    $FD44=#MDATE;
    $FD51=#WDAYS;
    $FD60=#PNNO;
    $FD67=#LIND;
    $FD68=#STS;
    $FD70=#ACTYPE;
    $FD84=getitems('#PINT');
