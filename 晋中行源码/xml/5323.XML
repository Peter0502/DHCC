				show(#AC_BR_NO,true);
				show(#BR_NAME  ,true);
				show(#PAY_AC_NO,true);
				show(#PAY_NAME,true);
				show(#CASH_AC_NO,true);
				show(#CASH_NAME,true);
				show(#TX_AMT,true);
				show(#OORDERNO,true);
				show(#O_WT_DAY,true);

