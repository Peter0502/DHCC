<ONLOSTFOCUS>
"{
	dim @shuju as string;
	initfd();
	commsend_001();
	$FD16 = '7830';//取现
	$FD30 = delspace(#CASH_ACCT);
	$FD31 = delspace(#AC_NO);
	$FD40 = itoa(#TX_AMT*100,'%012.0f');
	if(#WITHDRAW_FLAG == '1')
	{
	$FD62 = #ID_NO;
	$FD22 = #ID_TYPE;
	$FD25 = #ACNAME;
	}
	$FD79 = #PWD;
	$FD75 = $MSR2;
	$FD76 = $MSR3;
	$FD90 = #CUR_NO;
	$FD94 = #DQFS; //读取标志
	$FD67 = #WITHDRAW_FLAG;//大额取现标志 1为大额 0为小额
	if(#DQFS == '1')
	{
		@shuju=initiccard();
		#CARD_SEQ_ID=strfld(@shuju,'|',4);
		showmsg('#CARD_SEQ_ID='+#CARD_SEQ_ID);
		$FD23 =#CARD_SEQ_ID;//IC卡序列号
	}
	callagn();
	#SW_DATE = $FD44;
	#SW_TRACE_NO = $FD52;
	if('0000' != $FD12)
	{
		showmsg(geterror());
		return(false);
	}
	showmsg('交易成功	！！！');
	runoutput();
	rerun();
}"
</ONLOSTFOCUS>
