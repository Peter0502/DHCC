<ONLOSTFOCUS>
"{
	dim @tota as string;
	dim @txno_bak as string;
	//新增加将全角转换为半角处理080301
	if('01' == #ID_TYPE)
	{
		if(15 != len(#ID_NO) and 18 != len(#ID_NO))
		{
			showmsg('身份证号码位数错误,必须为15或18位!');
			return(false);
		}
		<!--
