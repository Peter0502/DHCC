  <ITEM LINE="5" COL="17" NAME="#CUSID" TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS="" >
    <ONLOSTFOCUS>
	"{
		dim @txno_bak as string;
		@txno_bak=$TXNO;
		$TXNO='9795';
		$FD28=#CUSID;
		commsend_001();
		$TXNO=@txno_bak;
		callserver();	
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
			}
	}"
    </ONLOSTFOCUS>
  </ITEM>
  <!--added by liuyong 2009-9-15 上传图片信息 start-->
  <!--A()函数参数1代表个人照片，2代表印件-->
  <ITEM LINE="5" COL="51" TYPE="%-10s" LINES="160" COLS="120" FUNCTIONS="A(1)" NAME="#PICTURE" VISABLE="FALSE"/>
  <ITEM NAME="#SETPICCONTENT" FUNCTIONS="" TYPE="%-10s">
  <ONLOSTFOCUS>
  "{
        setvfile(true,true);
        addvfile(#PICTURE);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#TRANPIC" TYPE="%-10s" FUNCTIONS="">
  <ONLOSTFOCUS>
  "{    
        if(#PICTURE !='')
        {
          $FD70='1';
        }
        else
        {
          $FD70='0';
        }
          dim @txnobak as string;
          @txnobak = $TXNO;
          $TXNO = '9667';
          $FD28 = #CUSID;
          $FD62 = '';
          $FD72= '1';//图片类型1为照片2为印件
          commsend_001();
          callserver();
          if($FD12 !='0000')
          {
            showmsg(geterror());
            return(false);
          }
          $TXNO = @txnobak;
          setvfile(false,false);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="7" COL="17" NAME="#XB" TYPE="%1s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="T(M)">
