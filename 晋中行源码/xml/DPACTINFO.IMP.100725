  <ITEM NAME="#ACTNO"   DEVICE="MSR" INPUT="TEXT" LINE="6"  COL="15" TYPE="%-19s"  FUNCTIONS="V(NB)m($MSR2,1,19)">
  <ONGOTFOCUS>"{
  	show(#ZHZT,false);
  }"
  </ONGOTFOCUS>
  <ONLOSTFOCUS>"{
    	dim @baktxno as string;
			initfd();
			@baktxno=$TXNO;
    	$TXNO='9952';
    	$FD102=#ACTNO;
    	commsend_001();
    	callserver();
    	$TXNO=@baktxno;
    	if($FD12!='0000'){
    		showmsg(geterror());
    		return(false);
    	}
   if(substr(#ACTNO,0,6)=='621223' and len(rtrim($MSR2))!=37)
    {
       showmsg('刷卡错误,请重试!');
      // return(false);
    }

    	setitems('#ACTNO24#ACTSEQ#VOCTYPE#VOCNUM#FETCHFLAG#PWDFLAG#QUE_PWD#DRAW_PWD#CERTFLAG#CERTNO20#STAMPFLAG#PWDFLAG2#PWD16#PRINTDATE#TXAMT15#CERTTYPE#CUSTNAME#AVBAL#CURCD#CASHFLAG#DSCPT#ACTTYPE#ACTSEQ_2#ACCOUNTFLAG#INTEREST1#INTEREST2#INTEREST3#PAYAMOUNT#NEWVOCNUM#SPEFLAG#PRODNAME#ACTNO_24#EDUCREDIT#DESC10',$FD102);

	// 卡户的判断条件是: 101-1域长度为19并且101-E域不为空
	if( atoi(substr($FD102,24,4))!=0 )
	{
		  #ACTSEQ=substr($FD102,24,4);
	    show(#ACTSEQ,true);
	    show(#LACTSEQ,true);
	    return(true);
	}else{
	    show(#ACTSEQ,false);
	    show(#LACTSEQ,false);
	    #CASHFLAG='1';
	    showblock('ACTINFO',true,true);
			show(#CASHFLAG,true);

    	if(#PWDFLAG=='Y')
    	{
		#DRAW_PWD='';
    		 show(#LDRAW_PWD,true);
    		 show(#DRAW_PWD,true);
    	}
    	else
    	{
    		 show(#LDRAW_PWD,false);
    		 show(#DRAW_PWD,false);
    	}

    	if(#CERTFLAG=='Y')
    	{
    		 show(#LCERTNO,true);
    		 show(#CERTNO,true);
    		 show(#LCERTTYPE,true);
    		 show(#CERTTYPE,true);
    	}
    	else
    	{
    		 show(#LCERTNO,false);
    		 show(#CERTNO,false);
    		 show(#LCERTTYPE,false);
    		 show(#CERTTYPE,false);
    	}
	 }
		
  }"
  </ONLOSTFOCUS>
  </ITEM>
