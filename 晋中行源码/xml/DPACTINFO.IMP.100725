  <!---为了使取款大于五万时输入证件,将金额这一行放在这里--->
  <ITEM NAME="#TXAMT" LINE="8"  COL="10" TYPE="%17.2f"  INPUT="TEXT" FUNCTIONS="V(0000000000000001,9999999999999999)J(#LTXAMT)">
  <ONLOSTFOCUS>
  "{  
  		if(#TXAMT &gt; #AVBAL)
    	{
    		showmsg('取款金额不允许大于余额');
    		return (false);
    	}
  		//五万以上输入证件号码
    	if(#TXAMT &gt; 50000 or #CERTFLAG=='Y')
    	{
    		 show(#LCERTNO,true);
    		 show(#CERTNO,true);
    		 show(#LCERTTYPE,true);
    		 show(#CERTTYPE,true);
    	}
    	else
    	{
    		 show(#LCERTNO,false);
    		 show(#CERTNO,false);
    		 show(#LCERTTYPE,false);
    		 show(#CERTTYPE,false);
    	}
		//改为不控制金额
    	if(#TXAMT &gt;= 0)
    	{
    		show(#S_SELFYN,true);
    		show(#SELFYN,true);
    		enable(#SELFYN,true);
    	}
    	else
    	{
    		show(#S_SELFYN,false);
    		show(#SELFYN,false);
    		enable(#SELFYN,false);
    	}
  }"
  </ONLOSTFOCUS>
  </ITEM>


