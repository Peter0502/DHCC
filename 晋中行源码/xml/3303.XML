<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="3303 个人理财登记"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD23 = #PNUM;
	$FD39 = itoa(#SAVEMONEY*100,'%016.0f');
	$FD62 = #CERTIFICATE;
	$FD67 = #CHOOSEPASSWORD;
	$FD68 = #CHOOSECERTIFICATE;
	$FD69 = #CERTIFYTYPE;
	$FD48 = #THREEMONTH;
	$FD49 = #SIXMONTH;
	$FD50 = #ONEYEAR;
	$FD51 = #TWOYEAR;

	
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(3303                       个人理财登记)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=2303)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <!--输入变量-->
  <ITEM LINE="8" COL="12" TYPE="%-13s" FUNCTIONS="T(理财协议编号:)"/>
  
  <ITEM LINE="10" COL="12" TYPE="%-5s" FUNCTIONS="T(卡号:)"/>
  <ITEM LINE="10" COL="47" TYPE="%-9s" FUNCTIONS="T(留存金额:)"/>
  <ITEM LINE="12" COL="12" TYPE="%-5s" FUNCTIONS="T(姓名:)"/>
  
  <ITEM LINE="14" COL="12" TYPE="%-7s" FUNCTIONS="T(三个月:)"/>
  <ITEM LINE="14" COL="22" TYPE="%1s" FUNCTIONS="T(%)"/>
  <ITEM LINE="14" COL="47" TYPE="%-7s" FUNCTIONS="T(六个月:)"/>
  <ITEM LINE="14" COL="57" TYPE="%1s" FUNCTIONS="T(%)"/>
  <ITEM LINE="15" COL="12" TYPE="%-7s" FUNCTIONS="T(一年:)"/>
  <ITEM LINE="15" COL="22" TYPE="%1s" FUNCTIONS="T(%)"/>
  <ITEM LINE="15" COL="47" TYPE="%-7s" FUNCTIONS="T(两年:)"/>
  <ITEM LINE="15" COL="57" TYPE="%1s" FUNCTIONS="T(%)"/>
  
  <ITEM LINE="17" COL="12" TYPE="%-9s" FUNCTIONS="T(密码支取:)"/>
  <ITEM LINE="17" COL="30" TYPE="%-9s" FUNCTIONS="T(密码:)"/>
  <ITEM LINE="18" COL="12" TYPE="%-9s" FUNCTIONS="T(证件支取:)"/>
  <ITEM LINE="18" COL="30" TYPE="%-9s" FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="19" COL="12" TYPE="%-9s" FUNCTIONS="T(证件号码:)"/>
  
  
  <ITEM NAME="#PNUM"    INPUT="SELECT"  LINE="8" COL="25" TYPE="%-3s" FUNCTIONS="">
    <SELECT>
		<OPTION VALUE="TCA" TITLE="0.TCA套餐A"/>
		<OPTION VALUE="TCB" TITLE="1.TCB套餐B"/>
		<OPTION VALUE="TCC" TITLE="2.TCC套餐C"/>
		<OPTION VALUE="TCD" TITLE="3.TCD套餐D"/>
		<OPTION VALUE="TCE" TITLE="4.TCE套餐E"/>
	</SELECT>
	<ONLOSTFOCUS>
	"{
		
	if(#PNUM=='TCA')
	{
		#THREEMONTH = '100';
		#SIXMONTH   = '0';
		#ONEYEAR    = '0';
		#TWOYEAR    = '0';
	}
	if(#PNUM=='TCB')
	{
		#THREEMONTH = '0';
		#SIXMONTH   = '100';
		#ONEYEAR    = '0';
		#TWOYEAR    = '0';
	}
	if(#PNUM=='TCC')
	{
		#THREEMONTH = '50';
		#SIXMONTH   = '0';
		#ONEYEAR    = '50';
		#TWOYEAR    = '0';
	}
	if(#PNUM=='TCD')
	{
		#THREEMONTH = '30';
		#SIXMONTH   = '20';
		#ONEYEAR    = '50';
		#TWOYEAR    = '0';
	}
	show(#THREEMONTH,true);
	show(#SIXMONTH ,true);
	show(#ONEYEAR,true);
	show(#TWOYEAR,true);
	}"
	</ONLOSTFOCUS>
  </ITEM>
 <ITEM NAME="#CARDNO" INPUT="TEXT" LINE="10" COL="17" TYPE="%-19s" FUNCTIONS="">
<ONLOSTFOCUS>
"{
	$FD37 = #CARDNO;
	commsend_001();
	$FD16 = '9781';
	callserver();
	if($FD12!='0000')
	{
		showmsg(geterror());
		return (false);
	}
	#USERNAME = $FD25;
	show(#USERNAME,true);

}"
</ONLOSTFOCUS>
</ITEM>
  <ITEM NAME="#SAVEMONEY" INPUT="TEXT"  LINE="10" COL="56" TYPE="%17.2f" FUNCTIONS="G(#PNUM?TCE,#CHOOSEPASSWORD)"/>
  <ITEM NAME="#USERNAME" INPUT="TEXT"  LINE="12" COL="17" TYPE="%-50s" FUNCTIONS=""/>
  
  <ITEM NAME="#THREEMONTH"   INPUT="TEXT"  LINE="14" COL="19" TYPE="%3s" FUNCTIONS=""/>
  <ITEM NAME="#SIXMONTH"   INPUT="TEXT"  LINE="14" COL="54" TYPE="%3s" FUNCTIONS=" "/> 
  <ITEM NAME="#ONEYEAR"   INPUT="TEXT"  LINE="15" COL="19" TYPE="%3s" FUNCTIONS=""/>
  <ITEM NAME="#TWOYEAR"   INPUT="TEXT"  LINE="15" COL="54" TYPE="%3s" FUNCTIONS=""/>
  
  <ITEM NAME="#CHOOSEPASSWORD"   INPUT="SELECT"  LINE="17" COL="21" TYPE="%1s" FUNCTIONS="V(NB)G(N,#CHOOSECERTIFICATE)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
  <ITEM NAME="#PASSWD"  LINE="17" COL="39" TYPE="%-6s" DEVICE="KEYBOARD" FUNCTIONS="i()"/>
  <ITEM NAME="#CHOOSECERTIFICATE"   INPUT="SELECT"  LINE="18" COL="21" TYPE="%1s" FUNCTIONS="G(N),#CONFIRM)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
    <ITEM NAME="#CERTIFYTYPE"   INPUT="SELECT"  LINE="18" COL="39" TYPE="%1s" FUNCTIONS="">
	<SELECT>
	<OPTION VALUE="1" TITLE="1 身份证"/>
	<OPTION VALUE="2" TITLE="2 户口簿"/>
	<OPTION VALUE="3" TITLE="3 护照"/>
	<OPTION VALUE="4" TITLE="4 军官证"/>
	<OPTION VALUE="5" TITLE="5 回乡证"/>
	</SELECT>
 </ITEM>


  <ITEM NAME="#CERTIFICATE"   INPUT="TEXT"  LINE="19" COL="21" TYPE="%-20s" FUNCTIONS=""/>
  
  
  
  
   
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#CONFIRM"  LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
<PACKAGE>
000$9999=9P120000A显示内容N505$FD2$FD3$FD32FD82#DEPOSITAVL
</PACKAGE>
<FORMAT NAME="P120000A">
"8                                   个人理财登记成功!
"
</FORMAT>
</OUTPUT>
</TRANSACTION>
