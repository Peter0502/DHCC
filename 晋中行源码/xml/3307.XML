<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="3307 理财帐单打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD31=#ACCOUNTNO;
	$FD34=#ACCTSEQ;
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(3307                        理财帐单打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=2307)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <!--输入变量-->	
  <ITEM LINE="8" COL="12" TYPE="%-9s" FUNCTIONS="T(结算帐号:)"/>
  <ITEM LINE="10" COL="12" TYPE="%-9s" FUNCTIONS="T(帐户序号:)"/>
  
  
  
  <ITEM NAME="#ACCOUNTNO"    INPUT="TEXT"  LINE="8" COL="21" TYPE="%-19s" FUNCTIONS="">
<ONLOSTFOCUS>
  "{
		initfd();
		commsend_001();
		$FD31 = #ACCOUNTNO;
		$FD16 = '9958';
		callserver();
		if($FD12!='0000')
		{
			showmsg(geterror());
			return (false);
		}
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#ACCTSEQ" INPUT="TEXT" LINE="10" COL="21" TYPE="%4d" FUNCTIONS=""/>
  
  
   
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>

<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE>
	NOTHING
	</PACKAGE>
	<PRINT>"{
		print(8,25,'理财帐单打印成功!');
		}"
	</PRINT>
</RESPONSE>
</TRANSACTION>
