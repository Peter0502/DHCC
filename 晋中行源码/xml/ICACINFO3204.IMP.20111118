<!-----取款交易： 显示帐号信息的变量组合----------> 
  <PAGE NAME="DEFAULT">
	  <ITEM NAME="#LACTNO"  LINE="5"  COL="5"   TYPE="%-10s"   FUNCTIONS="T(IC 卡 号: )"  INPUT="LABEL" VISABLE="TRUE"/>
	  <ITEM NAME="#LACTSEQ" LINE="5"  COL="45"  TYPE="%-10s"   FUNCTIONS="T(账户序号: )" INPUT="LABEL" VISABLE="FALSE"/>
	  <ITEM LINE="5" COL="55" TYPE="%-10s" FUNCTIONS="T(照    片:)"/>
	  <ITEM NAME="#LACTNO"  LINE="7"  COL="4"   TYPE="%-16s" VISABLE="TRUE" FUNCTIONS="T(【产品信息】)"/>
	  <ITEM  LINE="8" COL="5"   TYPE="%-70s" VISABLE="TRUE" FUNCTIONS="T(-----------------------------------------------------------------------)"/>
	  <ITEM NAME="#ACTNO"   INPUT="TEXT" LINE="5"  COL="15" TYPE="%-19s"  DEVICE="MSR" FUNCTIONS="V(NB)m($MSR2,1,19)">
		  <ONGOTFOCUS>
		  	"{
		  	  show(#LACCT_CLOSE,false);
		  	  show(#ACCT_CLOSE,false);
		    }"
		  </ONGOTFOCUS>
		  <ONLOSTFOCUS>
		  	"{
		    	dim @baktxno as string;
		    	@baktxno=$TXNO;
		    	$TXNO='9959';
		    	$FD102=#ACTNO;
		    	commsend_001();
		    	callserver();
		    	$TXNO=@baktxno;
		    	if($FD12!='0000')
		    	{
		    		showmsg(geterror());
		    		return(false);
		    	}
  	      setitems('#ACTNO24#ACTSEQ#VOCTYPE#VOCNUM#FETCHFLAG#PWDFLAG#QUE_PWD#CRE_PWD#CERTFLAG#CERTNO20#STAMPFLAG#PWDFLAG2#PWD16#PRINTDATE#TXAMT15#CERTTYPE#CUSTNAME#AVBAL#CURCD#CASHFLAG#DSCPT#ACTTYPE#ACTSEQ_2#ACCOUNTFLAG#INTEREST1#INTEREST2#INTEREST3#PAYAMOUNT#NEWVOCNUM#SPEFLAG#PRODNAME#ACTNO_24#EDUCREDIT#DESC10',$FD102);
						  // 卡户的判断条件是: 101-1域长度为19并且101-E域不为空
			    if( atoi(substr($FD102,24,4))!=0 )
			    {
				    #ACTSEQ=substr($FD102,24,4);
			      show(#ACTSEQ,true);
			      show(#LACTSEQ,true);
			      return(true);
			    }
			    else
			    {
			      #ACCOUNTFLAG='1';
		        show(#ACTSEQ,false);
			      show(#LACTSEQ,false);
			      #CASHFLAG='1';
			      showblock('ACTINFO',true,true);	
					  show(#CASHFLAG,true);
					  if(substr(#SPEFLAG,1,1)=='1')
					  {
					    #EDUCREDIT='Y';
					    show(#S_EDTYPE,true);
					    show(#EDUCREDIT,true);		   
					  }
		    	  if(#PWDFLAG=='Y')
		    	  {
		    		  show(#LDRAW_PWD,true);
		    		  show(#DRAW_PWD,true);
		    	  }
		    	  else
		    	  {
		    		  show(#LDRAW_PWD,false);
		    		  show(#DRAW_PWD,false);
		    	  }    	
		    	  if(#CERTFLAG=='Y')
		    	  {
		    		  show(#LCERTNO,true);
		    		  show(#CERTNO,true);
		    		  show(#LCERTTYPE,true);
		    		  show(#CERTTYPE,true);   		 
		    	  }	
		    	  else
		    	  {
		    		  show(#LCERTNO,false);
		    		  show(#CERTNO,false);
		    		  show(#LCERTTYPE,false);
		    		  show(#CERTTYPE,false);       		 
		    	  }	
		    	 if(substr(#SPEFLAG,2,1)=='1')
		    	 {
		    		 show(#LCERTNO,true);
		    		 show(#CERTNO,true);
		    		 show(#LCERTTYPE,true);
		    		 show(#CERTTYPE,true); 
		    	 }	
		    	 else
		    	 {
		    		 show(#LCERTNO,false);
		    		 show(#CERTNO,false);
		    		 show(#LCERTTYPE,false);
		    		 show(#CERTTYPE,false);  
		    	 }		 
			   }
			   if($FD67=='S')
			   {
			     #CASHFLAG='1';
			     enable(#CASHFLAG,false);
			   }
			   else
			   {
			    enable(#CASHFLAG,true);
			   }
		   }"
		  </ONLOSTFOCUS>
	  </ITEM>
	 <!--add  显示图片信息 start-->
	  <ITEM NAME="#LYACNO" TYPE="%-19s" FUNCTIONS="" />
	  <ITEM LINE="5" COL="65" LINES="240" COLS="160" NAME="#PIC" TYPE="%-10s" VISABLE="FALSE" FUNCTIONS=""/>
	  <ITEM NAME="#SETPIC" TYPE="%-10s" FUNCTIONS="">
		  <ONLOSTFOCUS>
		  "{
		      #LYACNO=#ACTNO;
		      call_96641();
		  }"
		  </ONLOSTFOCUS>
	  </ITEM>
	  <!--add  显示图片信息 end-->
	  <ITEM NAME="#ACTSEQ" INPUT="TEXT" LINE="5"  COL="55" TYPE="%-4s" VISABLE="FALSE" FUNCTIONS="V(NB)">
		  <ONLOSTFOCUS>"{
		    	dim @baktxno as string;
		    	@baktxno=$TXNO;
		    	$TXNO='9927';
		    	$FD102=#ACTNO24+#ACTSEQ+space(350);
		    	commsend_001();
		    	$MSGTYPE='03001';
		    	callserver();
		    	$TXNO=@baktxno;
		    	if($FD12!='0000')
		    	{
		    		showmsg(geterror());
		    		return(false);
		    	}
		    	if($FD2!=$FD3)
		      {
		        showmsg('销户请到开户行!');
		        return(false);
		      }
		    	setitems('#ACTNO24#ACTSEQ#VOCTYPE#VOCNUM#FETCHFLAG#PWDFLAG#QUE_PWD#CERTFLAG#CERTNO20#STAMPFLAG#PWDFLAG2#PWD16#PRINTDATE#TXAMT15#CERTTYPE#CUSTNAME#AVBAL#CURCD#CASHFLAG#DSCPT#ACTTYPE#ACTSEQ_2#ACCOUNTFLAG#INTEREST1#INTEREST2#INTEREST3#PAYAMOUNT#NEWVOCNUM#SPEFLAG#PRODNAME#ACTNO_24#EDUCREDIT#DESC10',$FD102);
		    	if(len(#ACTNO)==13)
					{
					  #ACCOUNTFLAG='1';
					}
			    #CASHFLAG='1';
		    	showblock('ACTINFO',true,true);	
					show(#CASHFLAG,true);
					if(substr(#SPEFLAG,1,1)=='1')
					{ 
					   #EDUCREDIT='Y'; 
					   show(#S_EDTYPE,true);
					   show(#EDUCREDIT,true);		   
					}
		    	if(#PWDFLAG=='Y')
		    	{
		    		 show(#LDRAW_PWD,true);
		    		 show(#DRAW_PWD,true);
		    	}
		    	else
		    	{
		    		 show(#LDRAW_PWD,false);
		    		 show(#DRAW_PWD,false);
		    	}    	
		    	if(#CERTFLAG=='Y')
		    	{
		    		 show(#LCERTNO,true);
		    		 show(#CERTNO,true);
		    		 show(#LCERTTYPE,true);
		    		 show(#CERTTYPE,true);   		 
		    	}	
		    	else
		    	{
		    		 show(#LCERTNO,false);
		    		 show(#CERTNO,false);
		    		 show(#LCERTTYPE,false);
		    		 show(#CERTTYPE,false);       		 
		    	}	 
		    	if(substr(#SPEFLAG,2,1)=='1')
		    	{
		    			show(#LCERTNO,true);
		    		  show(#CERTNO,true);
		    		  show(#LCERTTYPE,true);
		    		  show(#CERTTYPE,true); 
		    	}	
		    	else
		    	{
		    		 show(#LCERTNO,false);
		    		 show(#CERTNO,false);
		    		 show(#LCERTTYPE,false);
		    		 show(#CERTTYPE,false);  
		    	}		 				
		   }"
		  </ONLOSTFOCUS>
	  </ITEM> 
	  <ITEM NAME="#ACTNO24"   TYPE="%-24s" FUNCTIONS=""/>
	  <!-- #VOCTYPE   -->  
	  <!-- #VOCNUM   -->    
	  <ITEM NAME="#FETCHFLAG" TYPE="%1s" FUNCTIONS=""/>
	  <!-- #PWDFLAG   -->  
	  <ITEM NAME="#QUE_PWD"   TYPE="%6s" FUNCTIONS=""/>
	  <ITEM NAME="#CRE_PWD"   TYPE="%6s" FUNCTIONS=""/>  
	  <!-- #DRAW_PWD   --> 
	  <ITEM NAME="#SDRAW_PWD" TYPE="%6s" FUNCTIONS=""/><!---为避免密码自动填充6个*，定义此变量去接收密码--->
	   <!-- #CERTFLAG   -->    
	   <!-- #CERTNO   --> 
	  <ITEM NAME="#STAMPFLAG" TYPE="%1s" FUNCTIONS=""/>
	  <ITEM NAME="#PWDFLAG2" TYPE="%1s" FUNCTIONS=""/>  
	  <ITEM NAME="#PWD16"  TYPE="%-16s" FUNCTIONS=""/>  
	  <ITEM NAME="#PRINTDATE" TYPE="%-8s" FUNCTIONS=""/>      
	  <ITEM NAME="#TXAMT15"   TYPE="%-16s" FUNCTIONS=""/>
	  <!-- #CERTTYPE   -->     
	  <!-- #CUSTNAME   -->  
	  <!-- #AVBAL   --> 
	  <!-- #CURCD   --> 
	  <!-- #CASHFLAG   --> 
	  <ITEM NAME="#DSCPT"       TYPE="%-3s" FUNCTIONS=""/>
	  <ITEM NAME="#ACTTYPE"     TYPE="%-1s" FUNCTIONS=""/>
	  <ITEM NAME="#ACTSEQ_2"    TYPE="%-4s" FUNCTIONS=""/>
	  <ITEM NAME="#ACCOUNTFLAG" TYPE="%-1s" FUNCTIONS=""/>
	  <ITEM NAME="#INTEREST1"   TYPE="%-16s" FUNCTIONS=""/>
	  <ITEM NAME="#INTEREST2"   TYPE="%-16s" FUNCTIONS=""/>  
	  <ITEM NAME="#INTEREST3"   TYPE="%-16s" FUNCTIONS=""/>
	  <ITEM NAME="#PAYAMOUNT"   TYPE="%-16s" FUNCTIONS=""/>  
	  <ITEM NAME="#NEWVOCNUM"   TYPE="%-16d" FUNCTIONS=""/>
	  <ITEM NAME="#SPEFLAG"     TYPE="%-20s" FUNCTIONS=""/>    
