	  <ITEM NAME="#DESC10"      TYPE="%-10s" FUNCTIONS=""/>
	  <BLOCK NAME="ACTINFO" LINE="9" COL="5" VISABLE="FALSE">
		  <ITEM LINE="0"  COL="0"  TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
		  <ITEM LINE="1"  COL="0"  TYPE="%-10s" FUNCTIONS="T(币    种:!)"/>
		  <ITEM LINE="2"  COL="0"  TYPE="%-10s" FUNCTIONS="T(卡 余 额:)"/>
		  <ITEM LINE="3"  COL="0"  TYPE="%-10s" FUNCTIONS="T(产品名称:)"/>
		  <!--ITEM LINE="4"  COL="40" TYPE="%-10s" FUNCTIONS="T(电子现金:)"/-->
	    <ITEM LINE="5"  COL="0"  TYPE="%-10s" FUNCTIONS="T(凭证类型:!)"/>
		  <ITEM LINE="5"  COL="40" TYPE="%-10s" FUNCTIONS="T(凭证号码:)"/>
		  <ITEM LINE="6"  COL="0"  TYPE="%-10s" FUNCTIONS="T(密码支取:!)"/>
		  <ITEM LINE="6"  COL="20" TYPE="%-10s" INPUT="LABEL" VISABLE="FALSE"  FUNCTIONS="T(输入密码:)"  NAME="#LDRAW_PWD" /> 
		  <ITEM LINE="6"  COL="40" TYPE="%-10s" FUNCTIONS="T(证件支取:!)"/>   
		  <ITEM LINE="7"  COL="0"  TYPE="%-10s" INPUT="LABEL" VISABLE="FALSE"  FUNCTIONS="T(教育证明:!)" NAME="#S_EDTYPE"/> 
		  <ITEM LINE="7"  COL="40" TYPE="%-10s" INPUT="LABEL" VISABLE="FALSE"  FUNCTIONS="T(证件类型:!)" NAME="#LCERTTYPE"/>    
		  <ITEM LINE="8"  COL="0"  TYPE="%-10s" FUNCTIONS="T(现转标志:!)"/> 
		  <ITEM LINE="8"  COL="40" TYPE="%-10s" INPUT="LABEL" VISABLE="FALSE"  FUNCTIONS="T(证件号码:)"  NAME="#LCERTNO" /> 
		  <ITEM LINE="9"   COL="0"  TYPE="%-10s" NAME="#LACCT_CLOSE" FUNCTIONS="T(销户原因:!)" VISABLE="FALSE"/>   
		  <ITEM LINE="10" COL="0"  TYPE="%-12s" FUNCTIONS="T(是否本人:!)"/>
