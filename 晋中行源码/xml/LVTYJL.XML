<TRANSACTION RIGHTS="11111111"  TITLE="GKDJ 普通借记业务录入)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(                 通用截留附加域录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#INIT" TYPE="%-1s" >
  <ONLOSTFOCUS>"{
	if(#WTDAY!='' and #ORDERNO!=''){
		enable(#WTDAY,false);
		enable(#ORDERNO,false);
	}else{
		enable(#WTDAY,true);
		enable(#ORDERNO,true);
	}	
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="5"  COL="3"  TYPE="%-14s" FUNCTIONS="T(    委托日期:)"/>
  <ITEM LINE="5"  COL="27"  TYPE="%-10s" FUNCTIONS="T(交易序号:)"/>
  <ITEM LINE="5"  COL="51"  TYPE="%-10s" FUNCTIONS="T(出票日期:)"/>
  <ITEM LINE="6"  COL="3"  TYPE="%-14s" FUNCTIONS="T(    支票号码:)"/>
  <ITEM LINE="6"  COL="51"  TYPE="%-10s" FUNCTIONS="T(付款行号:)"/>
  <ITEM LINE="7"  COL="3" TYPE="%-14s" FUNCTIONS="T(代理付款行号:)"/>
  <ITEM LINE="7"  COL="45"  TYPE="%-10s" FUNCTIONS="T(出票金额:)"/>
  <ITEM LINE="8"  COL="3"  TYPE="%-14s" FUNCTIONS="T(        用途:)"/>
  <ITEM LINE="9"  COL="3"  TYPE="%-14s" FUNCTIONS="T(    票据种类:)"/>
  <ITEM LINE="9"  COL="33" TYPE="%-14s" FUNCTIONS="T(提示付款日期:)" />
  <ITEM LINE="10" COL="3" TYPE="%-6s" FUNCTIONS="T(密押:)" />
  <ITEM LINE="10" COL="29" TYPE="%-8s"  FUNCTIONS="T(到期日:)" />
  <ITEM LINE="10" COL="45" TYPE="%-14s" FUNCTIONS="T(承兑协议编号:)" />
  <ITEM LINE="11" COL="3" TYPE="%-14s" FUNCTIONS="T(交易合同号码:)" />
  <ITEM LINE="11" COL="45" TYPE="%-10s" FUNCTIONS="T(承兑日期:)" />
  <ITEM LINE="12" COL="3" TYPE="%-14s"  FUNCTIONS="T(      承兑人:)" />
  <ITEM LINE="13" COL="3" TYPE="%-14s"  FUNCTIONS="T(      申请人:)" />
  <ITEM LINE="14" COL="3" TYPE="%-14s"  FUNCTIONS="T(  申请人帐户:)" />
  <ITEM LINE="15" COL="3" TYPE="%-14s"  FUNCTIONS="T(申请人开户行:)" />
  <ITEM LINE="16" COL="3" TYPE="%-14s"  FUNCTIONS="T(收款人开户行:)" />
  <ITEM LINE="17" COL="3" TYPE="%-14s"  FUNCTIONS="T(  出票人全称:)" />
  <ITEM LINE="18" COL="3" TYPE="%-14s"  FUNCTIONS="T(  出票人帐号:)" />
  <ITEM LINE="19" COL="3" TYPE="%-14s"  FUNCTIONS="T(  付款行名称:)" />
  <ITEM LINE="20" COL="3" TYPE="%-14s"  FUNCTIONS="T(        附言:)" />
  <ITEM NAME="#WTDAY" LINE="5"  COL="17"  INPUT="TEXT" TYPE="%8d" FUNCTIONS="V(NB)T()"/>
  <ITEM NAME="#ORDERNO" LINE="5"  COL="37"  INPUT="TEXT" TYPE="%8d" FUNCTIONS="V(NB)T()"/>

  <ITEM NAME="#CPDAY" LINE="5"  COL="61"  DEVICE="KEYBOARD" TYPE="%8d" FUNCTIONS="V(NB)T(20061101)D()"/>
  <ITEM NAME="#PNUM" LINE="6"  COL="17"  DEVICE="KEYBOARD" TYPE="%20d" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#PAYNO" LINE="6"  COL="61"  DEVICE="KEYBOARD" TYPE="%12d" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#DLPAYNO" LINE="7"  COL="17"  DEVICE="KEYBOARD" TYPE="%12d" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#CPAMT" LINE="7"  COL="55"  DEVICE="KEYBOARD" TYPE="%16.2f" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#USE" LINE="8"  COL="17"  DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#LVPTYPE" LINE="9"  COL="17"  DEVICE="KEYBOARD" TYPE="%-2s" FUNCTIONS="V(NB)">
  <SELECT LINE="9" COL="19">
	<OPTION VALUE="02" TITLE="0.银行汇票"/>
	<OPTION VALUE="03" TITLE="0.商业承兑汇票"/>
	<OPTION VALUE="04" TITLE="0.银行承兑汇票"/>
	<OPTION VALUE="05" TITLE="0.商业本票"/>
	<OPTION VALUE="06" TITLE="0.银行本票"/>
  </SELECT>
  </ITEM>
  <ITEM NAME="#TSDAY" LINE="9"  COL="47" DEVICE="KEYBOARD" TYPE="%8d" FUNCTIONS="V(NB)T(20061101)D()"/>
  <ITEM NAME="#MAC" LINE="10"  COL="9" DEVICE="KEYBOARD" TYPE="%20d" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#STOPDAY" LINE="10"  COL="37" DEVICE="KEYBOARD" TYPE="%8d" FUNCTIONS="V(NB)T(20071101)D()"/>
  <ITEM NAME="#XYNO" LINE="10"  COL="59" DEVICE="KEYBOARD" TYPE="%20d" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#HTNO" LINE="11"  COL="17" DEVICE="KEYBOARD" TYPE="%20d" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#CDDAY" LINE="11"  COL="55" DEVICE="KEYBOARD" TYPE="%8d" FUNCTIONS="V(NB)T(20061201)D()"/>
  <ITEM NAME="#CDNAME" LINE="12"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#REQNAME" LINE="13"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#REQACTNO" LINE="14"  COL="17" DEVICE="KEYBOARD" TYPE="%-32s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#PAYBRNAME" LINE="15"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#CASHBRNAME" LINE="16"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#CPNAME" LINE="17"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#CPACTNO" LINE="18"  COL="17" DEVICE="KEYBOARD" TYPE="%-32s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#PAYBRNAME2" LINE="19"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#CONTENT" LINE="20"  COL="17" DEVICE="KEYBOARD" TYPE="%-60s" FUNCTIONS="V(NB)T(1)"/>
  <ITEM NAME="#SUCCESS" TYPE="%-1s" FUNCTIONS=""/>
  <ITEM LINE="21"  COL="51"  TYPE="%-8s" FUNCTIONS="T(背书清单)" INPUT="BUTTON">
  <ONCLICK>"{
	open('65203.XML','#WTDAY='+#WTDAY+';#ORDERNO='+#ORDERNO+';');
    }"
  </ONCLICK>
  </ITEM>
  <!--输入项定义到这里结束-->
  <!--系统变量定义 -->
  <ITEM NAME="#HTRMSEQ" TYPE="%-2s"  FUNCTIONS="T($HTRMSEQ)"/>
  <ITEM NAME="#HEJFNO"  TYPE="%-6s"  FUNCTIONS="T($HEJFNO)"/>
  <ITEM NAME="#KINBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#OPNBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#KINDNO"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#ACTNO"   TYPE="%-18s" FUNCTIONS="T(#CASHACTNO18,1,18)"/>
  <ITEM NAME="#TRMSEQ"  TYPE="%-2s"  FUNCTIONS="T($TRMSEQ)"/>
  <ITEM NAME="#EJFNO"   TYPE="%-6s"  FUNCTIONS="T($TXTNO)"/>
  <ITEM NAME="#TASKID"  TYPE="%-2s"  FUNCTIONS="T(UB)"/>
  <ITEM NAME="#TMTYPE"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#APTYPE"  TYPE="%-1s"  FUNCTIONS="T(l)"/>
  <ITEM NAME="#TXNO"    TYPE="%-2s"  FUNCTIONS="T(20)"/>
  <ITEM NAME="#PTYPE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#DSCPT"   TYPE="%-3s"  FUNCTIONS="j(#FEEFLAG=0,T{001})j(#FEEFLAG=1,T{001})j(#FEEFLAG=2,T{002})j(#FEEFLAG=3,T{002})j(#FEEFLAG=4,T{002})"/>
  <ITEM NAME="#HCODE"   TYPE="%-1s"  FUNCTIONS="T($HCODE)"/>
  <ITEM NAME="#RCODE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TXAMT"   TYPE="%13.2f" FUNCTIONS="T(#LTXAMT,4,12)"/>
  <ITEM NAME="#TXTYPE"  TYPE="%-2s"   FUNCTIONS="T(#ZCTYPE)"/>
  <ITEM NAME="#MULTTX"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CRDB"    TYPE="%-1s"  FUNCTIONS="T(1)"/>
  <ITEM NAME="#SUPIN1"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SUPIN2"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#REASON"  TYPE="%-6s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TLRNO"   TYPE="%6d"  FUNCTIONS="T($TLRNO)"/>
  <ITEM NAME="#TRNMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SBTMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CURCD"   TYPE="%-3s"  FUNCTIONS="T(000)"/>
  <ITEM NAME="#XCRT"    TYPE="%-8s"  FUNCTIONS="T($XCRT)"/>
  <ITEM NAME="#NBCD"    TYPE="%-1s"  FUNCTIONS="T(1)"/>
  <ITEM LINE="21"  COL="69"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
    <ONLOSTFOCUS>"{
        dim @tota as string;
        @tota=callserver();
        if(@tota==''){
           showmsg('无法发送数据到主机');
           return(true);
       }
       showmsg(substr(@tota,36,5) +'  '+substr(@tota,44,40));
       rerun();
       return(true);
  }"
  </ONLOSTFOCUS>
  </ITEM>
</VARIABLE>
<!--发送信息包-->
<REQUEST><PACKAGE SERVICE="dh65202">
$APCTLHD
$HTRMSEQ$HEJFNO#OPNBR#KINBR#TRMSEQ#EJFNO#TASKID#TMTYPE#APTYPE#TXNO#PTYPE
#DSCPT#HCODE#ACTNO#TXTYPE#MULTTX#CRDB$SUPCD1$SUPCD2#REASON#TLRNO
#NBCD#TRNMOD#SBTMOD#CURCD#XCRT#TXAMT
#WTDAY#ORDERNO#CPDAY#PNUM#PAYNO#DLPAYNO#CPAMT#USE
#LVPTYPE#TSDAY#MAC#STOPDAY#XYNO#HTNO#CDDAY#CDNAME#REQNAME#REQACTNO#PAYBRNAME#CASHBRNAME#CPNAME#CPACTNO#PAYBRNAME2#CONTENT
</PACKAGE></REQUEST>
<RESPONSE>
<PACKAGE>l65201#SUCCESS
</PACKAGE>
</RESPONSE>
<OUTPUT>
<PACKAGE>
000$9999=9B800500A前台显示N505
</PACKAGE> 
<FORMAT NAME="B800500A">
"05                      附加域录入成功 
"
</FORMAT>
</OUTPUT>
</TRANSACTION>
