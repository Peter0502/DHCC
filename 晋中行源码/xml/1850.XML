<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1850       冲正交易)"
	xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
"{
	refresh();
	initfd();
	commsend_001();
}"
</COMMSEND>
<COMMRECV>
"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
	<ITEM LINE="3" COL="3" TYPE="%-60s" FUNCTIONS="T(1850                        冲正交易)"/>
	<ITEM LINE="4" COL="3" TYPE="%-74s" FUNCTIONS="T(--------------------------------------------------------------------------)"/>
	<ITEM NAME="#TXCODE" TYPE="%-30s" FUNCTIONS="T(1850)"/>
	<ITEM TYPE="%-30s" NAME="#TXNAME" FUNCTIONS="T(冲正交易)"/>

	<ITEM LINE="7"  COL="15"  TYPE="%-12s"  FUNCTIONS="T(原交易序号:)" />
	<ITEM LINE="7"  COL="27"  NAME="#TxNo" TYPE="%-10s" INPUT="TEXT" FUNCTIONS="T()V(NB)"/>
	<ITEM LINE="7"  COL="37"  TYPE="%-12s"  FUNCTIONS="T(原交易日期:)" />
	<ITEM LINE="7"  COL="49"  NAME="#TxDate" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="T()V(NB)"/>

	<ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
	<ONCLICK>
	"{
		initfd();
		commsend_001();
		$FD16='1227';
		$FD18='pbp';
		$FD28='sxps';
		$FD44=#TxNo;
		$FD61=#TxDate;
		callagnpbp();
		if($FD12!='0000')
		{
			showmsg(geterror());
			return(false);
		}
		runoutput();
		showall();
	}"
	</ONCLICK>
	</ITEM>
</VARIABLE>

<REQUEST>
</REQUEST>

<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="按任意键退出">
NOTHING
</PACKAGE>
	<PRINT>
	"{
		PrintTxHead();
		print(10,10, '冲正交易成功');
	
		PrintTxTail();
		}"
	</PRINT>
</RESPONSE>
</TRANSACTION>
