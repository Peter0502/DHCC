<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5243 自动提入票据清单打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	$MSGTYPE = '03001';
	commsend_001();
	$FD44 = #INDATE;
	$FD91 = #BRNO;
   	
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5243                      自动提入票据清单打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4243)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  
  <ITEM LINE="10" COL="25" TYPE="%-10s" FUNCTIONS="T(网 点 号)"/>
  <ITEM LINE="12" COL="25" TYPE="%-10s" FUNCTIONS="T(提入日期)"/>

  <ITEM LINE="10" COL="35" TYPE="%-5s"  NAME="#BRNO"   INPUT="TEXT" FUNCTIONS="T($KINBR)"/>
  <ITEM LINE="12" COL="35" TYPE="%-8s"  NAME="#INDATE" INPUT="TEXT" FUNCTIONS="T($HDATE)"/>

  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="网点提出自动录入成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'自动提入票据清单打印!');			
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>



