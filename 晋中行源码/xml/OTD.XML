<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111" TITLE="OTD.CMD 前台系统参数设置"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">


<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->



<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
 "T(OTD.CMD                     前台系统参数设置)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3" TYPE="%-74s" FUNCTIONS=
 "T(─────────────────────────────────────)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->

  
  <ITEM LINE="5"  COL="5" TYPE="%-10s" FUNCTIONS="T(终端类型:!)"/>
  <ITEM LINE="5" COL="45" TYPE="%-10s" FUNCTIONS="T(验印仪器:!)"/>
  <ITEM LINE="7"  COL="5" TYPE="%-10s" FUNCTIONS="T(打印指令:!)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(打印间距:!)"/>
  <ITEM LINE="11" COL="5" TYPE="%-10s" FUNCTIONS="T(密码键盘:!)"/>
  <ITEM INPUT="LABEL" NAME="#LPINPADPORT" VISABLE="FALSE" LINE="11" COL="45" TYPE="%-14s" FUNCTIONS="T(密码键盘辅口:!)"/>
  <ITEM LINE="13" COL="5" TYPE="%-10s" FUNCTIONS="T(磁条读写:!)"/>
  <ITEM LINE="15" COL="5" TYPE="%-10s" FUNCTIONS="T(IC  读写:!)"/>
  <ITEM LINE="17" COL="5" TYPE="%-10s" FUNCTIONS="T(信息交互机:!)"/>
  <ITEM LINE="19" COL="5" TYPE="%-10s" FUNCTIONS="T(评 价 器:!)"/>
  <ITEM INPUT="LABEL" NAME="#LMSRPORT" VISABLE="FALSE" LINE="13" COL="45" TYPE="%-14s" FUNCTIONS="T(磁条读写辅口:!)"/>
  <ITEM INPUT="LABEL" NAME="#ICSRPORT" VISABLE="FALSE" LINE="15" COL="45" TYPE="%-14s" FUNCTIONS="T(IC读写辅口:!)"/>
  <ITEM INPUT="LABEL" NAME="#INFOVRPORT" VISABLE="FALSE" LINE="17" COL="30" TYPE="%-14s" FUNCTIONS="T(信息交互辅口:!)"/>
  <ITEM INPUT="LABEL" NAME="#EVALRPORT" VISABLE="FALSE" LINE="19" COL="30" TYPE="%-14s" FUNCTIONS="T(评价器辅口:!)"/>
  <ITEM INPUT="LABEL" NAME="#INFOVRPTIME" VISABLE="FALSE" LINE="17" COL="55" TYPE="%-14s" FUNCTIONS="T(超时时间:!)"/>
  <ITEM INPUT="LABEL" NAME="#EVALRPTIME" VISABLE="FALSE" LINE="19" COL="55" TYPE="%-14s" FUNCTIONS="T(超时时间:!)"/>
  <ITEM LINE="21" COL="5" TYPE="%-10s" FUNCTIONS="T(选单弹出:!)"/>
  <ITEM LINE="23" COL="5" TYPE="%-10s" FUNCTIONS="T(打印字体:!)"/>

  <ITEM LINE="5"  COL="15" NAME="#TERMINAL"  TYPE="%1d"  DEVICE="KEYBOARD" FUNCTIONS="T($TERM)V(0,6)">
    <SELECT>
      <OPTION VALUE="0  Ｗindows" TITLE="0.Ｗindows"/>
      <OPTION VALUE="1  实达终端" TITLE="1.实达终端"/>
      <OPTION VALUE="2  长城终端" TITLE="2.长城终端"/>
      <OPTION VALUE="3  昌霖终端" TITLE="3.昌霖终端"/>
      <OPTION VALUE="4  国光终端" TITLE="4.国光终端"/>
      <OPTION VALUE="5  WYSE终端" TITLE="5.WYSE终端"/>
      <OPTION VALUE="6  新大陆终端" TITLE="5.新大陆终端"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="5" COL="59" NAME="#YANYINKIND" TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($YANYINKIND)V(0,1)">
  	<SELECT >
      <OPTION VALUE="0  无验印仪器" TITLE="0.无验印仪器"/>
      <OPTION VALUE="1  银之杰验印仪器" TITLE="1.银之杰验印仪器"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="7" COL="15" NAME="#PRINTER"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PRINTER)V(0,2)G(#PRINTER?1,#PINPAD)">
    <SELECT >
      <OPTION VALUE="0  无打印机" TITLE="0.无打印机"/>
      <OPTION VALUE="1  OKI 指令" TITLE="1.OKI 指令"/>
      <OPTION VALUE="2  PR2 指令" TITLE="2.PR2 指令"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="9" COL="15" NAME="#LINESPACE"   TYPE="%1s" DEVICE="KEYBOARD" FUNCTIONS="T($LNSPACE)">
    <SELECT >
      <OPTION VALUE="5  1/5英寸换行量(仅适用于OKI指令)" TITLE="1.1/5英寸换行量(仅适用于OKI指令)"/>
      <OPTION VALUE="3  1/3英寸换行量(仅适用于OKI指令)" TITLE="1.1/3英寸换行量(仅适用于OKI指令)"/>
      <OPTION VALUE="4  1/4英寸换行量(仅适用于OKI指令)" TITLE="0.1/4英寸换行量(仅适用于OKI指令)"/>
      <OPTION VALUE="6  1/6英寸换行量(仅适用于OKI指令)" TITLE="1.1/6英寸换行量(仅适用于OKI指令)"/>
      <OPTION VALUE="C  1/12英寸换行量(仅适用于OKI指令)" TITLE="1.1/12英寸换行量(仅适用于OKI指令)"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="11" COL="15" NAME="#PINPAD"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PINPAD)V(0,3)">
    <SELECT>
      <OPTION VALUE="0  无密码小键盘" TITLE="0.无密码小键盘"/>
      <OPTION VALUE="1  实达密码键盘" TITLE="1.实达密码键盘"/>
      <OPTION VALUE="2  聚星密码键盘" TITLE="2.聚星密码键盘"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#PINPAD=='0'){
			show(#LPINPADPORT,false);
			show(#PINPADPORT,false);
		}else{
			show(#LPINPADPORT,true);
			show(#PINPADPORT,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="11" COL="59" NAME="#PINPADPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($PINPADPORT)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一9600"/><!--辅口1/9600-->
      <OPTION VALUE="2" TITLE="0.辅口二9600"/><!--辅口2/9600-->
      <OPTION VALUE="3" TITLE="0.辅口三9600"/><!--辅口3/9600-->
      <OPTION VALUE="4" TITLE="0.辅口一"/><!--辅口1/1200-->
      <OPTION VALUE="5" TITLE="0.辅口二"/><!--辅口2/1200-->
      <OPTION VALUE="6" TITLE="0.辅口三"/><!--辅口3/1200-->
    </SELECT>
  </ITEM>
  <ITEM LINE="13" COL="15" NAME="#MSR"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($MSR)V(0,3)">
    <SELECT >
      <OPTION VALUE="0  无磁条读写器  " TITLE="0.无磁条读写器  "/>
      <OPTION VALUE="1  实达磁条读写器" TITLE="1.实达磁条读写器"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#MSR=='0'){
			show(#LMSRPORT,false);
			show(#MSRPORT,false);
		}else{
			show(#LMSRPORT,true);
			show(#MSRPORT,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="13" COL="59" NAME="#MSRPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($MSRPORT)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/><!--辅口1/9600-->
      <OPTION VALUE="2" TITLE="0.辅口二"/><!--辅口2/9600-->
      <OPTION VALUE="3" TITLE="0.辅口三"/><!--辅口3/9600-->
      <OPTION VALUE="4" TITLE="0.辅口一1200"/><!--辅口1/1200-->
      <OPTION VALUE="5" TITLE="0.辅口二1200"/><!--辅口2/1200-->
      <OPTION VALUE="6" TITLE="0.辅口三1200"/><!--辅口3/1200-->
    </SELECT>
  </ITEM>

  <ITEM LINE="15" COL="15" NAME="#ICC"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($ICC)V(0,3)">
    <SELECT >
      <OPTION VALUE="0  无IC读写器  " TITLE="0.无IC读写器  "/>
      <OPTION VALUE="1  实达IC读写器" TITLE="1.实达IC读写器"/>
      <OPTION VALUE="2  实达IC读写器" TITLE="1.实达串口IC读写器"/>
      <OPTION VALUE="3  实达IC读写器" TITLE="1.升腾串口IC读写器"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#ICC=='0'){
			show(#ICSRPORT,false);
			show(#ICCPORT,false);
		}else{
			show(#ICSRPORT,true);
			show(#ICCPORT,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="15" COL="57" NAME="#ICCPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($ICCPORT)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/><!--辅口1/9600-->
      <OPTION VALUE="2" TITLE="0.辅口二"/><!--辅口2/9600-->
      <OPTION VALUE="3" TITLE="0.辅口三"/><!--辅口3/9600-->
      <OPTION VALUE="4" TITLE="0.辅口一1200"/><!--辅口1/1200-->
      <OPTION VALUE="5" TITLE="0.辅口二1200"/><!--辅口2/1200-->
      <OPTION VALUE="6" TITLE="0.辅口三1200"/><!--辅口3/1200-->
    </SELECT>
  </ITEM>
  
  
  <ITEM LINE="17" COL="15" NAME="#INFOVIEWER"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($InfoViewerFlag)V(0,3)">
    <SELECT >
      <OPTION VALUE="0  无信息交互机" TITLE="0.无信息交互机  "/>
      <OPTION VALUE="1  升腾i70交互机" TITLE="1.升腾i70交互机"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#INFOVIEWER=='0'){
		  show(#INFOVRPORT,false);
			show(#INFOVPORT,false);
		  show(#INFOVRPTIME,false);
			show(#INFOVTIME,false);
		}else{
		  show(#INFOVRPORT,true);
			show(#INFOVPORT,true);
			show(#INFOVRPTIME,true);
			show(#INFOVTIME,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="17" COL="40" NAME="#INFOVPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($InfoViewerComPort)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/>
      <OPTION VALUE="2" TITLE="0.辅口二"/>
      <OPTION VALUE="3" TITLE="0.辅口三"/>
      <OPTION VALUE="4" TITLE="0.辅口四"/>
      <OPTION VALUE="5" TITLE="0.辅口五"/>
      <OPTION VALUE="6" TITLE="0.辅口六"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="17" COL="65" NAME="#INFOVTIME"   INPUT="SELECT" VISABLE="FALSE" TYPE="%2d" FUNCTIONS="T($InfoViewerTimeOut)">
    <SELECT>
      <OPTION VALUE="10" TITLE="0.10秒"/>
      <OPTION VALUE="20" TITLE="0.20秒"/>
      <OPTION VALUE="30" TITLE="0.30秒"/>
      <OPTION VALUE="40" TITLE="0.40秒"/>
      <OPTION VALUE="50" TITLE="0.50秒"/>
      <OPTION VALUE="60" TITLE="0.60秒"/>
    </SELECT>
  </ITEM>
  
  <ITEM LINE="19" COL="15" NAME="#EVALFLAG"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($EvaluatorFlag)V(0,3)">
    <SELECT >
      <OPTION VALUE="0  评价器" TITLE="0.无评价器  "/>
      <OPTION VALUE="1  升腾i70评价器" TITLE="1.升腾i70评价器"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#EVALFLAG=='0'){
		  show(#EVALRPORT,false);
			show(#EVALPORT,false);
			show(#EVALRPTIME,false);
			show(#EVALTIME,false);
		}else{
		  show(#EVALRPORT,true);
			show(#EVALPORT,true);
			show(#EVALRPTIME,true);
			show(#EVALTIME,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="19" COL="40" NAME="#EVALPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($EvalComPort)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/>
      <OPTION VALUE="2" TITLE="0.辅口二"/>
      <OPTION VALUE="3" TITLE="0.辅口三"/>
      <OPTION VALUE="4" TITLE="0.辅口四"/>
      <OPTION VALUE="5" TITLE="0.辅口五"/>
      <OPTION VALUE="6" TITLE="0.辅口六"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="19" COL="65" NAME="#EVALTIME"   INPUT="SELECT" VISABLE="FALSE" TYPE="%2d" FUNCTIONS="T($EvalTimeOut)">
    <SELECT>
      <OPTION VALUE="10" TITLE="0.10秒"/>
      <OPTION VALUE="20" TITLE="0.20秒"/>
      <OPTION VALUE="30" TITLE="0.30秒"/>
      <OPTION VALUE="40" TITLE="0.40秒"/>
      <OPTION VALUE="50" TITLE="0.50秒"/>
      <OPTION VALUE="60" TITLE="0.60秒"/>
    </SELECT>
  </ITEM>

  <ITEM LINE="21" COL="15" NAME="#AUTOSELECT"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($AUTOSEL)V(0,1)">
    <SELECT >
      <OPTION VALUE="0  按F2键后弹出菜单" TITLE="0.按F2键后弹出菜单"/>
      <OPTION VALUE="1  自动弹出选择菜单" TITLE="1.自动弹出选择菜单"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="23" COL="15" NAME="#FONTFLAG"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PRFONT)V(0,1)">
    <SELECT >
      <OPTION VALUE="0  打印机支持字体放大指令"   TITLE="0.打印机支持字体放大指令"/>
      <OPTION VALUE="1  打印机不支持字体放大指令" TITLE="1.打印机不支持字体放大指令"/>
    </SELECT>
  </ITEM>
 <ITEM LINE="25"  COL="65" TYPE="%-6s" FUNCTIONS="T(保  存)" INPUT="BUTTON" > 
 <ONCLICK>"{
	setconfig($TLRNO+'.'+'TERM',#TERMINAL);
	setconfig($TLRNO+'.'+'YANYINKIND',#YANYINKIND);
	setconfig($TLRNO+'.'+'PRINTER',#PRINTER);
	setconfig($TLRNO+'.'+'PINPAD',#PINPAD);
	setconfig($TLRNO+'.'+'PINPADPORT',#PINPADPORT);
	setconfig($TLRNO+'.'+'MSR',#MSR);
	// showmsg('#ICC='+#ICC);
	// showmsg('#ICCPORT='+#ICCPORT);
	setconfig($TLRNO+'.'+'ICC',#ICC);
	setconfig($TLRNO+'.'+'ICCPORT',#ICCPORT);
	setconfig($TLRNO+'.'+'MSRPORT',#MSRPORT);
	setconfig($TLRNO+'.'+'AUTOSELECT',#AUTOSELECT);
	setconfig($TLRNO+'.'+'LINESPACE',#LINESPACE);
	setconfig($TLRNO+'.'+'FONTFLAG',#FONTFLAG);
	setconfig($TLRNO+'.'+'NETPRINTER','0');//无网络打印机
	setconfig($TLRNO+'.'+'InfoViewerFlag',#INFOVIEWER);
	setconfig($TLRNO+'.'+'InfoViewerComPort',#INFOVPORT);
	setconfig($TLRNO+'.'+'InfoViewerTimeOut',#INFOVTIME);
	setconfig($TLRNO+'.'+'EvaluatorFlag',#EVALFLAG);
	setconfig($TLRNO+'.'+'EvalComPort',#EVALPORT);
	setconfig($TLRNO+'.'+'EvalTimeOut',#EVALTIME);
	reconfig();
	showmsg('配置信息保存完毕!');
	rerun();
	
 }"
 </ONCLICK>
 </ITEM>
 </VARIABLE>

<REQUEST><PACKAGE>#TERMINAL#PRINTER#PINPAD#MSR#AUTOSELECT#LINESPACE#FONTFLAG#ICARD#SAVEFLAG</PACKAGE></REQUEST>

<RESPONSE><PACKAGE></PACKAGE></RESPONSE>
<OUTPUT></OUTPUT>

</TRANSACTION>


