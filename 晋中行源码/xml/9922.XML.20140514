<?xml version="1.0"  encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="用户绑定查询"
xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE="3" COL="3"   TYPE="%-70s"    FUNCTIONS="T(9922                        用户绑定查询)"/>
  <ITEM LINE="4" COL="3"   TYPE="%-70s"    FUNCTIONS="T(────────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"       TYPE="%-4s"     FUNCTIONS="T($TXNO=9040)"/>
  <ITEM NAME="#MSGTYPE"    TYPE="%-5s"     FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="7"  COL="6" TYPE="%-12s"    FUNCTIONS="T(用户代码:)"/>
  <ITEM LINE="7"  COL="15" NAME="#USER_NO" INPUT="TEXT" TYPE="%-6s"  FUNCTIONS="T()">
    <ONLOSTFOCUS>
    "{
       if(#USER_NO=='')
       {
         showmsg('用户代码不能为空!');
         return(false);
       }
       //if(len(#USER_NO)!=6){
       //  showmsg('用户代码无效，请输入6位的用户代码!!');
       //  return(false);
       //}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="9"  COL="6"  TYPE="%-80s" VISABLE="TRUE" FUNCTIONS="T(-------------------------------------------------------------------------------)"/>
  <ITEM LINE="11" COL="6"  NAME="#LAC_NO"   INPUT="LABEL" TYPE="%-10s"  FUNCTIONS="T(账    号:)" />
  <ITEM LINE="11" COL="17" NAME="#AC_NO"    INPUT="LABEL" TYPE="%-20s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="11" COL="41" NAME="#LTX_DATE" INPUT="LABEL" TYPE="%-14s"  FUNCTIONS="T(关  联 日 期:)"/>
  <ITEM LINE="11" COL="55" NAME="#TX_DATE"  INPUT="LABEL" TYPE="%-8s"   FUNCTIONS="V(NB)"/>

  <ITEM LINE="12" COL="6"  NAME="#LAG_STS"  INPUT="LABEL" TYPE="%-10s"  FUNCTIONS="T(批量代扣:)" />
  <ITEM LINE="12" COL="17" NAME="#AG_STS"   INPUT="LABEL" TYPE="%-18s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="12" COL="41" NAME="#LITEM_NO" INPUT="LABEL" TYPE="%-14s"  FUNCTIONS="T(个人代理编号:)"/>
  <ITEM LINE="12" COL="55" NAME="#ITEM_NO"  INPUT="LABEL" TYPE="%-20s"  FUNCTIONS="V(NB)"/>

  <ITEM  NAME="#BTN" LINE="21"  COL="63" TYPE="%-6s"    FUNCTIONS="T(确  定)" INPUT="BUTTON">
    <ONCLICK>"{
      initfd();
      commsend_001();

      $FD16='9040';             //平台用户绑定查询接口
      $FD60='RQDS';             //平台交易类型
      $FD30=#USER_NO;           //用户代码

      callagnpt();
      if($FD12!='0000'){
        showmsg(geterror());
        rerun();
        return(false);
      }

      #AC_NO=delspace($FD31);   //签约账号
      #TX_DATE=delspace($FD28); //关联日期
      #AG_STS=delspace($FD70);  //批量代扣签约状态
      if(#AG_STS=='0'){
        #AG_STS='签约';
      }else{
        #AG_STS='解约';
      }
      #ITEM_NO=delspace($FD63); //个人代理编号
      showmsg('查询成功！');
      rerun();
    }"
    </ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
  <PACKAGE>
    NOTHING
  </PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
