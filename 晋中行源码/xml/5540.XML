<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="01111001" TITLE="5540 登录退出历史查询"
xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    	commsend_001();
	
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(5540                            登录退出历史查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5560)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="10" COL="25" TYPE="%-7s" FUNCTIONS="T(日  期:)"/>
  <ITEM LINE="12" COL="25" TYPE="%-7s" FUNCTIONS="T(顺序号:)"/>

  <!--输入变量-->
  <ITEM NAME="#TX_DATE" LINE="10" COL="33" TYPE="%-8s" INPUT="TEXT" FUNCTIONS=""/>
  <ITEM NAME="#SEQNO"   LINE="12" COL="33" TYPE="%-9s" INPUT="TEXT" FUNCTIONS=""/>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
  "{
      dim @tota as string;
      dim @selid as number;
	    commsend_001();
	    $FD16='5560';
      	    $FD53=#TX_DATE;
	    $FD54=#SEQNO;
	    @tota=calltips();
	    if($FD12!='0000'){
	      showmsg(geterror());
	      return(false);
	    }
	    
	    if(autolist(@tota)){
              @selid=list_work();
      }
   
	    //runoutput();
	    // rerun();
   }"	
</ONCLICK>  
</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
</RESPONSE>
