<ONLOSTFOCUS>
"{
	if(#TX_AMT &gt;= 50000.00 )
	{
	<!--发往核心调用授权交易-->
	initfd();
	commsend_001();
	$FD16='3369';
	callserver();
	if($FD12 !='0000')
	{
	showmsg(geterror());
	return(false);
	}
	showmsg('授权成功!!...');
	}
	<!--转账交易必须先对转入方做客户身份验证-->
  dim @shuju as string;
	initfd();
	commsend_001();
	$FD16='7839';//客户身份验证
	$FD30 = delspace(#AC_NO_IN);
	$FD25=#ACNAMEIN;
	$FD26=#ACNAMEOUT;
	$FD62=#ID_NO_ZR;
	$FD22=#ID_TYPE_ZR;
	$FD75=$MSR2;
	$FD76=$MSR3;
	$FD50='15';//关联业务标识
	$FD83='ylgmt'; //银联柜面通交易标志
	if(#HAVE_NO_CARD == '1')
	{
		@shuju=initiccard('01','000000000000');
		#CARD_SEQ_ID=strfld(@shuju,'|',4);
		//showmsg('#CARD_SEQ_ID='+#CARD_SEQ_ID);
		$FD23 ='0'+#CARD_SEQ_ID;
	}
	if(#HAVE_NO_CARD == '2')//手动读取方式时按磁条卡看待
	{
	(#HAVE_NO_CARD='0';
	}
	$FD94=#HAVE_NO_CARD;
	callagn();
 <!--账户验证超时与否或者返回挂失卡CE88不影响转账交易的正常进行-->
 if('0000' != $FD12 and 'CF68' != $FD12 and 'CF09' != $FD12 and 'CE88' != $FD12)
	{
		showmsg(geterror());
		return(false);
	}
	//showmsg('客户身份验证成功！');
<!--转账交易必须先对转入方做客户身份验证end-->
	initfd();
	commsend_001();
	$FD16='7836';//转账
	$FD25=#ACNAMEOUT;
	$FD26=#ACNAMEIN;
	$FD23='0'+#ICSEQID;
	$FD30=#AC_NO;
	$FD31=#AC_NO_IN;
	$FD40=itoa(#TX_AMT*100,'%012.0f');
	$FD75=#MSR2BAK;
	$FD76=#MSR3BAK;
	$FD79=#PWD;
	$FD83=#BRF;
	$FD90=#CUR_NO;
	$FD94=#DQFS;
	$FD22=#ID_TYPE;
	$FD62=#ID_NO;
	callagn();
	<!--CF69转入方超时时，我们依然按照成功交易处理，银联会去发确认交易-->
	<!--CF13为有缺陷的成功，我们按照成功交易对待-->
	if($FD12!='0000' and $FD12!='CF69' and $FD12!='CF13')	
	{
		showmsg(geterror());
		return(false);
	}
	if($FD12=='CF13')
	{
	showmsg('有缺陷的成功,请打印凭证!');
	}
	#SW_TRACE_NO=$FD75;
	$FD4=substr(#SW_TRACE_NO,7,6);
	runoutput();
	rerun();
}"
