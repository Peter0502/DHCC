  <ITEM  LINE="8"   COL="10" TYPE="%-4s"  NAME="#JZZL" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
	"{
		dim @tota  as string;
		dim @crdno as string; //凭证号码
		commsend_001();
		$FD16=#JZ_SERV;
		$FD116 =#JZZL+space(31)+#ZJLX+#ZJHM;
		@tota=callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#PSWDCD=substr($FD115,1,1);
		#CERTCD=substr($FD115,14,1);
		show(#PSWDCD,true);
		show(#CERTCD,true);
		setsel(#PRODNO,@tota,'o5t20o1v3o2');
		if(#JZZL!='0020'){
			goitem(#VOCNUM);
			#CARDNO='                   ';
			show(#CARDNO,false);
			show(#A2,true);    //卡没有证件支取
			show(#CERTCD,true);
		}
		else
		{
			show(#CARDNO,true);
			show(#A2,false);    //卡没有证件支取
			show(#CERTCD,false);
			#CERTCD='N';
		}
		$FD12  = '2103';
		$FD16  = #CARD_SERV;   //关于卡的通讯
		$FD116 = space(4)+#CARDNO;
		callserver();
		if($FD12 != '0000')
		{
			showmsg(geterror());
			return(false);
		}
		@crdno = substr($FD116,135,16);
	}"
  </ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="9"  COL="10" TYPE="%-19s" NAME="#CARDNO"   DEVICE="KEYBOARD" INPUT="TEXT"  FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
