  <ITEM LINE="10"  COL="10" TYPE="%16d" NAME="#VOCNUM" DEVICE="KEYBOARD" INPUT="TEXT" FUNCTIONS="V(NB)">
	<ONLOSTFOCUS>"{
		commsend_001();
		$FD16=#PZ_SERV;
		if(#JZZL=='0020')
		{
			$FD116=#JZZL+#CARDNO+space(112)+#VOCNUM;
		}
		else
		{
			$FD116=#JZZL+space(131)+#VOCNUM;
		}
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
	}"
	</ONLOSTFOCUS>
