    {
       showmsg('卡号长度不正确，请重新输入!');
       return(false);
    }
	#VOCNUM=space(9)+substr(#CARDNO,11,5);
   if(len(rtrim($MSR2))!=37)
    {
      // showmsg('刷卡错误,请重试!');
      // return(false);
    }
	$FD75 = $MSR2;
	$FD76 = $MSR3;	
  }"
  </ONLOSTFOCUS>
