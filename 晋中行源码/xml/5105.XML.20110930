<ITEM LINE="8"  COL="15" NAME="#PAY_AC_NO" TYPE="%-32s"   INPUT="TEXT" FUNCTIONS="V(NB)T()">
<ONLOSTFOCUS>"{
	dim @a     as string;
	dim @name  as string; //户名
	dim @avbal as string; //余额
	dim @PACNO as string;
	dim @prdt_name as string;//产品名称
	dim @actype as string;//帐户类型

	$MSGTYPE = '03001';
	commsend_001();
	$FD16    = '9731';
	$FD102    = #PAY_AC_NO;	
	@a = callserver();
	if($FD12 != '0000')
	{
		showmsg(geterror());
		return(false);
	}
	setitems('#ACTNO24#ACTSEQ#VOCTYPE#VOCNUM#FETCHFLAG#PWDFLAG#QUE_PWD#DRAW_PWD#ID_TYPE#CERTNO20#STAMPFLAG#PWDFLAG2#PWD16#PRINTDATE#TXAMT15#CERTTYPE#CUSTNAME#AVBAL#CURCD#CT_IND#DSCPT#ACTTYPE#ACTSEQ_2#ACCOUNTFLAG#INTEREST1#INTEREST2#INTEREST3#PAYAMOUNT#NEWVOCNUM#SPEFLAG#PRODNAME#ACTNO_24#EDUCREDIT#DESC10',$FD102);
	#PWDFLAG='N';
showmsg(#ACTSEQ);
	@name  =  substr($FD102,125,60); 
	@avbal = itoa(val(substr($FD102,185,16),0.01),'%17.2f');
	
	@prdt_name=substr($FD102,313,30);
	@prdt_name=rtrim(@prdt_name);
	@actype=substr($FD102,207,1);
	if(@prdt_name!='应解汇款'){
		if(@actype!='1' and #PO_TYPE=='2' ){
			showmsg('［产品］'+@prdt_name+'不可以签发现金汇票');
			return(false);
		}
	}
	//#TX_AMT1 = atoi(substr($FD102,185,16))/100;
	#TX_AMT1 = val(substr($FD102,185,16))/100;
	showhelp(4,58,5,20,'帐户信息','户名:'+@name+' 余额:'+@avbal);
	#PAY_NAME=rtrim(@name);
	show(#PAY_NAME,true);
}"
</ONLOSTFOCUS>
