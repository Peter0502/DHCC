<?xml version="1.0" encoding="GBK" ?>


<TRANSACTION RIGHTS="11111100" TITLE="OTD.CMD 前台系统参数设置"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">


<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->



<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
 "T(OTD.CMD                     前台系统参数设置)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3" TYPE="%-74s" FUNCTIONS=
 "T(─────────────────────────────────────)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->

  <ITEM LINE="7"  COL="5" TYPE="%-10s" FUNCTIONS="T(打印指令:!)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(打印间距:!)"/>
  <ITEM LINE="11" COL="5" TYPE="%-10s" FUNCTIONS="T(密码键盘:!)"/>
  <ITEM INPUT="LABEL" NAME="#LBITRATE" VISABLE="FALSE" LINE="11" COL="65" TYPE="%-16s" FUNCTIONS="T(密码键盘比特率:!)"/>
  <ITEM INPUT="LABEL" NAME="#LBLCOM" VISABLE="FALSE" LINE="11" COL="35" TYPE="%-14s" FUNCTIONS="T(密码键盘辅口:!)"/>
  <ITEM LINE="13" COL="5" TYPE="%-10s" FUNCTIONS="T(磁条读写:!)"/>
  <ITEM INPUT="LABEL" NAME="#LMSRBPPORT" VISABLE="FALSE" LINE="13" COL="35" TYPE="%-14s" FUNCTIONS="T(磁卡设备辅口:!)"/>
  <ITEM LINE="15" COL="5" TYPE="%-10s" FUNCTIONS="T(选单弹出:!)"/>
  <ITEM LINE="17" COL="5" TYPE="%-10s" FUNCTIONS="T(打印字体:!)"/>

  <ITEM LINE="7" COL="15" NAME="#PRINTER"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PRINTER)V(0,2)G(#PRINTER?1,#PINPAD)">
    <SELECT >
      <OPTION VALUE="0  无打印机" TITLE="0.无打印机"/>
      <OPTION VALUE="1  OKI 指令" TITLE="1.OKI 指令"/>
      <OPTION VALUE="2  PR2 指令" TITLE="2.PR2 指令"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="9" COL="15" NAME="#LINESPACE"   TYPE="%1s" DEVICE="KEYBOARD" FUNCTIONS="T($LNSPACE)">
    <SELECT >
      <OPTION VALUE="5  1/5英寸换行量(仅适用于OKI指令)" TITLE="1.五分之一英寸(OKI指令)"/>
      <OPTION VALUE="3  1/3英寸换行量(仅适用于OKI指令)" TITLE="1.三分之一英寸(OKI指令)"/>
      <OPTION VALUE="4  1/4英寸换行量(仅适用于OKI指令)" TITLE="0.四分之一英寸(OKI指令)"/>
      <OPTION VALUE="6  1/6英寸换行量(仅适用于OKI指令)" TITLE="1.六分之一英寸(OKI指令)"/>
      <OPTION VALUE="C  1/12英寸换行量(仅适用于OKI指令)" TITLE="1.十二分之一英寸(OKI指令)"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="11" COL="15" NAME="#PINPAD"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PINPAD)V(0,1)">
    <SELECT>
      <OPTION VALUE="0  无密码小键盘" TITLE="0.无密码小键盘"/>
      <OPTION VALUE="1  实达密码键盘" TITLE="1.实达密码键盘"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#PINPAD=='0'){
			show(#LBITRATE,false);
			show(#BITRATE,false);
                        show(#LBLCOM,false);
                        show(#PINPADPORT,false);
		}else{
			show(#LBITRATE,true);
			show(#BITRATE,true);
                        show(#LBLCOM,true);
                        show(#PINPADPORT,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="11" COL="50" NAME="#PINPADPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($PINPADPORT)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/>
      <OPTION VALUE="2" TITLE="0.辅口二"/>
      <OPTION VALUE="3" TITLE="0.辅口三"/>
      <OPTION VALUE="4" TITLE="0.辅口四"/>
      <OPTION VALUE="5" TITLE="0.辅口五"/>
      <OPTION VALUE="6" TITLE="0.辅口六"/>
    </SELECT>
 </ITEM>
  <ITEM LINE="11" COL="80" NAME="#BITRATE"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($BITRATE)">
    <SELECT>
      <OPTION VALUE="9" TITLE="0.9600"/><!--比特率/9600-->
      <OPTION VALUE="1" TITLE="0.1200"/><!--比特率/1200-->
    </SELECT>
 </ITEM>
  
  <ITEM LINE="13" COL="15" NAME="#MSR"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($MSR)V(0,1)">
    <SELECT >
      <OPTION VALUE="0  无磁条读写器  " TITLE="0.无磁条读写器  "/>
      <OPTION VALUE="1  实达磁条读写器" TITLE="1.实达磁条读写器"/>
    </SELECT>
    <ONLOSTFOCUS>"{
		if(#MSR=='0'){
			show(#LMSRBPPORT,false);
			show(#MSRPORT,false);
		}else{
			show(#LMSRBPPORT,true);
			show(#MSRPORT,true);
		}
    }"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="13" COL="50" NAME="#MSRPORT"   INPUT="SELECT" VISABLE="FALSE" TYPE="%1d" FUNCTIONS="T($MSRPORT)">
    <SELECT>
      <OPTION VALUE="1" TITLE="0.辅口一"/>
      <OPTION VALUE="2" TITLE="0.辅口二"/>
      <OPTION VALUE="3" TITLE="0.辅口三"/>
      <OPTION VALUE="4" TITLE="0.辅口四"/>
      <OPTION VALUE="5" TITLE="0.辅口五"/>
      <OPTION VALUE="6" TITLE="0.辅口六"/>
      
    </SELECT>
  </ITEM>
  <ITEM LINE="15" COL="15" NAME="#AUTOSELECT"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($AUTOSEL)V(0,1)">
    <SELECT >
      <OPTION VALUE="0  按F2键后弹出菜单" TITLE="0.按F2键后弹出菜单"/>
      <OPTION VALUE="1  自动弹出选择菜单" TITLE="1.自动弹出选择菜单"/>
    </SELECT>
  </ITEM>
  <ITEM LINE="17" COL="15" NAME="#FONTFLAG"   TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="T($PRFONT)V(0,1)">
    <SELECT >
      <OPTION VALUE="0  打印机支持字体放大指令"   TITLE="0.打印机支持字体放大指令"/>
      <OPTION VALUE="1  打印机不支持字体放大指令" TITLE="1.打印机不支持字体放大指令"/>
    </SELECT>
  </ITEM>
 <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(保  存)" INPUT="BUTTON" > 
 <ONCLICK>"{
	setconfig($TLRNO+'.'+'PRINTER',#PRINTER);
	setconfig($TLRNO+'.'+'PINPAD',#PINPAD);
	setconfig($TLRNO+'.'+'PINPADPORT',#PINPADPORT);
	setconfig($TLRNO+'.'+'BITRATE',#BITRATE);
	setconfig($TLRNO+'.'+'MSR',#MSR);
	setconfig($TLRNO+'.'+'MSRPORT',#MSRPORT);
	setconfig($TLRNO+'.'+'AUTOSELECT',#AUTOSELECT);
	setconfig($TLRNO+'.'+'LINESPACE',#LINESPACE);
	setconfig($TLRNO+'.'+'FONTFLAG',#FONTFLAG);
	setconfig($TLRNO+'.'+'NETPRINTER','0');//无网络打印机
	reconfig();
	showmsg('配置信息保存完毕!');
	rerun();
	
 }"
 </ONCLICK>
 </ITEM>
 </VARIABLE>

<REQUEST><PACKAGE>#TERMINAL#PRINTER#PINPAD#MSR#AUTOSELECT#LINESPACE#FONTFLAG#ICARD#SAVEFLAG</PACKAGE></REQUEST>

<RESPONSE><PACKAGE></PACKAGE></RESPONSE>
<OUTPUT></OUTPUT>

</TRANSACTION>


