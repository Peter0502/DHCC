<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111" TITLE=" 撤销申请的回执"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    initfd();
    commsend_001();
	$FD96=getitems('#REPCONTENT#PACKBAK1#PACKBAK2#PACKBAK');
 	$FD28=#PACKID;
	$FD29=#ORDERNO;
	$FD46=#WTDAY;
	$FD21=#REPSTAT;
}"
</COMMSEND>
<COMMRECV>"{
    #OPFLAG=$FD69;
    #ERRCODE=$FD74;
}"
</COMMRECV>

  <VARIABLE UPLOOP="FALSE">
  <ITEM LINE="3"  COL="3" TYPE="%-49s" FUNCTIONS=
    "T(6584                      撤销申请的回执)"/>

  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=6584)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="8"  COL="15" TYPE="%-10s" FUNCTIONS="T(包 序 号: )"/>
  <ITEM LINE="10" COL="15" TYPE="%-10s" FUNCTIONS="T(交易序号: )"/>
  <ITEM LINE="12" COL="15" TYPE="%-10s" FUNCTIONS="T(委托日期: )"/>
  <ITEM LINE="14" COL="15" TYPE="%-10s" FUNCTIONS="T(回执状态: )"/>
  <ITEM LINE="16" COL="15" TYPE="%-10s" FUNCTIONS="T(附    言: )"/>

  <ITEM LINE="8" COL="25" NAME="#PACKID" TYPE="%8d" DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>
  <ITEM LINE="10" COL="25" NAME="#ORDERNO" TYPE="%8d" DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>
  <ITEM LINE="12" COL="25" NAME="#WTDAY" TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS="D()T($HDATE)"/>
  <ITEM LINE="14" COL="25" NAME="#REPSTAT" TYPE="%-2s" DEVICE="KEYBOARD" FUNCTIONS="T(00)V(NB)">
	<SELECT LINE="14" COL="27">
             <OPTION VALUE="00 成功"               TITLE="00.成功"/>
             <OPTION VALUE="01 账号不存在"         TITLE="01.账号不存在"/>
             <OPTION VALUE="02 账号，户名不符"         TITLE="02.账号，户名不符"/>
             <OPTION VALUE="03 余额不足"         TITLE="03.余额不足"/>
             <OPTION VALUE="10 账号,密码错误"         TITLE="10.账号,密码错误"/>
             <OPTION VALUE="11 账号状态错"         TITLE="11.账号状态错"/>
             <OPTION VALUE="20 业务已撤销"         TITLE="20.业务已撤销"/>
             <OPTION VALUE="90 其它错误"         TITLE="90.其它错误"/>
     	</SELECT>
   </ITEM>

  <ITEM LINE="17" COL="10" NAME="#REPCONTENT" TYPE="%-60s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  

  <ITEM NAME="#OPFLAG"  TYPE="%-1s"/>
  <ITEM NAME="#ERRCODE"  TYPE="%-60s"/>
  <ITEM NAME="#PACKBAK1"  TYPE="%-60s"  FUNCTIONS="T(#PACKID)"/>
  <ITEM NAME="#PACKBAK2"  TYPE="%-60s"  FUNCTIONS="T(#ORDERNO)"/>
  <ITEM NAME="#PACKBAK"  TYPE="%-64s"/>

  <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />  
</VARIABLE>

<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="显示内容" CHECK="#OPFLAG=0">
NOTHING
</PACKAGE>
    <PRINT>"{
        print( 8,25,'操作成功');
}"
    </PRINT>
</RESPONSE>

<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="显示内容" CHECK="#OPFLAG=1">
NOTHING
</PACKAGE>
    <PRINT>"{
        print( 4,25,'操作失败');
        print( 6, 5,'错误描述 :[60]',#ERRCODE);
}"
    </PRINT>
</RESPONSE>
