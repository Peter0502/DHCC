<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9501 内部账信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
  <COMMSEND>"{
	commsend_001();
	$FD24=#CBDM;
	$FD26=#NAME;
	$FD29=#KHDATE;
	$FD38=#ACTNO;
	$FD44=#DQDATE;
	$FD91=#BRNO;
	$FD100=itoa(#AVBAL1*100,'%016.0f')+itoa(#AVBAL2*100,'%016.0f');

	}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	
}"
</COMMRECV>
<FIRSTWORK>"{
        setsel(#CBDM,getfiletxt('../etc/nbzprodt.dat'),'v3o1t26o2');
}"
</FIRSTWORK>



<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(9501                            内部账信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8501)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="7"   COL="15" TYPE="%-10s" FUNCTIONS="T(开户机构:)"/>
  <ITEM LINE="9"   COL="15" TYPE="%-10s" FUNCTIONS="T(帐    号:)"/>
  <ITEM LINE="10"  COL="15" TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="11"  COL="15" TYPE="%-10s" FUNCTIONS="T(余    额:)"/>
  <ITEM LINE="11"  COL="48" TYPE="%-3s"  FUNCTIONS="T(->)"/>
  <ITEM LINE="14"  COL="15" TYPE="%-10s" FUNCTIONS="T(产品代码:)"/>
  <ITEM LINE="16"  COL="15" TYPE="%-10s" FUNCTIONS="T(开户日期:)"/>
  <ITEM LINE="17"  COL="15" TYPE="%-10s" FUNCTIONS="T(到期日期:)">
<ONLOSTFOCUS>
"{
	if($KINBR=='10001'){
		enable(#BRNO,true);
	}
}"
</ONLOSTFOCUS>
</ITEM>
  <ITEM NAME="#BRNO" INPUT="TEXT" LINE="7"  COL="25" TYPE="%-5s" ENABLE="FALSE" FUNCTIONS="T($KINBR)"/>

  <ITEM NAME="#ACTNO" 	INPUT="TEXT" LINE="9"  COL="25" TYPE="%-20s" 	FUNCTIONS="T()"/>
  <ITEM NAME="#NAME" 	INPUT="TEXT" LINE="10" COL="25" TYPE="%-40s" 	FUNCTIONS="T()"/>
  <ITEM NAME="#AVBAL1" 	INPUT="TEXT" LINE="11" COL="25" TYPE="%16.2f" 	DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM NAME="#AVBAL2" 	INPUT="TEXT" LINE="11" COL="51" TYPE="%16.2f" 	DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM NAME="#CBDM" INPUT="SELECT"  LINE="14" COL="25" TYPE="%-3s" 	DEVICE="KEYBOARD" FUNCTIONS="T()">
</ITEM>
  <ITEM NAME="#KHDATE" 	INPUT="TEXT" LINE="16" COL="25" TYPE="%-8s" 	DEVICE="KEYBOARD" FUNCTIONS="D()"/>
  <ITEM NAME="#DQDATE" 	INPUT="TEXT" LINE="17" COL="25" TYPE="%-8s" 	DEVICE="KEYBOARD" FUNCTIONS="D()"/>

  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
