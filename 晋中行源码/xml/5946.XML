              <OPTION VALUE="CC00"  TITLE="绑定"/>   
              <OPTION VALUE="CC02"  TITLE="撤消"/>
       </SELECT>
</ITEM>
<ITEM LINE="18"  COL="05" TYPE="%-60s"   FUNCTIONS="T(注:如果没有更换新证书，请不要做证书撤消操作！)"/>

<ITEM LINE="20"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
<ONCLICK>"{
  commsend_001();
  $FD48 = #OPER;
  $FD18 = 'SNB';

	callagncjwy();
  if($FD12!='0000')
  {
  	showmsg(geterror());
  	showmsg($FD13);
  	return(false);
  }
  else
  {
  	if(#OPER=='CC00')
  	{
  		showmsg('证书绑定成功');
  	}
  	else
  	{
  		showmsg('证书撤消成功');
  	}
  }
  }"
</ONCLICK>
