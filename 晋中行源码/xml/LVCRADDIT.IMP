    <ITEM LINE="7"  COL="5" TYPE="%-13s" FUNCTIONS="T(预 算 种 类 :)"/>
    <ITEM LINE="8"  COL="5" TYPE="%-13s" FUNCTIONS="T(报 表 日 期 :)"/>
    <ITEM LINE="9"  COL="5" TYPE="%-13s" FUNCTIONS="T(报 表 序 号 :)"/>
