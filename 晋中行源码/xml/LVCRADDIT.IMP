  <PAGE NAME="LVCRADDIT">
    <ITEM NAME="#BLOCK_INIT"    TYPE="%-1s" FUNCTIONS="">
    <ONLOSTFOCUS>
    "{
        showblock('TSCJ_BLOCK',false,false);
        showblock('WT_BLOCK',false,false);
        showblock('GKZZDJHB_BLOCK',false,false);
        showblock('GZ_BLOCK',false,false);
        showblock('YBTYXX_BLOCK',false,false);
        showblock('FSSR_BLOCK',false,false);
        showblock('ZJZF_BLOCK',false,false);
        showblock('JF_BLOCK',false,false);
        if(#TXNUM=='00102'){
            showblock('TSCJ_BLOCK',true,true);
        } else if(#TXNUM=='00101') {
            showblock('WT_BLOCK',true,true);
        } else if(#TXNUM=='00103') {
            showblock('GKZZDJHB_BLOCK',true,true);
        } else if(#TXNUM=='20001') {
            showblock('JS_BLOCK',true,true);
        } else if(#TXNUM=='20002') {
            showblock('FSSR_BLOCK',true,true);
        } else if(#TXNUM=='20003' or #TXNUM=='20004') {
            showblock('ZJZF_BLOCK',true,true);
        } else if(#TXNUM=='20005') {
            showblock('GZ_BLOCK',true,true);
        }
    }"
    </ONLOSTFOCUS>
    </ITEM>
    <!-- 6.3	托收承付（划回）业务 -->
    <BLOCK NAME="TSCJ_BLOCK" LINE="7" COL="0" VISABLE="FALSE">
    <ITEM LINE="1"  COL="5" TYPE="%-14s" FUNCTIONS="T(托收凭证日期:)"/>
    <ITEM LINE="2"  COL="5" TYPE="%-14s" FUNCTIONS="T(托收凭证号码:)"/>
    <ITEM LINE="3"  COL="5" TYPE="%-14s" FUNCTIONS="T(赔 偿 金 额 :)"/>
    <ITEM LINE="4"  COL="5" TYPE="%-14s" FUNCTIONS="T(拒 付 金 额 :)"/>
    <ITEM LINE="5"  COL="5" TYPE="%-14s" FUNCTIONS="T(原 托 金 额 :)"/>
    <ITEM LINE="6"  COL="5" TYPE="%-14s" FUNCTIONS="T(支 付 金 额 :)"/>
    <ITEM LINE="7"  COL="5" TYPE="%-14s" FUNCTIONS="T(多 付 金 额 :)"/>
    <ITEM LINE="1" COL="19" NAME="#TSPZ_DATA"    TYPE="%-8s"    DEVICE="KEYBOARD" FUNCTIONS="V(NB)T($HDATE)"/>
    <ITEM LINE="2" COL="19" NAME="#TSPZ_NOTE_NO" TYPE="%8d"     DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(1)"/>
    <ITEM LINE="3" COL="19" NAME="#TSPC_AMT"     TYPE="%16.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(100)"/>
    <ITEM LINE="4" COL="19" NAME="#TSJF_AMT"     TYPE="%16.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(100)"/>
    <ITEM LINE="5" COL="19" NAME="#TSYT_AMT"     TYPE="%16.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(100)"/>
    <ITEM LINE="6" COL="19" NAME="#TSZF_AMT"     TYPE="%16.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(100)"/>
    <ITEM LINE="7" COL="19" NAME="#TSDF_AMT"     TYPE="%16.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(100)"/>
    <ITEM NAME="#SET_FILE1"    TYPE="%-1s" FUNCTIONS="">
    <ONLOSTFOCUS>
    "{
        setvfile(true,true);
        $BUFFER=getitems('#TSPZ_DATA#TSPZ_NOTE_NO#TSPC_AMT#TSJF_AMT#TSYT_AMT#TSZF_AMT#TSDF_AMT');
        addvfile($BUFFER);
    }"
    </ONLOSTFOCUS>
    </ITEM>
    </BLOCK>
    <!-- 6.2	委托收款（划回）业务-->
    <BLOCK NAME="WT_BLOCK" LINE="7" COL="0" VISABLE="FALSE">
    <ITEM LINE="1"  COL="5" TYPE="%-14s" FUNCTIONS="T(委托凭证日期:)"/>
    <ITEM LINE="2"  COL="5" TYPE="%-14s" FUNCTIONS="T(委托凭证号码:)"/>
    <ITEM LINE="3"  COL="5" TYPE="%-14s" FUNCTIONS="T(凭 证 种类号:)"/>
    <ITEM LINE="1" COL="19" NAME="#WTPZ_DATA"     TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T($HDATE)"/>
    <ITEM LINE="2" COL="19" NAME="#WTPZ_NOTE_NO"  TYPE="%8d"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(1)"/>
    <ITEM LINE="3" COL="19" NAME="#WTPZ_NOTE_IND" TYPE="%2s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(10)"/>
    </BLOCK>
