    </ITEM>
    <ITEM LINE="6"  COL="15" NAME="#GKDJ_TZQ_IND"  TYPE="%-1s" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="V(NB)T(1)">
    <SELECT LINE="5" COL="13">
      <OPTION VALUE="0" TITLE="0.正常  "/>
      <OPTION VALUE="1" TITLE="1.调整期"/>
    </SELECT>
    </ITEM>
    <ITEM LINE="7"  COL="15" NAME="#GKDJ_YS_IND"   TYPE="%-1s" DEVICE="KEYBOARD" INPUT="SELECT" FUNCTIONS="V(NB)T(1)">
    <SELECT LINE="5" COL="13">
      <OPTION VALUE="1" TITLE="1.预算内"/>
      <OPTION VALUE="2" TITLE="2.预算外"/>
