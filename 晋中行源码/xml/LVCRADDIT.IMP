      <OPTION VALUE="1111111111" TITLE="1.国税"/>
      <OPTION VALUE="2222222222" TITLE="2.地税"/>
      <OPTION VALUE="3333333333" TITLE="3.海关"/>
      <OPTION VALUE="4444444444" TITLE="4.财政"/>
      <OPTION VALUE="5555555555" TITLE="5.其他"/>
    </SELECT>
    </ITEM>
    <ITEM LINE="11"  COL="15" NAME="#GK_YSKM_CODE"   TYPE="%-20s" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T()"/>
    <ITEM LINE="12"  COL="15" NAME="#GK_AMT"   TYPE="%19.2f" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T()"/>
    </BLOCK>
