  <ITEM LINE="12"  COL="0" NAME="#VOCDSCPT" TYPE="%-3s" INPUT="SELECT" ENABLE="TRUE" DEVICE="KEYBOARD" FUNCTIONS=""/><!---改为3位长度--->
  <ITEM NAME="#SVOCDSCPT"  TYPE="%-20s"     FUNCTIONS="S(#VOCDSCPT,#VOCDSCPT)">
  <ONLOSTFOCUS>
  "{
	if(#VOCDSCPT != '031')
	{
		#FVOCDSCPT = #SVOCDSCPT;
		enable(#FVOCDSCPT,false);
	}
	else
	{
		#FVOCDSCPT = '';
		show(#FVOCDSCPT,true);
		enable(#FVOCDSCPT,true);
	}
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="12"  COL="3"  NAME="#FVOCDSCPT" TYPE="%-20s" INPUT="TEXT"  FUNCTIONS="">
