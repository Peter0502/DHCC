  <ITEM LINE="10" COL="40" NAME="#NEWVOCNUM"    TYPE="%16d" DEVICE="KEYBOARD" FUNCTIONS="T()V(NB)">
	<ONLOSTFOCUS>
    "{
    dim @s as string;
    dim @tota as string;
    initfd();
    commsend_001();
    $FD16='9782';
    $FD116='0'+#VOCTYPE+space(131,' ')+#NEWVOCNUM;
    @tota=callserver();
    if($FD12!='0000'){
    showmsg(geterror());
    return(false);
    }
    }"
</ONLOSTFOCUS>
