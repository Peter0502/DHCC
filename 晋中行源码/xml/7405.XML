    "T(7405                          上存/拆借查询  )"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5405)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <!--输入变量-->
  <ITEM LINE="7"   COL="20" TYPE="%-10s" FUNCTIONS="T(合 同 号:)"/>
  <ITEM LINE="9"   COL="20" TYPE="%-10s" FUNCTIONS="T(机 构 号:)"/>
  <ITEM LINE="11"  COL="20" TYPE="%-10s" FUNCTIONS="T(打印标志:)"/>
<!--
  <ITEM LINE="9"  COL="20" TYPE="%-11s" NAME="#A1" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(考核机构号:)"/>
  <ITEM LINE="10" COL="20" TYPE="%-10s" NAME="#A2" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(拆借机构:)"/>
  <ITEM LINE="11" COL="20" TYPE="%-10s" NAME="#A3" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(机构名称:)"/>
  <ITEM LINE="12" COL="20" TYPE="%-10s" NAME="#A4" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(拆借类型:)"/>
  <ITEM LINE="13" COL="20" TYPE="%-10s" NAME="#A5" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(拆借金额:)"/>
  <ITEM LINE="14" COL="20" TYPE="%-10s" NAME="#A6" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(拆借日期:)"/>
  <ITEM LINE="15" COL="20" TYPE="%-10s" NAME="#A7" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(到期日期:)"/>
