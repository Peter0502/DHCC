  <ITEM NAME="#HTH"       INPUT="TEXT"   LINE="7" COL="30" TYPE="%8d" 	FUNCTIONS=""/>
  <ITEM NAME="#BR_NO"     INPUT="TEXT"   LINE="9" COL="30" TYPE="%-5s" 	FUNCTIONS="T($KINBR)">
  <ONLOSTFOCUS>
  	"{
  		if($KINBR != '10001' and #BR_NO != $KINBR)
  		{
  			showmsg ('只能查询本机构相关信息');
  			#BR_NO = $KINBR;
  			show(#BR_NO, true);
  			return (false);
  		}
  	}"
</ONLOSTFOCUS>
</ITEM>

  <ITEM NAME="#PRTFLAG"   INPUT="SELECT" LINE="11" COL="30" TYPE="%-1s" 	FUNCTIONS="T(0)">
   <SELECT LINE="0" COL="0">
			  <OPTION VALUE="0" TITLE="不打印"/>
			  <OPTION VALUE="1" TITLE="打  印"/>
	  </SELECT>
  </ITEM>
<!---
  <ITEM NAME="#B1"  LINE="9"  COL="31" TYPE="%-5s"  VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  <ITEM NAME="#B2"  LINE="10" COL="31" TYPE="%-5s"  VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  <ITEM NAME="#B3"  LINE="11" COL="31" TYPE="%-41s" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  <ITEM NAME="#B4"  LINE="12" COL="31" TYPE="%-1s"  VISABLE="FALSE" INPUT="SELECT" FUNCTIONS="">
	  <SELECT LINE="0" COL="0">
			  <OPTION VALUE="1" TITLE="拆出"/>
			  <OPTION VALUE="2" TITLE="上存"/>
	  </SELECT>
  </ITEM>
  <ITEM NAME="#B5"  LINE="13" COL="31" TYPE="%-16s" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  <ITEM NAME="#B6"  LINE="14" COL="31" TYPE="%-8s"  VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  <ITEM NAME="#B7"  LINE="15" COL="31" TYPE="%-8s"  VISABLE="FALSE" INPUT="LABEL" FUNCTIONS=""/>
  --->
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
  "{
    dim @f as file;
    dim @tota as string;
    commsend_001();
	  $FD39=space(16);
	  $FD47=#HTH;
	  $FD67=#PRTFLAG;
	  $FD91=#BR_NO;
	  $TXNO="5405";
	  $MSGTYPE='03001';
	   if($FD67=='1')
	   {
	      @tota=callserver();
        if($FD12!='0000')
        {
                showmsg(geterror());
                return(false);
        }
        @f=openfile('../tmp/dzd['+$HDATE+$TLRNO+$TTYNAME+']','w');
        writeline(@f,@tota);
        closefile(@f);
        call('fview ../tmp/dzd['+$HDATE+$TLRNO+$TTYNAME+']');
   }
   else
   {
        @tota=callserver();
        if($FD12!='0000')
        {
                showmsg(geterror());
                return(false);
        }
        if(autolist(@tota))
        {
				list_work();
			  }

   }
  }"
  </ONCLICK>
  </ITEM>
  
</VARIABLE>
<REQUEST>
</REQUEST>
</TRANSACTION>
