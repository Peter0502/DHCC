<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110100000000000000000000000000" TITLE="5517 	财政资金划款明细清单打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{ 
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-50s" FUNCTIONS=
	"T(5517                     财政资金划款明细清单打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5516)"/>
  <ITEM NAME="#TXNAME" TYPE="%-12s" FUNCTIONS="T(财政资金划款明细清单打印)"/>
  <!---交易名称--->
  
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <BLOCK LINE="4" COL="10" >
  <ITEM LINE="5" COL="8" TYPE="%-10s" FUNCTIONS="T(网 点 号:)"/>
  <ITEM LINE="5" COL="18" TYPE="%-5s" NAME="#TX_BR_NO" INPUT="TEXT" FUNCTIONS="T($KINBR)" />
  <ITEM LINE="6" COL="8" TYPE="%-10s" FUNCTIONS="T(清算银行:)"/>
  <ITEM LINE="6" COL="18" TYPE="%-16s" NAME="#BR_NO" INPUT="TEXT" FUNCTIONS="V(NB)T(101001)"/>
  <ITEM LINE="6" COL="36" TYPE="%-14s" FUNCTIONS="T(此栏为人行代码)"/>
  <ITEM LINE="7" COL="8" TYPE="%-10s"  FUNCTIONS="T(银行编码:)"/>
  <ITEM LINE="7" COL="18" TYPE="%-16s" NAME="#BK_CODE" INPUT="TEXT" FUNCTIONS="V(NB)T(007)"/>
  <ITEM LINE="8" COL="8" TYPE="%-13s"  FUNCTIONS="T(划款凭证编号:)"/>
  <ITEM LINE="8" COL="21" TYPE="%-20s" NAME="#HK_CODE" INPUT="TEXT" FUNCTIONS="V(NB)">
  	<ONLOSTFOCUS>
  	"{
  	    if(len(#HK_CODE)!=20)
  	    {
  	        showmsg('位数不足!');
            return(false);
  	    }
  	 }"	
  	</ONLOSTFOCUS>
  </ITEM>
  <!--
  <ITEM LINE="9" COL="8" TYPE="%-10s"  FUNCTIONS="T(资金性质:)"/>
  <ITEM LINE="9" COL="18" TYPE="%-16s" NAME="#ZJ_XZ" INPUT="TEXT" FUNCTIONS="V(NB)"/>
  -->
    <ITEM LINE="9" COL="8" TYPE="%-14s" FUNCTIONS="T(预 算  类 型: )"/>
  <ITEM LINE="9" COL="22" TYPE="%-2s" NAME="#YS_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)T(1)">
    <SELECT>
      <OPTION VALUE="1"  TITLE="预算内" />
    </SELECT>
   </ITEM>
  <ITEM LINE="10" COL="8" TYPE="%-10s" FUNCTIONS="T(支付类型:)"/>
  <ITEM LINE="10" COL="18" TYPE="%-1s" NAME="#PAY_NOTE_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)">
    <SELECT>
  	  <OPTION VALUE="1"  TITLE="1.直接支付" />
  	  <OPTION VALUE="2"  TITLE="2.授权支付" />
  	</SELECT>
  </ITEM>
  <ITEM LINE="11" COL="8" TYPE="%-10s" FUNCTIONS="T(打印日期:)"/>
  <ITEM LINE="11" COL="18" TYPE="%-8d" NAME="#PRINT_DATE" INPUT="TEXT" FUNCTIONS="V(NB)T($HDATE)"/>
  <!-----
  <ITEM LINE="12" COL="8" TYPE="%-13s" FUNCTIONS="T(支付凭证状态:!)"/>
  <ITEM LINE="12" COL="21" TYPE="%-2s" NAME="#PAYSTS" INPUT="SELECT" FUNCTIONS="V(NB)">
    <SELECT>
  	  <OPTION VALUE="02"  TITLE="已支付确认" />
  	  <OPTION VALUE="03"  TITLE="已申请划款" />
  	  <OPTION VALUE="13"  TITLE="已收到清算回执" />
  	  <OPTION VALUE="04"  TITLE="已清算确认" />
  	  <OPTION VALUE="05"  TITLE="已退款通知" />
  	  <OPTION VALUE="06"  TITLE="已退款支付确认" />
  	  <OPTION VALUE="08"  TITLE="已退款请求" />
  	  <OPTION VALUE="18"  TITLE="已收到退款回执" />
  	  <OPTION VALUE="09"  TITLE="已退款清算确认" />
  	</SELECT>
  </ITEM>
  -------->
  </BLOCK>
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
 
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
    "{
        dim @tota as string; 
        dim @filename as string;
        initfd();
        commsend_001();
        $FD16=#TXNO;
        $FD91=#TX_BR_NO;
        $FD38=#HK_CODE;
        $FD58=#BR_NO;
        $FD59=#BK_CODE;
        $FD66=#PAY_NOTE_TYPE;
        $FD45=#PRINT_DATE;
        //$FD21=#PAYSTS;
        //$FD64=#ZJ_XZ;
        @tota=calltips();
        if( $FD12!='0000')
        {
            showmsg(geterror());
            return(false);
        }
        @filename='../tmp/'+$KINBR+$TTYNAME+#TXNO;
        savefiletxt(@filename,@tota); 
        call('fview '+@filename);
    }"
  </ONCLICK> 
   </ITEM> 
</VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
	<PACKAGE DEVICE="SCREEN"  DESC="交易完成">  
		NOTHING
	</PACKAGE>
		<PRINT>
		"{
			
		}"
		</PRINT>
</RESPONSE>

<RESPONSE>
<PACKAGE DEVICE="PRINTER" DESC="请打印保单凭证" LINESPACE="24" >
NOTHING
</PACKAGE>
<INIT>
"{
}"
</INIT>
<PRINT>
"{
	

}"
</PRINT>
</RESPONSE>

<OUTPUT>
</OUTPUT>
</TRANSACTION>

