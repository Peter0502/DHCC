<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="2713   公司高管信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
		initfd();
    commsend_001();
		$FD26=#NAME;
		$FD28=#CUSTNO;	
		$FD29=#GGBH;			
		$FD67=#GGLB;
		$FD68=#GGXB;
		$FD69=#ZJLX;
		$FD62=#ZJHM;
}"
</COMMSEND>
<COMMRECV>"{
    commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(2713                        公司高管信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1723)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--显示变量-->
  <BLOCK NAME="COMMON" LINE="9" COL="20"> 
  <ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(客 户 号:)"/>

  <ITEM LINE="1" COL="0" TYPE="%-10S" FUNCTIONS="T(高管编号:)"/>  
 	<ITEM LINE="2"  COL="0" TYPE="%-10s" FUNCTIONS="T(高管类型:)"/>
  <ITEM LINE="3"  COL="0" TYPE="%-10s" FUNCTIONS="T(高管姓名:)"/>
  <ITEM LINE="4"  COL="0" TYPE="%-10s" FUNCTIONS="T(高管性别:)"/>
  <ITEM LINE="5" COL="0" TYPE="%-10s" FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="6" COL="0" TYPE="%-10s" FUNCTIONS="T(证件号码:)"/>
  <!--输入内容变量-->
  <ITEM NAME="#CUSTNO" INPUT="TEXT" LINE="0" COL="10"  TYPE="%-10s"FUNCTIONS="T()"/>
  <ITEM NAME="#GGBH" INPUT="TEXT" LINE="1" COL="10"  TYPE="%-10s"FUNCTIONS="T()"/>
 	<ITEM NAME="#GGLB" 	INPUT="SELECT" 	LINE="2" COL="10" TYPE="%-1s" 	FUNCTIONS="T(A)">
   <SELECT>
   	<OPTION VALUE="A" TITLE="0.全部"/>
		<OPTION VALUE="0" TITLE="1.实际控制人"/>
		<OPTION VALUE="1" TITLE="2.董事长"/>
		<OPTION VALUE="2" TITLE="3.总经理/主要负责人"/>
		<OPTION VALUE="3" TITLE="4.财务负责人"/>
		<OPTION VALUE="4" TITLE="5.监事长"/>
		<OPTION VALUE="5" TITLE="5.法定代表人"/>		
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#NAME" 	INPUT="TEXT" LINE="3" COL="10" TYPE="%-20s" 	FUNCTIONS=""/>
 
  
  <ITEM NAME="#S" TYPE="%-30s" FUNCTIONS="">
   <ONLOSTFOCUS>
    "{   //获取证件类型
  		dim @txno_bak as string;
			dim @tota as string;
			@txno_bak=$TXNO;
			$TXNO='9799';
			$FD67='1';
			commsend_001();
			@tota=callserver();
			if($FD12 != '0000')
			{
				showmsg(geterror());
				return(false);
			}
		
			$TXNO=@txno_bak;
			setsel(#ZJLX,@tota,'o5t20o1v1o2');
			}"
  	</ONLOSTFOCUS>
  </ITEM>
  
   <ITEM NAME="#GGXB" INPUT="SELECT" LINE="4" COL="10" TYPE="%-1s" 	FUNCTIONS="T(A)">
  	<SELECT>
  	<OPTION VALUE="A" TITLE="0.全部"/>
		<OPTION VALUE="M" TITLE="1.男"/>
		<OPTION VALUE="F" TITLE="2.女"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#ZJLX" INPUT="SELECT" LINE="5" COL="10" TYPE="%-50s" FUNCTIONS="T()"> 	
  	<SELECT>
  		<OPTION VALUE="1" TITLE="1.身份证">
  	</SELECT>
  </ITEM>
  <ITEM NAME="#ZJHM" INPUT="TEXT" LINE="6" COL="10" TYPE="%-20s" FUNCTIONS="">
  <ONLOSTFOCUS>
  	"{
  	dim @l as number;
		@l = len(#ZJHM);
		if('1' == #ZJLX){
    		if(@l == 15 or  @l == 18){
		}else{
	  		showmsg('身份证号码位数错误');
        	return(false);
			}
    	}
	}"
	</ONLOSTFOCUS>
	</ITEM>
 
  </BLOCK>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">
  </ITEM>
</VARIABLE>
