<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="6655 营业状态查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
  
<COMMSEND>
"{
	commsend_001();
	$FD44=#TXDATE;
 }"
</COMMSEND>
<COMMRECV>"
{
  commrecv_001();
  runoutput();
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
	 "T(6655                          营业状态查询)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
	 "T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=6655)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="10"  COL="25"  TYPE="%10s" FUNCTIONS="T(营业日期:)" />
  <ITEM LINE="10"  COL="36"  TYPE="%-8d" INPUT="TEXT" NAME="#TXDATE" FUNCTIONS="D()"/>	
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
	<ITEM LINE="20"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="营业状态查询">
  CKPT|@txdate|@txstat|@message|
</PACKAGE>
<PRINT>
 "{
	print(6,23,'营业日期: [8]',@txdate);
	print(8,23,'营业状态: [40]',@txstat);
	print(10,23,'状态变更描述: [60]',@message);
  }"
</PRINT>
</RESPONSE>
</TRANSACTION>
