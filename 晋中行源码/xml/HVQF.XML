<?xml version="1.0" encoding="GBK" ?>

<TRANSACTION RIGHTS="11111111" TITLE="     汇票交易明细"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVQF                          汇票交易明细)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
    <ITEM LINE="6"  COL="3"  TYPE="%-15s"  FUNCTIONS="T(交  易  日  期:)"/>
    <ITEM LINE="6"  COL="31" TYPE="%-12s"  FUNCTIONS="T(交易  序号:)"/>
    <ITEM LINE="6"  COL="57" TYPE="%-10s"  FUNCTIONS="T(交易状态:)"/>
    <ITEM LINE="7"  COL="3"  TYPE="%-15s"  FUNCTIONS="T(票  据  日  期:)"/>
    <ITEM LINE="7"  COL="31" TYPE="%-12s"  FUNCTIONS="T(票据  号码:)"/>
    <ITEM LINE="7"  COL="53" TYPE="%-14s"  FUNCTIONS="T(支付交易序号:)"/>
    <ITEM LINE="9"  COL="3"  TYPE="%-15s"  FUNCTIONS="T(发 起 行 行 号:)"/>
    <ITEM LINE="9"  COL="31" TYPE="%-12s"  FUNCTIONS="T(接收行行号:)"/>
    <ITEM LINE="9"  COL="57" TYPE="%-10s"  FUNCTIONS="T(汇票类型:)"/>
    <ITEM LINE="10" COL="3"  TYPE="%-15s"  FUNCTIONS="T(签 发 行 行 号:)"/>
    <ITEM LINE="10" COL="31" TYPE="%-12s"  FUNCTIONS="T(票面  金额:)"/>
    <ITEM LINE="12" COL="3"  TYPE="%-15s"  FUNCTIONS="T(兑 付 行 行 号:)"/>
    <ITEM LINE="12" COL="31" TYPE="%-12s"  FUNCTIONS="T(报文  编号:)"/>
    <ITEM LINE="13" COL="3"  TYPE="%-13s"  FUNCTIONS="T(实际结算金额:)"/>
    <ITEM LINE="13" COL="50" TYPE="%-10s"  FUNCTIONS="T(多余金额:)"/>
    <ITEM LINE="15" COL="3"  TYPE="%-15s"  FUNCTIONS="T(付 款 人 帐 号:)"/>
    <ITEM LINE="16" COL="3"  TYPE="%-15s"  FUNCTIONS="T(付 款 人 名 称:)"/>
    <ITEM LINE="18" COL="3"  TYPE="%-15s"  FUNCTIONS="T(最后持票人帐号:)"/>
    <ITEM LINE="18" COL="57" TYPE="%-10s"  FUNCTIONS="T(汇票密押:)"/>
    <ITEM LINE="19" COL="3"  TYPE="%-15s"  FUNCTIONS="T(最后持票人名称:)"/>
    <ITEM LINE="20" COL="3"  TYPE="%-15s"  FUNCTIONS="T(附          言:)"/>
    
    
    
    <ITEM LINE="6"   COL="18" NAME="#HDATE"       TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,61,8)"/>
    <ITEM LINE="6"   COL="43" NAME="#ORDERNO"     TYPE="%-6s"   FUNCTIONS="V(NB)T($BUFFER,69,6)"/>
    <ITEM LINE="6"   COL="67" NAME="#TXSTAT"      TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,75,8)"/>
    <ITEM LINE="7"   COL="18" NAME="#PDATE"       TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,83,8)"/>
    <ITEM LINE="7"   COL="43" NAME="#PONO"        TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,91,8)"/>
    <ITEM LINE="7"   COL="67" NAME="#ORDERNOHV"   TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,99,8)"/>
    <ITEM LINE="9"  COL="18" NAME="#ORBRNO"      TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,107,12)"/>
    <ITEM LINE="9"  COL="43" NAME="#ACBRNO"      TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,119,12)"/>
    <ITEM LINE="21"  COL="4" NAME="#HPTYPE"      TYPE="%-1s"   INPUT="SELECT" VISABLE="FALSE"   FUNCTIONS="V(0,2)T($BUFFER,131,1)">
 		<SELECT LINE="21" COL="4">
      		<OPTION VALUE="0可转让汇票"   TITLE="0.可转让汇票"/>
      		<OPTION VALUE="1不可转让汇票" TITLE="1.不可转让汇票"/>
      		<OPTION VALUE="2现金汇票"     TITLE="2.现金汇票"/>
    	</SELECT>
	</ITEM>
	 <ITEM LINE="9"  COL="67" NAME="#LHPTYPE" TYPE="%-12s" FUNCTIONS="S(#HPTYPE,#HPTYPE)"/>
   
    <ITEM LINE="10"  COL="18" NAME="#PAYNO"       TYPE="%-12s"    FUNCTIONS="V(NB)T($BUFFER,132,12)"/>
    <ITEM NAME="#AVBAL"       TYPE="%16.2f"   FUNCTIONS="V(NB)T($BUFFER,144,15)"/>
    <ITEM LINE="10"  COL="43" NAME="#LAVBAL" TYPE="%-17s"   FUNCTIONS="X(#AVBAL)"/>
    
    <ITEM LINE="12" COL="18"  NAME="#CASHBRNO" TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,159,12)"/>
    <ITEM LINE="21"   COL="3" NAME="#CMTNO"     TYPE="%-3s"   INPUT="SELECT" VISABLE="FALSE"  FUNCTIONS="V(NB)T($BUFFER,171,3)">
     <SELECT LINE="21" COL="3">
    	 <OPTION VALUE="121 资金移存"          TITLE="1.资金移存"/>           
         <OPTION VALUE="721 申请兑付"          TITLE="2.申请兑付"/>           
         <OPTION VALUE="122 银行资金清算"      TITLE="3.银行资金清算"/>       
         <OPTION VALUE="123 多余资金退回"      TITLE="4.多余资金退回"/>       
         <OPTION VALUE="725 全额兑付通知"      TITLE="5.全额兑付通知"/>       
         <OPTION VALUE="724 汇票申请退回"      TITLE="6.汇票申请退回"/>       
         <OPTION VALUE="124 汇票未用退回"      TITLE="7.汇票未用退回"/>       
    </SELECT>  
                                       
   </ITEM>       
                                   
   <ITEM LINE="12"  COL="43" NAME="#LCMTNO"    TYPE="%-12s" FUNCTIONS="S(#CMTNO,#CMTNO)"/>
   <ITEM  NAME="#USEAMT"      TYPE="%16.2f"   FUNCTIONS="V(NB)T($BUFFER,174,15)"/>                                                                                                                                                                                                
   <ITEM LINE="13"  COL="16" NAME="#LUSEAMT" TYPE="%-17s"   FUNCTIONS="X(#USEAMT)"/>

    <ITEM NAME="#LEAVEBAL"    TYPE="%16.2f"   FUNCTIONS="V(NB)T($BUFFER,189,15)"/>
    <ITEM LINE="13"  COL="60" NAME="#LLEAVEBAL" TYPE="%-17s"   FUNCTIONS="X(#LEAVEBAL)"/>
    
   <ITEM NAME="#CAVBAL"       TYPE="%16.2f"   FUNCTIONS=""/>     
   <ITEM NAME="#AA"  TYPE="%-1s" FUNCTIONS="T(1)">
    <ONLOSTFOCUS>"{
         if(#CMTNO=='121' or  #CMTNO=='124')
         { 
         #CAVBAL=#AVBAL;   
        if(#CMTNO=='121') 
           {
             show(#QUERY,true);
             show(#HVPUT,true);
             show(#HVUNTREAD,true);
          }
             
       }
       if(#CMTNO=='122' or #CMTNO=='123')
       {
        show(#QUERY,true);
        if(#CMTNO=='122')
         {
             #CAVBAL=#USEAMT;
         }else{
              #CAVBAL=#LEAVEBAL;
              } 
       }
    }"
    </ONLOSTFOCUS>                     
   </ITEM>
           
    <ITEM LINE="15"  COL="18" NAME="#PAYACTNO"    TYPE="%-32s"    FUNCTIONS="V(NB)T($BUFFER,204,32)"/>
    <ITEM LINE="16"  COL="18" NAME="#PAYNAME"     TYPE="%-60s"    FUNCTIONS="V(NB)T($BUFFER,236,60)"/>     
    <ITEM LINE="18"  COL="18" NAME="#HOLDACTNO"   TYPE="%-32s"    FUNCTIONS="V(NB)T($BUFFER,296,32)"/>
    <ITEM LINE="18"  COL="67" NAME="#PASSWD"      TYPE="%-10s"    FUNCTIONS="V(NB)T($BUFFER,328,10)"/>
    <ITEM LINE="19"  COL="18" NAME="#HOLDNAME"    TYPE="%-60s"    FUNCTIONS="V(NB)T($BUFFER,338,60)"/>
    <ITEM LINE="20"  COL="18" NAME="#REASON1"     TYPE="%-60s"    FUNCTIONS="V(NB)T($BUFFER,398,60)"/>
    
    

  <ITEM LINE="21"  COL="35" NAME="#CLOSE"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
  }"
  </ONCLICK>
  </ITEM>
  
  <ITEM NAME="#CORU" TYPE="%-3s" FUNCTIONS="T(RMB)"/>
  <ITEM LINE="21"  COL="45" NAME="#QUERY"    INPUT="BUTTON" VISABLE="FALSE" TYPE="%-6s" FUNCTIONS="T(查 询)">
  <ONCLICK>"{
  	if(substr(#TXSTAT,0,4)=='来账' or substr(#TXSTAT,4,4)=='不符' or #TXSTAT=='全国押错' or #TXSTAT=='地方押错' or #TXSTAT=='正常接收'){
  		open('6601.XML','#CBRNO='+#ORBRNO+';'+
	  					'#OCORU='+#CORU+';'+
	  					'#OCMTNO='+#CMTNO+';'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#ORBRNO+';'+
	  					'#OACBRNO='+#ACBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#CAVBAL*100,'%.0f')+';'
	  					);
  	}else{
  		open('6601.XML','#CBRNO='+#ACBRNO+';'+
	  					'#OCORU='+#CORU+';'+
	  					'#OCMTNO='+#CMTNO+';'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#ORBRNO+';'+
	  					'#OACBRNO='+#ACBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#CAVBAL*100,'%.0f')+';'
	  					);
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="55" NAME="#HVPUT"    INPUT="BUTTON" VISABLE="FALSE" TYPE="%-8s" FUNCTIONS="T(申请撤销)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账排队' or #TXSTAT=='发    送'){
  		open('6604.XML','#OLDDATE='+#HDATE+';'+
	  					'#OLDORD='+#ORDERNO+';'+
	  					'#OLDTYPE='+#CMTNO+';'+
	  					'#CORU='+#CORU+';'+
	  					'#MONEY='+itoa(#CAVBAL*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请撤销');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
   <ITEM LINE="21"  COL="67" NAME="#HVUNTREAD"    INPUT="BUTTON" VISABLE="FALSE" TYPE="%-8s" FUNCTIONS="T(申请退回)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账清算' or substr(#TXSTAT,0,4)=='退回'){
  		open('6730.XML','#IOPTYPE=1;'+
	  					'#IPONO='+#PONO+';'+
	  					'#IACBRNO='+#ACBRNO+';'+
	  					'#IPDATE='+#PDATE+';'+  
	  					'#IHPTYPE='+#HPTYPE+';'+  
	  					'#ICASHBRNO='+#CASHBRNO+';'+  
	  					'#IAVBAL='+itoa(#CAVBAL*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请退回');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
</TRANSACTION>
