<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="3412 吞卡归还处理"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$MSGTYPE='03001';
	$TXNO='2412';
	$FD37=#ACTNO;
	$FD62=#CERTNO;
	$FD67=#CERTTP;
	$FD70=#TXTP;
	$FD79=#PASSWD;
	$FD16='2412';
	}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
   "T(3412                            吞卡归还处理)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <!--输入变量-->
  <ITEM LINE="7"   COL="09" TYPE="%-10s" FUNCTIONS="T(处理类型!:)"/>
  <ITEM LINE="9"   COL="09" TYPE="%-60s" FUNCTIONS="T(---------------------------------------------------------------)"/>
  <ITEM LINE="11"  COL="09" TYPE="%-10s" FUNCTIONS="T(卡    号 :)"/>
  <ITEM LINE="13"  COL="09" TYPE="%-10s" FUNCTIONS="T(证件类型 :)"/>
  <ITEM LINE="15"  COL="09" TYPE="%-10s" FUNCTIONS="T(证件号码 :)"/>

  <ITEM NAME="#TXTP" 	INPUT="SELECT" 	LINE="7"  COL="19	" TYPE="%-1s" 	FUNCTIONS="T(1)">
  <SELECT>
     <OPTION VALUE="0" TITLE="吞卡--测试用创造吞卡状态"/>
	 <!-- 添加 select coll_sts,ac_no from mdm_ac_rel where ac_no=0000000000000000250 -->

     <OPTION VALUE="1" TITLE="归还"/>
  </SELECT>
  </ITEM>

  <ITEM NAME="#ACTNO" 	INPUT="TEXT" 	LINE="11"   COL="19	" TYPE="%-19s" 	FUNCTIONS="T()V(NB)"/>

  <ITEM NAME="#CERTTP" 	INPUT="SELECT" 	LINE="13"  COL="19	" TYPE="%-1s" 	FUNCTIONS="T(1)" >
  <SELECT>
        <OPTION VALUE="1" TITLE="0.身份证"/>
        <OPTION VALUE="2" TITLE="1.户口簿"/>
        <OPTION VALUE="3" TITLE="2.护照"/>
        <OPTION VALUE="4" TITLE="3.军官证"/>
        <OPTION VALUE="5" TITLE="4.回乡证"/>
        <OPTION VALUE="8" TITLE="7.企业代码证"/>
        <OPTION VALUE="9" TITLE="8.组织机构号"/>
        <OPTION VALUE="A" TITLE="9.营业执照"/>
        <OPTION VALUE="B" TITLE="a.经营许可证"/>
        <OPTION VALUE="C" TITLE="b.事业法人证书"/>
  </SELECT>
  </ITEM>
  <ITEM NAME="#CERTNO" 	INPUT="TEXT" 	LINE="15"  COL="19	" TYPE="%-20s" 	FUNCTIONS="" />

  <ITEM NAME="#WSSRNO" 	TYPE="%-6s" 	/>

  <ITEM LINE="21" COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">
  <!--
  <ITEM LINE="21" COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
  "{dim @txno as string;
    @txno=$TXNO;
    $FD16='2412';
//   $TXNO='9986';
   $TXNO='2412';
    $FD37=#ACTNO;
    $FD62=#CERTNO;
    $FD67=#CERTTP;
    $FD70=#TXTP;
    commsend_001();
    callserver();
    $TXNO=@txno;
  if($FD12!='0000')
    {showmsg(geterror());
     return(false);}
    }"
    </ONCLICK>
	-->
    </ITEM>
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="吞卡归还成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		if(#TXTP == '1')
		{
			print( 5, 25,'吞  卡  归  还  成  功 ！');
			print( 6, 20,'卡    号:[19]',#ACTNO);
			print( 7, 20,'证件类型:[16]',getseltitle(#CERTTP,#CERTTP));
			print( 8, 20,'证件号码:[20]',#CERTNO);
		}
		else
		{
			print( 5,20,'吞卡成功，可以做归还测试  ');

		
		}
	}"
	</PRINT>
</RESPONSE>
<RESPONSE>
	  <PACKAGE DEVICE="PRINTER" LINESPACE="38" DESC="请打印业务专用凭证">
    NOTHING
    </PACKAGE>
    <PRINT>"{
		if(#TXTP == '1')
		{
			print( 5, 25,'吞卡归还单');
			print( 6, 20,'卡    号:[19]',#ACTNO);
			print( 7, 20,'证件类型:[16]',getseltitle(#CERTTP,#CERTTP));
			print( 8, 20,'证件号码:[20]',#CERTNO);
			print( 9, 20,'流 水 号:[ 6]',$FD4);
		}
    }"
    </PRINT>
</RESPONSE>
<IMPORT>CXHZ.IMP</IMPORT>
</TRANSACTION>

