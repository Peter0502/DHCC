  <ONCLICK>
    "{
        dim @tota as string; 
        dim @filename as string; 
        initfd();
	      commsend_001();
	      $FD16='5615';
	      $FD60=#Seq;
	      $FD47=#NoteDate;
	      $FD58=#BR_NO;
	      $FD59=#FIN_CODE;
	      
	     
	      @tota=calltips();
        if( $FD12!='0000')
        {
            showmsg(geterror());
  		      return(false);
        }
        
        showmsg('划款申请报文发送成功!');
        @filename='../tmp/'+$KINBR+$TTYNAME+#TXNO;
        savefiletxt(@filename,@tota); 
        call('fview '+@filename);

    }"
