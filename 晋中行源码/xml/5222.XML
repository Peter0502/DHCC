<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5222 中心提出自动打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
   dim @baktxno as string;
   @baktxno=$TXNO;
	 $MSGTYPE='03001';
   $TXNO = '9986';
   $FD12 = '4222';
	 $FD48 = getitems('#STARTNO');
	 $FD49 = getitems('#ENDNO');
	 $FD67='Y';
	 commsend_001();
	 callserver();
	 $TXNO=@baktxno;
	 if($FD12 != '0000')
	 {
		showmsg(geterror());
		return(false);
	 }
   $FD16 = '4222';
	 $MSGTYPE = '03001';
   	
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5222                        中心提出自动打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4222)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  
  <ITEM LINE="7"  COL="15" TYPE="%-40s" FUNCTIONS="T(┌──────────────────┐)"/>
  <ITEM LINE="8"  COL="15" TYPE="%-40s" FUNCTIONS="T(│                                    │)"/>
  <ITEM LINE="9"  COL="15" TYPE="%-40s" FUNCTIONS="T(│                                    │)"/>
  <ITEM LINE="10" COL="15" TYPE="%-40s" FUNCTIONS="T(│       起始编号 1234                │)"/>
  <ITEM LINE="11" COL="15" TYPE="%-40s" FUNCTIONS="T(│                                    │)"/>
  <ITEM LINE="12" COL="15" TYPE="%-40s" FUNCTIONS="T(│       终止编号 5678                │)"/>
  <ITEM LINE="13" COL="15" TYPE="%-40s" FUNCTIONS="T(│                                    │)"/>
  <ITEM LINE="14" COL="15" TYPE="%-40s" FUNCTIONS="T(│                                    │)"/>
  <ITEM LINE="15" COL="15" TYPE="%-40s" FUNCTIONS="T(└──────────────────┘)"/>


  <ITEM LINE="10" COL="33" TYPE="%4d"   NAME="#STARTNO" INPUT="TEXT" FUNCTIONS="T()V(NB)"/>
  <ITEM LINE="12" COL="33" TYPE="%4d"   NAME="#ENDNO"   INPUT="TEXT" FUNCTIONS="T()V(NB)"/>

  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="网点提出自动录入成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'网 点 提 出 自 动 录 入 成 功 ！');

				
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>



