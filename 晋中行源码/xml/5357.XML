<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111100" TITLE="5357 非支付交易查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	    initfd();
	    commsend_001();
	    $FD23=#CMTNO;
	    $FD44=#TX_DATE;
	    $FD88=#BR_NO;
	    $FD99=#LW_IND;
}"
</COMMSEND>
<COMMRECV>"{

}"
</COMMRECV>
<VARIABLE>
      <ITEM LINE="3"  COL="3" TYPE="%-50s" FUNCTIONS="T(5357                  非支付交易查询)"/>
      <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
       "T(─────────────────────────────────────)"/>
      <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5897)"/>
      <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
      
      <BLOCK NAME="COMM" LINE="0" COL="7">
        <ITEM LINE="8"  COL="15" TYPE="%-14s" FUNCTIONS="T(交 易  行 号:)"/>
        <ITEM LINE="10" COL="15" TYPE="%-14s" FUNCTIONS="T(交 易  日 期:)"/>
        <ITEM LINE="12" COL="15" TYPE="%-14s" FUNCTIONS="T(信 息  来 源:!)"/>
        <ITEM LINE="14" COL="15" TYPE="%-14s" FUNCTIONS="T(交 易  类 型:!)"/>
        
        <ITEM LINE="8"  COL="29" NAME="#BR_NO"    TYPE="%-5s"   INPUT="TEXT"  FUNCTIONS="T()"/>
        <ITEM LINE="10" COL="29" NAME="#TX_DATE"  TYPE="%-8s"   INPUT="TEXT"  FUNCTIONS="T()"/>
        <ITEM LINE="12" COL="29" NAME="#LW_IND"   TYPE="%-1s"   INPUT="TEXT"  FUNCTIONS="T(0)">
          <SELECT LINE="12" COL="30">
                  <OPTION VALUE="0 本地发送"      TITLE="0.本地发送"/>
                  <OPTION VALUE="1 本地接收"      TITLE="1.本地接收"/>
          </SELECT>
        </ITEM>
        <ITEM LINE="14" COL="30" NAME="#CMTNO"   TYPE="%-3s"   INPUT="TEXT" FUNCTIONS="T()">
          <SELECT LINE="12" COL="33">
                  <OPTION       VALUE="000 所有报文"      TITLE="0.所有报文"/>
                  <OPTION       VALUE="301 查询报文"      TITLE="1.查询报文"/>
                  <OPTION       VALUE="302 查复报文"      TITLE="2.查复报文"/>
                  <OPTION       VALUE="303 自由格式"      TITLE="3.自由格式"/>
                  <OPTION       VALUE="304 退回申请"      TITLE="4.退回申请"/>
                  <OPTION       VALUE="305 退回应答"      TITLE="5.退回应答"/>
                  <OPTION       VALUE="306 撤销申请"      TITLE="6.撤销申请"/>
                  <OPTION       VALUE="307 撤销应答"      TITLE="7.撤销应答"/>
                  <OPTION       VALUE="308 冲正申请"      TITLE="8.冲正申请"/>
                  <OPTION       VALUE="309 冲正应答"      TITLE="9.冲正应答"/>
                  <OPTION       VALUE="310 冲正通知"      TITLE="a.冲正通知"/>
                  <OPTION       VALUE="311 止付申请"      TITLE="b.止付申请"/>
                  <OPTION       VALUE="312 支付应答"      TITLE="c.支付应答"/>
                  <OPTION       VALUE="313 通用信息"      TITLE="d.通用信息"/>
                  <OPTION       VALUE="314 实时信息"      TITLE="e.实时信息"/>
          </SELECT>
        </ITEM> 
      </BLOCK>
       <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
      <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/> 
      <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>  
     <!---
      <ONCLICK>
      "{      
       }"
    </ONCLICK>
  </ITEM> 
   -->
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
