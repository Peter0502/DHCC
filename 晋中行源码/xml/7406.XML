		  	show(#JXLX,true);
		  	show(#JYLMT,true);
		  	show(#LMTPERIOD,true);
		  	show(#BDATE,true);
		  	show(#EDATE,true);
		  	show(#RATE,true);
		  	show(#OVERRATE,true);
		  }		    
