		    $FD91=#BRNO;
		    $FD22=#CUR_NO;
		    $FD65=#ACCT;
		    $FD67=#FLAG;
		    commsend_001();		
		    callserver();
		    if($FD12!='0000')
		    {
			    showmsg(geterror());
			    return(false);
		    }
		    $TXNO=@txno_bak;
		  	#BRNAME=$FD25;
		  	#OPN_DATE=$FD46;
		  	#JXLX=delspace($FD53);
		  	#JYLMT=val($FD40,0.01);
