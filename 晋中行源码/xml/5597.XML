<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110100000000000000000000000000" TITLE="5597 支付划款/退款处理结果查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{

}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-64s" FUNCTIONS=
    "T(5597                      支付划款/退款处理结果查询)"/>
  <ITEM LINE="5"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5612)"/>
  <ITEM NAME="#TXNAME" TYPE="%-29s" FUNCTIONS="T(支付划款/退款处理结果查询)"/><!---交易名称--->
  <ITEM NAME="#TXNUMBER" TYPE="%-4s" FUNCTIONS="T(5612)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

 <BLOCK LINE="4" COL="15" >
 	<ITEM LINE="6"   COL="5"   TYPE="%-14s"  FUNCTIONS="T(查 询  机 构:)" />
  <ITEM LINE="6"  COL="19"  NAME="#ORBRNO"      TYPE="%-5s"    INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM LINE="7"   COL="5"   TYPE="%-14s"  FUNCTIONS="T(财政机关代码:)" />
  <ITEM LINE="7"  COL="19"  NAME="#FINCODE"      TYPE="%-20s"    INPUT="TEXT" FUNCTIONS="T()V(NB)"/>
  
  <ITEM LINE="8"   COL="5"  TYPE="%-14s"  FUNCTIONS="T(国库主体代码:)" />
  <ITEM LINE="8"  COL="19"  NAME="#TRECODE"      TYPE="%-20s"    INPUT="TEXT" FUNCTIONS="T()V(NB)"/>

  <ITEM LINE="9"   COL="5"  TYPE="%-14s"  FUNCTIONS="T(委 托  日 期:)" />
  <ITEM LINE="9"  COL="19"  NAME="#WTDATE"    TYPE="%8d"    INPUT="TEXT" FUNCTIONS="T($HDATE)"/>
 
  <ITEM LINE="10" COL="5" TYPE="%-14s" FUNCTIONS="T(报 文  类 型:)"/>
  <ITEM LINE="10" COL="19" TYPE="%-1s" NAME="#PKG_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)">
    <SELECT>
  	  <OPTION VALUE="4"  TITLE="1.划款申请" />
  	  <OPTION VALUE="5"  TITLE="2.退款请求" />
  	</SELECT>
  </ITEM>
  
  <ITEM LINE="11" COL="5" TYPE="%-14s" FUNCTIONS="T(凭 证  类 型:)"/>
  <ITEM LINE="11" COL="19" TYPE="%-1s" NAME="#NOTE_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)">
    <SELECT>
  	  <OPTION VALUE="1"  TITLE="0.直接支付" />
  	  <OPTION VALUE="2"  TITLE="1.授权支付" />
  	</SELECT>
  	</ITEM>
 </BLOCK>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
  "{
      dim @tota as string;
  	  initfd();
	    commsend_001();
	    $FD16=#TXNO;
	   
	   
	    $FD60=#TRECODE;
	    $FD61=#FINCODE;
	    $FD64=#ORBRNO;
	    $FD45=#WTDATE;
	    $FD70=#PKG_TYPE;
	    $FD71=#NOTE_TYPE;
	    
	    @tota = calltips();
	    if($FD12!='0000')
	    {
	      showmsg(geterror());
	      return(false);
	    }
	    if(autolist(@tota))
	    {
	       list_work();
	    }
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>


