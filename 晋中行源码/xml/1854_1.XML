<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1854  定期挂账查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE=" 3"  COL=" 1"  TYPE="%-60s"  FUNCTIONS=
  	"T(1854                            定期挂账查询)"/>
  <ITEM LINE=" 4"  COL=" 1"  TYPE="%-74s"  FUNCTIONS=
  	"T(--------------------------------------------------------------------------)"/>
  <ITEM NAME ="#TXNAME" TYPE="%-30s" FUNCTIONS="T(定期挂账查询)"/>
  <ITEM NAME ="#Teller" TYPE="%-30s"/>
  <ITEM NAME ="#Date" TYPE="%-30s"/>
  <ITEM NAME ="#Trace" TYPE="%-30s"/>
  <ITEM LINE=" 6"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(报文  类型：)"/>
  <ITEM LINE=" 6"  COL="15"  TYPE="%-18s"  NAME="#MesTp"       INPUT="LABEL" FUNCTIONS=""/>
  <ITEM LINE=" 7"  COL="3"  TYPE="%-12s"  FUNCTIONS="T(交易  日期：)"/>
  <ITEM LINE=" 7"  COL="15"  TYPE="%-18s"  NAME="#TxDt"	INPUT="LABEL"/>    
  <ITEM LINE=" 7"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(交易  序号：)"/>
  <ITEM LINE=" 7"  COL="47"  TYPE="%-18s"  NAME="#OrderNo"	INPUT="LABEL"/>
  <ITEM LINE="8"  COL="3"  TYPE="%-12s"  FUNCTIONS="T(包委托日期：)"/>
  <ITEM LINE="8"  COL="15"  TYPE="%-18s"  NAME="#PackDt"	INPUT="LABEL"/> 
  <ITEM LINE="8"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(包  序  号：)"/>
  <ITEM LINE="8"  COL="47"  TYPE="%-18s"  NAME="#PackNo"         INPUT="LABEL" FUNCTIONS=""/>
  <ITEM LINE=" 9"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(业务  类型：)"/>
  <ITEM LINE=" 9"  COL="15"  TYPE="%-18s"  NAME="#TxTp"       INPUT="LABEL" FUNCTIONS=""/>  
  <ITEM LINE=" 9"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(业务  种类：)"/>
  <ITEM LINE=" 9"  COL="47"  TYPE="%-18s"  NAME="#BusKind"       INPUT="LABEL" FUNCTIONS=""/>  
  <ITEM LINE="10"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(发起行行号：)"/>
  <ITEM LINE="10"  COL="15"  TYPE="%-18s"  NAME="#OutBankNo"	INPUT="LABEL"/> 
  <ITEM LINE="10"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(接收行行号：)"/>
  <ITEM LINE="10"  COL="47"  TYPE="%-18s"  NAME="#InBankNo"	INPUT="LABEL"/> 
  <ITEM LINE="11"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(货币  符号：)"/>
  <ITEM LINE="11"  COL="15"  TYPE="%-18s"  NAME="#Cur"	INPUT="LABEL"/>
  <ITEM LINE="11"  COL="35"  TYPE="%-14s"  FUNCTIONS="T(交易  金额：)"/>
  <ITEM LINE="11"  COL="47"  TYPE="%-18s"  NAME="#Amt"	INPUT="LABEL"/>  
  <ITEM LINE="12"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(手续费标志：)"/>
  <ITEM LINE="12"  COL="15"  TYPE="%-18s"  NAME="#FeeWay"	INPUT="LABEL"/>
  <ITEM LINE="12"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(手续费金额：)"/>
  <ITEM LINE="12"  COL="47"  TYPE="%-18s"  NAME="#FeeAmt"	INPUT="LABEL"/> 
  <ITEM LINE="13"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(往来  标志：)"/>
  <ITEM LINE="13"  COL="15"  TYPE="%-18s"  NAME="#SrFlag"	INPUT="LABEL"/> 
  <ITEM LINE="13"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(交易  状态：)"/>
  <ITEM LINE="13"  COL="47"  TYPE="%-18s"  NAME="#MsgSts"	INPUT="LABEL"/>  
  <ITEM LINE="14"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(凭证  种类：)"/>
  <ITEM LINE="14"  COL="15"  TYPE="%-18s"  NAME="#CreTp"       INPUT="LABEL" FUNCTIONS=""/>    
  <ITEM LINE="14"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(凭证  号码：)"/>
  <ITEM LINE="14"  COL="47"  TYPE="%-18s"  NAME="#CreNo"	INPUT="LABEL"/>  
  
  <ITEM LINE="19"  COL="60"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON"> 
	<ONCLICK>
		"{
			$FD7=#Teller;
			$FD5=#Date;
			$FD56=#Trace;
			runoutput();
			close();
		}"
	</ONCLICK>
  </ITEM>
</VARIABLE>
	
<RESPONSE>
<PACKAGE DEVICE="SCREEN" LINESPACE="24" DESC="显示内容" >
NOTHING
</PACKAGE>
		<PRINT>
		"{	
		    
		    		PrintTxHead();
		 						
				print( 3,3, '报文  类型： [20]', #MesTp);
				print( 4,3, '交易  日期： [20]  交易  序号：[20]', #TxDt, #OrderNo);
				print(5,3, '包委托日期： [20]  包  序  号：[20]', #PackDt, #PackNo);
				print( 6,3, '业务  类型： [20]  业务  种类：[20]', #TxTp, #BusKind);
				print( 7,3, '发起行行号： [20]  接收行行号：[20]', #OutBankNo, #InBankNo);
				print( 8,3, '货币  符号： [20]  交易  金额：[20]', #Cur, #Amt);
				print( 9,3, '手续费标志： [20]  手续费金额：[20]', #FeeWay, #FeeAmt);	
				print( 10,3, '往来  标志： [20]  交易  状态：[20]', #SrFlag, #MsgSts);
				print(11,3, '凭证  种类： [20]  凭证  号码：[20]', #CreTp, #CreNo);
				
				PrintTxTail();
		}"
	</PRINT>
</RESPONSE>
