<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110100000000000000000000000000" TITLE="5635 直接/授权支出日报打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	 
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-50s" FUNCTIONS=
	"T(5635                       直接/授权支出日报打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5624)"/>
  <ITEM NAME="#TXNAME" TYPE="%-12s" FUNCTIONS="T(直接/授权支出日报打印)"/><!---交易名称--->
  <ITEM NAME="#TXNUMBER" TYPE="%-4s" FUNCTIONS="T(5624)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <BLOCK LINE="6" COL="10" >
  
  <ITEM LINE="4" COL="8" TYPE="%-14s"  FUNCTIONS="T(财 政  编 码: )"/>
  <ITEM LINE="4" COL="22" TYPE="%-16s" NAME="#FIN_CODE" INPUT="TEXT" FUNCTIONS="V(NB)"/>
  
  <ITEM LINE="5" COL="8" TYPE="%-14s" FUNCTIONS="T(交易机构代码: )"/>
  <ITEM LINE="5" COL="22" TYPE="%-5s" NAME="#TX_BR_NO" INPUT="TEXT" FUNCTIONS=""/>
  
  <ITEM LINE="6" COL="8" TYPE="%-14s" FUNCTIONS="T(支 付  类 型: )"/>
  <ITEM LINE="6" COL="22" TYPE="%-2s" NAME="#PAY_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)T(1)">
    <SELECT>
      <OPTION VALUE="1"  TITLE="直接支付" />
      <OPTION VALUE="2"  TITLE="授权支付" />
    </SELECT>
   </ITEM>
  
  <ITEM LINE="7" COL="8" TYPE="%-14s" FUNCTIONS="T(预 算  类 型: )"/>
  <ITEM LINE="7" COL="22" TYPE="%-2s" NAME="#YS_TYPE" INPUT="SELECT" FUNCTIONS="V(NB)T(1)">
    <SELECT>
      <OPTION VALUE="1"  TITLE="预算内" />
      <OPTION VALUE="2"  TITLE="预算外" />
    </SELECT>
    <ONLOSTFOCUS>
  	"{
  	    if(#YS_TYPE=='1')
  	    {
  	       show(#LVOU_NO,true);
  	       show(#VOU_NO,true);
  	       
  	    }
  	    else
  	    {
  	       show(#LVOU_NO,false);
  	       show(#VOU_NO,false);
  	       
  	    }
  	 }"	
  	</ONLOSTFOCUS>
   </ITEM>
  
  <ITEM LINE="8" COL="8" TYPE="%-14s" NAME="#LVOU_NO" INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T(划款凭证编号: )"/>
  <ITEM LINE="8" COL="22" TYPE="%-20s" NAME="#VOU_NO" INPUT="TEXT" VISABLE="FALSE" FUNCTIONS="T()">
  	<ONLOSTFOCUS>
  	"{
  	    if(#YS_TYPE=='1' and len(#VOU_NO)!=20)
  	    {
  	        showmsg('预算内资金支出日报，请输入划款凭证编号!');
            return(false);
  	    }
  	 }"	
  	</ONLOSTFOCUS>
  </ITEM>
  <!--
  <ITEM LINE="8" COL="8" TYPE="%-14s" FUNCTIONS="T(资 金  性 质: )"/>
  <ITEM LINE="8" COL="22" TYPE="%-12s" NAME="#FUND_TYPE" INPUT="TEXT" FUNCTIONS="V(NB)"/>
  -->
  
  <ITEM LINE="9" COL="8" TYPE="%-14s" NAME="#LPRINT_DATE" INPUT="LABEL"   FUNCTIONS="T(交 易  日 期: )"/>
  <ITEM LINE="9" COL="22" TYPE="%-8d" NAME="#PRINT_DATE" INPUT="TEXT"  FUNCTIONS="V(NB)V(19000101,99991231)"/>

 
  </BLOCK>
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
    "{
        dim @tota as string; 
        dim @filename as string; 
        initfd();
	      commsend_001();
	      $FD16=#TXNO;
	      $FD21=#PAY_TYPE;
	      //$FD58=#FUND_TYPE;
	      $FD53=#PRINT_DATE;
	      $FD62=#VOU_NO;
	      $FD64=#TX_BR_NO;
	      $FD67=#YS_TYPE;
	      $FD59=#FIN_CODE;
	      
	      @tota=calltips();
        if( $FD12!='0000')
        {
            showmsg(geterror());
  		      return(false);
        }
        @filename='../tmp/'+$KINBR+$TTYNAME+#TXNO;
        savefiletxt(@filename,@tota); 
        call('fview '+@filename);  

    }"
  </ONCLICK> 
   </ITEM> 
</VARIABLE>
<REQUEST>
</REQUEST>

<RESPONSE>
	<PACKAGE DEVICE="SCREEN"  DESC="交易完成">  
		NOTHING
	</PACKAGE>
		<PRINT>
		"{
			
		}"
		</PRINT>
</RESPONSE>

<RESPONSE>
<PACKAGE DEVICE="PRINTER" DESC="请打印凭证" LINESPACE="24" >
NOTHING
</PACKAGE>
<INIT>
"{
}"
</INIT>
<PRINT>
"{
	

}"
</PRINT>
</RESPONSE>

<OUTPUT>
</OUTPUT>
</TRANSACTION>

