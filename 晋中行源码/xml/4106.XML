<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="4106 虚拟母帐号管理"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD30=#TACCT;
	$FD31=#SACCT;
	$FD25=#TNAME;
	$FD62=#ZJHM;
	$FD67=#ZJLX;
	$FD68=#OPCODE;
	$FD34=#SERNO;
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-70s" FUNCTIONS=
    "T(4106                                   虚拟母帐号管理)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=2110)"/>

  <ITEM LINE="6"  COL="10"  TYPE="%-13s" FUNCTIONS="T(业务操作码:!)" />
  <ITEM LINE="8"  COL="10"  TYPE="%-15s" FUNCTIONS="T(子公司账号:)" />
  <ITEM LINE="8"  COL="48"  TYPE="%-15s" FUNCTIONS="T(子帐号序号:)" />
  <ITEM LINE="10" COL="10"  TYPE="%-16s" FUNCTIONS="T(总公司证件类型:!)" />
  <ITEM LINE="12" COL="10"  TYPE="%-16s" FUNCTIONS="T(总公司证件号码:!)" />
  <ITEM LINE="14" COL="10"  TYPE="%-13s" FUNCTIONS="T(总公司户名:)" />
  <ITEM LINE="16" COL="10"  TYPE="%-15s" FUNCTIONS="T(总公司虚拟帐号:)" />

  <ITEM LINE="6" COL="22" NAME="#OPCODE" TYPE="%1s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="T(1)" >
	<SELECT>
		<OPTION VALUE="1" TITLE="登记"/>
		<OPTION VALUE="2" TITLE="取消"/>
	</SELECT>
  </ITEM>
  <ITEM LINE="8" COL="22" NAME="#SACCT" TYPE="%-24s" INPUT="TEXT" DEVICE="KEYBOARD" FUNCTIONS="" />
  <ITEM LINE="8" COL="60" NAME="#SERNO" TYPE="%-4s" INPUT="TEXT" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="10" COL="25" NAME="#ZJLX" TYPE="%1s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="T(8)" >
	<SELECT>
		<OPTION VALUE="8" TITLE="企业代码证" />
		<OPTION VALUE="9" TITLE="经营许可证" />
		<OPTION VALUE="A" TITLE="营业执照" />
		<OPTION VALUE="B" TITLE="事业法人证书" />
		<OPTION VALUE="C" TITLE="工商核准号" />
	</SELECT>
  </ITEM>
  <ITEM LINE="12" COL="27" NAME="#ZJHM" TYPE="%-20s" INPUT="TEXT" DEVICE="KEYBOARD" FUNCTIONS="T()">
	<ONLOSTFOCUS>
		"{
			dim @txnobak as string;
			@txnobak=$TXNO;
			$TXNO='9963';
			$FD67=#ZJLX;
			$FD62=#ZJHM;
			commsend_001();
			$TXNO=@txnobak;
			callserver();
			if($FD12!='0000'){
				showmsg(geterror());
				return(false);
				}
			#TNAME=$FD25;
			#TACCT=$FD30;
		}"
	</ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="14" COL="27" NAME="#TNAME" TYPE="%-50s" INPUT="LABEL" />
  <ITEM LINE="16" COL="27" NAME="#TACCT" TYPE="%-24s" INPUT="LABEL" />

  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
<!--返回变量 -->


  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />
</VARIABLE>

<REQUEST>
</REQUEST>
<RESPONSE>
