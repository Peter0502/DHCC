<TRANSACTION RIGHTS="11111001" TITLE="9302 柜员凭证查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD59=#VOCSNO;
	$FD60=#VOCENO;
	$FD67=#VOCSTAT;
	$FD89=#VOCTYPE;
	$FD91=#TLRNO2;
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<FIRSTWORK >"{
	dim @txno_bak as string;
	dim @tota as string;
	@txno_bak=$TXNO;
	$TXNO='9902';
	commsend_001();
	@tota=callserver();	
	$TXNO=@txno_bak;
	setsel(#VOCTYPE,@tota,'o5t20o1v3o2');

}"
</FIRSTWORK>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9302                          柜员凭证查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8302)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <BLOCK NAME="COMMON" LINE="7" COL="28">
  <ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(操 作 员:)"/>
  <ITEM LINE="2" COL="0" TYPE="%-10s" FUNCTIONS="T(凭证类型:!)"/>
  <ITEM LINE="4" COL="0" TYPE="%-10s" FUNCTIONS="T(凭证状态:!)"/>
  <ITEM LINE="6" COL="0" TYPE="%-10s" FUNCTIONS="T(起始号码:)"/>
  <ITEM LINE="8" COL="0" TYPE="%-10s" FUNCTIONS="T(结束号码:)"/>
  <ITEM NAME="#TLRNO2"   INPUT="TEXT"   LINE="0" COL="10" TYPE="%4d"  FUNCTIONS="T()"/>
  <ITEM NAME="#VOCTYPE"  INPUT="SELECT" LINE="2" COL="10" TYPE="%-3s" FUNCTIONS="T()">
	<SELECT>
		<OPTION VALUE="001" TITLE="0.现金支票"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#VOCSTAT" INPUT="SELECT" LINE="4" COL="10" TYPE="%-1s" FUNCTIONS="T()">
	<SELECT><!---让柜员自己选择,不留默认值了--->
		<OPTION VALUE="*" TITLE="销毁"/>
		<OPTION VALUE="0" TITLE="正常"/>
		<OPTION VALUE="1" TITLE="挂失"/>
		<OPTION VALUE="2" TITLE="调出待收(正常)"/>
		<OPTION VALUE="3" TITLE="调出待收(作废)"/>
		<OPTION VALUE="4" TITLE="柜员使用"/>
		<OPTION VALUE="5" TITLE="出售"/>
		<OPTION VALUE="6" TITLE="作废/出售回收"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#VOCSNO" INPUT="TEXT" LINE="6" COL="10" TYPE="%16d" FUNCTIONS=""/>
  <ITEM NAME="#VOCENO" INPUT="TEXT" LINE="8" COL="10" TYPE="%16d" FUNCTIONS=""/>
  </BLOCK>
  <!--输出变量--->
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"> 
