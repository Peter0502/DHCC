<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="4516 贷款罚息启用"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$MSGTYPE='03001';
	$TXNO='3317';
	$FD16='3317';
	$FD26=#WYYY;
	$FD70=#CZBZ;
	$FD65='3317';
	}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
   "T(4516                            贷款罚息启用)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <!--输入变量-->
  <ITEM LINE="7"   COL="05" TYPE="%-14s" FUNCTIONS="T(贷款账号 :)"/>
  <ITEM LINE="07"  COL="41" TYPE="%-14s" FUNCTIONS="T(帐户序号 :)" NAME="#S_SEQNO" INPUT="LABEL" VISABLE="FALSE"/>
  <ITEM LINE="08"  COL="05" TYPE="%-14s" FUNCTIONS="T(贷款户名 :)"/>
  <ITEM LINE="10"  COL="05" TYPE="%-14s" FUNCTIONS="T(罚息利率 :)"/>
  <ITEM LINE="13"  COL="05" TYPE="%-14s" FUNCTIONS="T(操作标志!:)"/>
  <ITEM LINE="15"  COL="05" TYPE="%-14s" FUNCTIONS="T(违约原因 :)"/>


  <ITEM NAME="#ACTNO" 	INPUT="TEXT" 	LINE="7"   COL="15	" TYPE="%-19s" 	FUNCTIONS="T()V(NB)"/>

  <ITEM NAME="#CON1"    TYPE="%-1s" >
  <ONLOSTFOCUS>
 "{
		$MSGTYPE='03001';
        $TXNO='9807';
        $FD2=$OPNBR;
        $FD3=$KINBR;
        $FD5=$HDATE;
        $FD6='';
        $FD7=$TLRNO;
        $FD8='';
        $FD9='';
        $FD10=$TTYNAME;
        $FD16='9807';
        $FD17='';
        $FD37=#ACTNO;
        $FD65='3317';

        callserver();
        if($FD12!='0000')
        {
            showmsg(geterror());
            return(false);
        }

		#NAME=$FD25;
		#SEQNO=$FD34;
		#RATE=atoi($FD84)/1000000;

		if(atoi($FD34)>1)
		{
			show(#S_SEQNO,true);
			show(#SEQNO,true);
		}

		show(#NAME,true);
		show(#RATE,true);
		enable(#NAME,false);
		enable(#RATE,false);
  }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#SEQNO" 	INPUT="TEXT" 	LINE="7"     COL="51	" TYPE="%4d" 	FUNCTIONS="" VISABLE="FALSE"/>
  <ITEM NAME="#NAME" 	INPUT="TEXT" 	LINE="08"    COL="15	" TYPE="%-50s" 	FUNCTIONS="" />

  <ITEM NAME="#RATE" 	INPUT="TEXT" 	LINE="10"    COL="15	" TYPE="%8.5f" 	    FUNCTIONS="T()J(#XRATE)" />
  <ITEM NAME="#CZBZ" 	INPUT="TEXT" 	LINE="13"    COL="15	" TYPE="%-1s" 	    FUNCTIONS="T(0)" >
  <SELECT>
  <OPTION TITLE="启用" VALUE="0" />
  <OPTION TITLE="止用" VALUE="1" />
  </SELECT>
  </ITEM>
  <ITEM NAME="#WYYY" 	INPUT="TEXT" 	LINE="15"    COL="15	" TYPE="%-50s" 	    FUNCTIONS="" />
  <ITEM NAME="#XRATE" 	TYPE="%-7s" 	    FUNCTIONS="X(#RATE)" />

  <ITEM NAME="#PASSWD"  TYPE="%-6s"     />
  <ITEM NAME="#WSSRNO"  TYPE="%-6s"     />
  <ITEM NAME="#OTXDAY"  TYPE="%-10s"    />
  <ITEM NAME="#OSTDAY"  TYPE="%-10s"    />
  <ITEM NAME="#OENDAY"  TYPE="%-10s"    />
  <ITEM NAME="#OOPAMT"  TYPE="%-21s"    />
  <ITEM NAME="#OTXAMT"  TYPE="%-21s"    />
  <ITEM NAME="#OAVBAL"  TYPE="%-21s"    />
  <ITEM NAME="#ODINUM"  TYPE="%-21s"    />
  <ITEM NAME="#DESC"    TYPE="%-6s"     FUNCTIONS="">
  <ONLOSTFOCUS>"{
	if(#CZBZ=='0'){
		#DESC='启用';
	}
	else{
		#DESC='止用';
	}
  }"
  </ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="PRINTER" LINESPACE="38" DESC="操作成功,请打印凭证">
NOTHING
</PACKAGE>
<PRINT>"{
print(7,14,'贷款账号: [20] 户名: [30]',#ACTNO,#NAME);
print(9,14,'罚息利率: [10] 操作: [8]',#XRATE,#DESC);
print(11,14,'原因: [30]',#WYYY);
}"
</PRINT>
</RESPONSE>
</TRANSACTION>
