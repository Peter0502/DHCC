<COMMSEND>"{
    commsend_001();
    $FD44  = #WT_DAY;
	$FD7=$TLRNO;
	$FD3 =$KINBR;
	$FD20='1';
	commsend_123();
	commsend_hv();
}"
</COMMSEND>
