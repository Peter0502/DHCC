  <ITEM LINE="15" COL="12" TYPE="%-9s" VISABLE="FALSE" NAME="#JYXH4" INPUT="LABEL" FUNCTIONS="T(交易序号:)"/>
  <ITEM LINE="16" COL="12" TYPE="%-9s" VISABLE="FALSE" NAME="#JYXH5" INPUT="LABEL" FUNCTIONS="T(交易序号:)"/>
  
  <ITEM LINE="6" COL="22" NAME="#OTYPE" 	TYPE="%1d"   DEVICE="KAYBOARD" FUNCTIONS="T(0)V(NB)">
	<SELECT>
		<OPTION VALUE="1" TITLE="0.大额"/>
		<OPTION VALUE="2" TITLE="1.小额"/>
	</SELECT>
  </ITEM>	
  <ITEM LINE="8" COL="21"  NAME="#WT_DAY" 	TYPE="%-8s"  INPUT="TEXT"   DEVICE="KEYBOARD" FUNCTIONS="T($HDATE)V(NB)"/>
  <ITEM LINE="10" COL="21"  NAME="#CMTNO"	TYPE="%3s"     DEVICE="KAYBOARD" FUNCTIONS="T(311)V(NB)">
	<SELECT>
		<OPTION VALUE="311" TITLE="0.撤消申请"/>
		<OPTION VALUE="313" TITLE="1.退回申请"/>
		<OPTION VALUE="721" TITLE="2.申请汇票兑付"/>
		<OPTION VALUE="724" TITLE="3.申请汇票退回"/>
		<OPTION VALUE="314" TITLE="4.退回应答"/>
	</SELECT>
  </ITEM>	
  <ITEM LINE="12" COL="21" NAME="#AC_BR_NO" TYPE="%-12s"  INPUT="TEXT"   DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
 <ONLOSTFOCUS>
  "{
  		 if(#CMTNO=='721' or #CMTNO=='724')
  		 {	
  		 	showmsg('这里应是汇票处理中心的号码')
  		 	#AC_BR_NO='904290099992';
  		 }
  		 refresh();
		 dim @baktxno as string;
		 @baktxno=$TXNO;
		 $TXNO='9553';
		 $FD32=#AC_BR_NO;
		 commsend_001();
		 callserver();
		 $TXNO=@baktxno;
		 if($FD12!='0000'){
		 showmsg(geterror());
		 return(false);
		 }
		 showhelp(3,70,8,3,'支付系统行名信息',substr($FD96,0,60));
		if(#CMTNO=='721' or #CMTNO=='724')
		{
		        show(#NOTPAY_ORDERNO,false);
		        show(#JYXH3,true);
		        show(#O_NOTPAY_ORDERNO,false);
		        show(#JYXH4,false);
		        show(#ORDERNO,true);
		        show(#JYXH5,false);
		}
		else if(#CMTNO=='314')
		{
		        show(#NOTPAY_ORDERNO,false);
		        show(#JYXH3,false);
		        show(#O_NOTPAY_ORDERNO,true);
		        show(#JYXH5,true);
		        show(#ORDERNO,false);
		        show(#JYXH4,false);
		}
		else
		{
		        show(#NOTPAY_ORDERNO,true);
		        show(#JYXH3,false);
		        show(#O_NOTPAY_ORDERNO,false);
		        show(#JYXH5,false);
		        show(#ORDERNO,false);
		        show(#JYXH4,true);
		}
  }"
  </ONLOSTFOCUS>

  </ITEM>
<ITEM LINE="14" COL="21"  NAME="#ORDERNO" 			TYPE="%8d" INPUT="TEXT" VISABLE="FALSE"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>  
