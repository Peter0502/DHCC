	 	}
	 	if(#DQFS=='1')
	 	{
	 		#AC_NO=#ACNO_CT;
	 	}
	 	if( substr(#AC_NO,6,2) != '10' ) 
	 	{
	 		showmsg('请输入金太阳卡卡号!');
	 		return(false);
	 	}
    	
	    commsend_001();
	    $FD31=#AC_NO;
	    $FD16='9958';
	    callserver();
	    if($FD12 != '0000')
	    {
	        showmsg(geterror());
	        return(false);
	    }
	    #AC_NO=delspace($FD31);
	    
	    commsend_001();
	    $FD16='9331';
	    $FD102=getitems('#AC_NO');
	    callserver();
	    if($FD12 != '0000')
	    {
	        showmsg(geterror());
	        return(false);
	    }
      }"
