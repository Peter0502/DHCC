			{
				addvfile(@acno+'|'+@amt+'|'+$HDATE+'|'+$KINBR+'|'+'2现金取款'+'|'+chr(10));
			}
			else if(@jd=='0' and @xz=='1')
			{
				addvfile(@acno+'|'+@amt+'|'+$HDATE+'|'+$KINBR+'|'+'1现金存款 '+'|'+chr(10));
			}
			else if(@jd=='1' and @xz=='2')
			{
				addvfile(@acno+'|'+@amt+'|'+$HDATE+'|'+$KINBR+'|'+'2转账取款 '+'|'+chr(10));
			}
			else if(@jd=='0' and @xz=='2')
			{
				addvfile(@acno+'|'+@amt+'|'+$HDATE+'|'+$KINBR+'|'+'1转账存款 '+'|'+chr(10));
			}
		}
