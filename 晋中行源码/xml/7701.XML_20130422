			return(true);
		}

		setvfile(true,true);
		//把多账户的文件读出,提取二级账户的,组文件给平台算账户批量变动
		while(true)
		{
			@line=readline(@file1);
			if(@line=='')
			{
				break;
			}
			@acno=strfld(@line,'|',0);
			@amt =strfld(@line,'|',1);
			@jd  =strfld(@line,'|',2);

			if(strfld(@line,'|',1)=='')
			{
				continue;
			}
      //由于是取消交易需要把原交易的借贷标志反过来发
