  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(3356                         银联柜面通打印柜员流水)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s"    FUNCTIONS="T($TXNO=3356)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM NAME="#SDATE" LINE="3"  COL="50" TYPE="%-5s" INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T(日期:)"/>
  <ITEM NAME="#STEL"  LINE="3"  COL="64" TYPE="%-5s" INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T(柜员:)"/>
  
  <ITEM NAME="#DATE" LINE="3"  COL="55" TYPE="%-8s"  INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T(#JYRQ)"/>
  <ITEM NAME="#TEL"  LINE="3"  COL="70" TYPE="%-4s"  INPUT="LABEL" VISABLE="FALSE" FUNCTIONS="T($TLRNO)"/>

  <ITEM LINE="10" COL="20" TYPE="%-10s" FUNCTIONS="T(柜员编号:)"/>
  <ITEM LINE="12"  COL="20"  TYPE="%-10s" FUNCTIONS="T(交易日期:)"/>
