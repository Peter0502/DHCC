<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11000000" TITLE="4404  贷款放款交易补充"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(                      贷款放款 -放款帐号信息)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  
  <ITEM LINE="8" COL="12" TYPE="%-9s" FUNCTIONS="T(对方帐号:)"/>


  <BLOCK NAME="ACTNOINFO1"  LINE="12" COL="12" VISABLE="FALSE">
  <ITEM LINE="1" COL="0" TYPE="%-9s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="2" COL="0" TYPE="%-9s" FUNCTIONS="T(币    种:)"/>
  <ITEM LINE="3" COL="0" TYPE="%-9s" FUNCTIONS="T(余    额:)"/>
  <ITEM LINE="4" COL="0" TYPE="%-9s" FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="6" COL="0" TYPE="%-9s" FUNCTIONS="T(转入金额:)"/>
  </BLOCK>


  <ITEM NAME="#AVL" TYPE="%16s" FUNCTIONS=""/>

  <ITEM NAME="#ACTNO" LINE="8" COL="21" INPUT="TEXT"  TYPE="%-20s" FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
		showblock('ACTNOINFO1',true,true);
		showblock('ACTNOINFO2',true,true);
		initfd();
		commsend_001();
		$FD10 = '03001';
		$FD16 = '9730';
		$FD101= #ACTNO;
		callserver();
		if($FD12!='0000')
		{
			showmsg($FD12);
			return (false);
		}
		
		#USERNAME = substr($FD101,73,50);
		#AVL      = substr($FD101,123,16);
		#MONEYTYPE= substr($FD101,140,2);
		#PRODUCTNAME = substr($FD101,151,30);
	
  }"
  </ONLOSTFOCUS>
  </ITEM>

  <BLOCK NAME="ACTNOINFO2" LINE="12" COL="21" VISABLE="FALSE">
  <ITEM NAME="#USERNAME"    INPUT="LABEL"   LINE="1" COL="0" TYPE="%50s" FUNCTIONS=""/>
  <ITEM NAME="#MONEYTYPE"   INPUT="SELECT" LINE="2" COL="0" TYPE="%2s" FUNCTIONS="">
  <SELECT>
		<OPTION VALUE="01" TITLE="0.人民币"/>
  </SELECT>
  </ITEM>

  <ITEM NAME="#BALANCE"     INPUT="LABEL"   LINE="3" COL="0" TYPE="%21s" FUNCTIONS="X(#AVL)"/>
  <ITEM NAME="#PRODUCTNAME" INPUT="LABEL"   LINE="4" COL="0" TYPE="%30s" FUNCTIONS=""/>
  <ITEM NAME="#DEPOSITAVL"  TYPE="%16s" FUNCTIONS=""/>
  <ITEM NAME="#DEPOSIT"     INPUT="LABEL"   LINE="6" COL="0" TYPE="%21s" FUNCTIONS="X(#DEPOSITAVL)"/>
  </BLOCK>


  <ITEM NAME="#CONFIRM"  LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
<ONCLICK>"{
	close('#CONFIRM','');	
}"
</ONCLICK>
</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
