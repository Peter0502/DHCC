<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="7406    资金管理状态维护"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD21=#CUR_NO;
    $FD22=#LMTPERIOD;
    $FD25=#BRNAME;
    $FD40=getitems('#JYLMT');
    $FD53=#JXLX;
    $FD46=#OPN_DATE;
    $FD44=#EDATE;
    $FD45=#BDATE;
    $FD67=#FLAG;
    $FD65=#ACCT;
    $FD84=getitems('#RATE');
    $FD85=getitems('#OVERRATE');
    $FD91=#BRNO;      
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(7406                      资金管理状态维护)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8924)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/> 
  <ITEM LINE="5"   COL="05"   TYPE="%-10s" FUNCTIONS="T(处理类型:!)"/>
  <ITEM LINE="7"   COL="05"   TYPE="%-10s" FUNCTIONS="T(行    号:)"/>
  <ITEM LINE="7"   COL="24"   TYPE="%-10s" FUNCTIONS="T(科 目 号:)"/>
  <ITEM LINE="9"   COL="05"   TYPE="%-10s" FUNCTIONS="T(币    种:!)"/>
  <ITEM LINE="9"   COL="30"   TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="11"  COL="05"   TYPE="%-10s" FUNCTIONS="T(开 户 日:)"/>
  <ITEM LINE="11"  COL="24"   TYPE="%-10s" FUNCTIONS="T(计息类型:!)"/>
  <ITEM LINE="13"  COL="05"   TYPE="%-10s" FUNCTIONS="T(交易限额:)"/>
  <ITEM LINE="13"  COL="40"   TYPE="%-10s" FUNCTIONS="T(限额周期:)"/>
  <ITEM LINE="15"  COL="05"   TYPE="%-14s" FUNCTIONS="T(限制开始日期:)"/>
  <ITEM LINE="15"  COL="24"   TYPE="%-14s" FUNCTIONS="T(限制结束日期:)"/>
  <ITEM LINE="17"  COL="05"   TYPE="%-10s" FUNCTIONS="T(利    率:)"/>
  <ITEM LINE="17"  COL="25"   TYPE="%-10s" FUNCTIONS="T(逾期利率:)"/>
  <ITEM LINE="5" COL="16" NAME="#FLAG" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="T(1)">
    <SELECT>
       <OPTION VALUE ="1" TITLE="增加" />
       <OPTION VALUE ="2" TITLE="修改" />
       <OPTION VALUE ="3" TITLE="删除" />
    </SELECT>
  </ITEM>  
  <ITEM LINE="7"   COL="16"  NAME="#BRNO"        TYPE="%-5s"        INPUT="TEXT"    FUNCTIONS="V(NB)"/>
  <ITEM LINE="7"   COL="35"  NAME="#ACCT"        TYPE="%-7s"        INPUT="TEXT"    FUNCTIONS="V(NB)"/>
  <ITEM LINE="9"   COL="16"  NAME="#CUR_NO"      TYPE="%-2s"        INPUT="SELECT"  FUNCTIONS="T(01)V(NB)">
  <SELECT>
      <OPTION VALUE ="01" TITLE="01.人民币" />
  </SELECT>
  </ITEM>
  <ITEM NAME="#AAAA" TYPE="%-1s" FUNCTIONS="">
   <ONLOSTFOCUS>
		"{
		  if(#FLAG!='1')
		  {
		    dim @txno_bak as string;
        @txno_bak=$TXNO;
		    $TXNO='8923';
		    $FD91=#BRNO;
		    $FD22=#CUR_NO;
		    $FD65=#ACCT;
		    $FD67=#FLAG;
		    commsend_001();		
		    callserver();
		    if($FD12!='0000')
		    {
			    showmsg(geterror());
			    return(false);
		    }
		    $TXNO=@txno_bak;
		  	#BRNAME=$FD25;
		  	#OPN_DATE=$FD46;
		  	#JXLX=delspace($FD53);
		  	#JYLMT=val($FD40,0.01);
		  	#LMTPERIOD=delspace($FD22);
		  	#BDATE=$FD45;
		  	#EDATE=$FD44;
		  	#RATE=$FD84;
		  	#OVERRATE=$FD85;
		  	show(#BRNAME,true);
		  	show(#OPN_DATE,true);
		  	show(#JXLX,true);
		  	show(#JYLMT,true);
		  	show(#LMTPERIOD,true);
		  	show(#BDATE,true);
		  	show(#EDATE,true);
		  	show(#RATE,true);
		  	show(#OVERRATE,true);
		  }		    
		}"
	</ONLOSTFOCUS>
	</ITEM>
  <ITEM LINE="9"  COL="40"  NAME="#BRNAME"      TYPE="%-20s"        INPUT="TEXT"  FUNCTIONS=""/>   
  <ITEM LINE="11"  COL="16"  NAME="#OPN_DATE"    TYPE="%-8s"        INPUT="TEXT"  FUNCTIONS=""/>
  <ITEM LINE="11"  COL="35"  NAME="#JXLX"        TYPE="%-1s"        INPUT="SELECT"    FUNCTIONS="T(0)">
  <SELECT>
       <OPTION VALUE="0" TITLE="不 计 息" />
       <OPTION VALUE="1" TITLE="利随本清" />
       <OPTION VALUE="2" TITLE="按日计息" />
       <OPTION VALUE="3" TITLE="按月计息" />
       <OPTION VALUE="4" TITLE="按季计息" />
       <OPTION VALUE="5" TITLE="按年计息" />
       <OPTION VALUE="6" TITLE="利息变动计息" />
  </SELECT>
  </ITEM>
  <ITEM LINE="13"  COL="16"  NAME="#JYLMT"        TYPE="%17.2f"         INPUT="TEXT"  FUNCTIONS=""/>  
  <ITEM LINE="13"  COL="50"  NAME="#LMTPERIOD"    TYPE="%-2s"           INPUT="SELECT"  FUNCTIONS="T(0)">
   <SELECT>
       <OPTION VALUE ="0" TITLE="无" />
       <OPTION VALUE ="1" TITLE="5日" />
       <OPTION VALUE ="2" TITLE="7日" />
       <OPTION VALUE ="3" TITLE="旬" />
       <OPTION VALUE ="4" TITLE="半月" />
       <OPTION VALUE ="5" TITLE="月" />
       <OPTION VALUE ="6" TITLE="季" />
       <OPTION VALUE ="7" TITLE="半年" />
       <OPTION VALUE ="8" TITLE="年" /> 
    </SELECT>
  </ITEM>
  <ITEM LINE="15"   COL="20"  NAME="#BDATE"    TYPE="%8d"        INPUT="TEXT"  FUNCTIONS=""/>
  <ITEM LINE="15"   COL="40"  NAME="#EDATE"    TYPE="%8d"        INPUT="TEXT"  FUNCTIONS=""/>
 
  <ITEM LINE="17"   COL="16"  NAME="#RATE"     TYPE="%8.6f"      INPUT="TEXT"  FUNCTIONS=""/>
  <ITEM LINE="17"   COL="35"  NAME="#OVERRATE" TYPE="%8.6f"      INPUT="TEXT"  FUNCTIONS=""/>
	 <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" 	 TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
</RESPONSE>
</TRANSACTION>
