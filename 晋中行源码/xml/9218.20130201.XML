<TRANSACTION RIGHTS="11111111" TITLE="BD01           交易信息补打" 
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	
}"
</COMMSEND>
<COMMRECV>"{

}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
   "T(9218                          交易补打 )"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

 <ITEM NAME="#S_TX_NAME" INPUT="LABEL" LINE="8"  COL="15" TYPE="%-10s"  FUNCTIONS="T(交易号码:)"/>
 <ITEM NAME="#TX_NAME"  INPUT="TEXT" LINE="8"  COL="25" TYPE="%4d"  FUNCTIONS=""/>
 <ITEM NAME="#S_TX_DATE" INPUT="LABEL" LINE="9"  COL="15" TYPE="%-10s"  FUNCTIONS="T(交易日期:)"/>
 <ITEM NAME="#TX_DATE"  INPUT="TEXT" LINE="9"  COL="25" TYPE="%8d"  FUNCTIONS=""/>
 <ITEM NAME="#S_TRACE_NO" INPUT="LABEL" LINE="10"  COL="15" TYPE="%-10s"  FUNCTIONS="T(交易流水:)"/>
 <ITEM NAME="#TRACE_NO" INPUT="TEXT" LINE="10"  COL="25" TYPE="%8d"  FUNCTIONS=""/>
  
 <ITEM LINE="15" COL="49" NAME="#ITEM06" TYPE="%-10s" INPUT="BUTTON" FUNCTIONS="T(确  定)">
  <ONCLICK>
  "{
	    initfd();
      commsend_001();
      $FD16 = '9191';
      $FD45 = #TX_DATE;
      $FD46 = #TRACE_NO;      
      savebdsfile();
      callserver();
      if($FD12 !='0000')
      {
      	showmsg('补打失败');
	      return(false);
      }
     
      printbds($FD104,#TX_NAME);
   }"
</ONCLICK>
</ITEM>
</VARIABLE>
