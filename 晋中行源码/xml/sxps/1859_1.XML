<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1859 定期交易登记查询"
  xmlns="http://www.otd.org/MENU" xmlns：W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE=" 3"  COL=" 1"  TYPE="%-60s"  FUNCTIONS=
  	"T(1859                            定期交易登记查询)"/>
  <ITEM LINE=" 4"  COL=" 1"  TYPE="%-74s"  FUNCTIONS=
  	"T(--------------------------------------------------------------------------)"/>
  <ITEM NAME ="#TXNAME" TYPE="%-30s" FUNCTIONS="T(定期交易登记查询)"/>
  <ITEM NAME ="#Teller" TYPE="%-30s"/>
  <ITEM NAME ="#Date" TYPE="%-30s"/>
  <ITEM NAME ="#Trace" TYPE="%-30s"/>
  <ITEM LINE=" 7"  COL="3"  TYPE="%-14s"  FUNCTIONS="T(项目登记日期: )"/>
  <ITEM LINE=" 7"  COL="17"  TYPE="%-18s"  NAME="#TxDt"	INPUT="LABEL"/>   
  <ITEM LINE=" 7"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(批次  序号: )"/>
  <ITEM LINE=" 7"  COL="47"  TYPE="%-20s"  NAME="#BatNo"	INPUT="LABEL"/>
  <ITEM LINE=" 8"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(业务  类型: )"/>
  <ITEM LINE=" 8"  COL="15"  TYPE="%-20s"  NAME="#TxTp"   INPUT="LABEL"/>  
  <ITEM LINE=" 8"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(业务  种类: )"/>
  <ITEM LINE=" 8"  COL="47"  TYPE="%-20s"  NAME="#BusKind" INPUT="LABEL"/>  
  <ITEM LINE="9"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(交易总笔数: )"/>
  <ITEM LINE="9"  COL="15"  TYPE="%-20s"  NAME="#TxSum"	INPUT="LABEL"/> 
  <ITEM LINE="9"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(交易总金额: )"/>
  <ITEM LINE="9"  COL="47"  TYPE="%-20s"  NAME="#TxSumAmt"	INPUT="LABEL"/> 
  <ITEM LINE="10"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(凭证  号码: )"/>
  <ITEM LINE="10"  COL="15"  TYPE="%-20s"  NAME="#CreNo"	INPUT="LABEL"/>
  <ITEM LINE="10"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(回执  期限: )"/>
  <ITEM LINE="10"  COL="47"  TYPE="%-20s"  NAME="#RspDays"	INPUT="LABEL"/>   
  <ITEM LINE="11"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(付款人账号: )"/>
  <ITEM LINE="11"  COL="15"  TYPE="%-20s"  NAME="#PayerAcct"  INPUT="LABEL"/> 
  <ITEM LINE="12"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(付款人名称: )"/>
  <ITEM LINE="12"  COL="15"  TYPE="%-20s"  NAME="#PayerName"	INPUT="LABEL"/>     
  <ITEM LINE="13"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(付款人地址: )"/>
  <ITEM LINE="13"  COL="15"  TYPE="%-20s"  NAME="#PayerAdd"	INPUT="LABEL"/> 
 
  <ITEM LINE="19"  COL="60"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON"> 
	<ONCLICK>
		"{
			$FD7=#Teller;
			$FD5=#Date;
			$FD56=#Trace;
			runoutput();
			close();
		}"
	</ONCLICK>
  </ITEM>
</VARIABLE>
	
<RESPONSE>
<PACKAGE DEVICE="SCREEN" LINESPACE="24" DESC="显示内容" >
NOTHING
</PACKAGE>
		<PRINT>
		"{	
		    
		    		PrintTxHead();
		
				print( 4,3, '项目登记日期: [20]批次  序号: [20]', #TxDt, #BatNo);
				print( 5,3, '业务  类型: [20]  业务  种类: [20]', #TxTp, #BusKind);
				print( 6,3, '交易总笔数: [20]  交易总金额: [20]', #TxSum, #TxSumAmt);
				print( 7,3, '凭证  号码: [20]  回执  期限: [20]', #CreNo, #RspDays);	
				print( 8,3, '付款人账号: [60]', #PayerAcct);
				print( 9,3, '付款人名称: [60]', #PayerName);
				print( 10,3, '付款人地址: [60]', #PayerAdd);
				
				PrintTxTail();
		}"
	</PRINT>
</RESPONSE>
