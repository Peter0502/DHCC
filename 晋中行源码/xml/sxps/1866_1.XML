  <ITEM LINE="9"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(对账  状态：)"/>
  <ITEM LINE="9"  COL="47"  TYPE="%-20s"  NAME="#Chksts" INPUT="LABEL"/>
  
  <ITEM LINE="10"  COL="3"  TYPE="%-12s"  FUNCTIONS="T(核心  状态：)"/>
  <ITEM LINE="10"  COL="15"  TYPE="%-20s"  NAME="#Hoststs"	INPUT="LABEL"/>
  
  
  <ITEM LINE="21"  COL="60"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON"> 
	<ONCLICK>
		"{
			runoutput();
			close();
		}"
	</ONCLICK>
  </ITEM>
</VARIABLE>
	
<RESPONSE>
