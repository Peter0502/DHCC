<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1856 行名行号查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE=" 3"  COL=" 1"  TYPE="%-60s"  FUNCTIONS=
  	"T(1856                            行名行号查询)"/>
  <ITEM LINE=" 4"  COL=" 1"  TYPE="%-74s"  FUNCTIONS=
  	"T(--------------------------------------------------------------------------)"/>
  <ITEM NAME="#TXCODE" TYPE="%-30s" FUNCTIONS="T(1856)"/>
  <ITEM NAME ="#TXNAME" TYPE="%-30s" FUNCTIONS="T(行名行号查询)"/>
  <ITEM NAME ="#Teller" TYPE="%-30s"/>
  <ITEM NAME ="#Date" TYPE="%-30s"/>
  <ITEM NAME ="#Trace" TYPE="%-30s"/>
  <ITEM LINE=" 6"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(行      号: )"/>
  <ITEM LINE=" 6"  COL="15"  TYPE="%-40s"  NAME="#BnkId"       INPUT="LABEL" FUNCTIONS=""/>
  
  <ITEM LINE=" 7"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(行      名: )"/>
  <ITEM LINE=" 7"  COL="15"  TYPE="%-40s"  NAME="#BnkNm"	INPUT="LABEL"/>  
   
  <ITEM LINE=" 8"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(参与者简称: )"/>
  <ITEM LINE=" 8"  COL="15"  TYPE="%-40s"  NAME="#PrtNm"       INPUT="LABEL" FUNCTIONS=""/> 
     
  <ITEM LINE=" 9"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(参与者类别: )"/>
  <ITEM LINE=" 9"  COL="15"  TYPE="%-18s"  NAME="#AdmBId"	INPUT="LABEL"/> 
  <ITEM LINE=" 9"  COL="35"  TYPE="%-12s"  FUNCTIONS="T(参与者行别: )"/>
  <ITEM LINE=" 9"  COL="47"  TYPE="%-18s"  NAME="#PrtBlng"	INPUT="LABEL"/> 
  
  <ITEM LINE="10"  COL=" 3"  TYPE="%-14s"  FUNCTIONS="T(管辖人行行号: )"/>
  <ITEM LINE="10"  COL="17"  TYPE="%-18s"  NAME="#PubBnk"	INPUT="LABEL"/>
 
  <ITEM LINE="10"  COL="35"  TYPE="%-16s"  FUNCTIONS="T(参与者所属CCPC: )"/>
  <ITEM LINE="10"  COL="51"  TYPE="%-18s"  NAME="#CCPC"	INPUT="LABEL"/>
  
  <ITEM LINE="11"  COL=" 3"  TYPE="%-18s"  FUNCTIONS="T(所属直接参与机构: )"/>
  <ITEM LINE="11"  COL="21"  TYPE="%-18s"  NAME="#BlngDp"	INPUT="LABEL"/>
 
  <ITEM LINE="12"  COL="3"  TYPE="%-18s"  FUNCTIONS="T(代理清算参与机构: )"/>
  <ITEM LINE="12"  COL="21"  TYPE="%-18s"  NAME="#AngtDp"	INPUT="LABEL"/>
  
  <ITEM LINE="13"  COL=" 3"  TYPE="%-12s"  FUNCTIONS="T(上级参与者: )"/>
  <ITEM LINE="13"  COL="15"  TYPE="%-18s"  NAME="#SuPrt" INPUT="LABEL"/> 
    
  <ITEM LINE="14"  COL="3"  TYPE="%-12s"  FUNCTIONS="T(承接行行号: )"/>
  <ITEM LINE="14"  COL="15"  TYPE="%-18s"  NAME="#CrryId"	INPUT="LABEL"/>  
  <ITEM LINE="14"  COL="35"  TYPE="%-10s"  FUNCTIONS="T(所属城市: )"/>
  <ITEM LINE="14"  COL="45"  TYPE="%-18s"  NAME="#City"	INPUT="LABEL"/>
  
  <ITEM LINE="15"  COL="3"  TYPE="%-18s"  FUNCTIONS="T(加入业务系统标识: )"/>
  <ITEM LINE="15"  COL="21"  TYPE="%-18s"  NAME="#SysId"	INPUT="LABEL"/> 
  
  <ITEM LINE="19"  COL="60"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON"> 
	<ONCLICK>
		"{
			runoutput();
			close();
		}"
	</ONCLICK>
  </ITEM>
</VARIABLE>
	
<RESPONSE>
<PACKAGE DEVICE="SCREEN" LINESPACE="24" DESC="显示内容" >
NOTHING
</PACKAGE>
		<PRINT>
		"{	
		    
				PrintTxHead();
		 						
				print( 3,3, '行号: [40]', #BnkId);
				print( 4,3, '行名: [40]', #BnkNm);
				print( 5,3, '参与者简称: [40]', #PrtNm);
				print( 6,3, '参与者类别: [20]      参与者行别:[20]', #AdmBId, #PrtBlng);
				print( 7,3, '管辖人行行号: [20]    参与者所属CCPC:[20]', #PubBnk, #CCPC);
				print( 8,3, '所属直接参与机构: [40]', #BlngDp);
				print( 9,3, '代理清算参与机构: [40]', #AngtDp);
				print( 10,3, '上级参与者: [40]', #SuPrt);
				print( 11,3, '承接行行号: [20]     所属城市:[20]', #CrryId, #City);
				print( 12,3, '加入业务系统标识: [40]', #SysId);
				
				PrintTxTail();
		}"
	</PRINT>
</RESPONSE>
</TRANSACTION>		