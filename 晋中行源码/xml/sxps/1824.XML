<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1824 统一支付系统状态查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<FIRSTWORK RERUN="TRUE">
"{
	 dim @tota as string;
	 initfd();
	 commsend_001();
	 $FD16='1001';
	 $FD18='pbp';
	 @tota=callagnpbp();
	 if(@tota == '' or @tota == '1')
	 {
		 @tota=getfiletxt('../tmp/'+$KINBR+$TTYNAME);
		 //showmsg(@tota+$KINBR+$TTYNAME);
		 
	 }
	 setsel(#SYSCODE,@tota,'v4o1t8o1');
}"
</FIRSTWORK>
<COMMSEND>
"{
}"
</COMMSEND>
<COMMRECV>
"{
}"
</COMMRECV>

<VARIABLE>
	<ITEM LINE="3" COL="3" TYPE="%-42s" FUNCTIONS="T( 1824                   系统状态查询)"/>
	<ITEM LINE="4" COL="3" TYPE="%-74s" FUNCTIONS="T(─────────────────────────────────────)"/>
	<BLOCK NAME="block_in" LINE="5" VISABLE="TRUE">
		<ITEM LINE="0"  COL="20" TYPE="%-11s" FUNCTIONS="T(系统类型:)"/>
		<ITEM LINE="0"  COL="31" NAME="#SYSCODE" TYPE="%-4s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="T()V(NB)">
			<SELECT>
			</SELECT>
		</ITEM>
	</BLOCK>
	<ITEM  NAME="#OK" LINE="21"  COL="63"  TYPE="%-6s"   FUNCTIONS="T(确  定)" INPUT="BUTTON">
	<ONCLICK>
	"{
		initfd();
		$TXNO='1306';
		$FD18='pbp';
		$FD28=#SYSCODE; 
		$FD67='0';
		if(#SYSCODE=='sxps')  
		{
			 $FD67='1';
		}				    
		commsend_001();
		callagnpbp();
		if($FD12!='0000')
		{
			showmsg(geterror());
			showblock('block_out_edzf',false,false);
			showblock('block_out_nx',false,false);
			showblock('block_out_nxy',false,false);	
			showblock('block_out_sx',false,false);	
			return(false);
		}	
		if(#SYSCODE=='sxps')
		{
			#Status=$FD36;
			#TrnEnd=$FD68;
			//showmsg('['+#Status+']');
			
			#Workdate=$FD44;
			#PreWorkDate=$FD47;
			
			
			#ClearDate=$FD45;
			#ClearRound=itoa(atoi($FD54));
			
			#ExchgDate=$FD46;
			#ExchgRound=itoa(atoi($FD55));
			
			#WorkRound=itoa(atoi($FD52));
			#RoundNum=itoa(atoi($FD53));
			
		
			
			showblock('block_out_edzf',false,false);
			showblock('block_out_nx',false,false);
			showblock('block_out_nxy',false,false);	
			showblock('block_out_sx',true,true);		  
			return(true);
		}
		else if(#SYSCODE=='nxps')
		{
			showmsg('选择了宁夏同城');
			#nxsystemstatus=$FD36;
			#nxworkdate=$FD44;
			showblock('block_out_edzf',false,false);
			showblock('block_out_sx',false,false);
			showblock('block_out_nxy',false,false);	
			showblock('block_out_nx',true,true);
		}
	 }"
	 </ONCLICK>
	</ITEM>

	<BLOCK NAME="block_out_sx" LINE="7" VISABLE="FALSE">
  	<ITEM LINE="1" COL="7"  TYPE="%-11s" FUNCTIONS="T(系统状态:)"/>
  	<ITEM LINE="1" COL="18" NAME="#Status" TYPE="%-2s"> 	
  	<SELECT>
  		<OPTION VALUE="10" TITLE="1.运行" />
		  <OPTION VALUE="02" TITLE="0.停运" />
  	</SELECT>
  	</ITEM>
  	<ITEM LINE="1" COL="35"  TYPE="%-11s" FUNCTIONS="T(未决标志:)"/>
  	<ITEM LINE="1" COL="46" NAME="#TrnEnd" TYPE="%-2s">
  	<SELECT>
  		<OPTION VALUE="0" TITLE="0.不结束" />
		  <OPTION VALUE="1" TITLE="1.结束" />
  	</SELECT>
  	</ITEM>
  	  	
  	<ITEM LINE="2" COL="7"  TYPE="%-11s" FUNCTIONS="T(工作日期:)"/>
  	<ITEM LINE="2" COL="18"  TYPE="%-10s" NAME="#Workdate"/>  	
  	<ITEM LINE="2" COL="35"  TYPE="%-11s" FUNCTIONS="T(工作场次:)"/>
  	<ITEM LINE="2" COL="46"  TYPE="%-10s" NAME="#WorkRound"/> 
  	
  	  	
  	  	
  	<ITEM LINE="3" COL="7"  TYPE="%-11s" FUNCTIONS="T(清算日期:)"/>
  	<ITEM LINE="3" COL="18"  TYPE="%-10s" NAME="#ClearDate"/>
  	<ITEM LINE="3" COL="35"  TYPE="%-11s" FUNCTIONS="T(清算场次:)"/>
  	<ITEM LINE="3" COL="46"  TYPE="%-2s" NAME="#ClearRound"/>
  	
  	<ITEM LINE="4" COL="7"  TYPE="%-11s" FUNCTIONS="T(交换日期:)"/>
  	<ITEM LINE="4" COL="18"  TYPE="%-10s" NAME="#ExchgDate"/>
  	<ITEM LINE="4" COL="35"  TYPE="%-11s" FUNCTIONS="T(交换场次:)"/>
  	<ITEM LINE="4" COL="46"  TYPE="%-2s" NAME="#ExchgRound"/>
  	

  	<ITEM LINE="5" COL="7"  TYPE="%-11s" FUNCTIONS="T(前一日期:)"/>
  	<ITEM LINE="5" COL="18"  TYPE="%-10s" NAME="#PreWorkDate"/>
  	<ITEM LINE="5" COL="35"  TYPE="%-11s" FUNCTIONS="T(最大场次:)"/>
  	<ITEM LINE="5" COL="46"  TYPE="%-2s" NAME="#RoundNum"/>
  </BLOCK>
  
  <BLOCK NAME="block_out_nx" LINE="7" VISABLE="FALSE">
  	<ITEM LINE="1" COL="7"  TYPE="%-13s" FUNCTIONS="T(当前工作日期:)"/>
  	<ITEM LINE="1" COL="21" NAME="#nxworkdate" TYPE="%-10s"/>
  	
  	<ITEM LINE="3" COL="7"  TYPE="%-13s" FUNCTIONS="T(当前系统状态:)"/>
  	<ITEM LINE="3" COL="21"  TYPE="%-10s" NAME="#nxsystemstatus"/>
  </BLOCK>
  
  <BLOCK NAME="block_out_nxy" LINE="7" VISABLE="FALSE">
  	<ITEM LINE="1" COL="7"  TYPE="%-13s" FUNCTIONS="T(当前系统状态:)"/>
  	<ITEM LINE="1" COL="21" NAME="#nxysystemstatus" TYPE="%-10s"/>
  	
  	<ITEM LINE="1" COL="37"  TYPE="%-13s" FUNCTIONS="T(当前系统日期:)"/>
  	<ITEM LINE="1" COL="50"  TYPE="%-10s" NAME="#nxysysdate"/>
  	
  	<ITEM LINE="3" COL="7"  TYPE="%-11s" FUNCTIONS="T(工作日类型:)"/>
  	<ITEM LINE="3" COL="18" NAME="#nxyworkstyle" TYPE="%-10s"/>
  	
  	<ITEM LINE="3" COL="37"  TYPE="%-11s" FUNCTIONS="T(时序类型:)"/>
  	<ITEM LINE="3" COL="48"  TYPE="%-10s" NAME="#nxytimestyle"/>
  	
  	<ITEM LINE="5" COL="7"  TYPE="%-11s" FUNCTIONS="T(原系统状态:)"/>
  	<ITEM LINE="5" COL="18" NAME="#nxyoldsystemstatus" TYPE="%-10s"/>
  	
  	<ITEM LINE="5" COL="37"  TYPE="%-11s" FUNCTIONS="T(原系统日期:)"/>
  	<ITEM LINE="5" COL="48"  TYPE="%-10s" NAME="#nxyoldsysdate"/>
  </BLOCK>
  
  <BLOCK NAME="block_out_edzf" LINE="7" VISABLE="FALSE">
  	 <ITEM LINE="1" COL="7"  TYPE="%-11s" FUNCTIONS="T(二代支付:)"/>
  </BLOCK>
  	<ITEM NAME="#AW"  TYPE="%-5s" FUNCTIONS="" INPUT="BUTTON">
  		<ONCLICK>
		"{	
			rerun();
  			refresh();
		}"
		</ONCLICK>
  	</ITEM>
  	
  	
</VARIABLE>
</TRANSACTION>
