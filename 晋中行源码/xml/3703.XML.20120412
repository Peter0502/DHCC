		//$FD87 = '7890';//TOOD#TERMINAL_SEQ_NO ;//终端流水号
		//$FD23 = #CARD_SEQ_ID ;//IC卡序列号
		$FD39 = getitems('#AC_BAL');   // 电子现金余额
		$FD40= getitems('#AMT');   // 交易金额
		if(#QCLX == '0')//指定账户圈存
		{
			$FD37 = '';//转入卡号
			$FD16='7780';
			//showmsg('指定账户圈存');
		}
