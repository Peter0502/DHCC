		//$FD75 =$MSR2 ;//二磁
		//$FD76 =$MSR3; //三磁
		$FD75 =#TRACK2 ;//二磁
		$FD76 =#TRACK3; //三磁
		@shuju=initiccard();
		#IC_ATC=strfld(@shuju,'|',2);
		#IC_AC=strfld(@shuju,'|',3);
		#CARD_SEQ_ID=strfld(@shuju,'|',4);
		//showmsg('#IC_ATC'+#IC_ATC);
		//showmsg('#IC_AC'+#IC_AC);
		$FD34 =#IC_ATC ; //应用计数器
		$FD33 =#IC_AC ;//应用密文
