<TRANSACTION RIGHTS="11110100" TITLE="5345 系统状态查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD44=$HDATE;
    $FD3 =$KINBR;
    $FD7=$TLRNO;

    $FD67 = #FLAG;
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(5345                    系统状态查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5885)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="08" COL="15" TYPE="%-10s" FUNCTIONS="T(参与类别:)"/>


 

  <ITEM LINE="08" COL="25" NAME="#FLAG" TYPE="%-1s"    DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(1)">
    <SELECT LINE="8" COL="25" >
            <OPTION VALUE="1"  TITLE="0.大额系统状态查询"/>
            <OPTION VALUE="2"  TITLE="1.小额系统状态查询"/>
    </SELECT>
   </ITEM>


  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21"  COL="65" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
  <ONCLICK>
  "{
  	commsend_001();
    	$FD44=$HDATE;
    	$FD3 =$KINBR;
    	$FD7=$TLRNO;

    	$FD67 = #FLAG;
    	$FD16 = '5885';
    	callserver();
    	if($FD12!='0000')
    	{
    		showmsg(geterror());
    		return (false);
    	}
    	$BUFFER = $FD123;
  	if(#FLAG =='1')
  	{
  		if(opendlg('../xml/HVSTAT.DLG'))
  		{
            		return(true);
        	}
        	else
        	{
            		return(false);
        	}
  	}
  	else if(#FLAG=='2')
  	{
  		
  		if(opendlg('../xml/LVSTAT.DLG'))
  		{
            		return(true);
        	}
        	else
        	{
            		return(false);
        	}
  	}
   }"
   </ONCLICK>
   </ITEM>

  </VARIABLE>
  <REQUEST>
</REQUEST>

<RESPONSE>
    <PACKAGE>
    NOTHING
    </PACKAGE>
    <PRINT>"{
        }"
    </PRINT>
</RESPONSE>

<OUTPUT>
</OUTPUT>
</TRANSACTION>

