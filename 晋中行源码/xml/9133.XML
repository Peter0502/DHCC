   <ONLOSTFOCUS>
  "{ dim @txno as string;
     @txno=$TXNO;
         initfd();
         @txno=$TXNO;
         $TXNO='6700';
         commsend_001();
         $FD30=#ACTNO;
         $FD67='2';
         callserver();
         $TXNO=@txno;     
         if($FD12!='0000')
            {showmsg(geterror());
             return(false);
         }
         if($KINBR!=$FD91)
         {
             showmsg('只能查询本机构帐户!');
             return(false);
         } 
   }"
