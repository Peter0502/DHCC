<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6802 贴现审核"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	$FD25=#NAME;
    $FD45=#MDATE;
    $FD48=#WDAYS;
    $FD60=#PNNO;
    $FD62=#PANO;
    $FD67=#ACTYPE;
    $FD68=#LIND;
    $FD83=#ACNO;
    $FD84=getitems('#PINT');
    $FD90=#NOTYPE;
    $FD100=getitems('#TINT#AMT');
	commsend_001();
    $FD16='4912';
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(6802                            贴现审核)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4912)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="5"  COL="5" TYPE="%-15s" FUNCTIONS="T(贴现协议借据号:)"/>
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(-----------------------------【回显信息】----------------------------------)"/>
  <ITEM LINE="7"  COL="5" TYPE="%-10s" FUNCTIONS="T(转账类型!)"/>
  <ITEM LINE="7"  COL="30" TYPE="%-16s" FUNCTIONS="T(贴现申请人账号:)"/>
  <ITEM LINE="8"  COL="5" TYPE="%-16s" FUNCTIONS="T(贴现申请人户名:)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴 现 率:)"/>
  <ITEM LINE="9"  COL="29" TYPE="%-12s" FUNCTIONS="T(异地同城标!)"/>
  <ITEM LINE="9"  COL="54" TYPE="%-10s" FUNCTIONS="T(在途天数:)"/>
  <ITEM LINE="10"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴现利息:)"/>
  <ITEM LINE="15"  COL="5" TYPE="%-10s" FUNCTIONS="T(票据类型!)"/>
  <ITEM LINE="15"  COL="50" TYPE="%-10s" FUNCTIONS="T(票 据 号:)"/>
  <ITEM LINE="16"  COL="5" TYPE="%-10s" FUNCTIONS="T(汇票金额:)"/>
  <ITEM LINE="16"  COL="50" TYPE="%-12s" FUNCTIONS="T(汇票到期日:)"/>
 
  
  <!--输入变量-->
  <ITEM NAME="#PANO" LINE="5" COL="20" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
 "{
   initfd();
    commsend_001();
 		$FD16='9860';
		$FD62=#PANO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#NAME=$FD25;
		#MDATE=$FD45;
		#WDAYS=$FD48;
		#PNNO=$FD60;
		#ACTYPE=$FD67;
		#LIND=$FD68;
		#ACNO=$FD83;
		setitems('#PINT',$FD84);
		#NOTYPE=$FD90;
		setitems('#TINT#AMT',$FD100);
		show(#ACTYPE,true);
		show(#ACNO,true);
		show(#NAME,true);
		show(#PINT,true);
		show(#LIND,true);
		show(#WDAYS,true);
		show(#TINT,true);
		show(#NOTYPE,true);
		show(#PNNO,true);
		show(#AMT,true);
		show(#MDATE,true);
 }"
 </ONLOSTFOCUS>
 </ITEM>
  <ITEM NAME="#ACTYPE" LINE="7" COL="15" TYPE="%-1s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T(1)">
  <SELECT>
		<OPTION VALUE="1" TITLE="本行"/>
		<OPTION VALUE="2" TITLE="同城"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#ACNO" LINE="7" COL="44" TYPE="%-32s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>

  <ITEM NAME="#NAME" LINE="8" COL="21" TYPE="%-50s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#PINT" LINE="9" COL="15" TYPE="%9.6f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM LINE="9"  COL="25" TYPE="%-2s" FUNCTIONS="T(‰)"/>
  <ITEM NAME="#LIND" LINE="9" COL="40" TYPE="%-1s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T(0)">
  <SELECT>
		<OPTION VALUE="0" TITLE="本地"/>
		<OPTION VALUE="1" TITLE="异地"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#WDAYS" LINE="9" COL="63" TYPE="%4d" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#TINT" LINE="10" COL="15" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#NOTYPE" LINE="15" COL="15" TYPE="%-3s" ENABLE="FALSE" INPUT="SELECT" FUNCTIONS="T(111)">
  <SELECT>
		<OPTION VALUE="111" TITLE="银行承兑汇票"/>
		<OPTION VALUE="112" TITLE="商业承兑汇票"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#PNNO" LINE="15" COL="60" TYPE="%-16s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#AMT" LINE="16" COL="15" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#MDATE" LINE="16" COL="60" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>
 <ITEM NAME="#NOTYPESNAME" TYPE="%-30s" FUNCTIONS="S(#NOTYPE,#NOTYPE)"/>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
    <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
    <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
	<ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>

  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE  DEVICE="SCREEN" DESC="贴现审核">
	NOTHING
	</PACKAGE>
	<PRINT>"{
    	print( 5, 30,'贴现审核');
		print( 7, 20,'贴现协议编号: [20]',#PANO);
		print( 8, 20,'审核成功');
 	}"
 	</PRINT>
</RESPONSE>

<RESPONSE>
	<PACKAGE DEVICE="PRINTER" DESC="打印业务专用凭证">
	NOTHING
	</PACKAGE>
	<PRINT>"{
                dim @txtime  as string;
                @txtime=substr($SYSTIME,0,2)+':'+substr($SYSTIME,2,2)+':'+substr($SYSTIME,4,2);
		print( 5,20,'机构名称:[30]',$BRNAME);
                print( 5,2,'代码:6802');
                print( 5,13,'交易:贴现审核');
		print( 7,20,'贴现协议编号: [20]',#PANO);
		print( 7,10,'贴现申请人帐号:[32]', #ACNO);
		print( 8,20,'贴现申请人户名:[50]',#NAME);
                print( 9,20,'贴现率:[10]',itoa(#PINT,'%9.6f'));
                print(10,20,'异地同城标志(0.本地 1.异地):[1]',#LIND);
                print(10,10,'在途天数:[4]',#WDAYS);
                print(11,20,'贴现利息:[17]',itoa(#TINT,'%17.2f'));
                print(11,10,'票据类型:[30]',#NOTYPESNAME);
                print(12,20,'票据号:[16]',#PNNO);
                print(12,10,'金额:[17]',itoa(#AMT,'%17.2f'));
                print(13,20,'到期日期:[8]',#MDATE);
                print_sq();
		print( 24,20,'备注:')
                print( 24,27,'交易日期:[8] [8]',$HDATE,@txtime);
                print( 24,5,'柜员:[4]',$FD7);
                print( 24,5,'流水号:[6]',$FD4);
 	}"
	</PRINT>
</RESPONSE>
</TRANSACTION>
