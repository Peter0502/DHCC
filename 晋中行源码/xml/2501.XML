<?xml version="1.0" encoding="GBK" ?> 
<TRANSACTION RIGHTS="11111001" TITLE="2501 黑名单信息录入"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
  "{ commsend_001();
     $FD28=#CUSTNO;
     $FD67=#INSOU;
     $FD81=#HANRE;
   }" 
</COMMSEND>
<COMMRECV>
  "{ commrecv_001();
     if($FD12!='0000'){
			return(false);}
   }"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-45s" FUNCTIONS=
	 "T(2501                         黑名单信息录入)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
	 "T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1501)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
	<!--输入变量-->
	<ITEM LINE="9" COL="15" TYPE="%-10s" FUNCTIONS="T(客 户 号:)"/>
	<!--<ITEM LINE="8" COL="3" TYPE="%-74s" FUNCTIONS=
	"T(--------------------------------------------------------------------------)"/>-->
	<ITEM LINE="11" COL="15" TYPE="%-10s" FUNCTIONS="T(信息来源!)"/>
	<ITEM LINE="13" COL="15" TYPE="%-10s" FUNCTIONS="T(上单原因:)"/>
	<ITEM NAME="#CUSTNO" INPUT="TEXT" LINE="9" COL="25" TYPE="%-8s" FUNCTIONS="h()V(NB)"/>
	<ITEM NAME="#INSOU" INPUT="SELECT" LINE="11" COL="25" TYPE="%-1s" FUNCTIONS="T(1)">
	  <SELECT>
      <OPTION VALUE="1" TITLE="坏帐"/>
      <OPTION VALUE="2" TITLE="法院起诉"/>
      <OPTION VALUE="3" TITLE="外部信息"/>
      <OPTION VALUE="4" TITLE="冻结"/>
	 </SELECT>
  </ITEM>
  <ITEM NAME="#SINSOU" TYPE="%-8s" FUNCTIONS="S(#INSOU,#INSOU)"/>
  <ITEM NAME="#HANRE" INPUT="TEXT" LINE="13" COL="25" TYPE="%-50s" FUNCTIONS="T()V(NB)"/>
  <!--------------有些变量没有用,只是为了公共脚本执行时不报错------------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="20" COL="60"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="录入成功"> 
     NOTHING
</PACKAGE>
               <PRINT>
           "{
               print(8,30,'录入成功!');
            }"
        </PRINT>
</RESPONSE>
<RESPONSE>
<PACKAGE DEVICE="PRINTER" DESC="请打印业务专用凭证">
NOTHING
</PACKAGE>
        <PRINT>
         "{
          print(3,30,'[7      ]   [30]',$KINBR,$BRNAME);
          print(4,30,'黑名单信息录入');
          print(6,15,'客  户  号:[8]',#CUSTNO);
          print(7,15,'信息  来源:[8]',#SINSOU);
          print(8,15,'上单  原因:[50]',#HANRE);
          }"
           </PRINT>
</RESPONSE>
</TRANSACTION>
