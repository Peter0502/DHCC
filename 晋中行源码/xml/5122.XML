<COMMRECV>"{
	commrecv_001();
	commrecv_v123();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(5122                        大额查复报文录入)"/>
