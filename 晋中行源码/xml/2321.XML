<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11000000" TITLE="2321 身份证验证查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(2321                           身份证验证查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"        TYPE="%-4s"  FUNCTIONS="T($TXNO=9321)"/>
  <ITEM NAME="#MSGTYPE"     TYPE="%-5s"  FUNCTIONS="T($MSGTYPE=03001)"/>
	
	<ITEM LINE="9" COL="10" TYPE="%-10s" FUNCTIONS="T(查询方式:)"/>
	<ITEM LINE="9" COL="20" TYPE="%1s" NAME="#SOURCE_INFO"  INPUT="SELECT" FUNCTIONS="T(1)V(NB)">
		<SELECT>
			<OPTION VALUE="1" TITLE="根据账号查询"/>
			<OPTION VALUE="2" TITLE="根据客户信息查询"/>
		</SELECT>
		<ONLOSTFOCUS>
		"{
			if(#SOURCE_INFO=='1')
			{
				showblock('ACTNO',true,false);
				showblock('CIF',false,false);
				showblock('MESSAGE',false,false);
				showblock('MESSAGE1',false,false);
				show(#L_WH_FLAG,false);
				show(#WH_FLAG,false);
			}
			else
			{
				showblock('ACTNO',false,false);
				showblock('CIF',true,false);
				showblock('MESSAGE',false,false);
				showblock('MESSAGE1',false,false);
				show(#L_WH_FLAG,true);
				show(#WH_FLAG,true);
			}
		}"
		</ONLOSTFOCUS>
	</ITEM>
	
	<ITEM LINE="10" COL="10" TYPE="%-10s" NAME="#L_WH_FLAG" VISABLE="FALSE" INPUT="LABEL" FUNCTIONS="T(维护标志:)" />
	<ITEM LINE="10" COL="20" TYPE="%1s" NAME="#WH_FLAG" VISABLE="FALSE" INPUT="SELECT" FUNCTIONS="T(1)V(NB)">
		<SELECT>
			<OPTION VALUE="1" TITLE="已维护"/>
			<OPTION VALUE="2" TITLE="未维护"/>
		</SELECT>
		<ONLOSTFOCUS>
		"{
			if(#WH_FLAG=='1')
			{
				showblock('CIF',true,false);
				showblock('MESSAGE',false,false);
				showblock('MESSAGE1',true,false);
			}
			else
			{
				showblock('CIF',false,false);
				showblock('MESSAGE',true,false);
				showblock('MESSAGE1',false,false);
			}
		}"
		</ONLOSTFOCUS>
	</ITEM>
	
	<BLOCK NAME="ACTNO" LINE="11" COL="10" VISABLE="FALSE">
		<ITEM LINE="1" COL="0" TYPE="%-10s" NAME="#LACNO" FUNCTIONS="T(账    号:)"/>
		<ITEM LINE="1" COL="10" TYPE="%-19s" NAME="#ACNO"  FUNCTIONS="T()V(NB)" INPUT="TEXT" />
	</BLOCK>
	<BLOCK NAME="CIF" LINE="11" COL="10" VISABLE="FALSE">
		<ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(客 户 号:)"/>
		<ITEM LINE="0" COL="10" TYPE="%8d" INPUT="TEXT"   NAME="#CIFNO"   FUNCTIONS="T()"/>
		<ITEM LINE="1" COL="0" TYPE="%-10s" FUNCTIONS="T(客户名称:)"/>
		<ITEM LINE="1" COL="10" TYPE="%-50s" INPUT="TEXT" NAME="#CIFNAME" FUNCTIONS="T()"/>
		<ITEM LINE="2" COL="0" TYPE="%-10s" FUNCTIONS="T(证件类型:)"/>
		<ITEM LINE="2" COL="10" TYPE="%-1s" INPUT="SELECT" NAME="#ID_TYPE" FUNCTIONS="T(1)">
		<SELECT>
			<OPTION VALUE="1" TITLE="身份证"/>
			<OPTION VALUE="2" TITLE="户口薄"/>
			<OPTION VALUE="3" TITLE="护照  "/>
			<OPTION VALUE="4" TITLE="军官证"/>
			<OPTION VALUE="5" TITLE="回乡证"/>
			<OPTION VALUE="6" TITLE="士兵证"/>
			<OPTION VALUE="7" TITLE="港澳居民来往通行证"/>
			<OPTION VALUE="E" TITLE="临时身份证"/>
			<OPTION VALUE="F" TITLE="外国人居留证"/>
			<OPTION VALUE="G" TITLE="警官证"/>
			<OPTION VALUE="H" TITLE="其他证件"/>
			<OPTION VALUE="I" TITLE="台湾同胞来往通行证"/>
		</SELECT>
		</ITEM>
		<ITEM LINE="3" COL="0" TYPE="%-10s" FUNCTIONS="T(证件号码:)"/>
		<ITEM LINE="3" COL="10" TYPE="%-18s" INPUT="TEXT" NAME="#IDNO" FUNCTIONS="T()"/>
	</BLOCK>	
	<BLOCK NAME="MESSAGE" LINE="15" COL="10" VISABLE="FALSE">
		<ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(证件类型:)"/>
		<ITEM LINE="0" COL="10" TYPE="%-1s" INPUT="SELECT" NAME="#ID_TYPE1" FUNCTIONS="T(1)V(NB)">
		<SELECT>
			<OPTION VALUE="1" TITLE="身份证"/>
			<OPTION VALUE="2" TITLE="户口薄"/>
			<OPTION VALUE="3" TITLE="护照  "/>
			<OPTION VALUE="4" TITLE="军官证"/>
			<OPTION VALUE="5" TITLE="回乡证"/>
			<OPTION VALUE="6" TITLE="士兵证"/>
			<OPTION VALUE="7" TITLE="港澳居民来往通行证"/>
			<OPTION VALUE="E" TITLE="临时身份证"/>
			<OPTION VALUE="F" TITLE="外国人居留证"/>
			<OPTION VALUE="G" TITLE="警官证"/>
			<OPTION VALUE="H" TITLE="其他证件"/>
			<OPTION VALUE="I" TITLE="台湾同胞来往通行证"/>
		</SELECT>
		</ITEM>
		<ITEM LINE="1" COL="0" TYPE="%-10s" FUNCTIONS="T(查询机构:)"/>
		<ITEM LINE="1" COL="10" TYPE="%-5s" FUNCTIONS="T()V(NB)" NAME="#BRNO1" INPUT="TEXT" />
		<ITEM LINE="1" COL="20" TYPE="%-10s" FUNCTIONS="T(查询日期:)" />
		<ITEM LINE="1" COL="30" TYPE="-8s" FUNCTIONS="T()D()V(NB)" NAME="#DATE1" INPUT="TEXT" />
		<ITEM LINE="1" COL="38" TYPE="%-2s" FUNCTIONS="T(→)"/>
		<ITEM LINE="1" COL="40" TYPE="-8s" FUNCTIONS="T()D()V(NB)" NAME="#DATE2" INPUT="TEXT" />
		<ITEM NAME="#TEMP1" TYPE="%-10s" FUNCTIONS="">
			<ONLOSTFOCUS>
			"{
				if((atoi(#DATE2)-atoi(#DATE1)) &gt; 10000)
				{
					showmsg('时间区间应为1年以内');
					return(false);
				}
				if(#DATE1 &gt; #DATE2)
				{
					showmsg('起始日期应该小于终止日期');
					return(false);
				}
				if(#DATE2 &gt; $HDATE)
				{
					showmsg('终止日期应该小于当前日期');
					return(false);
				}
			}"
			</ONLOSTFOCUS>
		</ITEM>
	</BLOCK>	
	<BLOCK NAME="MESSAGE1" LINE="17" COL="10" VISABLE="FALSE">
		<ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(维护柜员:)"/>
		<ITEM LINE="0" COL="10" TYPE="%-6s" FUNCTIONS="T()" NAME="#TEL" INPUT="TEXT" />
		<ITEM LINE="1" COL="0" TYPE="%-10s" FUNCTIONS="T(维护机构:)"/>
		<ITEM LINE="1" COL="10" TYPE="%-5s" FUNCTIONS="T()" NAME="#BRNO" INPUT="TEXT" />
		<ITEM LINE="2" COL="0" TYPE="%-10s" FUNCTIONS="T(维护日期:)"/>
		<ITEM LINE="2" COL="10" TYPE="%8d" FUNCTIONS="T()" NAME="#DATE" INPUT="TEXT" />

		<ITEM NAME="#TEMP" TYPE="%-10s" FUNCTIONS="">
			<ONLOSTFOCUS>
			"{
				if(#SOURCE_INFO=='2' and #WH_FLAG=='1')
				{
					dim @temp as string;
					@temp=#CIFNO+#CIFNAME+#IDNO+#TEL+#BRNO+#DATE;
					if(len(@temp) &lt; 4)
					{
						showmsg('请输入的查询信息太少');
						return(false);
					}
				}
			}"
			</ONLOSTFOCUS>
		</ITEM>

	</BLOCK>
	

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  	<ONCLICK>
  	"{
  		dim @tota as string;
  		initfd();
  		commsend_001();
  		$FD16='9321';
  		$FD67=#SOURCE_INFO;
  		$FD68=#WH_FLAG;
  		$FD30=#ACNO;
  		$FD25=#CIFNAME;
  		$FD28=#CIFNO;
  		$FD62=#IDNO;
  		$FD44=#DATE;
  		$FD45=#DATE1;
  		$FD46=#DATE2;
  		$FD92=#TEL;
  		if(#WH_FLAG=='1')
  		{
  			$FD2=#BRNO;
  			$FD69=#ID_TYPE;
  		}
  		else
  		{
  			$FD2=#BRNO1;
  			$FD69=#ID_TYPE1;
  		}
  		@tota=callserver();
  		if($FD12!='0000')
  		{
  			showmsg(geterror());
  			return(false);
  		}
			if(autolist(@tota))
			{
				list_work();
			}
			#SOURCE_INFO='';
			return(false);
  	}"
  	</ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE>
	NOTHING
	</PACKAGE>
</RESPONSE>

</TRANSACTION>
