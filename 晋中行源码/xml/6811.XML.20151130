<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6811 转贴现转入到期回购"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    initfd();
 	$MSGTYPE='03001';
	commsend_001();
	$FD12='4925';
	$FD16='9986';
	$FD25=#NAME ;
    $FD33=#INACNO;
	$FD44=#MDATE;
	$FD46=#RDATE;
    $FD62=#PANO;
    $FD69=#ACTYPE;
	$FD84=getitems('#PINT');
    $FD100=getitems('#TINT#AMT');
	$FD123=getitems('#KKKK$FD5$FD4');
    callserver();
    if($FD12!='0000')
	   {
			showmsg(geterror());
			return(false);
	   }
	$MSGTYPE='03001';	
	$FD16='4925';
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(6811                      转贴现转入到期回购)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4925)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <ITEM LINE="5"  COL="10" TYPE="%-10s" FUNCTIONS="T(还款类型!)"/>
  
  <ITEM LINE="7"  COL="10" TYPE="%-12s" FUNCTIONS="T(转贴现账号:)"/>
  <ITEM LINE="8"  COL="10" TYPE="%-16s" FUNCTIONS="T(转贴现协议编号:)"/>
  
  <ITEM LINE="10"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(-----------------------------【回显信息】----------------------------------)"/>

  <ITEM LINE="12"  COL="10" TYPE="%-12s" FUNCTIONS="T(申请人名称:)"/>
  <ITEM LINE="13"  COL="10" TYPE="%-14s" FUNCTIONS="T(汇总票据金额:)"/>
  <ITEM LINE="14"  COL="10" TYPE="%-12s" FUNCTIONS="T(转贴现日期:)"/>
  <ITEM LINE="15"  COL="10" TYPE="%-10s" FUNCTIONS="T(回购日期:)"/>
  <ITEM LINE="16"  COL="10" TYPE="%-12s" FUNCTIONS="T(转贴现利率:)"/>
  <ITEM LINE="16"  COL="32" TYPE="%-2s" FUNCTIONS="T(‰)"/>
  <ITEM LINE="17"  COL="10" TYPE="%-12s" FUNCTIONS="T(转贴现利息:)"/>
  
  
  <!--输入变量-->
  
  <ITEM NAME="#ACTYPE" LINE="5" COL="20" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="1" TITLE="同城"/>
		<!---
		<OPTION VALUE="2" TITLE="大额"/>
		-->
	</SELECT>
  </ITEM>
   <ITEM NAME="#FLAGACTYPE" TYPE="%4s" FUNCTIONS="S(#ACTYPE,#ACTYPE)"/>
  <ITEM NAME="#INACNO" LINE="7" COL="22" TYPE="%-24s"  INPUT="TEXT" FUNCTIONS="T()">
  <ONLOSTFOCUS>
 "{
    commsend_001();
 		$FD16='9922';
		$FD33=#INACNO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#NAME=$FD25;
		#INACNO=$FD33;
		#MDATE=$FD44;
		#RDATE=$FD46;
		#PANO=$FD62;
		setitems('#PINT',$FD84);
		#MSG=$FD96;
		setitems('#TINT#AMT',$FD100);
		show(#PANO,true);
		show(#NAME,true);
		show(#INACNO,true);
		show(#PINT,true);
		show(#MDATE,true);
        show(#RDATE,true);
		show(#AMT,true);
		show(#TINT,true);
		show(#MSG,true);
 }"
 </ONLOSTFOCUS>
 </ITEM>
 <ITEM NAME="#PANO" LINE="8" COL="26" TYPE="%-20s"  INPUT="TEXT" FUNCTIONS="T()">
 <ONLOSTFOCUS>
 "{
		initfd();
    	commsend_001();
 		$FD16='9926';
		$FD62=#PANO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#NAME=$FD25;
		#INACNO=$FD33;
		#MDATE=$FD44;
		#RDATE=$FD46;
		setitems('#PINT',$FD84);
		setitems('#TINT#AMT',$FD100);
		show(#PANO,true);
		show(#NAME,true);
		show(#INACNO,true);
		show(#PINT,true);
		show(#MDATE,true);
        show(#RDATE,true);
		show(#AMT,true);
		show(#TINT,true);
		show(#MSG,false);
 }"
 </ONLOSTFOCUS>
 </ITEM>
  
  <ITEM NAME="#NAME" LINE="12" COL="22" TYPE="%-50s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#AMT" LINE="13" COL="24" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#MDATE" LINE="14" COL="22" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#RDATE" LINE="15" COL="20" TYPE="%-8s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#PINT" LINE="16" COL="22" TYPE="%9.6f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#TINT" LINE="17" COL="22" TYPE="%17.2f" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#MSG" LINE="19" COL="10" TYPE="%-50s" ENABLE="FALSE" INPUT="TEXT" FUNCTIONS="T()"/>
  
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM NAME="#KKKK" TYPE="%-90s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
  <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
  <ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>

  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="转贴现转入到期回购!">
NOTHING
</PACKAGE>
<PRINT>"{
	  print( 5, 30,'转贴现转入到期回购');
      print( 7, 20,'转账类型: [4]',#FLAGACTYPE);
      print( 8, 20,'转贴现账号: [24]',#INACNO);
	  print( 9, 20,'转贴现协议编号: [20]',#PANO);
	  print(10, 20,'记帐成功');
 }"
 </PRINT>
</RESPONSE>
<RESPONSE>
<PACKAGE DEVICE="PRINTER"  LINESPACE="38" DESC="打印凭条">
NOTHING
</PACKAGE>
<PRINT>"{
	  print( 5, 16,'[4]     [2]     [2]',#YEAR,#MON,#DAY);
	  print( 7,  8,'[5]',$FD2);
	  print( 7, 24,'转贴现转入到期回购');
	  print( 7, 46,'[4]',$FD7);
	  print( 7, 62,'[6]',$FD4);
	  print( 8, 20,'转账类型: [4]',#FLAGACTYPE);
      print( 9, 20,'转贴现账号: [24]',#INACNO);
	  print(10, 20,'转贴现协议编号: [20]',#PANO);
	  print(11, 20,'记帐成功');
 }"
</PRINT>
</RESPONSE>
</TRANSACTION>
