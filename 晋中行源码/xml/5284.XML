<?xml version="1.0" encoding="GBK" ?> 
<TRANSACTION RIGHTS="00000000" TITLE="5284  票据核销查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
  "{ commsend_001();
     $FD68=#NOTYPE;
     $FD48=#PCNO;
     $FD67=#YNCAVE;
     $FD44=#TXDATE;
   }" 
</COMMSEND>
<COMMRECV>
  "{ commrecv_001();
   }"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
	 "T(5284                          票据核销查询)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
	 "T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4284)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="8" COL="24" TYPE="%-10s" FUNCTIONS="T(票据类型!)"/>
	<ITEM LINE="10" COL="24" TYPE="%-10s" FUNCTIONS="T(批 次 号:)"/>
	<ITEM LINE="12" COL="24" TYPE="%-10s" FUNCTIONS="T(是否核销!)"/>
  <ITEM LINE="14" COL="24" TYPE="%-10s" FUNCTIONS="T(交易日期:)"/>
  <ITEM NAME="#NOTYPE" INPUT="SELECT" LINE="8" COL="34" TYPE="%1s" FUNCTIONS="T()V(NB)">
	  <SELECT>
      <OPTION VALUE="1" TITLE="0. 提出"/>
      <OPTION VALUE="2" TITLE="1. 提入"/> 
	  </SELECT>
  </ITEM>
  <ITEM NAME="#PCNO" INPUT="TEXT" LINE="10" COL="34" TYPE="%-4s" FUNCTIONS="T()"/>
  <ITEM NAME="#YNCAVE" INPUT="SELECT" LINE="12" COL="34" TYPE="%1s" FUNCTIONS="G(#YNCAVE?Y,#PASSWD)">
	  <SELECT>
      <OPTION VALUE="N" TITLE="N. 否"/> 
      <OPTION VALUE="Y" TITLE="Y. 是"/>
	  </SELECT>
  </ITEM>
  <ITEM NAME="#TXDATE" INPUT="TEXT" LINE="14" COL="34" TYPE="%-8s" FUNCTIONS="T()"/>
  <!--------------有些变量没有用,只是为了公共脚本执行时不报错------------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="20" COL="60"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
</TRANSACTION>	
