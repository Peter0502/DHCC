<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9202 贷款登记簿查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD16='8202';
	$FD30=$KINBR;
	$FD29=#LDATE;
	$FD38=#ACNO;
	$FD44=#HDATE;
	$FD66=#STS;
	$FD70=#TYPE;
	$FD100=getitems('#AMT');
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9202                        贷款登记簿查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=9202)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <ITEM LINE="5"  COL="5" TYPE="%-12s" FUNCTIONS="T(【查询条件】)"/>
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(---------------------------------------------------------------------------)"/>
  
  <ITEM LINE="8"  COL="5" TYPE="%-10s" FUNCTIONS="T(账    号:)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-10s" FUNCTIONS="T(类    型!)"/>

  <ITEM LINE="10"  COL="5" TYPE="%-10s" FUNCTIONS="T(欠款状态!)"/>
  <ITEM LINE="11"  COL="5" TYPE="%-10s" FUNCTIONS="T(欠款金额:)"/>
  
  <ITEM LINE="12"  COL="5" TYPE="%-10s" FUNCTIONS="T(产生日期:)"/>
  <ITEM LINE="12"  COL="25" TYPE="%-2s" FUNCTIONS="T(→)"/>
  
 <!--输入变量-->  
 
  <ITEM NAME="#ACNO" LINE="8" COL="15" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="">
  <ONLOSTFOCUS>
 "{
    commsend_001();
 		$FD16='9958';
		$FD38=#ACNO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#ACNO=$FD38;
 }"
 </ONLOSTFOCUS>
 </ITEM>
  
  <ITEM NAME="#TYPE" LINE="9" COL="15" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="">
  <SELECT>
		<OPTION VALUE="1" TITLE="本金"/>
		<OPTION VALUE="2" TITLE="表内利息"/>
		<OPTION VALUE="3" TITLE="表外利息"/>
		<OPTION VALUE="4" TITLE="复利"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#STS" LINE="10" COL="15" TYPE="%-1s" INPUT="SELECT" FUNCTIONS="">
  <SELECT>
		<OPTION VALUE="0" TITLE="未还清"/>
		<OPTION VALUE="1" TITLE="已还清"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#AMT" LINE="11" COL="15" TYPE="%17.2f" INPUT="TEXT" FUNCTIONS=""/>
  
  <ITEM NAME="#LDATE" LINE="12" COL="15" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#HDATE" LINE="12" COL="28" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
