<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="3306 预约转存取消"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD62 = #CERTIFICATE;
	$FD67 = #CHOOSEPASSWORD;
	$FD68 = #CHOOSECERTIFICATE;
	$FD69 = #CERTIFYTYPE;

	
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(3306                       预约转存取消)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=2306)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <!--输入变量-->
  
  <ITEM LINE="8" COL="12" TYPE="%-5s" FUNCTIONS="T(卡号:)"/>
  <ITEM LINE="10" COL="12" TYPE="%-5s" FUNCTIONS="T(姓名:)"/>
  
  
  <ITEM LINE="12" COL="12" TYPE="%-9s" FUNCTIONS="T(密码支取:)"/>
  <ITEM LINE="12" COL="30" TYPE="%-9s" FUNCTIONS="T(密码:)"/>
  <ITEM LINE="14" COL="12" TYPE="%-9s" FUNCTIONS="T(证件支取:)"/>
  <ITEM LINE="14" COL="30" TYPE="%-9s" FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="16" COL="12" TYPE="%-9s" FUNCTIONS="T(证件号码:)"/>
  
  
 <ITEM NAME="#CARDNO" INPUT="TEXT" LINE="8" COL="17" TYPE="%-19s" FUNCTIONS="V(NB)">
<ONLOSTFOCUS>
"{
	$FD37 = #CARDNO;
	commsend_001();
	$FD16 = '9781';
	callserver();
	if($FD12!='0000')
	{
		showmsg(geterror());
		return (false);
	}
	#USERNAME = $FD25;
}"
</ONLOSTFOCUS>
</ITEM>
  <ITEM NAME="#USERNAME" INPUT="TEXT"  LINE="10" COL="17" TYPE="%-50s" FUNCTIONS=""/>
  <ITEM NAME="#CHOOSEPASSWORD"   INPUT="SELECT"  LINE="12" COL="21" TYPE="%1s" FUNCTIONS="G(N,#CHOOSECERTIFICATE)V(NB)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
  <ITEM NAME="#PASSWD"   INPUT="TEXT"  LINE="12" COL="39" TYPE="%-6s" DEVICE="PINPAD" FUNCTIONS="i()I()V(NB)"/>
  <ITEM NAME="#CHOOSECERTIFICATE"   INPUT="SELECT"  LINE="14" COL="21" TYPE="%1s" FUNCTIONS="G(N,#CONFIRM)V(NB)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
    <ITEM NAME="#CERTIFYTYPE"   INPUT="SELECT"  LINE="14" COL="39" TYPE="%1s" FUNCTIONS="V(NB)">
	<SELECT>
	<OPTION VALUE="1" TITLE="1 身份证"/>
	<OPTION VALUE="2" TITLE="2 户口簿"/>
	<OPTION VALUE="3" TITLE="3 护照"/>
	<OPTION VALUE="4" TITLE="4 军官证"/>
	<OPTION VALUE="5" TITLE="5 回乡证"/>
	<OPTION VALUE="8" TITLE="8 企业代码证"/>
	<OPTION VALUE="9" TITLE="9 经营许可证"/>
	<OPTION VALUE="A" TITLE="A 营业执照"/>
	<OPTION VALUE="B" TITLE="B 事业法人证"/>
	<OPTION VALUE="C" TITLE="C 工商核准号"/>
	</SELECT>
 </ITEM>


  <ITEM NAME="#CERTIFICATE"   INPUT="TEXT"  LINE="16" COL="21" TYPE="%-20s" FUNCTIONS="V(NB)"/>
  
  
  
  
   
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#CONFIRM"  LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE>
	NOTHING
	</PACKAGE>
	<PRINT>"{
		print(8,25,'个人理财取消成功!');
		}"
	</PRINT>
</RESPONSE>
</TRANSACTION>
