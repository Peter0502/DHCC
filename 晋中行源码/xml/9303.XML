<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9303 凭证历史查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$MSGTYPE=03001;
	$FD44=#CDATE;
	$FD59=#SNUM;
	$FD60=#ENUM;
	$FD64=#BRNO;
	$FD89=#VOCTYPE;
	$FD91=#TLRNO2;
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<FIRSTWORK >"{
	dim @txno_bak as string;
	dim @tota as string;
	@txno_bak=$TXNO;
	$TXNO='9902';
	commsend_001();
	@tota=callserver();	
	$TXNO=@txno_bak;
	setsel(#VOCTYPE,@tota,'o5t20o1v3o2');
}"
</FIRSTWORK>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9303                          凭证历史查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8303)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="6"  COL="35"  TYPE="%-14s" FUNCTIONS="T([查询条件])"/>
  <ITEM LINE="7"  COL="5"  TYPE="%-74s" FUNCTIONS="T(----------------------------------------------------------------------)"/>
  
  <BLOCK NAME="COMM" LINE="0" COL="20">
  <ITEM LINE="9"  COL="10" TYPE="%-10s" FUNCTIONS="T(网 点 号:)"/>
  <ITEM LINE="10" COL="10" TYPE="%-10s" FUNCTIONS="T(操 作 员:)"/>
  <ITEM LINE="12" COL="10" TYPE="%-10s" FUNCTIONS="T(凭证种类:!)"/>
  <ITEM LINE="13" COL="10" TYPE="%-10s" FUNCTIONS="T(起始号码:)"/>
  <ITEM LINE="15" COL="10" TYPE="%-10s" FUNCTIONS="T(终止号码:)"/>
  <ITEM LINE="16" COL="10" TYPE="%-10s" FUNCTIONS="T(变更日期:)"/>

  <ITEM LINE="9"   COL="20" NAME="#BRNO"  TYPE="%-5s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="10"  COL="20" NAME="#TLRNO2"  TYPE="%4d" DEVICE="KEYBOARD" FUNCTIONS="">
  <ONLOSTFOCUS>
    "{
        dim @baktxno as string;
        @baktxno=$TXNO;
        $TXNO='9902';
        $FD91 = #TLRNO2;
        commsend_001();
        callserver();
        $TXNO=@baktxno;
        if($FD12!='0000'){
            showmsg(geterror());
            return(false);
        }
	//返回有FD58,FD89,但是为空值
    }"
  </ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="12"  COL="20" NAME="#VOCTYPE" TYPE="%-3s" DEVICE="KEYBOARD" FUNCTIONS="T()">
	<SELECT>
		<OPTION VALUE="001" TITLE="0.现金支票"/>
	</SELECT>
  </ITEM>

  <ITEM LINE="13"  COL="20" NAME="#SNUM" TYPE="%16d" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="15"  COL="20" NAME="#ENUM" TYPE="%16d" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="16"  COL="20" NAME="#CDATE" TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  </BLOCK>
  <!--输出变量--->
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
