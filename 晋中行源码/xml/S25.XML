000#TXFLAG=1B390000AN505%LOP#OTXDAY#OUTCHANGE#OUTNAME#INCHANGE#INNAME#OAVBAL#CRACNO#DRACNO#TIAVBAL#TICNT#TOAVBAL#TOCNT#CEAMT%BAK
</PACKAGE>
<PACKAGE>
000#TXFLAG=2B390000AN205%LOP#OTXDAY#OUTCHANGE#OUTNAME#INCHANGE#INNAME#OAVBAL#CRACNO#DRACNO#TIAVBAL#TICNT#TOAVBAL#TOCNT#CEAMT%BAK
</PACKAGE>
-->
<PACKAGE>
000#TXFLAG=1B390000AN505%LOP#OTXDAY#OUTNAME#TIAVBAL#TICNT#TOAVBAL#TOCNT#CEAMT#DRACNO#CRACNO%BAK
</PACKAGE>
<PACKAGE>
000#TXFLAG=2B390000AN205%LOP#OTXDAY#OUTNAME#TIAVBAL#TICNT#TOAVBAL#TOCNT#CEAMT#DRACNO#CRACNO%BAK
</PACKAGE>
