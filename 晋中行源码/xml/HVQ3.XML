<?xml version="1.0" encoding="GBK" ?>



<TRANSACTION RIGHTS="11111111" TITLE="T(同业拆借明细)" 
   xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVQ3                          同业拆借明细)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="6"  COL="05" TYPE="%-10s"  FUNCTIONS="T(交易日期:)"/>
  <ITEM LINE="6"  COL="29" TYPE="%-15s"  FUNCTIONS="T(支付交易序号:)"/>
  <ITEM LINE="6"  COL="57" TYPE="%-10s"  FUNCTIONS="T(交易状态:)"/>
  <ITEM LINE="8"  COL="05" TYPE="%-10s"  FUNCTIONS="T(优 先 级:)"/>
  <ITEM LINE="8"  COL="29" TYPE="%-12s"  FUNCTIONS="T(币    种:)"/>
  <ITEM LINE="10"  COL="05" TYPE="%-10s" FUNCTIONS="T(发起行号:)"/>
  <ITEM LINE="10"  COL="29" TYPE="%-15s" FUNCTIONS="T(接 收  行 号:)"/>
  <ITEM LINE="12" COL="05" TYPE="%-10s"  FUNCTIONS="T(业务类型:)"/>
  <ITEM LINE="12" COL="29" TYPE="%-15s"  FUNCTIONS="T(凭 证  种 类:)"/>
  <ITEM LINE="12" COL="57" TYPE="%-10s"  FUNCTIONS="T(凭证号码:)"/>
  <ITEM LINE="14" COL="05" TYPE="%-12s"  FUNCTIONS="T(拆借利率:)"/>
  <ITEM LINE="14" COL="29" TYPE="%-15s"  FUNCTIONS="T(拆 借  期 限:)"/>
  <ITEM LINE="14" COL="49" TYPE="%-12s"  FUNCTIONS="T(天)"/>
  <ITEM LINE="16" COL="05" TYPE="%-11s"  FUNCTIONS="T(本    金:)"/>
  <ITEM LINE="16" COL="43" TYPE="%-11s"  FUNCTIONS="T(利    息:)"/>
  <ITEM LINE="18" COL="05" TYPE="%-11s"  FUNCTIONS="T(附    言:)"/>

  <ITEM LINE="6"   COL="15" NAME="#HDATE"     TYPE="%-8s"    FUNCTIONS="V(NB)T($BUFFER,61,8)"/>
  <ITEM LINE="6"   COL="43" NAME="#ORDERNOHV" TYPE="%-8s"    FUNCTIONS="T($BUFFER,84,8)V(NB)"/>
  <ITEM LINE="6"   COL="66" NAME="#TXSTAT"    TYPE="%-14s"    FUNCTIONS="V(NB)T($BUFFER,75,14)"/>

  <ITEM LINE="21"   COL="4" NAME="#SUP"   TYPE="%-1s"  INPUT="SELECT" VISABLE="FALSE" FUNCTIONS="T($BUFFER,83,1)V(0,2)J(#OSUP)">
   <SELECT LINE="21" COL="4" >
           <OPTION VALUE="0 普通" TITLE="0 普通"/>
           <OPTION VALUE="1 紧急" TITLE="1 紧急"/>
           <OPTION VALUE="2 特急" TITLE="2 特急"/>
   </SELECT>
   </ITEM>
   <ITEM LINE="8"   COL="15" NAME="#LSUP"   TYPE="%-6s" FUNCTIONS="S(#SUP,#SUP)"/>   
  
   <ITEM LINE="21"   COL="5" NAME="#CORU"   TYPE="%-3s"  INPUT="SELECT" VISABLE="FALSE"  FUNCTIONS="V(NB)T($BUFFER,92,3)">
   <SELECT LINE="21" COL="5" >
           <OPTION VALUE="RMB" TITLE="人民币"/>
   </SELECT>
   </ITEM>
   <ITEM LINE="8"   COL="39" NAME="#LCORU" TYPE="%-9s"  FUNCTIONS="S(#CORU,#CORU)"/>
  <ITEM LINE="10"   COL="15" NAME="#ORBRNO" TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,95,12)"/>
  
  <ITEM LINE="10"   COL="43" NAME="#ACBRNO" TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,107,12)"/>
  <ITEM LINE="21"  COL="4" NAME="#YWTYPE" TYPE="%-2s"     INPUT="SELECT" VISABLE="FALSE" FUNCTIONS="T($BUFFER,119,2)V(NB)">
   <SELECT LINE="21" COL="4" >
            <OPTION VALUE="10 场内拆借" 	TITLE="场内拆借"/>
            <OPTION VALUE="11 场内拆借还效" TITLE="场内拆借还效"/>
            <OPTION VALUE="20 场外拆借" 	TITLE="场外拆借"/>
            <OPTION VALUE="21 场外拆借还效" TITLE="场外拆借还效"/>
 
   </SELECT>
   </ITEM>
  <ITEM LINE="12"   COL="15" NAME="#LYWTYPE"   TYPE="%-12s" FUNCTIONS="S(#YWTYPE,#YWTYPE)"/>  
  <ITEM LINE="21" COL="4"  NAME="#VOCTYPE" TYPE="%-2s"  INPUT="SELECT" VISABLE="FALSE"  FUNCTIONS="T($BUFFER,121,2)V(NB)">
  <SELECT LINE="21" COL="4">
        <OPTION VALUE="99 旧凭证        " TITLE="99旧凭证"/>
        <OPTION VALUE="22 现金支票      " TITLE="22现金支票      "/>
        <OPTION VALUE="21 转帐支票      " TITLE="21转帐支票      "/>
        <OPTION VALUE="37 单位对公活折  " TITLE="37单位对公活折  "/>
        <OPTION VALUE="29 定期存款证实书" TITLE="29定期存款证实书"/>
        <OPTION VALUE="38 贴现凭证      " TITLE="38贴现凭证      "/>
        <OPTION VALUE="40 支款凭条      " TITLE="40支款凭条      "/>
        <OPTION VALUE="41 现金缴款单    " TITLE="41现金缴款单    "/>
        <OPTION VALUE="42 转帐借方凭证  " TITLE="42转帐借方凭证  "/>
        <OPTION VALUE="43 转帐贷方凭证  " TITLE="43转帐贷方凭证  "/>
        <OPTION VALUE="44 现金付出凭证  " TITLE="44现金付出凭证  "/>
        <OPTION VALUE="45 现金收入凭证  " TITLE="45现金收入凭证  "/>
        <OPTION VALUE="46 特种转帐凭证  " TITLE="46特种转帐凭证  "/>
        <OPTION VALUE="47 普通凭证      " TITLE="47普通凭证      "/>
        <OPTION VALUE="48 专用凭证      " TITLE="48专用凭证      "/>
        <OPTION VALUE="49 借款票据      " TITLE="49借款票据      "/>
        <OPTION VALUE="64 收费凭证      " TITLE="64收费凭证      "/>
 </SELECT>
 </ITEM>
  <ITEM LINE="12"  COL="43" NAME="#LVOCTYPE" TYPE="%-14s" FUNCTIONS="S(#VOCTYPE,#VOCTYPE)"/> 
  <ITEM LINE="12"  COL="67" NAME="#VOCNUM"   TYPE="%-12s"  FUNCTIONS="T($BUFFER,123,12)"/>

  <!--
  <ITEM LINE="14"  COL="17" NAME="#CRATE"   TYPE="%8.5f"  FUNCTIONS="T($BUFFER,135,7)"/>
   -->
  <ITEM NAME="#CRATE"  TYPE="%8.5f"  FUNCTIONS="J(#LCRATE)"/>
  <ITEM LINE="14"  COL="17" NAME="#LCRATE"  TYPE="%-9s"  FUNCTIONS="X(#CRATE)"/>
  
  <ITEM LINE="14"  COL="43" NAME="#CDATE"   TYPE="%5d"   FUNCTIONS="V(NB)T($BUFFER,142,5)"/>

  <!--
  <ITEM NAME="#HTXAMT"  TYPE="%16.2f" FUNCTIONS="T($BUFFER,147,15)"/>
  -->
  <ITEM NAME="#HTXAMT"  TYPE="%16.2f" FUNCTIONS="J(#LHTXAMT)"/>
  <ITEM LINE="16"  COL="15" NAME="#LHTXAMT" TYPE="%-20s"   FUNCTIONS="X(#HTXAMT)"/>
 <!---
  <ITEM NAME="#LTXAMT"  TYPE="%16.2f" FUNCTIONS="T($BUFFER,162,15)"/>
   --->
   <ITEM NAME="#LTXAMT"  TYPE="%16.2f" FUNCTIONS="J(#LLTXAMT)"/>
  <ITEM LINE="16"  COL="53" NAME="#LLTXAMT" TYPE="%-20s"   FUNCTIONS="X(#LTXAMT)"/>
  <ITEM LINE="18"  COL="15" NAME="#RESUM1"  TYPE="%-40s"  FUNCTIONS="T($BUFFER,177,40)"/>
   
  <ITEM LINE="21"  COL="35" NAME="#CLOSE"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="45" NAME="#QUERY"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(查 询)">
  <ONCLICK>"{
  	if(substr(#TXSTAT,0,4)=='来账' or substr(#TXSTAT,4,4)=='不符' or #TXSTAT=='全国押错' or #TXSTAT=='地方押错' or #TXSTAT=='正常接收'){
  		open('6601.XML','#CBRNO='+#ORBRNO+';'+
	  					'#OCORU='+#CORU+';'+
	  					'#OCMTNO=105;'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#ORBRNO+';'+
	  					'#OACBRNO='+#ACBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		open('6601.XML','#CBRNO='+#ACBRNO+';'+
	  					'#OCORU='+#CORU+';'+
	  					'#OCMTNO=105;'+
	  					'#OWDATE='+#HDATE+';'+
	  					'#OORBRNO='+#ORBRNO+';'+
	  					'#OACBRNO='+#ACBRNO+';'+
	  					'#OLDDER='+#ORDERNO+';'+
	  					'#OTXAMT='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="55" NAME="#HVPUT"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请撤销)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账排队' or #TXSTAT=='发    送'){
  		open('6604.XML','#OLDDATE='+#HDATE+';'+
	  					'#OLDORD='+#ORDERNO+';'+
	  					'#OLDTYPE=105;'+
	  					'#CORU='+#CORU+';'+
	  					'#MONEY='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请撤销');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
   <ITEM LINE="21"  COL="67" NAME="#HVUNTREAD"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请退回)">
  <ONCLICK>"{
  	if(#TXSTAT=='往账清算' or substr(#TXSTAT,0,4)=='退回'){
  		open('6605.XML','#OLDDATE='+#HDATE+';'+
	  					'#RECBRNO='+#ACBRNO+';'+
	  					'#OLDORD='+#ORDERNO+';'+
	  					'#OLDTYPE=105;'+
	  					'#CORU='+#CORU+';'+
	  					'#MONEY='+itoa(#HTXAMT*100,'%.0f')+';'
	  					);
  	}else{
  		showmsg('此种状态交易无法申请退回');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
</TRANSACTION>
