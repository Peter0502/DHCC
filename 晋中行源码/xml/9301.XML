<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="9301 客户凭证查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	commsend_001();
	$MSGTYPE=03001;
	$FD29=#BDATE1;
  $FD38=#ACTNO1;
	$FD39=space(16);
	$FD44=#BDATE2;
	$FD59=#SNUM;
	$FD60=#ENUM;
	$FD67=#VOCFLAG;
	$FD89=#VOCTYPE;
	$FD91=#BRNO;
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<FIRSTWORK >"{
	dim @txno_bak as string;
	dim @tota as string;
	@txno_bak=$TXNO;
	$TXNO='9903';
	$FD89=space(3);
	commsend_001();
	@tota=callserver();	
	$TXNO=@txno_bak;
}"
</FIRSTWORK>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9301                          客户凭证查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8301)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <ITEM LINE="6"  COL="33"  TYPE="%-14s" FUNCTIONS="T([查询条件])"/>
  <ITEM LINE="7"  COL="5"  TYPE="%-74s" FUNCTIONS="T(----------------------------------------------------------------------)"/>
  <BLOCK NAME="COMM" LINE="0" COL="16">
  <ITEM LINE="9"  COL="10" TYPE="%-10s" FUNCTIONS="T(账    号:)"/>
  <ITEM LINE="10" COL="10" TYPE="%-10s" FUNCTIONS="T(凭证种类:!)"/>
  <ITEM LINE="11" COL="10" TYPE="%-10s" FUNCTIONS="T(凭证状态:!)"/>
  <ITEM LINE="13" COL="10" TYPE="%-10s" FUNCTIONS="T(起始号码:)"/>
  <ITEM LINE="14" COL="10" TYPE="%-10s" FUNCTIONS="T(终止号码:)"/>
  <ITEM LINE="16" COL="10" TYPE="%-10s" FUNCTIONS="T(购买日期:)"/>
  <ITEM LINE="16" COL="30" TYPE="%-3s" FUNCTIONS="T(-->)"/>
  <ITEM LINE="17" COL="10" TYPE="%-10s" FUNCTIONS="T(机构编码:)"/>

  <ITEM LINE="9"   COL="20" NAME="#ACTNO1"  TYPE="%-20s" DEVICE="KEYBOARD" FUNCTIONS="">
  <ONLOSTFOCUS>
    "{
		initfd();
        dim @baktxno as string;
        @baktxno=$TXNO;
        $TXNO='9958';
        $FD38 = #ACTNO1;
        commsend_001();
        callserver();
        $TXNO=@baktxno;
        if($FD12!='0000'){
            showmsg(geterror());
            return(false);
        }
	//返回有FD30,FD31,FD37,FD38,但是为空值
    }"
  </ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="10"  COL="20" NAME="#VOCTYPE" TYPE="%-3s" DEVICE="KEYBOARD" FUNCTIONS="T()">
	<SELECT>
		<OPTION VALUE="001" TITLE="现金支票"/>
    <OPTION VALUE="002" TITLE="转帐支票"/>
    <OPTION VALUE="005" TITLE="业务委托书"/>
    <OPTION VALUE="026" TITLE="印鉴卡"/>
	</SELECT>
  </ITEM>

  <ITEM LINE="11"  COL="20" NAME="#VOCFLAG" TYPE="%-1s" DEVICE="KEYBOARD" FUNCTIONS="T()">
	<SELECT>
        <OPTION VALUE="1" TITLE="0.出售未使用"/>
        <OPTION VALUE="2" TITLE="0.挂失"/>
        <OPTION VALUE="3" TITLE="0.回收"/>
        <OPTION VALUE="4" TITLE="0.作废"/>
        <OPTION VALUE="5" TITLE="0.使用"/>
	</SELECT>
  </ITEM>

  <ITEM LINE="13"  COL="20" NAME="#SNUM"   TYPE="%16d" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="14"  COL="20" NAME="#ENUM"   TYPE="%16d" DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="16"  COL="20" NAME="#BDATE1" TYPE="%8d"  DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="16"  COL="35" NAME="#BDATE2" TYPE="%8d"  DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM LINE="17"  COL="20" NAME="#BRNO"   TYPE="%-5s" DEVICE="KEYBOARD" FUNCTIONS=""/>
  </BLOCK>

  <!--输出变量--->
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
