<BLOCK  LINE="9" COL="4"  NAME="IC_BUHUAN_APPLY" VISABLE="FALSE">
	<ITEM LINE="1" COL="1"  TYPE="%-11s" FUNCTIONS="T(持卡人户名:)" />
	<ITEM LINE="1" COL="12" TYPE="%-20s" NAME="#KH_NAME" INPUT="TEXT"  FUNCTIONS=""/>
	<ITEM LINE="1" COL="37" TYPE="%-7s"  FUNCTIONS="T(客户号:)" /> 
	<ITEM LINE="1" COL="46" TYPE="%-12s" NAME="#KH_HM" INPUT="TEXT"  FUNCTIONS="T()" />
	<ITEM LINE="3" COL="1"  TYPE="%-9s"  FUNCTIONS="T(IC卡密码:)" />
	<ITEM LINE="3" COL="10" TYPE="%6d"  NAME="#CARDPWD" INPUT="TEXT"  FUNCTIONS="i()T()V(NB)" >
		<ONLOSTFOCUS>
			"{
			if(substr(#CARD_NO,0,6)=='621780' or substr(#CARD_NO,0,6)=='621223')
			{
		    initfd();
				commsend_001();
				$FD16='7007';
				$FD30=#CARD_NO;
				$FD79=#CARDPWD;
				$FD68='4';
				callagn();
				if($FD12!='0000')
				{
					showmsg(geterror());
					return (false);
				}
			 }
			 else
			 {
				showmsg('介质种类错误,应为IC卡');
				return (false);
			 }
		 }
		}"
		</ONLOSTFOCUS>	
	</ITEM>
	<ITEM LINE="4" COL="1"  TYPE="%-9s" FUNCTIONS="T(更换原因:)" />
	<ITEM LINE="4" COL="10" TYPE="%-1s" NAME="#CHANGEREASON" INPUT="SELECT"  FUNCTIONS="T(1)" >	
		<SELECT>
			<OPTION VALUE="1" TITLE="损坏补换卡"/>
			<OPTION VALUE="2" TITLE="挂失补发新卡"/>
		</SELECT>
		<ONGOTFOCUS>
    	"{
    	}"
		</ONGOTFOCUS>
		<ONLOSTFOCUS>
		"{
			if (#CHANGEREASON == '1')
			{
				show(#lLOSTNO,  false);
				show(#LOSTNO,   false);
				enable(#lLOSTNO,false);
				enable(#LOSTNO, false);
			}
			else if (#CHANGEREASON == '2')
			{
				show(#lLOSTNO,  true);
				show(#LOSTNO,   true);
				enable(#lLOSTNO,true);
				enable(#LOSTNO, true);
			}
		}"
		</ONLOSTFOCUS>
	</ITEM>
	<ITEM LINE="5" COL="1" TYPE="%-9s"   NAME="#lLOSTNO"  FUNCTIONS="T(挂失编号:)" /> 
	<ITEM LINE="5" COL="10" TYPE="%-16s" NAME="#LOSTNO"   INPUT="TEXT"  FUNCTIONS="V(NB)" />	
</BLOCK>