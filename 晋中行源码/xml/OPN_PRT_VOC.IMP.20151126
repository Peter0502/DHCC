	print(15, 0,'[4   ][8       ]  [4   ][2 ][2 ][8       ][14            ][14            ] [4   ]   [4]',@ac_seqn,@opn_date,@memo,@term,@term_type,@end_day,substr(@txamt,7,14),substr(@txamt,7,14),@rate,@tel);

}"
</PRINT>
</RESPONSE>


