<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11000000" TITLE="3709 IC卡账户信息查询"
	xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
	"{
	}"
</COMMSEND>
<COMMRECV>
	"{
	}"
</COMMRECV>
<VARIABLE>
	<ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
		"T(3709                        IC卡账户信息查询)"/>
	<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
		"T(─────────────────────────────────────)"/>
	<ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=3709)"/>
	<ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>	
	<BLOCK NAME="COMMON" LINE="3" COL="10">
		<ITEM LINE="3"  COL="6" TYPE="%-12s" FUNCTIONS="T(卡    号:)"/>
		<ITEM LINE="5"  COL="6" TYPE="%-12s" FUNCTIONS="T(姓    名:)"/>
		<ITEM LINE="7"  COL="6" TYPE="%-12s" FUNCTIONS="T(子业务种类:)"/>
		<!---ITEM LINE="11" COL="6" TYPE="%-12s" FUNCTIONS="T(起始日期:)"/>
		<ITEM LINE="13" COL="6" TYPE="%-12s" FUNCTIONS="T(终止日期:)"/--->	
		<ITEM NAME="#CARDNO"    INPUT="TEXT"   LINE="3"  COL="18" TYPE="%-20s" DEVICE="ICC"  FUNCTIONS="V(NB)M($MSR2,1)">
			<ONLOSTFOCUS>
			"{
				dim @tota as string;
				if(len(rtrim(#CARDNO))!=19)
				{
					showmsg('卡号长度不正确，请重新输入!');
					return(false);
				}
				initfd();
				commsend_001();
				$FD16='9959';
				$FD102=#CARDNO;
				callserver();
				if($FD12!='0000'){
					showmsg(geterror());
					return(false);
				}
				#NAME=substr($FD102,125,35);//户名
				enable(#NAME,false);
				initfd();
				commsend_001();
				$FD16='9160';
				$FD30=#CARDNO;
				$FD67='2';
				@tota=callserver();
				if($FD12 != '0000')
				{
					showmsg(geterror());
					return(false);
				}
				setsel(#SUB_SEQN,@tota,'o5t20o1v4o2');
			}"
			</ONLOSTFOCUS>
		</ITEM>
		<ITEM NAME="#NAME"      INPUT="TEXT"   LINE="5"  COL="18" TYPE="%-40s"    FUNCTIONS="V(NB)"/>
		<ITEM NAME="#SUB_SEQN"  INPUT="SELECT" LINE="7"  COL="18" TYPE="%4d"      FUNCTIONS="T()">
			<SELECT>
				<OPTION VALUE="0001"   TITLE="0.公交卡"/>
			</SELECT>
		</ITEM>
		<!---
		<ITEM NAME="#Beg_date"  INPUT="TEXT"   LINE="11" COL="18" TYPE="%8d"      FUNCTIONS="D()"/>
		<ITEM NAME="#End_date"  INPUT="TEXT"   LINE="13" COL="18" TYPE="%8d"      FUNCTIONS="D()">
			<ONLOSTFOCUS>
			"{
				if(#Beg_date &gt; #End_date)
				{
					showmsg('起始日期大于终止日期，请重新输入!');
					return(false);
				}
				if(#Beg_date &gt; $HDATE)
				{
					showmsg('起始日期大于当前日期，无记录!');
					return(false);
				}
			}"
			</ONLOSTFOCUS>
		</ITEM>
		--->
	</BLOCK>
	<ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
		<ONCLICK>
		"{
			dim @tota as string;
			initfd();
			commsend_001();
			$FD30=#CARDNO;
			$FD25=#NAME;
			$FD48=#SUB_SEQN;
			//$FD44=#Beg_date;
			//$FD45=#End_date;
			@tota=callserver();
			if($FD12 != '0000')
			{
				showmsg(geterror());
				return(false);
			}
			if(autolist(@tota))
			{
				list_work();
			}
		}"
		</ONCLICK>
	</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE>
		NOTHING
	</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
