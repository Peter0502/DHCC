  <ITEM LINE="3" COL="53" NAME="#M_ACNO" TYPE="%-24s" VISABLE="FALSE" DEVICE="KEYBOARD" INPUT="TEXT" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
	#M_ACNO=delspace(#M_ACNO);
	if(substr(#M_ACNO,len(#M_ACNO)-2,2)!='-1')
	{
		showmsg('非有效主账号');
		return(false);
	}
	initfd();
	commsend_001();
	$FD16='9679';
	$FD30=#M_ACNO;
	callserver();
	if($FD12!='0000')
	{
        	showmsg(geterror());
                return(false);
	}
	#MACNO_NAME=$FD27;
	show(#M_ACNONAME,true);
	show(#MACNO_NAME,true);
	enable(#MACNO_NAME,false);
	showmsg('请注意核对主帐户名');
   }"
