  <ITEM LINE="7" COL="15" NAME="#ZJHM" TYPE="%-20s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)I()">
   <ONLOSTFOCUS>
   "{
	dim @s as string;
	dim @tota as string;
	dim @list1 as string;
	dim @list2 as string;
	dim @i as number;
	dim @khlx as string;
	dim @tmp      as string;
	dim @tmp_head as string;

	commsend_001();
	$FD16=#CP_SERV;
  $FD116=space(35)+#ZJLX+#ZJHM;
	@tota=callserver();

	if($FD12!='0000'){
			showmsg(geterror());
			return(false);
//		showmsg('第一次开户请先做客户资料登记!');
//		@khlx='2';
//		if(#ZJLX=='B'){
//			@khlx='3';
//		}
//    @s='#KHLX='+@khlx+';#ZJLX='+#ZJLX+';#ZJHM='+#ZJHM+';';
//    open('2101.XML',@s);
       
	}
	
	@tmp='';
	@tmp_head='';
	while(@i &lt; len(@tota))
	{
		@tmp=substr(@tota,@i,20);
		@tmp_head=substr(@tmp,0,4);
		if(@tmp_head=='1031' or rtrim(@tmp_head)=='')
		{
			break;
		}
		@i=@i+32;
	}	

	show(#S_NAME,true);
	show(#S_JZZL,true);
	show(#CUSTNAME,true);
	show(#JZZL,true);
	
	setsel(#JZZL,substr(@tota,0,@i),'o5t20o1v4o2');
	
	#JZZL='0000';
	show(#JZZL,true);
	
	@list1=substr(@tota, @i, len(@tota)-@i);
	setsel(#PRODNO,@list1,'o5t30o1v3o2');
	
	#PRODNO='121';
	show(#PRODNO,true);

	#CUSTNAME=substr($FD116,56,50);
	//按财务部的要求，户名可以自己修改，modify by wudw 20130306
	enable(#CUSTNAME,true);
	
	if(#ZJLX == 'Z')
                {
                       #CUSTNAME = '';
                       enable(#CUSTNAME,true);
                }
  }"
  </ONLOSTFOCUS>
