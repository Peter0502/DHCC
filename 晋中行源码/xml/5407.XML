<TRANSACTION RIGHTS="00000000"  TITLE="GKDJ 背书人清单录入)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
<COMMSEND>"{

    initfd();
    $FD21=#LISTCNT;
    $FD31=#WTDAY1;
    $FD32=#ORDERNO1;
    $FD67='1';
    $FD68=#SENDYN;
//用文件
//    $FD95=getitems('#NAME1#NAME2#NAME3#NAME4#NAME5#NAME6#NAME7#NAME8#NAME9#NAME10');
    setvfile(true,true);
    $BUFFER=getitems('#NAME1#NAME2#NAME3#NAME4#NAME5#NAME6#NAME7#NAME8#NAME9#NAME10');
    addvfile($BUFFER);

    commsend_001();

}"
</COMMSEND>
<COMMRECV>"{
    if($FD12!='0000'){
        showmsg(geterror());
        return(false);
    }
    else
    {
        showmsg('背书人录入成功!');
    }
}"
</COMMRECV>

