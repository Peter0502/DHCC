<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111" TITLE="T(大额汇兑明细)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVQ1                          大额汇兑明细)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="05"  COL="03" TYPE="%-12s"  FUNCTIONS="T(交易日期  :)"/>
  <ITEM LINE="05"  COL="35" TYPE="%-12s"  FUNCTIONS="T(交易序号:)"/>
  <ITEM LINE="06"  COL="3" TYPE="%-12s"  FUNCTIONS="T(交易状态  :)"/>
  <ITEM LINE="07"  COL="03" TYPE="%-16s"  FUNCTIONS="T(发起行行号:)"/>
  <ITEM LINE="08"  COL="03" TYPE="%-16s"  FUNCTIONS="T(接收行行号:)"/>
  <ITEM LINE="09" COL="03" TYPE="%-12s" FUNCTIONS="T(汇款金额  :)"/>
  <ITEM LINE="10" COL="03" TYPE="%-12s" FUNCTIONS="T(付款人帐号:)"/>
  <ITEM LINE="11" COL="03" TYPE="%-12s" FUNCTIONS="T(付款人名称:)"/>
  <ITEM LINE="12" COL="03" TYPE="%-12s" FUNCTIONS="T(收款人帐号:)"/>
  <ITEM LINE="13" COL="03" TYPE="%-12s" FUNCTIONS="T(收款人名称:)"/>
  <ITEM LINE="14" COL="03" TYPE="%-12s" FUNCTIONS="T(付款人地址:)"/> 
  <ITEM LINE="15" COL="03" TYPE="%-12s" FUNCTIONS="T(附      言:)"/>
  
  <ITEM LINE="5"   COL="15" NAME="#TX_DATE" TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="5"   COL="47" NAME="#ORDERNO"   TYPE="%-8s"   FUNCTIONS="V(NB)T($BUFFER,393,8)"/>
  <ITEM NAME="#TMP_TXSTAT"  TYPE="%-1s" FUNCTIONS="T($BUFFER,801,1)">
  <ONLOSTFOCUS>
  "{
	HV_SET_STAT();	
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="6"   COL="15" NAME="#TXSTAT"    TYPE="%-20s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="7"   COL="15" NAME="#PAYBRNO"   TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,19,12)"/>
  <ITEM LINE="8"   COL="15" NAME="#CASHBRNO"  TYPE="%-12s"  FUNCTIONS="V(NB)T($BUFFER,211,12)"/>
 
  <ITEM NAME="#TMP_TXAMT" TYPE="%16s" FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
		#HTXAMT = atoi(#TMP_TXAMT);			
   }"
   </ONLOSTFOCUS>
   </ITEM> 
  <ITEM NAME="#HTXAMT" TYPE="%16.2f"   FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
		#HTXAMT = #HTXAMT/100;
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="9"  COL="15" NAME="#LHTXAMT" TYPE="%-20s"   FUNCTIONS="X(#HTXAMT)"/>

   <ITEM LINE="10"  COL="15" NAME="#PAYACTNO"   TYPE="%-32s"  FUNCTIONS="T($BUFFER,47,32)"/>
  
   
  <ITEM LINE="11"  COL="15" NAME="#PAYNAME"   TYPE="%-60s"  FUNCTIONS="V(NB)T($BUFFER,79,60)"/>

  <ITEM LINE="12"  COL="15" NAME="#CASHACTNO"  TYPE="%-32s" FUNCTIONS="T($BUFFER,239,32)"/>

  <ITEM LINE="13"  COL="15" NAME="#CASHNAME"   TYPE="%-60s" FUNCTIONS="V(NB)T($BUFFER,271,60)"/>
  <ITEM LINE="14"  COL="15" NAME="#PAYADDR"    TYPE="%-60s" FUNCTIONS="T($BUFFER,331,60)"/>
<!--
  <ITEM LINE="16"  COL="15" NAME="#PACKID"    TYPE="%-8s" FUNCTIONS="T($BUFFER,331,60)"/>
  <ITEM LINE="17"  COL="15" NAME="#PACKDATE"    TYPE="%-8s" FUNCTIONS="T($BUFFER,331,60)"/>
-->

  <ITEM LINE="15"  COL="15" NAME="#RESUME1"    TYPE="%-60s" FUNCTIONS="T($BUFFER,401,60)"/>
  
  <ITEM LINE="21"  COL="35" NAME="#CLOSE"  INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
	return (true);
  }"
  </ONCLICK>
  </ITEM>
  
  <ITEM LINE="21"  COL="45" NAME="#QUERY"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(查 询)">
  <ONCLICK>"{
  		open('5121.XML','#OORDERNO='+#ORDERNO+';#O_WT_DAY='+#TX_DATE+';#O_CMTNO=100;#OR_BR_NO='+#PAYBRNO+';');
  }"
  </ONCLICK>
  </ITEM>
  <ITEM LINE="21"  COL="55" NAME="#HVPUT"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请撤销)">
  <ONCLICK>"{
  	if(#TMP_TXSTAT=='J' or #TMP_TXSTAT=='1'){
  		open('5125.XML','#OORDERNO='+#ORDERNO+';#O_WT_DATE='+#TX_DATE+';#O_CMTNO=100;');
  	}else{
  		showmsg('此种状态交易无法申请撤销');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
   <ITEM LINE="21"  COL="67" NAME="#HVUNTREAD"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(申请退回)">
  <ONCLICK>"{
  	if(#TMP_TXSTAT=='2'){
  		open('5119.XML','#OORDERNO='+#ORDERNO+';#O_WT_DAY='+#TX_DATE+';#O_CMTNO=100;#OR_BR_NO='+#PAYBRNO+';');
  	}else{
  		showmsg('此种状态交易无法申请退回');
  	}
  					
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
</TRANSACTION>
