  "{
        dim @ch as string;
        if(#TXTYPE=='0'){
            @ch=showmsg('选择要转入的交易，1-定期贷记导盘录入;9-退出 ：');
            if(@ch == '9')
                close();
            else if(@ch == '1'){
                if(#TX_MX_NO!='00000000'){
                    showmsg('请使用明细序号是 00000000 的明细进行联动交易');
                    // return(true);
                    close();
                }else{
                    open('5309.XML', '#XMNO='+#XY_NO+';#PAYACTNO19='+#PAY_AC_NO+';#CNAME='+#FILE_NAME+';#TX_AMT='+#TX_AMT+';');
                }
            }
        }
        else if(#TXTYPE=='1'){
             @ch=showmsg('选择要转入的交易，1-定期借记导盘录入;9-退出 ：');
            if(@ch == '9')
                close();
            else if(@ch == '1'){
