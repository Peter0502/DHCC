	<ITEM NAME="#PO_DATE"         		TYPE="%-8s"     FUNCTIONS="T()"/>    <!--票据日期     ---->
	<ITEM NAME="#PO_NUM"          		TYPE="%-8s"     FUNCTIONS="T()"/>    <!--票据号码     ---->
	<ITEM NAME="#PC_TX_AMT"       		TYPE="%-16.2f"  FUNCTIONS="T()"/>   <!--赔偿金额---->
	<ITEM NAME="#JF_TX_AMT"       		TYPE="%-16.2f"  FUNCTIONS="T()"/>   <!--拒付金额---->
