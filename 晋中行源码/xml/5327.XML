<TRANSACTION RIGHTS="11110000" TITLE="5327 通用信息业务录入"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
    $FD44=$HDATE;
    $FD3 =$KINBR;
    $FD7=$TLRNO;
    lvsend_123();
}"
</COMMSEND>
<COMMRECV>"{
	lvrecv_123();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(5327                    通用信息业务录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5867)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="7"  COL="05" TYPE="%-10s" FUNCTIONS="T(业务类型:!)"/>
  <ITEM LINE="9"  COL="05" TYPE="%-10s" FUNCTIONS="T(接收行号:)"/>
  <ITEM LINE="11" COL="05" TYPE="%-10s" FUNCTIONS="T(附    言:)"/>

  <ITEM   NAME="#NOTPAY_DATE"  TYPE="%8d"   FUNCTIONS="T($HDATE)"/>    <!----查询日期--->
  <ITEM   NAME="#CMTNO"        TYPE="%-3s"  FUNCTIONS="T(012)"/>       <!----CMTNO--->
  <ITEM   NAME="#OPCD"         TYPE="%-2s"  FUNCTIONS="T(10)"/>        <!----交易处理码--->
  <ITEM   NAME="#LW_IND"       TYPE="%-1s"  FUNCTIONS="T(1)"/>         <!---来往标识 1往,2来--->
  <ITEM   NAME="#OTYPE"        TYPE="%-1s"  FUNCTIONS="T(2)"/>         <!---大小额标志 1大额，2小额--->


  <ITEM LINE="7"   COL="15" NAME="#TXNUM"   TYPE="%-5s" DEVICE="KEYBOARD" FUNCTIONS="T(00500)">
  <SELECT LINE="8" COL="17" >
           <OPTION VALUE="00500 通用信息业务"      TITLE="0.通用信息业务"/>
   </SELECT>
   </ITEM>

  <ITEM LINE="9"   COL="15" NAME="#AC_BR_NO"    TYPE="%-12s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
        $FD32=#AC_BR_NO;
        commsend_001();
	$FD16 = '9553';
        callserver();
        if($FD12!='0000'){
            showmsg(geterror());
            return(false);
        }
        showhelp(3,70,8,3,'支付系统行名信息',substr($FD96,0,60));

   }"
  </ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="11"   COL="15" NAME="#BRF"  TYPE="%-60s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
    if (strfldcnt(#BRF,':') != 1 or strfldcnt(#BRF,'{') != 1 or strfldcnt(#BRF,'}') != 1)
    {
        showmsg('输入内容中不能含有半角:{}字符,请修改为全角其它字符');
        return(false);
    }
  }"
  </ONLOSTFOCUS>
  </ITEM>




	<ITEM NAME="#CRDB"			TYPE="%1d"	FUNCTIONS="T(1)"/>
