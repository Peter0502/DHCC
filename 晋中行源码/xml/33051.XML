<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="3305 预约转存登记"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD23 = #PNUM;
	$FD39 = itoa(#SAVEMONEY*100,'%016.0f');
	$FD62 = #CERTIFICATE;
	$FD67 = #CHOOSEPASSWORD;
	$FD68 = #CHOOSECERTIFICATE;
	$FD69 = #CERTIFYTYPE;
	$FD30 = #ACCOUNTNO;
	$FD34 = #ACCTSEQ;

	
}"
</COMMSEND>
<COMMRECV>"{
		commrecv_001();
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(                       预约转存登记-支取信息录入)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  
  <!--输入变量-->
  
  
  
  <ITEM LINE="12" COL="12" TYPE="%-9s" FUNCTIONS="T(密码支取:)"/>
  <ITEM LINE="12" COL="30" TYPE="%-9s" FUNCTIONS="T(密码:)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(---------------------------------------------------)"/>
  <ITEM LINE="14" COL="12" TYPE="%-9s" FUNCTIONS="T(证件支取:)"/>
  <ITEM LINE="14" COL="30" TYPE="%-9s" FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="15" COL="12" TYPE="%-9s" FUNCTIONS="T(证件号码:)"/>
  
  
  <ITEM NAME="#CHOOSEPASSWORD"   INPUT="SELECT"  LINE="12" COL="21" TYPE="%-1s" FUNCTIONS="G(N,#CHOOSECERTIFICATE)V(NB)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
  <ITEM NAME="#PASSWORD"   INPUT="TEXT"  LINE="12" COL="39" TYPE="%-6s" DEVICE="PINPAD" FUNCTIONS="i()I()V(NB)"/>
  <ITEM NAME="#CHOOSECERTIFICATE"   INPUT="SELECT"  LINE="14" COL="21" TYPE="%1s" FUNCTIONS="G(N,#CONFIRM)V(NB)">
      <SELECT>
		<OPTION VALUE="N" TITLE="0.否"/>
		<OPTION VALUE="Y" TITLE="1.是"/>
     </SELECT>
  </ITEM>
   <ITEM NAME="#CERTIFYTYPE"   INPUT="SELECT"  LINE="14" COL="39" TYPE="%-1s" FUNCTIONS="">
    <SELECT>
    <OPTION VALUE="1" TITLE="1 身份证"/>
    <OPTION VALUE="2" TITLE="2 户口簿"/>
    <OPTION VALUE="3" TITLE="3 护照"/>
    <OPTION VALUE="4" TITLE="4 军官证"/>
    <OPTION VALUE="5" TITLE="5 回乡证"/>
    <OPTION VALUE="6" TITLE="6 居住证"/>
    <OPTION VALUE="7" TITLE="7 驾照"/>
    <OPTION VALUE="8" TITLE="8 企业代码证"/>
    <OPTION VALUE="9" TITLE="9 经营许可证"/>
    <OPTION VALUE="A" TITLE="A 营业执照"/>
    <OPTION VALUE="B" TITLE="B 事业法人证"/>
    <OPTION VALUE="C" TITLE="C 工商核准号"/>
    </SELECT>
 </ITEM>


  <ITEM NAME="#CERTIFICATE"   INPUT="TEXT"  LINE="15" COL="21" TYPE="%-20s" FUNCTIONS="V(NB)"/>
  <ITEM NAME="#CONFIRM"  LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
<ONCLICK>"{
	dim @a as number;
	dim @b as number;
	dim @c as number;
	dim @d as number;
	@a = len(#CERTIFICATE);
	@b = 20 - @a;
	@a = len(#PASSWORD);
	@c = 6 - @a;
	@a = len(#CERTIFYTYPE);
	@d = 1 - @a ;
	$BUFFER = 'N'+#CHOOSEPASSWORD+space(6)+#PASSWORD+space(@c)+#CHOOSECERTIFICATE+#CERTIFICATE+space(@b)+'N'+#CERTIFYTYPE+space(@d)+'N'+'N';
	close('#CONFIRM','');	
}"
</ONCLICK>
</ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
