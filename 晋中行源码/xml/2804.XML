<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="2804 柜员揽存信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	commsend_001();
	$FD25=#NAME;
	$FD37=#ACTNO;
	$FD86=#GYNAME;
	$FD92=#GYDM;
	}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(2804                        柜员揽存信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1804)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--输入变量-->
  <BLOCK NAME="COMMON" LINE="7" COL="5">
  <ITEM LINE="1" COL="15" TYPE="%-10s" FUNCTIONS="T(揽存账号:)"/>
  <ITEM LINE="3" COL="15" TYPE="%-10s" FUNCTIONS="T(客户名称:)"/>
  <ITEM LINE="5" COL="15" TYPE="%-10s" FUNCTIONS="T(柜员代码:)"/>
  <ITEM LINE="7" COL="15" TYPE="%-10s" FUNCTIONS="T(柜员名称:)"/>

  <ITEM NAME="#ACTNO" 	INPUT="TEXT" 	LINE="1" COL="25" TYPE="%-19s" 	FUNCTIONS="T()">
    <ONLOSTFOCUS>
	"{
		dim @txno_bak as string;
		@txno_bak=$TXNO;
		$TXNO='9759';
		commsend_001();
		$FD37=#ACTNO;
		$TXNO=@txno_bak;
		callserver();
		#NAME=$FD25;	
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
			}
	}"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#NAME" 	INPUT="TEXT" 	LINE="3" COL="25" TYPE="%-30s" 	FUNCTIONS="T()"/>
  <ITEM NAME="#GYDM" 	INPUT="TEXT" 	LINE="5" COL="25" TYPE="%-4s" 	FUNCTIONS="T()">
  <ONLOSTFOCUS>
	"{
		dim @txno_bak as string;
		@txno_bak=$TXNO;
		$TXNO='9718';
		commsend_001();
		$FD92=#GYDM;
		callserver();
		$TXNO=@txno_bak;
		#GYNAME=$FD86;	
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
			}
	}"
    </ONLOSTFOCUS>
  </ITEM>
  <ITEM NAME="#GYNAME" 	INPUT="TEXT" 	LINE="7" COL="25" TYPE="%-10s" 	FUNCTIONS="T()"/>
  </BLOCK>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>

</OUTPUT>
</TRANSACTION>




