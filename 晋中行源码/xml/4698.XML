<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="4698 晋中理财协议书打印"
	 xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(4700                      	晋中理财协议书打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <BLOCK NAME="COMMON" LINE="6" COL="3">
  <ITEM NAME="#STAT" TYPE="%-1s" FUNCTIONS=""/>
  <ITEM LINE="0"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(客户信息)"/>
  <ITEM LINE="1"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(甲方:)"/>
  <ITEM LINE="2"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="2"  COL="35" TYPE="%-10s"  FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="3"  COL="5"  TYPE="%-12s"  FUNCTIONS="T(证件号码:)"/>
  <ITEM LINE="3"  COL="35" TYPE="%-12s"  FUNCTIONS="T(邮政编码:)"/>
  <ITEM LINE="4"  COL="5"  TYPE="%-12s"  FUNCTIONS="T(联系地址:)"/>
  <ITEM LINE="4"  COL="35" TYPE="%-12s"  FUNCTIONS="T(固定电话:)"/>
  <ITEM LINE="5"  COL="5" TYPE="%-10s"  FUNCTIONS="T(移动电话)"/> 
  <ITEM LINE="5"  COL="35"  TYPE="%-10s"  FUNCTIONS="T(电子邮箱:)"/>
  <ITEM NAME="#JF"   INPUT="TEXT"   LINE="1"   COL="15" TYPE="%-10s"  DEVICE="KEYBOARD" FUNCTIONS=""/>
  <ITEM NAME="#CPMC"  INPUT="TEXT"   LINE="2"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#ZJLX"  INPUT="TEXT"   LINE="2"   COL="45" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#ZJHM"  INPUT="TEXT"   LINE="3"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#YZBM1"  INPUT="TEXT"   LINE="3"   COL="45" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#LXDZ"  INPUT="TEXT"   LINE="4"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#GDDH"  INPUT="TEXT"   LINE="4"   COL="45" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#YDDH"  INPUT="TEXT"   LINE="5"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#DZYX"  INPUT="TEXT"   LINE="5"   COL="45" TYPE="%-20s" FUNCTIONS=""/>
   <ITEM LINE="6"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(银行信息)"/>
  <ITEM LINE="7"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(乙方:)"/>
