  <ITEM LINE="8"  COL="5" TYPE="%-12s"  FUNCTIONS="T(地址:)"/>
  <ITEM NAME="#DZ"  INPUT="TEXT"   LINE="8"   COL="15" TYPE="%-40s" FUNCTIONS=""/>
  <ITEM LINE="9"  COL="5"  TYPE="%-10s"  FUNCTIONS="T(邮政编码:)"/>
  <ITEM NAME="#YZBM2"  INPUT="TEXT"   LINE="9"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM LINE="9"  COL="35" TYPE="%-10s"  FUNCTIONS="T(理财经理:)"/>
  <ITEM NAME="#LCJL"  INPUT="TEXT"   LINE="9"   COL="45" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM LINE="10" COL="5"  TYPE="%-12s"  FUNCTIONS="T(咨询电话:)"/>
  <ITEM NAME="#ZXDH"  INPUT="TEXT"   LINE="10"   COL="15" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM LINE="11" COL="5"  TYPE="%-12s"  FUNCTIONS="T(交易金额:)"/>
  <ITEM NAME="#AMT"  INPUT="TEXT"   LINE="11"   COL="15" TYPE="%17.2f" FUNCTIONS=""/>
  <ITEM LINE="12" COL="5"  TYPE="%-12s"  FUNCTIONS="T(产品收益率:)"/>
  <ITEM NAME="#LV"  INPUT="TEXT"   LINE="12"   COL="15" TYPE="%-8s" FUNCTIONS=""/>
  <ITEM NAME="#TXAMT" TYPE="%60s" FUNCTIONS="U(#AMT)"/>  
  </BLOCK>
  <ITEM NAME="#LC" TYPE="%-10s" FUNCTIONS=""/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>"{
  		runoutput();		
  			rerun();
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="按任意键继续">
 		NOTHING
	</PACKAGE>
	<PRINT>
	"{
		print(7,15,'产品名称:[60     ]',#CPMC);
	}"
	</PRINT>
</RESPONSE>
<RESPONSE>
  <PACKAGE DEVICE="PRINTER" LINESPACE="24" DESC="请打印理财产品凭证">
  NOTHING
  </PACKAGE>
  <PRINT>
  "{

	print( 11,34,'[20][15][20]',#JF,#LC,#CPMC);
	print( 12,34,'[20][15][20]',#ZJLX,#LC,#ZJHM);
	print( 14,34,'[20][15][20]',#YZBM1,#LC,#YDDH);
	print( 15,34,'[20][15][20][10][20]',#GDDH,#LC,#YDDH,#LC,#DZYX);
	print( 20,34,'[22][17][40]',#YF,#LC,#DZ);
	print( 22,34,'[22][17][17][10][20]',#YZBM2,#LC,#LCJL,#LC,#ZXDH);
	//print( 25,35,'[3][2][3][2][3]',substr(#YZBM2,0,1),#LC,substr(#YZBM2,1,1),#LC,substr(#YZBM2,2,1));
	print( 30,38,'[22][17][60]',delspace(comma(itoa(#AMT,'%17.2f'),22)),#LC,delspace(#TXAMT));
	print( 32,32,'[10]％[17][40]',#LV,#LC,#LCJL);//产品收益率
	
  }"
  </PRINT>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
