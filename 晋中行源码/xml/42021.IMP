<ITEM NAME="#AAA" TYPE="%-19s" FUNCTIONS="">
   <ONLOSTFOCUS>
   "{
	dim @s as string;
	dim @tota as string;
	dim @list1 as string;
	dim @list2 as string;
	dim @i as number;

	$FD102=#ACTNO;
	// showmsg('$FD102==='+$FD102);
	commsend_001();
	@tota=callserver();
	if($FD12!='0000'){
		//showmsg('帐号输入错误!');
		showmsg(geterror());
		return(false);
	}
	#ZFDBHWB='';
	#ZQJEWB=0;
	setitems('#ACTNO24#ACTSEQ#VOCTYPE#VOCNUM_IMP#FETCHFLAG#PWDFLAG#QUE_PWD#DRAW_PWD#CERTFLAG#CERTNO#STAMPFLAG#PWDFLAG2#PWD16#PRINTDATE#TXAMT#CERTTYPE#CUSTNAME#AVBAL#CURCD#CASHFLAG#DSCPT#ACTTYPE#ACTSEQ_2#ACCOUNTFLAG#INTEREST1#INTEREST2#INTEREST3#PAYAMOUNT#NEWVOCNUM#SPEFLAG#PRODNAME#ACTNO_24#EDUCREDIT#DESC10',$FD102);
   if($FD68 == '6')
   {
   		showmsg('此账户为协定户，请谨慎操作！');
   }
 
	 @list2=substr(@tota, 0, 30);
	 setsel(#VOCTYPE,@tota,'o5t20o1v3o2');
	 #VOCTYPE='001';
	 #CASHFLAG='1';
	 #PRINTDATE=$HDATE;
	
	 showblock('ACTINFO',true,true);
	 if(#PWDFLAG2=='Y')
	 {
	   show(#S_PWD,true);
	   show(#DRAW_PWD,true);
	 }else{
	   show(#S_PWD,false);
	   show(#DRAW_PWD,false);
	 }
	 enable(#ACCOUNTFLAG,false);
	
	// add by chenchao 2011-7-31 16:08:05 增加支取检查 begin 
	initfd();
	commsend_001();
	$FD16='5580';
	$FD30=#ACTNO;
	$FD69='1';
	@tota=callserver();
	if($FD12!='0000')
	{
		showmsg(geterror());
		return(false);
	}
	savefiletxt('../tmp/'+$KINBR+$TLRNO+$HDATE+$TXNO+'.txt',@tota);
	if(@tota!='')
	{
		opendlg('../xml/DRAW_LOAD.DLG'); 
	}
	if(#ZFDBHWB!='')
	{
		#CASHFLAG = '2';
		enable(#CASHFLAG,false);
	}else{
		enable(#CASHFLAG,true);
	}
	// add by chenchao 2011-7-31 16:08:05 增加支取检查 end 
	//若为贷款还款户,且有未还的本金或利息,提示柜员 20070621
	if($FD71 == '1')
	{
		showhelp(5,60,7,10,'提示', '该户为贷款还款户,且有未还的本金或利息!');
		return(true);
	}

	//add by lwb 20141216增加短信欠费提示
	initfd();
		commsend_001();
		$FD16='9597';
		$FD30=delspace(#ACTNO);
		callserver();
		if($FD12 !='0000')
		{
		showmsg(geterror());
		return(false);
		}
		if(val($FD40,0.01) &gt; 0.00)
		{
		showmsg('该账户签约短信服务已欠费，连续欠费3个月将暂停短信服务，请及时缴清欠费!');
		
		}
	
	//add end

  }"
  </ONLOSTFOCUS>
  </ITEM>
