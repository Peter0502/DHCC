  <ITEM   NAME="#CCC"  TYPE="%-1s"  FUNCTIONS="T()">
   <ONLOSTFOCUS>
   "{
		dim @tota as string;
		if(#VOCTYPE!='001' and #VOCTYPE!='299'){
			if(#CASHFLAG=='1'){
				showmsg('该凭证类型不能选择现金取款!');
				return(false);
			}
		}
		if(#VOCTYPE=='001' and #CASHFLAG!='1'){
			showmsg('现金支票必须选择现金取款!');
			return(false);
		}
		initfd();		
		commsend_001();
		$FD16='9725';
		$FD102=space(203)+#CASHFLAG;
		@tota=callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		setsel(#VOCDSCPT,@tota,'o5t20o1v3o2');
	}"
   </ONLOSTFOCUS>
