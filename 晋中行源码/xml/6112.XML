<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="6112    自动提入票据清单打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
   commsend_001();
   $MSGTYPE='03001';
   dim @baktxno as string;
   @baktxno=$TXNO;
   $TXNO = '9986';
   $FD12 = '4243';
   $FD44=#DATE;
   $FD91=#NETID;
   commsend_001();
   callserver();
   $TXNO=@baktxno;
   if($FD12!='0000')
   {
      showmsg(geterror());
      return(false);
   }
   $FD16 = '4243';
   $MSGTYPE='03001';
}"
</COMMSEND>
<COMMRECV>"{
commrecv_001();
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(6112                           自动提入票据清单打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO"    TYPE="%-4s" FUNCTIONS="T($TXNO=4242)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>  
  
  <ITEM LINE="9"     COL="20"   TYPE="%-10s"  FUNCTIONS="T(网 点 号:)"/>
  <ITEM LINE="10"    COL="20"   TYPE="%-10s"  FUNCTIONS="T(提入日期:)"/>
  <ITEM LINE="9"  COL="30" NAME="#NETID" TYPE="%-5s"    INPUT="TEXT" FUNCTIONS="T($KINBR)"/>
  <ITEM LINE="10" COL="30" NAME="#DATE"  TYPE="%-8s"    INPUT="TEXT" FUNCTIONS="T($HDATE)"/>
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">
  
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
</RESPONSE>

</TRANSACTION>
