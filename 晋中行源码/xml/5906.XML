        print( 5, 16,'[4]      [2]     [2]',#YEAR,#MON,#DAY);
        print( 7,  8,'[5]',$FD2);
        print( 7, 24,'担保关联-[4]',#CZNAME);
        print( 7, 46,'[4]',$FD7);
        print( 7, 62,'[6]',$FD4);
   		print( 8,  6,'担保帐号:[24]',#DBACTNO);
   		print( 9,  6,'户名:[50]',#FD102_17);
   		print(10,  6,'担保日期:[8]',$FD5);
   		print(11,  6,'担保金额:[16]',ltrim(itoa(#DBAVBAL,'%17.2f')));
   		print(12,  6,'担保户余额:[16]',ltrim(comma(#FD102_18,16)));
   		print(13,  6,'流水号:[6]',$FD4);
    }"
    </PRINT>
</RESPONSE>

