{ 
	initfd(); 
	$FD32 = #DBACTNO; 
	$FD33 = #JJNUM; 
	$FD40 = getitems('#DBAVBAL');
	$FD67 = #CZCODE;
	$FD102= #CD102;
	commsend_001();
}"
</COMMSEND>
