  <ITEM LINE="9"  COL="28" TYPE="%-1s" NAME="#CZCODE" DEVICE="KEYBOARD" FUNCTIONS="T(0)">
    <SELECT LINE="0" COL="0">
        <OPTION VALUE="0" TITLE="建立"/>
        <OPTION VALUE="1" TITLE="删除"/>
    </SELECT>
  </ITEM>
  <ITEM NAME="#CZNAME" TYPE="%-10s" FUNCTIONS="S(#CZCODE,#CZCODE)"/>

  <ITEM LINE="11"  COL="28" TYPE="%-24s"  NAME="#DBACTNO"   DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
    	dim @baktxno as string;
    	@baktxno=$TXNO;
    	$TXNO='9731';
    	$FD102=#DBACTNO;
    	commsend_001();
    	callserver();
    	$TXNO=@baktxno;
    	if($FD12!='0000'){
        	showmsg(geterror());
        	return(false);
    	}
		setitems('#FD102_1#FD102_2#FD102_3#FD102_4#FD102_5#FD102_6#FD102_7#FD102_8#FD102_9#FD102_10#FD102_11#FD102_12#FD102_13#FD102_14#FD102_15#FD102_16#FD102_17#FD102_18#FD102_19#FD102_20#FD102_21#FD102_22#FD102_23#FD102_24#FD102_25#FD102_26#FD102_27#FD102_28#FD102_29#FD102_30#FD102_31#FD102_32#FD102_33#FD102_34',$FD102);

		#CD102=$FD102;
  }"
  <ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="13"  COL="28" TYPE="%-20s"  NAME="#JJNUM"     DEVICE="KEYBOARD" FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
  		dim @baktxno as string;
        initfd();
		@baktxno=$TXNO;
		$TXNO='9939';
        $FD63=#JJNUM;
        commsend_001();
        callserver();
		$TXNO=@baktxno;
        if($FD12!='0000'){
                showmsg(geterror());
                return(false);
        }
		if(#CZCODE == '1')
			goitem(#PASSWD,false);
  }"
  <ONLOSTFOCUS>
  </ITEM>

  <ITEM LINE="15"  COL="28" TYPE="%17.2f" NAME="#DBAVBAL"   DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>

  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
  <ITEM NAME="#MON"  TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
  <ITEM NAME="#DAY"  TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
  <ITEM NAME="#CD102"  TYPE="%-378s" FUNCTIONS=""/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
