<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="9418 账户余额查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
  $PBLINE='05';
	$KINDNO='02';
	
}"
</COMMRECV>
<VARIABLE UPLOOP="TRUE">
<ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=    "T(9418                           帐户余额查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=8120)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  

  <ITEM LINE="7" COL="10" TYPE="%-10s" FUNCTIONS="T(帐    号:)"/>
  <ITEM NAME="#ACNO" INPUT="TEXT" LINE="7" COL="20" TYPE="%-24s" FUNCTIONS="">
  
    <ONLOSTFOCUS>"{
    	dim @txno as string;
    	@txno=$TXNO;
    	$TXNO='9990';
    	$FD12=8120;
    	$FD30=#ACNO;	
    	commsend_001();
    	callserver();
    	
    	$TXNO=@txno;
    	
    	if($FD12!='0000'){
    		showmsg(geterror());
    		return(false);
    	}
    	else{
    	
    	setitems('#NAME',$FD25);
    	setitems('#MMZQ',$FD67);
    	setitems('#ZJZQ',$FD68);
      setitems('#ZJLX',$FD69);
      
      showblock('COMMON1',true,true)
      
      show(#NAME,true);    
      show(#MMZQ,true);		   
      show(#ZJZQ,true);
        	  
        if (#MMZQ=='Y')
        {
         	  showblock('COMMON2',true,true);
      
        }
        else
        {
            showblock('COMMON2',false,false);	  
        }
        
    	 if (#ZJZQ=='Y')
        {
             showblock('COMMON3',true,true);
            
        }
        else
        {
        	   showblock('COMMON3',false,false); 
        }
        }
    	 
  }"
  </ONLOSTFOCUS>
  </ITEM> 

  <BLOCK NAME="COMMON1" LINE="10" COL="10" VISABLE="FALSE">
  	
  <ITEM LINE="0" COL="0" TYPE="%-10s" FUNCTIONS="T(户    名:)"/>
  <ITEM LINE="0"  COL="10" NAME="#NAME" TYPE="%-50s" INPUT="TEXT" FUNCTIONS="T()" ENABLE="FALSE"/>
  <ITEM LINE="2"  COL="0"  TYPE="%-10s" FUNCTIONS="T(密码支取:)"/>
  <ITEM LINE="2"  COL="10" NAME="#MMZQ" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()" ENABLE="FALSE" >
	<SELECT>
		<OPTION VALUE="Y" TITLE="Y.是"/>
		<OPTION VALUE="N" TITLE="N.否"/>
  </SELECT>
  </ITEM>
  
  <ITEM LINE="4"  COL="0"  TYPE="%-10s" FUNCTIONS="T(证件支取:)"/>
  <ITEM LINE="4"  COL="10" NAME="#ZJZQ" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()" ENABLE="FALSE" >
	<SELECT>
		<OPTION VALUE="Y" TITLE="Y.是"/>
		<OPTION VALUE="N" TITLE="N.否"/>
  </SELECT>
  </ITEM>
  
  </BLOCK>
  
  <BLOCK NAME="COMMON2" LINE="10" COL="10" VISABLE="FALSE">
  
  <ITEM LINE="2"  COL="30" NAME="#QZQMM" TYPE="%-10s"  FUNCTIONS="T(支取密码:)"/>
  <ITEM LINE="2"  COL="40" NAME="#ZQMM" TYPE="%-6s"  INPUT="TEXT" DEVICE="PINPAD"  FUNCTIONS="i()"/>
  
  </BLOCK>
  
  <BLOCK NAME="COMMON3" LINE="10" COL="10" VISABLE="FALSE">
  
  <ITEM LINE="4"  COL="30" NAME="#QZJLX" TYPE="%-10s"  FUNCTIONS="T(证件类型:)"/>
  <ITEM LINE="4"  COL="40" NAME="#ZJLX" TYPE="%-1s"  INPUT="SELECT"  FUNCTIONS="T()"  >
    <SELECT>
		<OPTION VALUE="1" TITLE="0.身份证"/>
		<OPTION VALUE="2" TITLE="1.户口簿"/>
		<OPTION VALUE="3" TITLE="2.护照"/>
		<OPTION VALUE="4" TITLE="3.军官证"/>
		<OPTION VALUE="5" TITLE="4.回乡证"/>
		<OPTION VALUE="8" TITLE="7.企业代码证"/>
		<OPTION VALUE="9" TITLE="8.组织机构号"/>
		<OPTION VALUE="A" TITLE="9.营业执照"/>
		<OPTION VALUE="B" TITLE="a.经营许可证"/>
		<OPTION VALUE="C" TITLE="b.事业法人证书"/>
	</SELECT>
  </ITEM>
  
  <ITEM LINE="5"  COL="30" NAME="#QZJHM" TYPE="%-10s"  FUNCTIONS="T(证件号码:)"/>
  <ITEM LINE="5"  COL="40" NAME="#ZJHM" TYPE="%-24s"  INPUT="TEXT"  FUNCTIONS="T()"/>
  
  </BLOCK>
  
  
  <BLOCK NAME="COMMON4" LINE="0" COL="0" VISABLE="FALSE">
  <ITEM LINE="19"  COL="10"  TYPE="%-10s"   FUNCTIONS="T(帐户余额:)"/>
  <ITEM LINE="19"  COL="20" NAME="#ZHYE" TYPE="%-21s"  INPUT="LABEL" VARIABLE="FALSE" FUNCTIONS="T()"/>
  <!--ITEM LINE="19"  COL="20" NAME="#YE"   TYPE="%-21s"   INPUT="LABEL" VARIABLE="FALSE" FUNCTIONS="X(#ZHYE)"/-->
   </BLOCK>
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>

  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON">
  <ONCLICK>
  "{
      dim @txno as string;
    	@txno=$TXNO;
    	$TXNO='8120';
    	
    	$FD30=#ACNO;
    	$FD25=#NAME;
    	$FD67=#MMZQ;
    	$FD68=#ZJZQ;
   // 	$FD69=#ZJLX;
    //	$FD80=#ZQMM;
    //	$FD31=#ZJHM;
   	 // showmsg($FD80);
    	commsend_001();
    	callserver();
    	
    	$TXNO=@txno;
    	
    	if($FD12!='0000'){
    		showmsg(geterror());
    		return(false);
    	}
    	else{
  //	 setitems('#ZHYE',$FD42);  
    	 #ZHYE=itoa(val($FD42,0.01),'%.2f');
    	 showblock('COMMON4',true,true);
       show(#ZHYE,true); 
      }
    }"
   </ONCLICK>   
   </ITEM>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
</TRANSACTION>
