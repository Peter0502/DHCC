<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="8819   预留问题答案维护"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE UPLOOP="TRUE">
		<ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
		"T(8802                             账单列表查询)"/>
		<ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
		"T(─────────────────────────────────────)"/>
		<ITEM NAME="#TXNO"    TYPE="%-4s"  FUNCTIONS="T($TXNO=8819)"/>
		<ITEM NAME="#MSGTYPE" TYPE="%-5s"  FUNCTIONS="T($MSGTYPE=03001)"/>
		<ITEM NAME="#TXCD"    TYPE="%4s"   FUNCTIONS="T(8819)"/>      <!---交易代码--->

	
  <ITEM LINE="7"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#CRD_ID_NO" FUNCTIONS="T(卡    号:)"/>
  <ITEM LINE="7"  COL="25" NAME="#CRD_NO"  INPUT="TEXT" TYPE="%-19s"  DEVICE="KEYBOARD" FUNCTIONS="">
		<ONLOSTFOCUS>"{
			dim @l as number;
			@l = len(#CRD_NO);
		
				if (@l == 19)
				{
					commsend_001();
					//callserver();
					if ($FD12 != '0000')
					{
						showmsg(geterror());
					//	return(false);
					}
				}
				else
				{
					showmsg('卡号位数错误！');
					//return(false);
				}
			
		}"</ONLOSTFOCUS>
	</ITEM>
	  <ITEM LINE="9"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#LBL_ID_TYPE" FUNCTIONS="T(功能码:)"/>
<ITEM LINE="9" COL="25" NAME="#OPT" TYPE="%-1s" INPUT="SELECT" DEVICE="KEYBOARD" FUNCTIONS="V(NB)T(1)">
		<SELECT>
			<OPTION VALUE="I" TITLE = "0.查询"/>
			<OPTION VALUE="T" TITLE = "1.设定"/>
		</SELECT>
	</ITEM>
	
	<ITEM LINE="11"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#STMT_START_DATE" FUNCTIONS="T(预留问题:)"/>
		<ITEM LINE="11"  COL="25" NAME="#OBLIGATE_QUESTION"  INPUT="TEXT" TYPE="%-19s"  DEVICE="KEYBOARD" FUNCTIONS=""/>

  <ITEM LINE="13"  COL="10" TYPE="%-13s" INPUT="LABEL" NAME="#STMT_START_DATE" FUNCTIONS="T(预留答案:)"/>
  <ITEM LINE="13"  COL="25" NAME="#OBLIGATE_ANSWER"  INPUT="TEXT" TYPE="%-19s"  DEVICE="KEYBOARD" FUNCTIONS=""/>
	
		
		<!--输出-->
			
		<ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
			<ONCLICK>
					"{
							dim @tagvalue as string;
							initfd();
							commsend_001();
							$FD77='11050';
							$FD77='#OPT';
							$FD77=#OBLIGATE_QUESTION;
							$FD77=#OBLIGATE_ANSWER;
							//callagn_credit();
							if($FD12 != '0000')
							{		
								showmsg(geterror());
							//	return(false);
							}
              runoutput();
              close();
              }"
        </ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
        <PACKAGE DEVICE="SCREEN" DESC="按任意键继续">
        NOTHING
        </PACKAGE>
        <PRINT>"{
        print( 8, 20,'卡    号:[19]',$FD77);
        print( 9,20,'预留问题:[40]',$FD77);
        print( 10, 20,'预留答案:[40] ',$FD77);
        }"
        </PRINT>
</RESPONSE>

</TRANSACTION>

