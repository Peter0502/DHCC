  <COMMSEND>"{

        initfd();
        $FD25=#JGMC;
        $FD39=getitems('#HKJE');
        $FD47=#HTH;
        $FD67='1';
        $FD91=#CJJG;
        commsend_001();
        $FD16='5402';
        $MSGTYPE='03001';

	}"
