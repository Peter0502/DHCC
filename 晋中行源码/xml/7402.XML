<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(7402                         系统内拆出还款  )"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5402)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <!--输入变量-->
  <ITEM LINE="7"   COL="3" TYPE="%-18s" FUNCTIONS="T(合同号:)"/>
  <ITEM LINE="9"   COL="3" TYPE="%-18s" FUNCTIONS="T(拆借机构:)"/>
  <ITEM LINE="11"  COL="3" TYPE="%-14s" FUNCTIONS="T(拆借行名: )"/>
  <ITEM LINE="13"  COL="3" TYPE="%-14s" FUNCTIONS="T(还款金额: )"/>
 
  <ITEM NAME="#HTH"	LINE="7" COL="17" TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS="T()V(NB)">
   <ONLOSTFOCUS>
  "{
		dim @txno as string;
		@txno=$TXNO;
		$TXNO='9862';
		$FD47=#HTH;
		$FD67='1';
		commsend_001();
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#HKJE=val($FD39,0.01);
		#CJJG=$FD91;

	//第2次callserver
		initfd();
		$MSGTYPE='03001';
		$TXNO='9802';
		$FD91=#CJJG;
		commsend_001();
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		#JGMC=$FD25;
		show(#CJJG,true);
		show(#JGMC,true);
		show(#HKJE,true);

  }"
  </ONLOSTFOCUS>

  </ITEM>

  <ITEM NAME="#CJJG" LINE="9"  COL="17" TYPE="%-5s"   INPUT="LABEL"     FUNCTIONS=""/>
  <ITEM NAME="#JGMC" LINE="11" COL="17" TYPE="%-44s"  INPUT="LABEL"     FUNCTIONS=""/>
  <ITEM NAME="#HKJE" LINE="13" COL="17" TYPE="%17.2f" DEVICE="KEYBOARD" FUNCTIONS="V(0000000000000001,9999999999999999)"/>
  <ITEM NAME="#XHKJE" TYPE="%-21s" FUNCTIONS="X(#HKJE)"/>

  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  
</VARIABLE>

<REQUEST>
</REQUEST>

<RESPONSE>
