<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111100" TITLE="5353 支付业务收费清单明细查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	commsend_001();
	$FD24=#ORDERNO;
	$FD44=#PACK_DATE;
	$FD45=#WT_DATE;
	$FD88=#BR_NO;
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	commrecv_124();
	dim @tota as string;
	@tota=commrecv_124();
	#RECECO=substr(@tota,0,4);
	#QSBRNO=substr(@tota,4,12);
	#SDATE=substr(@tota,16,8);
  #EDATE=substr(@tota,24,8);
	#TXAMT=substr(@tota,32,15);
	#IDRATE=substr(@tota,47,6);        
	#SERATE=substr(@tota,53,6);       
  #MORATE=substr(@tota,59,6);        
  #ZFTXAMT=substr(@tota,65,15);     
  #ZFNUM=substr(@tota,80,8);         
  #DBTXAMT=substr(@tota,88,15);     
  #DBNUM=substr(@tota,103,8);
  #MESTXAMT=substr(@tota,111,15);     
  #MESNUM=substr(@tota,126,8);
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="4"  COL="3" TYPE="%-50s" FUNCTIONS="T(5353                  支付业务收费清单明细查询)"/>
  <ITEM LINE="5"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5893)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <BLOCK name="COMM" LINE="0" COL="7">
  <ITEM LINE="8"   COL="10" TYPE="%-12s"  FUNCTIONS="T(交易机构号:)"/>
  <ITEM LINE="10"  COL="10" TYPE="%-10s"  FUNCTIONS="T(交易日期: )"/>
  <ITEM LINE="12"  COL="10" TYPE="%-10s"  FUNCTIONS="T(委托日期: )"/>
  <ITEM LINE="14"  COL="10" TYPE="%-10s"  FUNCTIONS="T(交易序号: )"/>
    <!--输入变量-->
  <ITEM LINE="8"   COL="22" NAME="#BR_NO"      TYPE="%-5s"   INPUT="TEXT"   FUNCTIONS="T(50101)"/>
  <ITEM LINE="10"  COL="20" NAME="#PACK_DATE"  TYPE="%-8s"   INPUT="TEXT"   FUNCTIONS="D()"/>
  <ITEM LINE="12"  COL="20" NAME="#WT_DATE"    TYPE="%-8s"   INPUT="TEXT"   FUNCTIONS="D()"/>
  <ITEM LINE="14"  COL="20" NAME="#ORDERNO"    TYPE="%-3s"   INPUT="TEXT"   FUNCTIONS="T(101)"/>
  
  </BLOCK>

       <!----------接收后台传来的部分信息------------>
    <ITEM NAME="#RECECO"    TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#QSBRNO"    TYPE="%-12s" FUNCTIONS="T()"/> 
    <ITEM NAME="#SDATE"     TYPE="%-8s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#EDATE"     TYPE="%-8s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#TXAMT"     TYPE="%-15s" FUNCTIONS="T()"/> 
    <ITEM NAME="#IDRATE"    TYPE="%-6s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#SERATE"    TYPE="%-6s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#MORATE"    TYPE="%-6s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#ZFTXAMT"   TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#ZFNUM"     TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#DBTXAMT"   TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#DBNUM"     TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#MESTXAMT"  TYPE="%-4s"  FUNCTIONS="T()"/> 
    <ITEM NAME="#MESNUM"    TYPE="%-4s"  FUNCTIONS="T()"/> 
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/> 
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>   
  </VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
