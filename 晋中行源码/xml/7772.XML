<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111" TITLE="7772 测试文件" 
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(7772                            写柜员卡)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=7772)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <ITEM LINE="12" COL="15" TYPE="%-10s" FUNCTIONS="T(柜员号:)"/>
  <ITEM LINE="12" COL="25" TYPE="%4d" INPUT="TEXT" NAME="#WTLRNO" FUNCTIONS=""/>
  
  <ITEM NAME="#OK" LINE="21" COL="59"  TYPE="%-8s" FUNCTIONS="T(确定)" INPUT="BUTTON">
  <ONCLICK>"{
	if(msgbox('写入柜员号['+#WTLRNO+']吗?'+chr(10)+'----------','提示','yes|no')==0){
		$MSR2=#WTLRNO;
		$MSR3='';
		showmsg('按任意键开始刷卡...');
		if(writemsr($MSR2,$MSR3)){
			showmsg('写磁成功!');
			#WTLRNO=itoa(val(#WTLRNO)+1,'%04.0f');
		}else{
			showmsg('写磁失败!');
		}
	}
  }"
  </ONCLICK>
  </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
</RESPONSE>
</TRANSACTION>
