<?xml version="1.0" encoding="GBK" ?>



<TRANSACTION RIGHTS="00110000" TITLE="L26 提出提入票据查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(S26                     提出提入票据查询)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="6"  COL="25" TYPE="%-18s" FUNCTIONS="T(操  作   类  型:!)"/>
  <ITEM LINE="8"  COL="25" TYPE="%-18s" FUNCTIONS="T(查  询   日  期:)"/>
  <ITEM LINE="10" COL="25" TYPE="%-18s" FUNCTIONS="T(提出  提入 标志:!)"/>
  <ITEM LINE="12" COL="25" TYPE="%-18s" FUNCTIONS="T(提取  纪录 状态:!)"/>

  <ITEM LINE="8"   COL="42" NAME="#TXFLAG"   TYPE="%-1s" DEVICE="KEYBOARD" FUNCTIONS="V(1|2)">
   <SELECT LINE="5" COL="59" >
           <OPTION VALUE="1 显示" TITLE="1.显示"/>
           <OPTION VALUE="2 打印" TITLE="2.打印"/>
   </SELECT>
   </ITEM>
	
  <ITEM LINE="8"   COL="42" NAME="#VACTNO"   TYPE="%-8s" DEVICE="KEYBOARD" FUNCTIONS="T($HDATE)"/>
  <ITEM LINE="10"  COL="42" NAME="#PUTFLAG"  TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="V(1|2|3)">
   <SELECT LINE="10" COL="59" >
           <OPTION VALUE="1" TITLE="1.提出"/>
           <OPTION VALUE="2" TITLE="2.提入"/>
           <OPTION VALUE="3" TITLE="3.全部"/>
   </SELECT>
   </ITEM>
  <ITEM NAME="#UPUTFLAG"  TYPE="%12s"  FUNCTIONS="j(#PUTFLAG=1,T{提入合计})j(#PUTFLAG=2,T{提出合计})j(#PUTFLAG=3,T{提出提入差额})"/>

  <ITEM LINE="12"  COL="42" NAME="#STAT"  TYPE="%1d" DEVICE="KEYBOARD" FUNCTIONS="V(1,2)">
   <SELECT LINE="12" COL="59" >
           <OPTION VALUE="1" TITLE="1.登记"/>
           <OPTION VALUE="2" TITLE="2.记账"/>
   </SELECT>
   </ITEM>
   
  <ITEM NAME="#FFF"     TYPE="%-8s"  FUNCTIONS="T($HDATE)"/>  
  <ITEM NAME="#DDD"     TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#HTRMSEQ" TYPE="%-2s"  FUNCTIONS="T($HTRMSEQ)"/>
  <ITEM NAME="#HEJFNO"  TYPE="%-6s"  FUNCTIONS="T($HEJFNO)"/>
  <ITEM NAME="#KINBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#OPNBR"   TYPE="%-7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#KINDNO"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#YYY"     TYPE="%-4s"  FUNCTIONS="T($KINBR,4,4)"/>
  <ITEM NAME="#ACTNO"   TYPE="%-18s" FUNCTIONS="T(000000000000000000)"/>
  <ITEM NAME="#TRMSEQ " TYPE="%-2s"  FUNCTIONS="T($TRMSEQ)"/>
  <ITEM NAME="#OTRMSEQ" TYPE="%-9s"  FUNCTIONS="T(#KINBR+#TRMSEQ)"/>
  <ITEM NAME="#EJFNO"   TYPE="%-6s"  FUNCTIONS="T($TXTNO)"/>
  <ITEM NAME="#TASKID"  TYPE="%-2s"  FUNCTIONS="T(UB)"/>
  <ITEM NAME="#TMTYPE"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#APTYPE"  TYPE="%-1s"  FUNCTIONS="T(a)"/>
  <ITEM NAME="#TXNO"    TYPE="%-2s"  FUNCTIONS="T(98)"/>
  <ITEM NAME="#PTYPE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#DSCPT"   TYPE="%-3s"  FUNCTIONS="T(001)"/>
  <ITEM NAME="#HCODE"   TYPE="%-1s"  FUNCTIONS="T($HCODE)"/>
  <ITEM NAME="#RCODE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TXTYPE"  TYPE="%-2s"  />  
  <ITEM NAME="#MULTTX"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CRDB"    TYPE="%-1s"  FUNCTIONS="T(1)"/>
  <ITEM NAME="#SUPIN1"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SUPIN2"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#REASON"  TYPE="%-6s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TLRNO"   TYPE="%-2s"  FUNCTIONS="T($TLRNO)"/>
  <ITEM NAME="#TRNMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#SBTMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#CURCD"   TYPE="%-3s"  FUNCTIONS="T(000)"/>
  <ITEM NAME="#XCRT"    TYPE="%-8s"  FUNCTIONS="T($XCRT)"/>
  <ITEM NAME="#NBCD"    TYPE="%-1s"  FUNCTIONS="T(0)"/>
  <ITEM NAME="#TXAMT"   TYPE="%-12s" FUNCTIONS="T(000000000000)"/>          
  <ITEM NAME="#EDATE"   TYPE="%-8s"  FUNCTIONS=""/>
  <ITEM NAME="#ATYPENO" TYPE="%-12s" FUNCTIONS=""/>

  <ITEM NAME="#ORCNT"      TYPE="%-4s"  FUNCTIONS=""/>
  <ITEM NAME="#AVBAL"      TYPE="%-16s"  FUNCTIONS=""/>
  <ITEM NAME="#OCHANGENO"  TYPE="%-4s"  FUNCTIONS=""/>
  <ITEM NAME="#ICHANGENO"  TYPE="%-4s"  FUNCTIONS=""/>
  <ITEM NAME="#payactno"   TYPE="%-32s"  FUNCTIONS=""/>
  <ITEM NAME="#PAYACTNO"   TYPE="%-20s"  FUNCTIONS="T()"/>
  <ITEM NAME="#cashactno"  TYPE="%-32s"  FUNCTIONS=""/>
  <ITEM NAME="#CASHACTNO"  TYPE="%-20s"  FUNCTIONS="T()"/>

  <ITEM NAME="#TXBAL"  TYPE="%13.2f"  FUNCTIONS="J(#XTXBAL)J(#XTXBAL2)"/>
  <ITEM NAME="#XTXBAL"  TYPE="%-16s"  FUNCTIONS="X(#TXBAL)"/>
  <ITEM NAME="#XTXBAL2"  TYPE="%16s"  FUNCTIONS="T(#XTXBAL)"/>
 

  <ITEM NAME="#HEAD"   TYPE="%-1s" FUNCTIONS="T($CCHED=1)"/>
  <ITEM NAME="#TXCNT"   TYPE="%2d" FUNCTIONS="T($OCCURS=#TXCNT)J(#TOTCNT)" />
  <ITEM NAME="#TOTCNT"  TYPE="%5d" FUNCTIONS="E(#TOTCNT+#TXCNT)J(#TOTCNTC)"/>
  <ITEM NAME="#TOTCNTC" TYPE="%5d" FUNCTIONS="T($TOTCNT=#TOTCNT)"/>
  <ITEM NAME="#SPACE"  TYPE="%-1s" FUNCTIONS="T( )"/>

  <ITEM NAME="#SERVER"  TYPE="%-1s"  FUNCTIONS="ST($ORKINBR=#OPNBR)T($TTSKID=IH)"/>
  
</VARIABLE>

<REQUEST><PACKAGE>
 $APCTLHD
 $HTRMSEQ$HEJFNO#OPNBR#KINBR$TRMSEQ#EJFNO#TASKID#TMTYPE#APTYPE#TXNO#PTYPE
 #DSCPT#HCODE#ACTNO#TXTYPE#MULTTX#CRDB$SUPCD1$SUPCD2#REASON#TLRNO
 #NBCD#TRNMOD#SBTMOD#CURCD#XCRT#TXAMT
 #VACTNO#PUTFLAG#STAT
</PACKAGE></REQUEST>

<RESPONSE>
<PACKAGE>a98101#TXCNT#TXBAL%LOP#OCHANGENO#PAYACTNO#AVBAL#ICHANGENO#CASHACTNO#ORCNT%BAK
</PACKAGE>
</RESPONSE>

<OUTPUT>
<PACKAGE>
 000#TXFLAG=1B200000A     N505#UPUTFLAG#XTXBAL%NRJ
</PACKAGE><PACKAGE>
 000#TXFLAG=1B200000B     N505%LOP#OCHANGENO#PAYACTNO#AVBAL#ICHANGENO#CASHACTNO#ORCNT%BAK%NRJ
</PACKAGE><PACKAGE>
 000#TXFLAG=1B200000C     N505%NRJ
</PACKAGE>
<PACKAGE>
 000#TXFLAG=2B200000A     N205#UPUTFLAG#XTXBAL%NRJ
</PACKAGE><PACKAGE>
 000#TXFLAG=2B200000B     N205%LOP#OCHANGENO#PAYACTNO#AVBAL#ICHANGENO#CASHACTNO#ORCNT%BAK%NRJ
</PACKAGE><PACKAGE>
 000#TXFLAG=2B200000C     N205%NRJ
</PACKAGE>

<FORMAT NAME="B200000A">
01                           提  出  提  入  票  据  查  询 
02 ############:################
03 提出行交换号  付款人账号  金 额    提入行交换号 收  款  人  账  号     笔数
04 ============================================================================
</FORMAT>
<FORMAT NAME="B200000B">
05 #### #################### ################ ####   ####################  ####
</FORMAT>
<FORMAT NAME="B200000C">
04
</FORMAT>

</OUTPUT>
</TRANSACTION>
