 <?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111"  TITLE="T(支票业务回执明细)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
"{
			initfd();
			commsend_001();
}"
</COMMSEND>

<COMMRECV>
"{
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(LVZP_002                      支票业务回执明细)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=5404)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>  
  
  <ITEM LINE="5"   COL="03" TYPE="%-12s" FUNCTIONS="T(委托日期  :)"/>
  <ITEM LINE="5"   COL="40" TYPE="%-12s" FUNCTIONS="T(交易状态  :)"/>
  <ITEM LINE="6"   COL="03" TYPE="%-12s" FUNCTIONS="T(原交易序号:)"/>
  <ITEM LINE="6"   COL="40" TYPE="%-12s" FUNCTIONS="T(新交易序号:)"/>
  <ITEM LINE="7"   COL="03" TYPE="%-12s" FUNCTIONS="T(来包日期  :)"/>
  <ITEM LINE="7"   COL="40" TYPE="%-12s" FUNCTIONS="T(来包序号  :)"/>
  <ITEM LINE="8"   COL="03" TYPE="%-12s" FUNCTIONS="T(接收行行号:)"/>
  <ITEM LINE="8"   COL="40" TYPE="%-12s" FUNCTIONS="T(发起行行号:)"/>
  <ITEM LINE="10"  COL="03" TYPE="%-12s" FUNCTIONS="T(业务类型号:)"/>
  <ITEM LINE="10"  COL="40" TYPE="%-12s" FUNCTIONS="T(业务种类  :)"/>
  <ITEM LINE="11"  COL="03" TYPE="%-12s" FUNCTIONS="T(汇款金额  :)"/>
  <ITEM LINE="11"  COL="40" TYPE="%-12s" FUNCTIONS="T(验印状态  :)"/>
  <ITEM LINE="13"  COL="03" TYPE="%-12s" FUNCTIONS="T(付款人帐号:)"/>
  <ITEM LINE="14"  COL="03" TYPE="%-12s" FUNCTIONS="T(付款人名称:)"/>
  <ITEM LINE="15"  COL="03" TYPE="%-12s" FUNCTIONS="T(付款人地址:)"/>
  <ITEM LINE="16"  COL="03" TYPE="%-12s" FUNCTIONS="T(收款人帐号:)"/>
  <ITEM LINE="17"  COL="03" TYPE="%-12s" FUNCTIONS="T(收款人名称:)"/>
  <ITEM LINE="18"  COL="03" TYPE="%-12s" FUNCTIONS="T(收款人地址:)"/>
  <ITEM LINE="19"  COL="03" TYPE="%-12s" FUNCTIONS="T(附      言:)"/>

  <ITEM LINE="5"   COL="15" NAME="#WTDATE"    TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="21"  COL="3"  NAME="#TXSTAT"    TYPE="%-1s" INPUT="SELECT" VISABLE="FALSE"
  FUNCTIONS="V(NB)">
        <SELECT LINE="21" COL="3">
                <OPTION VALUE="0" TITLE="0.无效状态"/>
                <OPTION VALUE="1" TITLE="0.往账录入"/>
                <OPTION VALUE="2" TITLE="0.往账复核"/>
                <OPTION VALUE="3" TITLE="0.往账复核，记帐"/>
                <OPTION VALUE="4" TITLE="0.往账发送"/>
                <OPTION VALUE="5" TITLE="0.往账拒绝"/>
                <OPTION VALUE="6" TITLE="0.往账扎差"/>
                <OPTION VALUE="7" TITLE="0.往账排队"/>
                <OPTION VALUE="8" TITLE="0.往账撤销等待"/>
                <OPTION VALUE="9" TITLE="0.往账撤销"/>
                <OPTION VALUE="A" TITLE="0.往账退回、冲正、止付等待"/>
                <OPTION VALUE="B" TITLE="0.往账退回"/>
                <OPTION VALUE="C" TITLE="0.往账已冲正"/>
                <OPTION VALUE="D" TITLE="0.往账已止付"/>
                <OPTION VALUE="G" TITLE="0.往账异常挂帐"/>
                <OPTION VALUE="H" TITLE="0.临时接收,不记帐"/>
                <OPTION VALUE="I" TITLE="0.正式接收，正常记帐"/>
                <OPTION VALUE="J" TITLE="0.全国押错"/>
                <OPTION VALUE="K" TITLE="0.地方押错"/>
                <OPTION VALUE="L" TITLE="0.全国押和地方押都错"/>
                <OPTION VALUE="M" TITLE="0.信息不符，清算中心挂帐"/>
                <OPTION VALUE="N" TITLE="0.信息不符，支行挂帐"/>
                <OPTION VALUE="O" TITLE="0.来账退回等待"/>
                <OPTION VALUE="P" TITLE="0.来账退回"/>
                <OPTION VALUE="Q" TITLE="0.已做同意应答且未发送"/>
                <OPTION VALUE="R" TITLE="0.已做同意应答且已发送"/>
                <OPTION VALUE="S" TITLE="0.已做不同意应答且未发送"/>
                <OPTION VALUE="T" TITLE="0.已做不同意应答且已发送"/>
                <OPTION VALUE="U" TITLE="0.来账已冲正"/>
                <OPTION VALUE="V" TITLE="0.来账已止付"/>
                <OPTION VALUE="W" TITLE="0.来账已付款"/>
                <OPTION VALUE="Z" TITLE="0.来账异常挂帐"/>
        </SELECT>
  </ITEM>
  <ITEM LINE="5"  COL="52" NAME="#LTXSTAT"   TYPE="%-22s"  FUNCTIONS="S(#TXSTAT,#TXSTAT)"/>
  <ITEM LINE="6"  COL="15" NAME="#ORDERNO"   TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="6"  COL="52" NAME="#NORDERNO"  TYPE="%-8s"   FUNCTIONS="V(NB)"/>
<!--
  <ITEM NAME="#TTT"    TYPE="%-1s"  FUNCTIONS="">
  <ONLOSTFOCUS>"{
        showmsg(#TXSTAT);
        showmsg(getseltitle(#TXSTAT,#TXSTAT));
        showmsg(#LTXSTAT);
  }"
  </ONLOSTFOCUS>
  </ITEM>
-->

  <ITEM LINE="7"   COL="15" NAME="#PACKDAY"   TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="7"   COL="52" NAME="#PACKID"    TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="8"   COL="15" NAME="#ORBRNO"    TYPE="%-12s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="8"   COL="52" NAME="#ACBRNO"    TYPE="%-12s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="21"  COL="3"  NAME="#TXNUM"     TYPE="%-5s"   INPUT="SELECT" VISABLE="FALSE"
  FUNCTIONS="V(NB)">
        <SELECT LINE="21" COL="3">
                <OPTION VALUE="00113" TITLE="0.国库资金借记划拨"/>
                <OPTION VALUE="00119" TITLE="1.其他借记业务"/>
                <OPTION VALUE="20103" TITLE="2.财政直接支付"/>
                <OPTION VALUE="20104" TITLE="3.财政授权支付"/>
                <OPTION VALUE="30102" TITLE="4.支票截留业务"/>
                <OPTION VALUE="30103" TITLE="5.通用截留业务"/>
        </SELECT>
  </ITEM>
  <ITEM LINE="10"  COL="15" NAME="#LTXNUM"    TYPE="%-16s"  FUNCTIONS="S(#TXNUM,#TXNUM)"/>
<!--
  <ITEM NAME="#TTT1"    TYPE="%-1s"  FUNCTIONS="">
  <ONLOSTFOCUS>"{
        showmsg(#TXNUM);
        showmsg(getseltitle(#TXNUM,#TXNUM));
        showmsg(#LTXNUM);
  }"
  </ONLOSTFOCUS>
  </ITEM>
-->
  <ITEM LINE="10"  COL="52" NAME="#YWTYPE"    TYPE="%-12s"  FUNCTIONS="V(NB)"/>

  <ITEM NAME="#HTXAMT" TYPE="%16.2f"   FUNCTIONS=""/>
  <ITEM LINE="11"  COL="15" NAME="#LHTXAMT"  TYPE="%-17s"   FUNCTIONS=""/>
  <ITEM LINE="24"  COL="3"  NAME="#QYYFLAG"  TYPE="%-1s"    VISABLE="FALSE"   FUNCTIONS="">
   <SELECT LINE="21" COL="3">
   <OPTION  VALUE="*"  TITLE="无状态"/>
   <OPTION VALUE="0"  TITLE="未验印" />
   <OPTION VALUE="1"  TITLE="验印通过"/>
   <OPTION VALUE="2"  TITLE="验印未通过"/>
   </SELECT>
  </ITEM>
  <ITEM LINE="11"  COL="52" NAME="#YYFLAG"    TYPE="%-10s"  FUNCTIONS="S(#QYYFLAG, #QYYFLAG)"/>
  <ITEM LINE="13"  COL="15" NAME="#PAYACTNO"  TYPE="%-32s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="14"  COL="15" NAME="#PAYNAME"   TYPE="%-60s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="15"  COL="15" NAME="#PAYADDR"   TYPE="%-60s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="16"  COL="15" NAME="#CASHACTNO" TYPE="%-32s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="17"  COL="15" NAME="#CASHNAME"  TYPE="%-60s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="18"  COL="15" NAME="#CASHADDR"  TYPE="%-60s"  FUNCTIONS="V(NB)"/>
  <ITEM LINE="19"  COL="15" NAME="#CONTENT"   TYPE="%-60s"  FUNCTIONS="V(NB)"/>

  <ITEM LINE="21"  COL="45" NAME="#CLOSE"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
        showall();
        close();
  }"
  </ONCLICK>
  </ITEM>

  <!--<ITEM LINE="21"  COL="55" NAME="#PRINT"    INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(打 印)">
  <ONCLICK>"{
            runoutput();
  }"
  </ONCLICK>
  </ITEM>-->

  <!-- <ITEM LINE="21"  COL="67" NAME="#LVUNTREAD"    INPUT="BUTTON" TYPE="%-8s" FUNCTIONS="T(撤销申请)">
  <ONCLICK>"{
        if(#TXSTAT=='4' or #TXSTAT=='7'){
                open('6532.XML','#ORDERNO='+';'+
                                                '#ACBRNO='+#ACBRNO+';'+
                                                '#OPKGNO=008;'+
                                                '#OPACKDAY='+#PACKDAY+';'+
                                                '#OPACKID='+#PACKID+';'+
                                                '#QCONTENT='+#CONTENT+';'
                                                );
        }else{
                showmsg('此种状态交易无法申请退回');
        }

  }"
  </ONCLICK>
  </ITEM>-->
</VARIABLE>

<OUTPUT>
  <PACKAGE>
000$9999=9Q200000B支票业务回执明细N205#WTDATE#LTXSTAT#ORDERNO#NORDERNO#PACKDAY#PACKID#ACBRNO#ORBRNO
#LTXNUM#YWTYPE#LHTXAMT#YYFLAG#PAYACTNO#PAYNAME#PAYADDR#CASHACTNO#CASHNAME#CASHADDR#CONTENT
</PACKAGE>
<FORMAT NAME="Q200000B">
"
02                                   支票业务回执明细
03 委托日期  : ########                 交易状态  : ######################
04 原交易序号: ########                 新交易序号: ########
05 来包日期  : ########                 来包序号  : ########
06 发起行行号: ############             接收行行号: ############
07 业务类型号: ################         原业务种类: ############
08 汇款金额  : #################       
09 付款人帐号: ################################
10 付款人名称: ############################################################
11 付款人地址: ############################################################
12 收款人帐号: ################################
13 收款人名称: ############################################################
14 付款人地址: ############################################################
15 附      言: ############################################################
"
</FORMAT>
</OUTPUT>

</TRANSACTION>


