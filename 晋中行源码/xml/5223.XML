<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5223 自动提出清单打印"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	$MSGTYPE = '03001';
	commsend_001();
   	
}"
</COMMSEND>

<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE UPLOOP="TRUE">
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(5223                        自动提出清单打印)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

    
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4223)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  
  <ITEM LINE="10" COL="25" TYPE="%-20s" FUNCTIONS="T(网点打印自动提出清单)"/>
  <ITEM LINE="12" COL="25" TYPE="%-20s" FUNCTIONS="T(请按确定键......)"/>


  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    

  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>


    
</VARIABLE>


<REQUEST>
</REQUEST>


<RESPONSE>
	<PACKAGE DEVICE="SCREEN" DESC="网点提出自动录入成功">
      NOTHING
	</PACKAGE>

	<PRINT>"{
		$KINDNO='00';
		print(8, 22,'网 点 提 出 自 动 录 入 成 功 ！');			
	}"
	</PRINT>
</RESPONSE>



</TRANSACTION>



