<?xml version="1.0" encoding="GBK" ?>


<TRANSACTION RIGHTS="00110000" TITLE="L24  打印票据清算[提出]"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->

<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->

<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-49s" FUNCTIONS=
    "T(S24                       打印票据清算[提出])"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  
  <ITEM LINE="8"  COL="25" TYPE="%-25s" FUNCTIONS="T(提入行交换号:)"/>
  <ITEM LINE="8"  COL="39" NAME="#CHANGENO"  TYPE="%-4s"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)"/>

  <ITEM NAME="#OPNBR"   TYPE="%7s"  FUNCTIONS="T($KINBR)"/>
  <ITEM NAME="#HEJFNO"  TYPE="%-6s"  FUNCTIONS="T($HEJFNO)"/>    <!--冲正日志档序号 -->
  <ITEM NAME="#HTRMSEQ" TYPE="%-2s"  FUNCTIONS="T($HTRMSEQ)"/>      <!-- 冲正终端机号 -->
  <ITEM NAME="#HCODE"   TYPE="%-1s"  FUNCTIONS="T($HCODE)"/>        <!-- 取消记号 -->
  <ITEM NAME="#TRMSEQ"  TYPE="%-2s"  FUNCTIONS="T($TRMSEQ)"/>       <!-- 柜台机序号 -->
  <ITEM NAME="#EJFNO"   TYPE="%6s"  FUNCTIONS="T($TXTNO)"/>        <!-- 日志序号 -->
  <ITEM NAME="#TASKID"  TYPE="%-2s"  FUNCTIONS="T(UC)"/>            <!-- 终端程序类别 -->

  <ITEM NAME="#TMTYPE"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 终端种类 -->

  <ITEM NAME="#APTYPE"  TYPE="%-1s"  FUNCTIONS="T(a)"/>             <!-- 业务类别 -->
  <ITEM NAME="#TXNO"    TYPE="%-2s"  FUNCTIONS="T(96)"/>            <!--交易代号-->
  <ITEM NAME="#PTYPE"   TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 处理型态 -->
  <ITEM NAME="#MULTTX"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 连动记号 -->

  <ITEM NAME="#CRDB"    TYPE="%-1s"  FUNCTIONS="T(1)"/>             <!-- 借贷别 -->
  <ITEM NAME="#TRNMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 训练记号 -->
  <ITEM NAME="#SBTMOD"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 代登记号 -->
  <ITEM NAME="#TLRNO"   TYPE="%-2s"  FUNCTIONS="T($TLRNO)"/>        <!-- 操作员号 -->
  <ITEM NAME="#CURCD"   TYPE="%-3s"  FUNCTIONS="T(RMB)"/>           <!-- 币别 -->
  <ITEM NAME="#NBCD"    TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 无折记号 -->
  <ITEM NAME="#XCRT"    TYPE="%-8s"  FUNCTIONS="T($XCRT)"/>         <!-- 外汇汇率 -->
  <ITEM NAME="#REASON"  TYPE="%-6s"  FUNCTIONS="T(0)"/>             <!-- 主管许可理由 -->
  <ITEM NAME="#SUPIN1"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 主管许可记号2 -->
  <ITEM NAME="#SUPIN2"  TYPE="%-1s"  FUNCTIONS="T(0)"/>             <!-- 主管许可记号1 -->
  <ITEM NAME="#TXTYPE"   TYPE="%-2s"  FUNCTIONS="T()"/>           <!-- 帐务别 -->
  <ITEM NAME="#DSCPT"   TYPE="%-3s"  FUNCTIONS="T(002)"/>           <!-- 摘要 -->
  <ITEM NAME="#TXAMT"   TYPE="%-12s" FUNCTIONS="T(000000000000)"/>           <!--金额-->
  <ITEM NAME="#ACTNO"   TYPE="%-18s" FUNCTIONS="T(000000000000000000)"/>    <!--帐号-->

  <ITEM NAME="#OTXDAY"      TYPE="%-8s"  />
  <ITEM NAME="#OUTCHANGE"  TYPE="%-4s"  />
  <ITEM NAME="#OUTNAME"     TYPE="%-40s"  />
  <ITEM NAME="#INCHANGE"   TYPE="%-4s"  />
  <ITEM NAME="#INNAME"     TYPE="%-40s"  />
  <ITEM NAME="#CASHACTNO"  TYPE="%-32s" FUNCTIONS="J(#CASHACTNO2)" />
  <ITEM NAME="#CASHACTNO2"  TYPE="%-20s" FUNCTIONS="T(#CASHACTNO,1,20)" />
  <ITEM NAME="#CASHNAME"  TYPE="%-40s"  />
  <ITEM NAME="#PAYACTNO"   TYPE="%-32s" FUNCTIONS="J(#PAYACTNO2)" />
  <ITEM NAME="#PAYACTNO2"   TYPE="%-20s" FUNCTIONS="T(#PAYACTNO,1,20)" />
  <ITEM NAME="#PNAME"   TYPE="%-20s"  />
  <ITEM NAME="#OAVBAL"     TYPE="%-16s" FUNCTIONS="J(#CAVBAL)"/>
  <ITEM NAME="#CAVBAL"     TYPE="%12s" FUNCTIONS="T(#OAVBAL,5,12)"/>
  <ITEM NAME="#TOTXAMT"     TYPE="%-16s" FUNCTIONS="J(#CTOTXAMT)"/>
  <ITEM NAME="#CTOTXAMT"     TYPE="%12s" FUNCTIONS="T(#TOTXAMT,5,12)"/>
  <ITEM NAME="#TOTXAMT1"     TYPE="%-16s" FUNCTIONS="J(#CTOTXAMT1)"/>
  <ITEM NAME="#CTOTXAMT1"     TYPE="%12s" FUNCTIONS="T(#TOTXAMT1,5,12)"/>

  <ITEM NAME="#OAVBAL3"     TYPE="%-16s" FUNCTIONS="J(#CAVBAL3)"/>
  <ITEM NAME="#CAVBAL3"     TYPE="%12s" FUNCTIONS="T(#OAVBAL3,5,12)"/>
  <ITEM NAME="#CASHACTNO3"  TYPE="%-32s" FUNCTIONS="J(#CASHACTNO4)" />
  <ITEM NAME="#CASHACTNO4"  TYPE="%-20s" FUNCTIONS="T(#CASHACTNO3,1,20)" />
  <ITEM NAME="#PAYACTNO3"   TYPE="%-32s" FUNCTIONS="J(#PAYACTNO4)" />
  <ITEM NAME="#PAYACTNO4"   TYPE="%-20s" FUNCTIONS="T(#PAYACTNO3,1,20)" />

  <ITEM NAME="#HEAD"   TYPE="%-1s" FUNCTIONS="T($CCHED=1)"/>


<ITEM NAME="#TXCNT"   TYPE="%2d" FUNCTIONS="T($OCCURS=#TXCNT)J(#TOTCNT)" />
<ITEM NAME="#TOTCNT"  TYPE="%5d" FUNCTIONS="E(#TOTCNT+#TXCNT)J(#TOTCNTC)"/>
<ITEM NAME="#TOTCNTC" TYPE="%5d" FUNCTIONS="T($TOTCNT=#TOTCNT)"/>

<ITEM NAME="#SERVER" TYPE="%-1s"  FUNCTIONS="T($ORKINBR=#OPNBR)T($TTSKID=UC)"/>
</VARIABLE>

<REQUEST><PACKAGE>
$APCTLHD$HTRMSEQ$HEJFNO#OPNBR$KINBR$TRMSEQ$TXTNO#TASKID$TRMTYP#APTYPE
#TXNO$PTYPE#DSCPT$HCODE#ACTNO$TXTYPE$MULTTX#CRDB$SUPCD1$SUPCD2$REASON
$TLRNO#NBCD$TRNMOD$SBTMOD$CURCD$XCRT#TXAMT#CHANGENO
</PACKAGE></REQUEST>

<RESPONSE>
<PACKAGE>a96101#TXCNT#OTXDAY#OUTCHANGE#OUTNAME#INCHANGE#INNAME#TOTXAMT#TOTXAMT1%LOP#CASHACTNO#PAYACTNO#OAVBAL
#CASHACTNO3#PAYACTNO3#OAVBAL3%BAK</PACKAGE>
</RESPONSE>

<OUTPUT>
<PACKAGE> 000$HCODE=0B200000A     N205#OTXDAY#OUTCHANGE#INCHANGE#OUTNAME#INNAME%NRJ </PACKAGE>
<!--
<PACKAGE> 000$HCODE=0B200000B     N205%LOP#PAYACTNO4#CASHACTNO4#CAVBAL3#CASHACTNO2#PAYACTNO2#CAVBAL%BAK%NRJ</PACKAGE>
-->
<PACKAGE> 000$HCODE=0B200000B     N205%LOP#CASHACTNO2#PAYACTNO2#CAVBAL#CASHACTNO4#PAYACTNO4#CAVBAL3%BAK%NRJ</PACKAGE>
