	  show(#TRAN_CNM,true);
	  enable(#TRAN_CTY,false);
	  enable(#TRAN_CNM,false);
	}
	//增加一个内部帐帐户的标志
	if(substr(#DFNO,0,1)=='9')
	{
		#ACT_LX_CK = '3';
