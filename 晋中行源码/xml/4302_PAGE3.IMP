	enable(#TRAN_AMT,false);


	if(#ACT_LX=='2')
	{
	   #ZY_TP2='001';
	   #TRAN_CNM = ''; 
	   show(#S_ZY,true);
	   show(#S_STDAY,true);
	   show(#ZY_TP2,true);
	   show(#STDAY,true);
	   show(#TRAN_CTY,true);
	   show(#TRAN_CNM,true);
	   enable(#TRAN_CTY,true);
	   enable(#TRAN_CNM,true);
	   
	}else{
	  #TRAN_CNM = #TTRAN_CNM; 
	  show(#S_ZY,false);
	  show(#S_STDAY,false);
	  show(#ZY_TP2,false);
