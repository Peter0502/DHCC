
  <!---转帐页面--->
  <PAGE NAME="PAGE3">
  <ITEM NAME="#ACT_LX"  TYPE="%-1s"/>
  <ITEM LINE="6"  COL="5"   TYPE="%-10s"   FUNCTIONS="T(对方帐号:)"/>
  <ITEM LINE="6"  COL="47"  TYPE="%-10s"   FUNCTIONS="T(帐户序号:)"  INPUT="LABEL" VISABLE="FALSE" NAME="#S_SEQNO"/>
  <ITEM LINE="8"  COL="5"   TYPE="%-20s"   FUNCTIONS="T(【转帐对方帐户信息】)"/>
  <ITEM LINE="9"  COL="5"   TYPE="%-70s"   FUNCTIONS="T(----------------------------------------------------------------------)"/>
  <ITEM NAME="#TRAN_AC_NO" LINE="6"  COL="15"  TYPE="%-19s"  INPUT="TEXT"  FUNCTIONS="V(NB)">
  <ONLOSTFOCUS>
  "{
	dim @txno     as string;
	dim @tota     as string;
	dim @tran_bal as string;
	dim @tmp      as string;
	dim @tmp_head as string;
	dim @sel_buf  as string;
	dim @cnt      as number;
	dim @totcnt   as number;
	

  initfd();

	@txno  = $TXNO;
	$FD12  = '2103';
	$TXNO  = '9731';
	$FD102 = #TRAN_AC_NO;

	if (#ACTNO==#TRAN_AC_NO)
	{
		showmsg('帐号与对方帐号不应为同一帐号');
		return(false);
	}
	if(substr(#TRAN_AC_NO,0,3)=='407' or substr(#TRAN_AC_NO,0,3)=='416'){
		showmsg('不可以记此科目的帐务!');
		return(false);
	}
	if(substr(#TRAN_AC_NO,0,3)=='408'){
		if($KINBR!='10001'){
			showmsg('不可以记此科目的帐务!');
			return(false);
		}
	}

	commsend_001();
	@tota  = callserver();
	$TXNO  =@txno;
	if($FD12 != '0000')
	{
		showmsg(geterror());
		return(false);
	}
	#TRAN_AC_NO=delspace(substr($FD102,0,24)); //将新帐号赋给#TRAN_AC_NO 
  if(atoi(substr($FD102,24,4))!=0)
  { 
      #SEQNO_102=substr($FD102,24,4);
  		show(#SEQNO_102,true);
  		show(#S_SEQNO,true);
  }else{
      show(#SEQNO_102,false);
  		show(#S_SEQNO,false);
  #SEQNO_102='0000';
	@cnt=0;
	@totcnt=0;
	@tmp='';
	@tmp_head='';
	while(@totcnt &lt; len(@tota))
	{
		@tmp=substr(@tota,@totcnt,20);
		@tmp_head=substr(@tmp,0,4);
		if(@tmp_head=='1023' or rtrim(@tmp_head)=='')
		{
			break;
		}
		@totcnt=@totcnt+31;
	}	

	if(@totcnt!=0)
	{
	  @sel_buf=substr(@tota,0,@totcnt);
	  setsel(#ZY_TP2,@sel_buf,'o5t20o1v3o2');
	}
	
	@sel_buf=substr(@tota,@totcnt,len(@tota)-@totcnt);
	setsel(#TRAN_CTY,@sel_buf,'o5t20o1v3o2');

	//setsel(#TRAN_CTY,@tota,'o5t20o1v3o2');  凭证种类
	#TRAN_NAME = substr($FD102,125,60);
	#TRAN_CUR  = substr($FD102,201,2);
	//showmsg('TRAN_CUR1'+#TRAN_CUR);
	@tran_bal  = substr($FD102,185,16);
	#ACT_LX    = substr($FD102,207,1);
	//showmsg('ACT_LX='+#ACT_LX);
	#TRAN_BAL  = val(@tran_bal,0.01);
	#TRAN_PDN  = substr($FD102,313,30);
	#SPECIAL   = substr($FD102,293,20);  //特殊标志
	#TRAN_CTY  = substr($FD102,28,3);
	//#TRAN_CNM  = substr($FD102,31,16);
	#TTRAN_CNM  = substr($FD102,31,16);
	#TRAN_PDOT = substr($FD102,48,1);
	#TRAN_CDOT = substr($FD102,61,1);
	#TRAN_CET  = substr($FD102,124,1);
	#TRAN_CNUM = substr($FD102,62,20);
	#TRAN_SDOT = substr($FD102,82,1);  //印鉴支取标志
	#TRAN_AMT  = #TXAMT;               //转出金额等于开户金额
	
	showblock('TRANSFAR',true,true);
	
	show(#TRAN_PDN,true);//add bu liucb

	show(#TRAN_CUR,true);
	enable(#TRAN_CUR,false);
	show(#TRAN_CTY,true);
	enable(#TRAN_CTY,false);
	show(#TRAN_CTY,true);
	enable(#TRAN_CTY,false);
	show(#TRAN_PDOT,true);
	enable(#TRAN_PDOT,false);
	show(#TRAN_CDOT,true);
	enable(#TRAN_CDOT,false);
	show(#TRAN_CET,true);
	enable(#TRAN_CET,false);
	show(#TRAN_BAL,true);
	enable(#TRAN_BAL,false);
	show(#TRAN_AMT,true);
	enable(#TRAN_AMT,false);

	if(#ACT_LX=='2')
	{
		 #ZY_TP2='001';
	   show(#S_ZY,true);
	   show(#S_STDAY,true);
	   show(#ZY_TP2,true);
	   show(#STDAY,true);
	   show(#TRAN_CTY,true);
	   show(#TRAN_CNM,true);
	   enable(#TRAN_CTY,true);
	   enable(#TRAN_CNM,true);
	   
	}else{
	  #TRAN_CNM = #TTRAN_CNM;
	  show(#S_ZY,false);
	  show(#S_STDAY,false);
	  show(#ZY_TP2,false);
	  show(#STDAY,false);
	  show(#TRAN_CNM,true);
	  enable(#TRAN_CTY,false);
	  enable(#TRAN_CNM,false);
	}
	//增加一个内部帐帐户的标志,另也作为科目标志
	if(substr(#DFNO,0,1)=='9' or len(#DFNO) &lt;= 7)
	{
		#ACT_LX_CK = '3';
	}

	if(#TRAN_PDOT == 'Y')
	{
			show(#PASSNAME,true);
			show(#TRAN_PWD,true);
			enable(#TRAN_PWD,true);
	}
	else
	{
			show(#PASSNAME,false);
			show(#TRAN_PWD,false);
			enable(#TRAN_PWD,false);
	}

	if(#TRAN_CDOT == 'Y')
	{
			show(#TRAN_CET,true);
			show(#TRAN_CNUM,true);
			enable(#TRAN_CET,false);
			enable(#TRAN_CNUM,true);
	}
	else
	{
			//if(#ACT_LX=='2')
			//{

			 //   show(#TRAN_CET,true);
			 //   show(#TRAN_CNUM,true);
			 //   show(#CERTIFTYPE,true);
			 //   show(#CERTIFNUM,true);
			 //   enable(#TRAN_CET,false);
			 //   enable(#TRAN_CNUM,true);
			//}else{

			    show(#TRAN_CET,false);
			    show(#TRAN_CNUM,false);
			    show(#CERTIFTYPE,false);
			    show(#CERTIFNUM,false);
			    enable(#TRAN_CET,false);
			    enable(#TRAN_CNUM,false);
			//}
	}
	
	}

	//若为贷款还款户,且有未还的本金或利息,提示柜员 20070621
	if($FD71 == '1')
	{
		showhelp(5,60,7,10,'提示', '该户为贷款还款户,且有未还的本金或利息!');
		return(true);
	}
  }"
  </ONLOSTFOCUS>
  </ITEM>
  
