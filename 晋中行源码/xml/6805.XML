<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6805 贴现信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
    $FD26=#NAME;
    $FD29=#DATE;
    $FD40=getitems('#AMT');
    $FD44=#MDATE;
    $FD51=#WDAYS;
    $FD59=#PNNO;
    $FD63=#PANO;
    $FD67=#LIND;
    $FD69=#STS;
    $FD70=#ACTYPE;
    $FD75=#ACNO;
    $FD84=getitems('#PINT');
    $FD90=#NOTYPE;
	commsend_001();
	$FD16='4915';
}"
</COMMSEND>
<COMMRECV>"{
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(6805                          贴现信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4915)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <ITEM LINE="5"  COL="5" TYPE="%-12s" FUNCTIONS="T(【查询条件】)"/>
  <ITEM LINE="6"  COL="3" TYPE="%-74s" FUNCTIONS=
  "T(---------------------------------------------------------------------------)"/>


  <ITEM LINE="8"  COL="5" TYPE="%-10s" FUNCTIONS="T(状    态!)"/>
  <ITEM LINE="9"  COL="5" TYPE="%-14s" FUNCTIONS="T(贴现协议编号:)"/>
  <ITEM LINE="10"  COL="5" TYPE="%-16s" FUNCTIONS="T(贴现申请人账号:)"/>
  <ITEM LINE="11"  COL="5" TYPE="%-16s" FUNCTIONS="T(贴现申请人户名:)"/>
  
  <ITEM LINE="12"  COL="5" TYPE="%-10s" FUNCTIONS="T(转账类型!)"/>
  <ITEM LINE="12"  COL="40" TYPE="%-14s" FUNCTIONS="T(异地同城标志!)"/>
  
  <ITEM LINE="13"  COL="5" TYPE="%-10s" FUNCTIONS="T(在途天数:)"/>
  <ITEM LINE="13"  COL="40" TYPE="%-10s" FUNCTIONS="T(贴 现 率:)"/>
  <ITEM LINE="13"  COL="64" TYPE="%-2s" FUNCTIONS="T(‰)"/>
  
  <ITEM LINE="14"  COL="5" TYPE="%-10s" FUNCTIONS="T(票据类型!)"/>
  <ITEM LINE="14"  COL="40" TYPE="%-10s" FUNCTIONS="T(票 据 号:)"/>
  
  <ITEM LINE="15"  COL="5" TYPE="%-10s" FUNCTIONS="T(汇票金额:)"/>
  
  <ITEM LINE="16"  COL="5" TYPE="%-10s" FUNCTIONS="T(贴 现 日:)"/>
  <ITEM LINE="16"  COL="40" TYPE="%-12s" FUNCTIONS="T(汇票到期日:)"/>
  
  <!--输入变量-->
  
  <ITEM NAME="#STS" LINE="8" COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T()">
  <SELECT>
		<OPTION VALUE="*" TITLE="销户"/>
		<OPTION VALUE="1" TITLE="录入"/>
		<OPTION VALUE="2" TITLE="已审核"/>
		<OPTION VALUE="3" TITLE="正常"/>
	</SELECT>
  </ITEM>
  
 <ITEM NAME="#PANO" LINE="9" COL="20" TYPE="%-20s"  INPUT="TEXT" FUNCTIONS="T()"/>
 <ITEM NAME="#ACNO" LINE="10" COL="21" TYPE="%-37s"  INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#NAME" LINE="11" COL="21" TYPE="%-50s"  INPUT="TEXT" FUNCTIONS="T()"/>
 
  <ITEM NAME="#ACTYPE" LINE="12" COL="15" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T(1)">
  <SELECT>
		<OPTION VALUE="1" TITLE="本行"/>
		<OPTION VALUE="2" TITLE="同城"/>
		<OPTION VALUE="3" TITLE="大额"/>
	</SELECT>
  </ITEM>
  <ITEM NAME="#LIND" LINE="12" COL="54" TYPE="%-1s"  INPUT="SELECT" FUNCTIONS="T(0)">
  <SELECT>
		<OPTION VALUE="0" TITLE="本地"/>
		<OPTION VALUE="1" TITLE="异地"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#WDAYS" LINE="13" COL="15" TYPE="%3d"  INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#PINT" LINE="13" COL="50" TYPE="%9.6f"  INPUT="TEXT" FUNCTIONS="T()"/>
  
  <ITEM NAME="#NOTYPE" LINE="14" COL="15" TYPE="%-3s"  INPUT="SELECT" FUNCTIONS="T(111)">
  <SELECT>
		<OPTION VALUE="111" TITLE="银行承兑汇票"/>
		<OPTION VALUE="112" TITLE="商业承兑汇票"/>
	</SELECT>
	</ITEM>
  <ITEM NAME="#PNNO" LINE="14" COL="50" TYPE="%-16s"  INPUT="TEXT" FUNCTIONS="T()"/>
  <ITEM NAME="#AMT" LINE="15" COL="15" TYPE="%17.2f"  INPUT="TEXT" FUNCTIONS="J(LAMT)"/>
  <ITEM NAME="#LAMT" TYPE="%-16s" FUNCTIONS="X(#AMT)"/>
  <ITEM NAME="#DATE" LINE="16" COL="15" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  <ITEM NAME="#MDATE" LINE="16" COL="52" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="D()"/>
  
   <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
  <ITEM NAME="#YEAR"  TYPE="%-4s" FUNCTIONS="T($FD5,1,4)"/>
  <ITEM NAME="#MON"   TYPE="%-2s" FUNCTIONS="T($FD5,5,2)"/>
  <ITEM NAME="#DAY"   TYPE="%-2s" FUNCTIONS="T($FD5,7,2)"/>
  </VARIABLE>
<REQUEST>
</REQUEST>
