<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00100000" TITLE="4701 生成制卡文件"
 xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890-->
<!--         1         2         3         4         5         6         7         8-->
<!--  注意：各个配置项的COL 属性最好是奇数, 这样可以避免出现乱码。                  -->
<!--        另外, 每个汉字的开始列号也最好是奇数, 每个字符串的长度也最好是偶数。    -->


<VARIABLE>
  <ITEM LINE="3"   COL="3"  TYPE="%-45s" FUNCTIONS=
    "T(4701                            生成制卡文件)"/>
<!--12345678901234567890123456789012345678901234567890123456789012345678901234567890--> 
<!--         1         2         3         4         5         6         7         8--> 
  <ITEM LINE="4"   COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  
  <ITEM LINE="8"   COL="20" TYPE="%-10s" FUNCTIONS="T(卡起始号:)"/>
  <ITEM LINE="10"  COL="20" TYPE="%-10s" FUNCTIONS="T(卡终止号:)"/>
  
  <ITEM NAME="#SCARDNO"   LINE="8"   COL="30"    TYPE="%16d"  DEVICE="KEYBOARD" FUNCTIONS="T()V(NB)"/>
  <ITEM NAME="#ECARDNO"   LINE="10"  COL="30"    TYPE="%16d"  DEVICE="KEYBOARD" FUNCTIONS="T()V(NB)"/>
  
  <ITEM LINE="21"  COL="59" TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT" />  
  
  <ITEM NAME="#SET1"  TYPE="%-8s" FUNCTIONS="T($HTTXTYPE=FINANCE)"/>
<!--以上为输入及变量定义-->
<!-- 处理日期时间MMDDHHMMSS形式值 -->
  <ITEM NAME="#DTIME" TYPE="%-4s" FUNCTIONS="T($HDATE,5,4)"/> 
  <ITEM NAME="#TIME1" TYPE="%-4s" FUNCTIONS="T($HDATE,5,6)"/> 

  <!-- 处理柜员%-8s -->
  <ITEM NAME="#DTELL" TYPE="%-8s" FUNCTIONS="T(#DTELL=$TLRNO)"/> 

<!-- 输出变量 --> 
  <ITEM NAME="#PFLAG"         TYPE="%1S" FUNCTIONS="" /><!--标志-->
 <!--第一部分-->
  <ITEM NAME="#OcaActNo"      TYPE="%-21S" />   <!--帐号-->
  <ITEM NAME="#OcaBal"        TYPE="%-13S" FUNCTIONS="T(#PFLAG=0)" /> <!--本金-->
  <ITEM NAME="#OcaInt"        TYPE="%-9S" />
  <ITEM NAME="#OcaTotalAmt"   TYPE="%-13S" />   <!--总计金额-->
  <ITEM NAME="#OcFiller"      TYPE="%-1S" />    <!--分隔符-->
 <!--第二部分-->
 
  <ITEM NAME="#OcaTxnCode"    TYPE="%-4s" />    <!--交易代码-->
  <ITEM NAME="#OcaCdNo1"       TYPE="%-19s" />   <!--卡号-->
  <ITEM NAME="#OcaName2"      TYPE="%-20s" FUNCTIONS="T(#PFLAG=3)"/>	  <!--持卡人姓名-->
  <ITEM NAME="#OcaActNo1"      TYPE="%-21s" />	<!--帐号-->
  <ITEM NAME="#OcaSavType1"    TYPE="%-8s" />	<!--储种-->
  <ITEM NAME="#OcaTxnType1"    TYPE="%-8s" />	<!--交易种类-->
  <ITEM NAME="#OcaTxnCur1"     TYPE="%-3s" />	<!--币种-->
  <ITEM NAME="#OcaTxnCurName1" TYPE="%-16s" />	<!--币种名称-->
  <ITEM NAME="#OcaAmt1"        TYPE="%-13s" />	<!--交易金额-->
  <ITEM NAME="#OcaInt1"        TYPE="%-9s" />	<!--存期-->
  <ITEM NAME="#OcaOpSeq1"      TYPE="%-12s" />	<!--流水号-->
  <ITEM NAME="#OcaDate1"       TYPE="%-8s" />	<!--交易日期-->
  <ITEM NAME="#OcaTime1"       TYPE="%-6s" />	<!--交易时间-->
  <ITEM NAME="#OcaACE1"        TYPE="%-6s" />	<!--会计分录-->
  <ITEM NAME="#OcaTelNo1"      TYPE="%-6s" />	<!--柜员号-->
  <ITEM NAME="#OcaSupv1"       TYPE="%-6s" />	<!--授权柜员-->
  <ITEM NAME="#OcaTermId"     TYPE="%-8s" />	<!--交易终端-->
  <ITEM NAME="#OcaActSeq1"     TYPE="%-4s" />	<!--帐户序号-->
  <ITEM NAME="#OcaActType1"    TYPE="%-16s" />	<!--帐户种类-->         
  <ITEM NAME="#OcaIntRat1"     TYPE="%-8s" />	<!--利率-->         
  <ITEM NAME="#OcaIntDate1"    TYPE="%-8s" />	<!--起息日-->
  <ITEM NAME="#OcaDueDate1"    TYPE="%-8s" />	<!--到期日-->
  <ITEM NAME="#caCurBal1"      TYPE="%-13s" />	<!--当前余额-->
  <ITEM NAME="#cFiller"       TYPE="%-1s" />	<!--分隔符-->
 
 <!--第三部分:卡内转帐委托书打印 -->
  <ITEM NAME="#OcaName3"      TYPE="%-20s" FUNCTIONS="T(#PFLAG=2)"/>     <!--持卡人姓名 -->
  <ITEM NAME="#OcaCdNo"       TYPE="%-19s" />   <!--卡号 -->
  <ITEM NAME="#OcaCur"        TYPE="%-3s" />    <!--币种 -->
  <ITEM NAME="#OcaActNo"      TYPE="%-21s"/>    <!--帐号 -->
  <ITEM NAME="#OcaActSeq"     TYPE="%-4s"/>	<!-- 转出帐户序号-->      
  <ITEM NAME="#OcaActType"    TYPE="%-16s"/>    <!--  转出帐户性质 -->      
  <ITEM NAME="#OcaTxnType"    TYPE="%-8s"/>     <!--  转出交易类别 -->      
  <ITEM NAME="#OcaSavType"    TYPE="%-8s"/>     <!--  转出储种 -->          
  <ITEM NAME="#OcaSavPerd"    TYPE="%-4s"/>     <!--  转出帐户存期 -->      
  <ITEM NAME="#OcaIntRat"     TYPE="%-9s"/>     <!--  转出利率 -->          
  <ITEM NAME="#OcaOpenDate"   TYPE="%-8s"/>     <!--  转出帐户开户日期 -->  
  <ITEM NAME="#OcaIntDate"    TYPE="%-8s"/>     <!--  转出帐户起息日 -->    
  <ITEM NAME="#OcaDueDate"    TYPE="%-8s"/>     <!--  转出帐户到期日 -->    
  <ITEM NAME="#OcaWithdPerd"  TYPE="%-1s"/>     <!--  转出帐户取息周期 -->  
  <ITEM NAME="#OcaAmt"        TYPE="%-13s"/> 	<!--  转出金额 -->  
  <ITEM NAME="#OcaCurBal"     TYPE="%-13s"/>    <!--  转出当前余额 -->      
  <ITEM NAME="#OcaAgaActNo"   TYPE="%-21s"/>    <!--  转入帐号 -->          
  <ITEM NAME="#OcaAgaActSeq"  TYPE="%-4s"/>     <!--  转入帐户序号 -->      
  <ITEM NAME="#OcaAgaActType" TYPE="%-16s"/>    <!--  转入帐户性质 -->      
  <ITEM NAME="#OcaAgaTxnType" TYPE="%-8s"/>     <!--  转入交易类别 -->      
  <ITEM NAME="#OcaAgaSavType" TYPE="%-8s"/>     <!--  转入储种 -->          
  <ITEM NAME="#OcaAgaSavPerd" TYPE="%-4s"/>     <!--  转入帐户存期 -->      
  <ITEM NAME="#OcaAgaIntRat"  TYPE="%-9s"/>     <!--  转入利率 -->          
  <ITEM NAME="#OcaAgaOpenDate" TYPE="%-8s"/>    <!--  转入帐户开户日期 -->  
  <ITEM NAME="#OcaAgaIntDate" TYPE="%-8s"/>     <!--  转入帐户起息日 -->    
  <ITEM NAME="#OcaAgaDueDate" TYPE="%-8s"/>     <!--  转入帐户到期日 -->    
  <ITEM NAME="#OcaWithdPerd"  TYPE="%-1s"/>     <!--  转入帐户取息周期 -->  
  <ITEM NAME="#OcaAgaAmt"     TYPE="%-13s"/>    <!--  转入金额 -->          
  <ITEM NAME="#OcaAgaCurBal"  TYPE="%-13s"/>    <!--  转入当前余额 -->      
  <ITEM NAME="#OcaTelNo"      TYPE="%-8s"/>     <!--  柜员号 -->    
  <ITEM NAME="#OcaSupvNo"     TYPE="%-8s"/>     <!--  主管号 -->            
  <ITEM NAME="#OcaOpSeq"      TYPE="%-6s"/> 	<!--  操作序号 -->  
  <ITEM NAME="#OcaOpDate"     TYPE="%-8s"/>     <!--  交易日期 -->          
  <ITEM NAME="#OcaTxnCode3"   TYPE="%-4s"/>     <!--  交易代码 -->          
  <ITEM NAME="#OcaCurName"    TYPE="%-16s"/>    <!--  币种名称 -->          
  <ITEM NAME="#OcFlag"        TYPE="%-1s"/>  	<!--  计息标志: 0 -->
  <ITEM NAME="#OcaIntAmt"     TYPE="%-13s"/>    <!--  利息金额 -->          
  <ITEM NAME="#OcaIntTaxRat"  TYPE="%-11s"/>    <!--  利息税率 -->          
  <ITEM NAME="#OcaIntTax"     TYPE="%-13s"/>    <!--  利息税 -->            
  <ITEM NAME="#OcaPureAmt"    TYPE="%-13s"/>    <!--  税后息 -->            
  <ITEM NAME="#OcaCertNo"     TYPE="%-20s"/>    <!--  证件号码 -->          
  <ITEM NAME="#OcFiller"      TYPE="%-1s" />	<!--  结束符 -->    
 
<!-- 输出变量结束 -->
</VARIABLE>

<REQUEST><PACKAGE TYPE="SCRIPT" SERVICE="dh47010"> 
$APCTLHD#HTRMSEQ$HEJFNO#OPNBR$KINBR$TRMSEQ$TXTNO#TASKID$TRMTYP#APTYPE#TXNO$PTYPE
  #DSCPT$HCODE#ACTNO$TXTYPE$MULTTX#CRDB$SUPCD1$SUPCD2$REASON$TLRNO
  #NBCD$TRNMOD$SBTMOD$CURCD$XCRT#TXAMT
  #SCARDNO#ECARDNO
  </PACKAGE></REQUEST>    

<RESPONSE>            
<PACKAGE>
    C00011#OcaActNo#OcaBal#OcaInt#OcaTotalAmt
</PACKAGE>

<PACKAGE>
    C00021#OcaTxnCode#OcaCdNo1#OcaName2#OcaActNo1#OcaSavType1#OcaTxnType1#OcaTxnCur1#OcaTxnCurName1#OcaAmt1#OcaInt1#OcaOpSeq1#OcaDate1#OcaTime1#OcaACE1#OcaTelNo1#OcaSupv1#OcaTermId#OcaActSeq1#OcaActType1#OcaIntRat1#OcaIntDate1#OcaDueDate1#caCurBal1#cFiller
</PACKAGE>

<PACKAGE>
    C00031#OcaName3#OcaCdNo#OcaCur#OcaActNo#OcaActSeq#OcaActType#OcaTxnType#OcaSavType#OcaSavPerd#OcaIntRat#OcaOpenDate#OcaIntDate#OcaDueDate#OcaWithdPerd#OcaAmt#OcaCurBal#OcaAgaActNo#OcaAgaActSeq#OcaAgaActType#OcaAgaTxnType#OcaAgaSavType#OcaAgaSavPerd#OcaAgaIntRat#OcaAgaOpenDate#OcaAgaIntDate#OcaAgaDueDate#OcaWithdPerd#OcaAgaAmt#OcaAgaCurBal#OcaTelNo#OcaSupvNo#OcaOpSeq#OcaOpDate#OcaTxnCode3#OcaCurName#OcFlag#OcaIntAmt#OcaIntTaxRat#OcaIntTax#OcaPureAmt#OcaCertNo     
</PACKAGE>

</RESPONSE>
 
<OUTPUT>
<PACKAGE>
    001#PFLAG=0C420900A主帐户现金销户N505$TXTNO#OcaActNo#OcaBal#OcaInt#OcaTotalAmt
</PACKAGE>

<PACKAGE>
    002#PFLAG=1C420900B主帐户现金销户利息清单N205#OcaName2#OcaDate#OcaCdNo#OcaActSeq#OcaActNo#OcaActType#OcaSavType#OcaSavPerd#OcaIntRat#OcaAmt#OcaIntDate#OcaDueDate#OcaIntAmt#OcaIntTaxRat$TXTNO#OcaIntTax#OcaPureAmt#OcaTelNo#OcaTelNo
</PACKAGE>

<PACKAGE>
    003#PFLAG=2C420900C客户通知书N205#OcaTermId#OcaOpDate$TXTNO#OcaOpDate$TXTNO#OcaCdNo#OcaActNo#OcaCdNo#OcaName3#OcaIntDate#OcaSavPerd#OcaAgaIntRat#OcaName3#OcaAgaAmt#OcaAgaCurBal#OcaOpDate#OcaIntAmt#OcaIntTax#OcaPureAmt#OcaSavPerd#OcaAgaAmt#OcaTelNo#OcaAgaCurBal#OcaIntAmt#OcaIntTax#OcaPureAmt
</PACKAGE>

