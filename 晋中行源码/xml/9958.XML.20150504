<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110110" TITLE="9958 短信业务信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
	initfd();
	commsend_001();
	$FD38=#ZH;
	$FD29=#JYRQ;
	$FD25=#CNAME;
	$FD44=#JYRQQ;
	$FD16='9015';
}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
}"
</COMMRECV>
<VARIABLE>
	<ITEM LINE="3"  COL="3" TYPE="%-44s" FUNCTIONS=
    "T(9958                        短信业务信息查询)"/>
  <ITEM LINE="4"  COL="3" TYPE="%-74s" FUNCTIONS=
  	"T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS=T($TXNO=9015)"/>	
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  
  <!--输出变量--->
    <BLOCK NAME="COMMON" LINE="7" COL="25">
  <ITEM LINE="3"  COL="0"  TYPE="%-10s" FUNCTIONS="T(帐    号:)"/>
  <ITEM LINE="4"  COL="0"  TYPE="%-10s"  FUNCTIONS="T(户    名:)"/>

  <ITEM LINE="8" COL="0"  TYPE="%-10s" FUNCTIONS="T(签约日期:)"/>
  <ITEM LINE="8" COL="17" TYPE="%-3s"  FUNCTIONS="T( → )"/>
  <ITEM NAME="#ZH" INPUT="TEXT" LINE="3" COL="10" TYPE="%-19s" DEVICE="MSR" FUNCTIONS="M($MSR2,1)">
  <ONLOSTFOCUS>
  "{
  			if('' == delspace(#ZH)){return(true);}
         initfd();
         commsend_001();
         $FD16='6700';
         $FD30=#ZH;
         $FD67='1';
         callserver();
         if($FD12!='0000')
         {
            showmsg(geterror());
            return(false);
         }
		   #CNAME=$FD25;//返回客户名
		   showmsg('#CNAME='+#CNAME);
		   show(#CNAME,true);
      
  }"
  </ONLOSTFOCUS>       
  </ITEM>


  <ITEM LINE="4"  COL="10" TYPE="%-40" NAME="#CNAME" INPUT="LABEL" VISABLE="FALSE"  FUNCTIONS=""/>

	
  <ITEM NAME="#JYRQ" INPUT="TEXT" LINE="8" COL="10" TYPE="%-8s" FUNCTIONS="T()"/>
  <ITEM NAME="#JYRQQ" INPUT="TEXT" LINE="8" COL="20" TYPE="%-8s" FUNCTIONS="T()"/>
  
</BLOCK>  
  
  <!------------一些没有用的变量,只是为了公共脚本执行时不报错------------>
  	 <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  
<ITEM LINE="18" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>

</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>

