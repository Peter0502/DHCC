		<OPTION VALUE="Y" TITLE="是"/>
		<OPTION VALUE="N" TITLE="否"/>
	</SELECT>
	<ONLOSTFOCUS>
	"{
		if(#TXNO=='2104' and #AUTOSVCD=='Y') //一卡通满足本金转定期，利息转活期
