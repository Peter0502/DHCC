   <ITEM TYPE="%-1s" NAME="#FIRSTINIT" >
	<ONLOSTFOCUS>"{
		if(#CASHCD=='2')
		{	  
			show(#SUBMIT_1,false);
			show(#PAGETRAN,true);
		}
		else
		{
			show(#PAGETRAN,false);
			show(#SUBMIT_1,true);
		}
		
		
	}"
	</ONLOSTFOCUS>
  </ITEM>
  
  <ITEM NAME="#LPRODNO" INPUT="LABEL" LINE="6"  COL="11" TYPE="%-10s" FUNCTIONS="T(产品代码:!)" VISABLE="TRUE"/>
  <ITEM LINE="6"  COL="21" TYPE="%-3s"  NAME="#PRODNO" INPUT="SELECT" FUNCTIONS="V(NB)" VISABLE="TRUE">
	<ONLOSTFOCUS>"{
		dim @custname as string;
		dim @showactno as string;
		
		@custname=#CUSTNAME;
		@showactno=#SHOWACTNO20;
		initfd();
		commsend_001();
		$FD16='9844';
		$FD103=#PRODNO;
		callserver();
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}
		commrecv_103();
		showblock('PRODINFO',true,true);
		
		// 整整要求输入自动转存标志
		if(#PRODNO &gt; '200' and #PRODNO &lt; '209')
		{
			show(#SAUTOSVCD,true);
			show(#AUTOSVCD,true);
		}else{
			show(#SAUTOSVCD,false);
			show(#AUTOSVCD,false);
			#AUTOSVCD='N';
		}
		#CUSTNAME=@custname;
		#SHOWACTNO20=@showactno;
	}"
	</ONLOSTFOCUS>
  </ITEM>
