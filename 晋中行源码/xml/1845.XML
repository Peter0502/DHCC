<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111011100000000000001100010" TITLE="1845        交易状态同步"
	xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>
"{
	refresh();
	initfd();
	commsend_001();
}"

</COMMSEND>

<COMMRECV>
"{
	commrecv_001();
}"
</COMMRECV>

<VARIABLE>
	
	<ITEM LINE="3" COL="3" TYPE="%-60s" FUNCTIONS="T(1845                         交易状态同步 )"/>
	<ITEM LINE="4" COL="3"  TYPE="%-74s" FUNCTIONS="T(--------------------------------------------------------------------------)"/>
	<ITEM NAME="#TXNAME"   TYPE="%-30s"  FUNCTIONS="T(交易状态同步业务)"/>
	<ITEM LINE="6" COL="3" TYPE="%-10s" FUNCTIONS="T(交易序号：)"/>
	<ITEM LINE="6" COL="13" NAME="#TxNo" TYPE="%-20s" INPUT="TEXT" FUNCTIONS="T()V(NB)"/>
	<!--ITEM LINE="6" COL="35" TYPE="%-10s" FUNCTIONS="T(交易日期：)"/>
	<ITEM LINE="6" COL="45" NAME="#TxDate" TYPE="%-8s" INPUT="TEXT" FUNCTIONS="T()D()V(NB)"/-->

	<ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="BUTTON" >
		<ONCLICK>
		"{
			initfd();

			commsend_001();
			$FD16 = '2050';
			$FD18 = 'pbp';
			$FD28 = 'sxps';
			$FD32 = #TxNo;
			
			callagnpbp();
			
			if($FD12 != '0000')
			{
				showmsg(geterror());
				return(false);
			}
			runoutput();
			refresh();
		}"
		</ONCLICK>
	</ITEM>

</VARIABLE>

<RESPONSE>
<PACKAGE DEVICE="SCREEN" DESC="按任意键退出">
NOTHING
</PACKAGE>
        <PRINT>
                "{
			PrintTxHead();

			print(10,30, '交易结果：[12]', $FD13);
			
			PrintTxTail();
		}"
        </PRINT>
</RESPONSE>
</TRANSACTION>

