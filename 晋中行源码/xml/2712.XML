  <SELECT>
  	<OPTION VALUE="A" TITLE="0.全部"/>
		<OPTION VALUE="1" TITLE="1.配偶"/>
		<OPTION VALUE="2" TITLE="2.父母"/>
		<OPTION VALUE="3" TITLE="3.子女"/>
		<OPTION VALUE="4" TITLE="4.其他血亲"/>
		<OPTION VALUE="5" TITLE="5.其他姻亲"/>		
	</SELECT>
  </ITEM>
  <ITEM NAME="#JTBH" INPUT="TEXT" LINE="3" COL="10" TYPE="%4d"  FUNCTIONS=""/>
  <ITEM NAME="#NAME" 	INPUT="TEXT" LINE="4" COL="10" TYPE="%-20s" 	FUNCTIONS=""/>
 
  
  <ITEM NAME="#S" TYPE="%-30s" FUNCTIONS="">
   <ONLOSTFOCUS>
    "{   //获取证件类型
  		dim @txno_bak as string;
			dim @tota as string;
			@txno_bak=$TXNO;
			$TXNO='9799';
			$FD67='1';
			commsend_001();
			@tota=callserver();
			if($FD12 != '0000')
			{
				showmsg(geterror());
				return(false);
			}
		
			$TXNO=@txno_bak;
			setsel(#ZJLX,@tota,'o5t20o1v1o2');
			}"
  	</ONLOSTFOCUS>
  </ITEM>
  
   <ITEM NAME="#CYXB" INPUT="SELECT" LINE="5" COL="10" TYPE="%-1s" 	FUNCTIONS="T(A)">
  	<SELECT>
  	<OPTION VALUE="A" TITLE="0.全部"/>
		<OPTION VALUE="M" TITLE="1.男"/>
		<OPTION VALUE="F" TITLE="2.女"/>
	</SELECT>
  </ITEM>
  
  <ITEM NAME="#ZJLX" INPUT="SELECT" LINE="6" COL="10" TYPE="%-50s" FUNCTIONS="T()"> 	
  	<SELECT>
  		<OPTION VALUE="1" TITLE="1.身份证">
  	</SELECT>
  </ITEM>
  <ITEM NAME="#ZJHM" INPUT="TEXT" LINE="7" COL="10" TYPE="%-20s" FUNCTIONS="">
  <ONLOSTFOCUS>
  	"{
  	dim @l as number;
		@l = len(#ZJHM);
		if('1' == #ZJLX){
    		if(@l == 15 or  @l == 18){
		}else{
	  		showmsg('身份证号码位数错误');
        	return(false);
			}
    	}
	}"
	</ONLOSTFOCUS>
	</ITEM>
 
  </BLOCK>
  <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT">
  </ITEM>
