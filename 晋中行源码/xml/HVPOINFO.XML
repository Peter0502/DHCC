<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111111" TITLE="T(银行汇票详细信息)"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">

<VARIABLE>

  <ITEM LINE="3"  COL="3" TYPE="%-52s" FUNCTIONS=
    "T(HVPOINFO                         汇票基本信息)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
    "T(─────────────────────────────────────)"/>
  <ITEM LINE="05"  COL="07" TYPE="%-09s"  FUNCTIONS="T(汇票号码:)"/>
  <ITEM LINE="06"  COL="07" TYPE="%-09s"  FUNCTIONS="T(签发行号:)"/>
  <ITEM LINE="07"  COL="07" TYPE="%-09s"  FUNCTIONS="T(汇票种类:)"/>
  <ITEM LINE="07"  COL="47" TYPE="%-09s"  FUNCTIONS="T(转出类型:)"/>

  <ITEM LINE="08"  COL="07" TYPE="%-12s"  FUNCTIONS="T(签发日期:)"/>
  <ITEM LINE="08"  COL="47" TYPE="%-12s"  FUNCTIONS="T(登记日期:)"/>
  <ITEM LINE="09"  COL="07" TYPE="%-12s"  FUNCTIONS="T(汇票状态:)"/>
  <ITEM LINE="10"  COL="07" TYPE="%-16s"  FUNCTIONS="T(兑付行号:)"/>
  <ITEM LINE="11"  COL="07" TYPE="%-12s"  FUNCTIONS="T(签发金额:)"/>
  <ITEM LINE="12"  COL="07" TYPE="%-12s"  FUNCTIONS="T(付款人帐号:)"/>
  <ITEM LINE="13"  COL="07" TYPE="%-12s"  FUNCTIONS="T(付款人名称:)"/>
  <ITEM LINE="14"  COL="07" TYPE="%-12s"  FUNCTIONS="T(收款人帐号:)"/>
  <ITEM LINE="15"  COL="07" TYPE="%-12s"  FUNCTIONS="T(收款人名称:)"/>
  <ITEM LINE="16"  COL="07" TYPE="%-12s"  FUNCTIONS="T(附      言:)"/>
  
  <ITEM LINE="5"   COL="19" NAME="#PO_NO" 	TYPE="%-08s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="6"   COL="19" NAME="#PAY_BR_NO" 	TYPE="%-12s"   FUNCTIONS="V(NB)T($BUFFER,35,12)"/>

  <ITEM NAME="#PO_TYPE" 			TYPE="%-01s"   FUNCTIONS="V(NB)T($BUFFER,974,1)">
  <ONLOSTFOCUS>
  "{
	switch(#PO_TYPE)
	{
		case '0':
			#POTYPE_DESP = '可转让汇票';
			break;
		case '1':
			#POTYPE_DESP = '不可转让汇票';
			break;
		case '2':
			#POTYPE_DESP = '现金汇票';
			break;
			
	}
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="7"   COL="19" NAME="#POTYPE_DESP"	TYPE="%-16s"   FUNCTIONS=""/>
  <ITEM NAME="#DEAL_MODE"			TYPE="%-1s"   FUNCTIONS=""><!--现金还是转帐处理方式-->
  <ONLOSTFOCUS>
  "{
	switch(#DEAL_MODE)
	{
		case '0':
			#DEAL_MODE_DESP = '现金方式';
			break;
		case '1':
			#DEAL_MODE_DESP = '本行帐号';
			break;
		case '2':
			#DEAL_MODE_DESP = '系统内科目';
			break;
		default:
			#DEAL_MODE_DESP = '错误的现金/转帐处理方式';			
	}
   }"
  </ONLOSTFOCUS>
  </ITEM>
  <ITEM LINE="7"   COL="56" NAME="#DEAL_MODE_DESP"	TYPE="%-16s"   FUNCTIONS="V(NB)"/>

  <ITEM LINE="8"   COL="19" NAME="#SIGN_DATE"   TYPE="%-8s"   FUNCTIONS="V(NB)"/>
  <ITEM LINE="8"   COL="56" NAME="#REG_DATE"    TYPE="%-8s"   FUNCTIONS="V(NB)"/>

  <ITEM NAME="#PO_STS"    		TYPE="%-1s"   FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
	HV_SET_POSTS_NAME();
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="9"   COL="19" NAME="#PO_STS_DESP"    	TYPE="%-14s"   FUNCTIONS=""/>


  <ITEM NAME="#TMP_TXAMT" TYPE="%16s" FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
		#HTXAMT = atoi(#TMP_TXAMT);			
   }"
   </ONLOSTFOCUS>
   </ITEM> 
  <ITEM NAME="#HTXAMT" TYPE="%16.2f"   FUNCTIONS="">
  <ONLOSTFOCUS>
  "{
		#HTXAMT = #HTXAMT/100;
   }"
   </ONLOSTFOCUS>
   </ITEM>
  <ITEM LINE="10"  COL="19" NAME="#CASH_OPN_BR_NO" TYPE="%-12s"   FUNCTIONS="T($BUFFER,227,12)"/>

  <ITEM LINE="11"  COL="19" NAME="#LHTXAMT" TYPE="%-20s"   FUNCTIONS="X(#HTXAMT)"/>

 

  <ITEM LINE="12"  COL="19" NAME="#PAYACTNO"   TYPE="%-32s"  FUNCTIONS="T($BUFFER,47,32)"/>
  
   
  <ITEM LINE="13"  COL="19" NAME="#PAYNAME"   TYPE="%-60s"  FUNCTIONS="V(NB)T($BUFFER,79,60)"/>

  <ITEM LINE="14"  COL="19" NAME="#CASHACTNO"  TYPE="%-32s" FUNCTIONS="T($BUFFER,239,32)"/>

  <ITEM LINE="15"  COL="19" NAME="#CASHNAME"   TYPE="%-60s" FUNCTIONS="V(NB)T($BUFFER,271,60)"/>
  <ITEM LINE="16"  COL="19" NAME="#BRF"    TYPE="%-60s" FUNCTIONS="T($BUFFER,401,60)"/>

  
  <ITEM LINE="21"  COL="65" NAME="#CLOSE"  INPUT="BUTTON" TYPE="%-6s" FUNCTIONS="T(确 定)">
  <ONCLICK>"{
  	close();
  }"
  </ONCLICK>
  </ITEM>
  
</VARIABLE>
</TRANSACTION>
