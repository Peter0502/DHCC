  <ITEM LINE="11"  COL="22" TYPE="%-20s"  NAME="#THDNUM"   DEVICE="KEYBOARD" FUNCTIONS="T( )"/>

  <ITEM LINE="15"  COL="22" TYPE="%16.2f" NAME="#THAVBAL1"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)J(#XTHAVBAL1)"/>
  <ITEM TYPE="%-16s" NAME="#XTHAVBAL1" FUNCTIONS="X(#THAVBAL1)"/>
  <ITEM LINE="15"  COL="48" TYPE="%16.2f" NAME="#THAVBAL2"  DEVICE="KEYBOARD" FUNCTIONS="V(NB)J(#XTHAVBAL2)"/>
