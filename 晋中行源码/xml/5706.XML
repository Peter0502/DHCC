  <ITEM LINE="11" COL="10"  TYPE="%-12s" FUNCTIONS="T(提货单号码:)"/>
  <ITEM LINE="15" COL="10"  TYPE="%-12s" FUNCTIONS="T(提货单金额:)"/>
