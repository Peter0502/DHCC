<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="00000000" TITLE="5706 提货单信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
		initfd();
		commsend_001();
        $MSGTYPE='03001';
        dim @baktxno as string;
        @baktxno=$TXNO;
        $TXNO = '9986';
        $FD12 = '4342';
        $FD38 = #THDNUM;
        $FD39 = space(16);  
        $FD40 = itoa(val(#XTHAVBAL1,100),'%016d');  
        $FD41 = itoa(val(#XTHAVBAL2,100),'%016d');  
        commsend_001();
        callserver();
        $TXNO=@baktxno;
        if($FD12!='0000'){
            showmsg(geterror());
            return(false);
        }
        $FD16 = '4342';
        $MSGTYPE='03001';

}"
</COMMSEND>
<COMMRECV>"{
	commrecv_001();
	$PBLINE='05';
	$KINDNO='02';
}"
</COMMRECV>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(5706                            提货单信息查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>

  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4342)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>

  <ITEM LINE="6"  COL="10"  TYPE="%-14s" FUNCTIONS="T([查询条件])"/>
