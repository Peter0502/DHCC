		//if(substr(#ZH,0,1)=='9'){
		  #HM=$FD26;
		//}else{
		//  #HM=$FD25;
		//}
		<!--end 20121211-->
		#BZ=$FD21;
		#YE=val($FD41, 0.01);
		enable(#HM, false);
		enable(#BZ, false);
		enable(#YE, false);
  }"
