  "{
		dim @txno as string;

		if(substr(#ZH,0,3)=='407' or substr(#ZH,0,3)=='416'){
			showmsg('不可以记此科目的帐务!');
			return(false);
		}
		if(substr(#ZH,0,3)=='408'){
			if($KINBR!='10001'){
				showmsg('不可以记此科目的帐务!');
				return(false);
			}
		}

		$FD30=#ZH;
		if(substr(#ZH,0,1)=='9'){
		  $FD34='0000';
		}
		else{
		  $FD34='0001';
		}
		@txno=$TXNO;
		$TXNO='9740';
		commsend_001();

		callserver();
		$TXNO = @txno;
		if($FD12!='0000'){
			showmsg(geterror());
			return(false);
		}

		if($FD72 == '1')
		{
			showmsg('代理单位代理户,未解除代理关系,不允许销户!');
			return(false);
		}
		<!--修改sp9740.c内部账户从第26域取值，其他从第25域取值的问题 20121211-->
