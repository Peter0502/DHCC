<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11111001" TITLE="4704 理财产品参数查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
<COMMSEND>"{
    commsend_001();
}"
</COMMSEND>
<COMMRECV>"{
commrecv_001();
}"
</COMMRECV>
<FIRSTWORK>
"{
	 #CPZT = 'A';
}"
</FIRSTWORK>
<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-54s" FUNCTIONS=
    "T(4704                          理财产品参数查询)"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
  <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=1701)"/>
  <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
  <!--显示变量-->
  <BLOCK NAME="COMMON" LINE="8" COL="3">
  <ITEM LINE="0" COL="5"  TYPE="%-15s"  FUNCTIONS="T(产品代码:)"/>
  <ITEM LINE="1" COL="5"  TYPE="%-15s"  FUNCTIONS="T(产品名称:)"/>
  <ITEM LINE="2" COL="5"  TYPE="%-15s"  FUNCTIONS="T(归还收益日期:)"/>
  <ITEM LINE="3" COL="5"  TYPE="%-15s"  FUNCTIONS="T(产品状态:)"/>
  <!--输入内容变量-->
  <ITEM NAME="#CPDM" INPUT="TEXT"   LINE="0" COL="20" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#CPMC" INPUT="TEXT"   LINE="1" COL="20" TYPE="%-20s" FUNCTIONS=""/>
  <ITEM NAME="#SYRQ" INPUT="TEXT"   LINE="2" COL="20" TYPE="%-10s" FUNCTIONS=""/>
  <ITEM NAME="#CPZT" INPUT="SELECT" LINE="3" COL="20" TYPE="%-10s" FUNCTIONS="">
    <SELECT>
        <OPTION VALUE="A" TITLE="0.全部"/>
        <OPTION VALUE="0" TITLE="1.募集期"/>
        <OPTION VALUE="1" TITLE="2.正在运行"/>
        <OPTION VALUE="2" TITLE="3.分配客户收益"/>
        <OPTION VALUE="3" TITLE="4.募集失败"/>
        <OPTION VALUE="4" TITLE="5.暂停认购"/>
        <OPTION VALUE="5" TITLE="6.产品成立"/>
        <OPTION VALUE="7" TITLE="7.分配支行收益录入"/>
        <OPTION VALUE="8" TITLE="8.分配支行收益"/>
    </SELECT>
  </ITEM>
  </BLOCK>
  <ITEM LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(查  询)" INPUT="BUTTON">
    <ONCLICK>"{
    	dim @tota as string;
    	initfd();
			commsend_001();
    	$FD16='8508';
    	$FD65='0001';
    	$FD63=#CPDM;
    	$FD25=#CPMC;
    	$FD46=#SYRQ;
    	if(#CPZT == 'A')
    	{
    		$FD66='';
    	}
    	else
    	{
    		$FD66=#CPZT;
    	}
    	@tota=callserver();
	    if($FD12 != '0000')
	    {
	    	showmsg(geterror());
	    	return(false);
	    }
	    autolist(@tota);
	    list_work(); 
  	}"
  	</ONCLICK>
 </ITEM>
</VARIABLE>
<REQUEST>
</REQUEST>
<RESPONSE>
<PACKAGE>
 NOTHING
</PACKAGE>
</RESPONSE>
<OUTPUT>
</OUTPUT>
</TRANSACTION>
