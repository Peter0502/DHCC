<?xml version="1.0" encoding="GBK" ?>
<TRANSACTION RIGHTS="11110000" TITLE="6825 再贴现信息查询"
  xmlns="http://www.otd.org/MENU" xmlns:W3CNS="http://www.w3c.org">
  <COMMSEND>
  "{
	   initfd();
	   $MSGTYPE='03001';
	   dim @baktxno as string;
	   @baktxno=$TXNO;
	   $FD12='4946';
	   $TXNO='9986';
	   $FD24=#PJLX;
       $FD26=#TXSQRHM;
       $FD29=#TXR;
	   $FD38=#XYBH;
	   $FD40=itoa(val(#LHPJES,100),'%016d');
	   $FD41=itoa(val(#LHPJEE,100),'%016d');
	   $FD44=#HPDQR;
	   $FD51=#ZTTS;
	   $FD60=#PJH;
	   $FD67=#YDTCBZ;
	   $FD68=#ZT;
	   $FD70=#ZZLX;
	   $FD84=getitems('#ZTXLV');
	   commsend_001();
	   callserver();
	   $TXNO=@baktxno;
	   if($FD12!='0000'){
			   showmsg(geterror());
			   return(false);
		   }
	   $MSGTYPE='03001';
	   $FD16='4946';
	}"
</COMMSEND>
<COMMRECV>
"{
	
}"
</COMMRECV>

<VARIABLE>
  <ITEM LINE="3"  COL="3" TYPE="%-60s" FUNCTIONS=
    "T(6825                         再贴现信息查询  )"/>
  <ITEM LINE="4"  COL="3"  TYPE="%-74s" FUNCTIONS=
   "T(─────────────────────────────────────)"/>
   <ITEM NAME="#TXNO" TYPE="%-4s" FUNCTIONS="T($TXNO=4946)"/>
   <ITEM NAME="#MSGTYPE" TYPE="%-5s" FUNCTIONS="T($MSGTYPE=03001)"/>
   <!--输入变量-->

   <ITEM LINE="6"   COL="3" TYPE="%-70s" FUNCTIONS="T(    -------------------------【查询条件】------------------------------)"/>

  <ITEM LINE="8"   COL="7" TYPE="%-14s" FUNCTIONS="T(状    态:!)"/>
  <ITEM LINE="9"   COL="7" TYPE="%-15s" FUNCTIONS="T(再贴现协议编号:)"/>
  <ITEM LINE="10"   COL="7" TYPE="%-15s" FUNCTIONS="T(贴现申请人户名:)"/>
  <ITEM LINE="11"   COL="7" TYPE="%-14s" FUNCTIONS="T(转账类型:!)"/>
  <ITEM LINE="11"   COL="41" TYPE="%-14s" FUNCTIONS="T(异地同城标志:!)"/>
   
  <ITEM LINE="13"   COL="41" TYPE="%-14s" FUNCTIONS="T(贴现率:)"/>
  <ITEM LINE="13"   COL="7" TYPE="%-14s" FUNCTIONS="T(在途天数:)"/>
    
  <ITEM LINE="16"   COL="7" TYPE="%-14s" FUNCTIONS="T(票据类型:)"/>
  <ITEM LINE="16"   COL="41" TYPE="%-14s" FUNCTIONS="T(票 据 号:)"/>
  <ITEM LINE="17"   COL="7" TYPE="%-14s" FUNCTIONS="T(汇票金额:)"/>
  <ITEM LINE="17"   COL="39" TYPE="%-4s" FUNCTIONS="T(→)"/>

  <ITEM LINE="18"   COL="7" TYPE="%-14s" FUNCTIONS="T(贴 现 日:)"/>
  <ITEM LINE="18"   COL="41" TYPE="%-14s" FUNCTIONS="T(汇票到期日:)"/>
  <ITEM NAME="#ZT" INPUT="SELECT" LINE="8"  COL="19" TYPE="%-1s" 	FUNCTIONS="T()">
   	<SELECT>
  		<OPTION VALUE="*" TITLE="销户 "/>
  		<OPTION VALUE="1" TITLE="录入 "/>
	    <OPTION VALUE="2" TITLE="正常 "/>
  	</SELECT>
  </ITEM>
  <ITEM NAME="#XYBH"	 LINE="9" COL="22" TYPE="%-20s" INPUT="TEXT"	FUNCTIONS="T()">

  </ITEM>
  	  <ITEM NAME="#TXSQRHM"	 LINE="10" COL="22" TYPE="%-50s" INPUT="TEXT"	FUNCTIONS="T()"/>
   <ITEM NAME="#ZZLX" INPUT="SELECT" LINE="11"  COL="19" TYPE="%-1s" 	FUNCTIONS="T()">
  	<SELECT>
  		<OPTION VALUE="1" TITLE="本行 "/>
  		<OPTION VALUE="2" TITLE="同城 "/>
	    <OPTION VALUE="3" TITLE="大额 "/>
		<!--
	              <OPTION VALUE="" TITLE="全部"/>
	  -->
 
    </SELECT>
    </ITEM>
	   <ITEM NAME="#YDTCBZ" INPUT="SELECT" LINE="11"  COL="57" TYPE="%-1s" 	FUNCTIONS="T()">
  	<SELECT>
  		<OPTION VALUE="0" TITLE="本地 "/>
  		<OPTION VALUE="1" TITLE="异地 "/>
		<!--
 		 <OPTION VALUE="" TITLE="全部"/>
		 -->

  	</SELECT>
	    </ITEM>

	  <ITEM NAME="#ZTTS"	 LINE="13" COL="19" TYPE="%3d" INPUT="TEXT"	FUNCTIONS="T()"/>
	  <ITEM NAME="#ZTXLV"	 LINE="13" COL="51" TYPE="%9.6f" INPUT="TEXT"	FUNCTIONS="T()"/>


   <ITEM NAME="#PJLX" INPUT="SELECT" LINE="16"  COL="17" TYPE="%-3s" 	FUNCTIONS="T()">
  	<SELECT>
  		<OPTION VALUE="111" TITLE="银行承兑汇票 "/>
  		<OPTION VALUE="112" TITLE="商业承兑汇票 "/>
		<!--
	           <OPTION VALUE="" TITLE="全部"/>
		 -->


  	</SELECT>
	</ITEM>
    <ITEM NAME="#PJH"	 LINE="16" COL="51" TYPE="%-16s" INPUT="TEXT"	FUNCTIONS="T()"/>
    <ITEM NAME="#HPJES"	 LINE="17" COL="17" TYPE="%17.2f" INPUT="TEXT"	FUNCTIONS="J(#LHPJES)"/> 
	<ITEM NAME="#LHPJES" TYPE="%-16s" FUNCTIONS="X(#HPJES)"/>
	<ITEM NAME="#HPJEE"	 LINE="17" COL="43" TYPE="%17.2f" INPUT="TEXT"	FUNCTIONS="J(LHPJEE)"/>
	<ITEM NAME="#LHPJEE" TYPE="%-16s" FUNCTIONS="X(#HPJEE)"/>
    <ITEM NAME="#TXR"	 LINE="18" COL="17" TYPE="%8d" INPUT="TEXT"	FUNCTIONS="T()D()"/>

    <ITEM NAME="#HPDQR"	 LINE="18" COL="53" TYPE="%8d" INPUT="TEXT"	FUNCTIONS="T()D()"/>

    <ITEM NAME="#PASSWD" TYPE="%-6s" FUNCTIONS="T()"/>
    <ITEM NAME="#FD" TYPE="%-1s">
  <ONLOSTFOCUS>
  "{
		$FD24=#PJLX;
		$FD26=#TXSQRHM;
		$FD29=#TXR;
		$FD38=#XYBH;
		$FD40=itoa(#HPJES*100, '%016.0f');
		$FD41=itoa(#HPJEE*100, '%016.0f');
		$FD44=#HPDQR;
		$FD51=#ZTTS;
		$FD60=#PJH;
		$FD67=#YDTCBZ;
		$FD68=#ZT;
		$FD70=#ZZLX;
		$FD84=itoa(#ZTXLV*1000000, '%08.0f');

  }"
  </ONLOSTFOCUS>
 </ITEM>

 <ITEM NAME="#SUBMIT" LINE="21" COL="59"  TYPE="%-6s" FUNCTIONS="T(确  定)" INPUT="SUBMIT"/>
