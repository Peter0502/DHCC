#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int cif_addr_inf_con ( struct cif_addr_inf_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct cif_addr_inf_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->addr_type);
	zip_tail(ps->country);
	zip_tail(ps->addr);
	zip_tail(ps->post_code);
}
