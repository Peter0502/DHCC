int Cif_addr_inf_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_addr_inf.* FROM cif_addr_inf WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_addr_inf_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_addr_inf CURSOR FOR cif_addr_inf_upd;

	EXEC SQL OPEN upd_cif_addr_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_addr_inf_Fet_Upd( struct cif_addr_inf_c *cif_addr_inf_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_addr_inf INTO :cif_addr_inf_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_addr_inf finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_addr_inf_c);

	return 0;
}

int Cif_addr_inf_Upd_Upd( struct cif_addr_inf_c cif_addr_inf_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_addr_inf SET cif_no=:cif_addr_inf_c.cif_no,
			addr_seqn=:cif_addr_inf_c.addr_seqn,
			addr_type=:cif_addr_inf_c.addr_type,
			country=:cif_addr_inf_c.country,
			addr=:cif_addr_inf_c.addr,
			post_code=:cif_addr_inf_c.post_code
	WHERE rowid=:cif_addr_inf_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_addr_inf_Del_Upd(struct cif_addr_inf_c cif_addr_inf_c , char * reply  ){

	EXEC SQL DELETE FROM cif_addr_inf WHERE rowid=:cif_addr_inf_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_addr_inf_Clo_Upd( ){
	EXEC SQL CLOSE upd_cif_addr_inf;
	return 0;
}

