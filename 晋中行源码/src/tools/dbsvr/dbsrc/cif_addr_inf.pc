int Cif_addr_inf_Ins( struct cif_addr_inf_c cif_addr_inf_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO cif_addr_inf VALUES (:cif_addr_inf_c.cif_no,
			:cif_addr_inf_c.addr_seqn,
			:cif_addr_inf_c.addr_type,
			:cif_addr_inf_c.country,
			:cif_addr_inf_c.addr,
			:cif_addr_inf_c.post_code);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT cif_addr_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
