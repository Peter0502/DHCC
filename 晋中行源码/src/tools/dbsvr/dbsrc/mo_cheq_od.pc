int Mo_cheq_od_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_cheq_od.* FROM mo_cheq_od WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_cheq_od_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_cheq_od CURSOR FOR mo_cheq_od_upd;

	EXEC SQL OPEN upd_mo_cheq_od;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_cheq_od error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_cheq_od_Fet_Upd( struct mo_cheq_od_c *mo_cheq_od_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_cheq_od INTO :mo_cheq_od_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_cheq_od error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_cheq_od finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_cheq_od_c);

	return 0;
}

int Mo_cheq_od_Upd_Upd( struct mo_cheq_od_c mo_cheq_od_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_cheq_od SET ac_id=:mo_cheq_od_c.ac_id,
			ac_seqn=:mo_cheq_od_c.ac_seqn,
			note_type=:mo_cheq_od_c.note_type,
			note_no=:mo_cheq_od_c.note_no,
			brf=:mo_cheq_od_c.brf,
			tel=:mo_cheq_od_c.tel,
