int Cif_ass_reg_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_ass_reg.* FROM cif_ass_reg WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_ass_reg_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_ass_reg CURSOR FOR cif_ass_reg_upd;

	EXEC SQL OPEN upd_cif_ass_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_ass_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_ass_reg_Fet_Upd( struct cif_ass_reg_c *cif_ass_reg_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_ass_reg INTO :cif_ass_reg_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_ass_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_ass_reg finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_ass_reg_c);

	return 0;
}

int Cif_ass_reg_Upd_Upd( struct cif_ass_reg_c cif_ass_reg_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_ass_reg SET cif_no=:cif_ass_reg_c.cif_no,
			ass_date=:cif_ass_reg_c.ass_date,
			end_date=:cif_ass_reg_c.end_date,
			ass_br_no=:cif_ass_reg_c.ass_br_no,
			ass_tel=:cif_ass_reg_c.ass_tel,
			qt_code=:cif_ass_reg_c.qt_code,
			answer=:cif_ass_reg_c.answer,
			score=:cif_ass_reg_c.score,
			ass_lvl=:cif_ass_reg_c.ass_lvl
	WHERE rowid=:cif_ass_reg_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_ass_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_ass_reg_Del_Upd(struct cif_ass_reg_c cif_ass_reg_c , char * reply  ){

	EXEC SQL DELETE FROM cif_ass_reg WHERE rowid=:cif_ass_reg_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE cif_ass_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_ass_reg_Clo_Upd( ){
