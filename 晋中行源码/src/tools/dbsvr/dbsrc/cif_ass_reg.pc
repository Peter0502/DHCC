/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_ass_reg_Sel(reply,cif_ass_reg_c,fmtstr,va_alist)
char    *reply;
struct  cif_ass_reg_c *cif_ass_reg_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_ass_reg_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_ass_reg_Fet_Sel( cif_ass_reg_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_ass_reg_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_ass_reg_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_ass_reg.* FROM cif_ass_reg WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_ass_reg_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_ass_reg CURSOR FOR cif_ass_reg_sel;

	EXEC SQL OPEN sel_cif_ass_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_ass_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_ass_reg_Fet_Sel ( struct cif_ass_reg_c *cif_ass_reg_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_ass_reg INTO :cif_ass_reg_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_ass_reg error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_ass_reg finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_ass_reg_c);

	return 0;
}

int Cif_ass_reg_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_ass_reg;
	return 0;
}

