int Seqctl_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,seqctl.* FROM seqctl WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE seqctl_upd FROM :comm;

	EXEC SQL DECLARE upd_seqctl CURSOR FOR seqctl_upd;

	EXEC SQL OPEN upd_seqctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Seqctl_Fet_Upd( struct seqctl_c *seqctl_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_seqctl INTO :seqctl_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH seqctl finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(seqctl_c);

	return 0;
}

int Seqctl_Upd_Upd( struct seqctl_c seqctl_c , char * reply){
	int ret;

	EXEC SQL UPDATE seqctl SET br_no=:seqctl_c.br_no,
			ac_ind=:seqctl_c.ac_ind,
			seqno=:seqctl_c.seqno
	WHERE rowid=:seqctl_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Seqctl_Del_Upd(struct seqctl_c seqctl_c , char * reply  ){

	EXEC SQL DELETE FROM seqctl WHERE rowid=:seqctl_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Seqctl_Clo_Upd( ){
	EXEC SQL CLOSE upd_seqctl;
	return 0;
}

/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Seqctl_Ins( struct seqctl_c seqctl_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO seqctl VALUES (:seqctl_c.br_no,
			:seqctl_c.ac_ind,
			:seqctl_c.seqno);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT seqctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
