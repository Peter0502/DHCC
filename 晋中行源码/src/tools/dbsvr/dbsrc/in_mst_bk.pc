int In_mst_bk_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,in_mst_bk.* FROM in_mst_bk WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE in_mst_bk_upd FROM :comm;

	EXEC SQL DECLARE upd_in_mst_bk CURSOR FOR in_mst_bk_upd;

	EXEC SQL OPEN upd_in_mst_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open in_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int In_mst_bk_Fet_Upd( struct in_mst_bk_c *in_mst_bk_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_in_mst_bk INTO :in_mst_bk_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd in_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
