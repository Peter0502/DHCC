		sprintf(acErrMsg,"FETCH in_mst_bk finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(in_mst_bk_c);

	return 0;
}

