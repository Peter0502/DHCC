int Tmp_tx_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,tmp_tx_def.* FROM tmp_tx_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tmp_tx_def_upd FROM :comm;

	EXEC SQL DECLARE upd_tmp_tx_def CURSOR FOR tmp_tx_def_upd;

	EXEC SQL OPEN upd_tmp_tx_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tmp_tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_tx_def_Fet_Upd( struct tmp_tx_def_c *tmp_tx_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tmp_tx_def INTO :tmp_tx_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tmp_tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tmp_tx_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tmp_tx_def_c);

	return 0;
}

int Tmp_tx_def_Upd_Upd( struct tmp_tx_def_c tmp_tx_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE tmp_tx_def SET tx_code=:tmp_tx_def_c.tx_code,
			tx_name=:tmp_tx_def_c.tx_name,
			bit_map=:tmp_tx_def_c.bit_map,
			log_ind=:tmp_tx_def_c.log_ind,
			tx_type=:tmp_tx_def_c.tx_type,
			wrk_sts=:tmp_tx_def_c.wrk_sts
	WHERE rowid=:tmp_tx_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tmp_tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Tmp_tx_def_Del_Upd(struct tmp_tx_def_c tmp_tx_def_c , char * reply  ){

	EXEC SQL DELETE FROM tmp_tx_def WHERE rowid=:tmp_tx_def_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE tmp_tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_tx_def_Clo_Upd( ){
	EXEC SQL CLOSE upd_tmp_tx_def;
	return 0;
}

