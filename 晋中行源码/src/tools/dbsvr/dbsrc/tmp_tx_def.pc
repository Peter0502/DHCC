	EXEC SQL INSERT INTO tmp_tx_def VALUES (:tmp_tx_def_c.tx_code,
			:tmp_tx_def_c.tx_name,
			:tmp_tx_def_c.bit_map,
			:tmp_tx_def_c.log_ind,
			:tmp_tx_def_c.tx_type,
			:tmp_tx_def_c.wrk_sts);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_tx_def error %d",sqlca.sqlcode);
