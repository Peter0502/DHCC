int Cif_cop_photo_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_cop_photo.* FROM cif_cop_photo WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_cop_photo_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_cop_photo CURSOR FOR cif_cop_photo_upd;

	EXEC SQL OPEN upd_cif_cop_photo;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_cop_photo error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_cop_photo_Fet_Upd( struct cif_cop_photo_c *cif_cop_photo_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_cop_photo INTO :cif_cop_photo_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_cop_photo error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_cop_photo finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_cop_photo_c);

	return 0;
}

int Cif_cop_photo_Upd_Upd( struct cif_cop_photo_c cif_cop_photo_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_cop_photo SET cif_no=:cif_cop_photo_c.cif_no,
			photo_seqn=:cif_cop_photo_c.photo_seqn,
			photo_type=:cif_cop_photo_c.photo_type,
			photo=:cif_cop_photo_c.photo
	WHERE rowid=:cif_cop_photo_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_cop_photo error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


