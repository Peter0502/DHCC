/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_cop_photo_Sel(reply,cif_cop_photo_c,fmtstr,va_alist)
char    *reply;
struct  cif_cop_photo_c *cif_cop_photo_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_cop_photo_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_cop_photo_Fet_Sel( cif_cop_photo_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_cop_photo_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_cop_photo_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_cop_photo.* FROM cif_cop_photo WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_cop_photo_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_cop_photo CURSOR FOR cif_cop_photo_sel;

	EXEC SQL OPEN sel_cif_cop_photo;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_cop_photo error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_cop_photo_Fet_Sel ( struct cif_cop_photo_c *cif_cop_photo_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_cop_photo INTO :cif_cop_photo_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_cop_photo error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_cop_photo finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_cop_photo_c);

	return 0;
}

int Cif_cop_photo_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_cop_photo;
	return 0;
}

