/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ln_gage_reg_Sel(reply,ln_gage_reg_c,fmtstr,va_alist)
char    *reply;
struct  ln_gage_reg_c *ln_gage_reg_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ln_gage_reg_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ln_gage_reg_Fet_Sel( ln_gage_reg_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ln_gage_reg_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ln_gage_reg_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ln_gage_reg.* FROM ln_gage_reg WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ln_gage_reg_sel FROM :comm;
	EXEC SQL DECLARE sel_ln_gage_reg CURSOR FOR ln_gage_reg_sel;

	EXEC SQL OPEN sel_ln_gage_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ln_gage_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ln_gage_reg_Fet_Sel ( struct ln_gage_reg_c *ln_gage_reg_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ln_gage_reg INTO :ln_gage_reg_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ln_gage_reg error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ln_gage_reg finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ln_gage_reg_c);

	return 0;
}

int Ln_gage_reg_Clo_Sel (){
	EXEC SQL CLOSE sel_ln_gage_reg;
	return 0;
}

