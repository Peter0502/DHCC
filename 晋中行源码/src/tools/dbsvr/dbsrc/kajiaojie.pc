int Kajiaojie_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,kajiaojie.* FROM kajiaojie WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE kajiaojie_upd FROM :comm;

	EXEC SQL DECLARE upd_kajiaojie CURSOR FOR kajiaojie_upd;

	EXEC SQL OPEN upd_kajiaojie;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open kajiaojie error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Kajiaojie_Fet_Upd( struct kajiaojie_c *kajiaojie_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_kajiaojie INTO :kajiaojie_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd kajiaojie error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH kajiaojie finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(kajiaojie_c);

	return 0;
}

int Kajiaojie_Upd_Upd( struct kajiaojie_c kajiaojie_c , char * reply){
	int ret;

	EXEC SQL UPDATE kajiaojie SET tel=:kajiaojie_c.tel,
			csts=:kajiaojie_c.csts,
			tels=:kajiaojie_c.tels,
			cst=:kajiaojie_c.cst
	WHERE rowid=:kajiaojie_c.rowid;
