/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Kajiaojie_Sel(reply,kajiaojie_c,fmtstr,va_alist)
char    *reply;
struct  kajiaojie_c *kajiaojie_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Kajiaojie_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Kajiaojie_Fet_Sel( kajiaojie_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Kajiaojie_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Kajiaojie_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,kajiaojie.* FROM kajiaojie WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE kajiaojie_sel FROM :comm;
	EXEC SQL DECLARE sel_kajiaojie CURSOR FOR kajiaojie_sel;

	EXEC SQL OPEN sel_kajiaojie;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN kajiaojie error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Kajiaojie_Fet_Sel ( struct kajiaojie_c *kajiaojie_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_kajiaojie INTO :kajiaojie_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch kajiaojie error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch kajiaojie finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(kajiaojie_c);

	return 0;
}

int Kajiaojie_Clo_Sel (){
	EXEC SQL CLOSE sel_kajiaojie;
	return 0;
}

