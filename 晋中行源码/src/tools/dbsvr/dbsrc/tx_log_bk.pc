/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Tx_log_bk_Sel(reply,tx_log_bk_c,fmtstr,va_alist)
char    *reply;
struct  tx_log_bk_c *tx_log_bk_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tx_log_bk_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tx_log_bk_Fet_Sel( tx_log_bk_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tx_log_bk_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tx_log_bk_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tx_log_bk.* FROM tx_log_bk WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_log_bk_sel FROM :comm;
	EXEC SQL DECLARE sel_tx_log_bk CURSOR FOR tx_log_bk_sel;

	EXEC SQL OPEN sel_tx_log_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tx_log_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_log_bk_Fet_Sel ( struct tx_log_bk_c *tx_log_bk_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tx_log_bk INTO :tx_log_bk_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tx_log_bk error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tx_log_bk finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tx_log_bk_c);

	return 0;
}

int Tx_log_bk_Clo_Sel (){
	EXEC SQL CLOSE sel_tx_log_bk;
	return 0;
}

