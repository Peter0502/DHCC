int Tx_log_bk_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,tx_log_bk.* FROM tx_log_bk WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_log_bk_upd FROM :comm;

	EXEC SQL DECLARE upd_tx_log_bk CURSOR FOR tx_log_bk_upd;

	EXEC SQL OPEN upd_tx_log_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tx_log_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_log_bk_Fet_Upd( struct tx_log_bk_c *tx_log_bk_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tx_log_bk INTO :tx_log_bk_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tx_log_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tx_log_bk finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tx_log_bk_c);

	return 0;
}

int Tx_log_bk_Upd_Upd( struct tx_log_bk_c tx_log_bk_c , char * reply){
	int ret;

	EXEC SQL UPDATE tx_log_bk SET trace_no=:tx_log_bk_c.trace_no,
			tx_code=:tx_log_bk_c.tx_code,
			tx_br_no=:tx_log_bk_c.tx_br_no,
			bit_map=:tx_log_bk_c.bit_map,
			tx_pack=:tx_log_bk_c.tx_pack
	WHERE rowid=:tx_log_bk_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tx_log_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


