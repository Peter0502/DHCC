	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ass_prdt_sale_sta_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ass_prdt_sale_sta_Fet_Sel( ass_prdt_sale_sta_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ass_prdt_sale_sta_Clo_Sel();

