int Ass_prdt_sale_sta_Sel(reply,ass_prdt_sale_sta_c,fmtstr,va_alist)
char    *reply;
struct  ass_prdt_sale_sta_c *ass_prdt_sale_sta_c;
char    *fmtstr;
va_dcl
{
