	}

	return 0;
}

int Lv_parad_Fet_Upd( struct lv_parad_c *lv_parad_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_lv_parad INTO :lv_parad_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd lv_parad error %d",sqlca.sqlcode);
