/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Lv_parad_Sel(reply,lv_parad_c,fmtstr,va_alist)
char    *reply;
struct  lv_parad_c *lv_parad_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Lv_parad_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Lv_parad_Fet_Sel( lv_parad_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Lv_parad_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Lv_parad_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,lv_parad.* FROM lv_parad WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE lv_parad_sel FROM :comm;
	EXEC SQL DECLARE sel_lv_parad CURSOR FOR lv_parad_sel;

	EXEC SQL OPEN sel_lv_parad;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN lv_parad error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Lv_parad_Fet_Sel ( struct lv_parad_c *lv_parad_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_lv_parad INTO :lv_parad_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch lv_parad error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch lv_parad finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(lv_parad_c);

	return 0;
}

int Lv_parad_Clo_Sel (){
	EXEC SQL CLOSE sel_lv_parad;
	return 0;
}

