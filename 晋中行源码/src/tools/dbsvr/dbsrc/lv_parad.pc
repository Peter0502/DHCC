int Lv_parad_Ins( struct lv_parad_c lv_parad_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO lv_parad VALUES (:lv_parad_c.wt_date,
			:lv_parad_c.tx_time,
			:lv_parad_c.flg,
			:lv_parad_c.chg_type,
			:lv_parad_c.eff_type,
			:lv_parad_c.eff_date,
			:lv_parad_c.chg_date,
			:lv_parad_c.par_code,
			:lv_parad_c.par_name,
			:lv_parad_c.par_value,
			:lv_parad_c.filler);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT lv_parad error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT lv_parad error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
