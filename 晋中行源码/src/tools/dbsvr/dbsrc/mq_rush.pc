int Mq_rush_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT mq_rush.* FROM mq_rush WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_rush_upd FROM :comm;

	EXEC SQL DECLARE upd_mq_rush CURSOR FOR mq_rush_upd;

	EXEC SQL OPEN upd_mq_rush;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mq_rush error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_rush_Fet_Upd( struct mq_rush_c *mq_rush_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mq_rush INTO :mq_rush_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mq_rush error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mq_rush finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mq_rush_c);

	return 0;
}

int Mq_rush_Upd_Upd( struct mq_rush_c mq_rush_c , char * reply){
	int ret;

	EXEC SQL UPDATE mq_rush SET taxorg_code=:mq_rush_c.taxorg_code,
			entrust_date=:mq_rush_c.entrust_date,
			cancel_no=:mq_rush_c.cancel_no,
			ori_entrust_date=:mq_rush_c.ori_entrust_date,
			ori_tra_no=:mq_rush_c.ori_tra_no,
			cancel_reson=:mq_rush_c.cancel_reson,
			cancel_answer=:mq_rush_c.cancel_answer,
			addwork_no=:mq_rush_c.addwork_no
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mq_rush error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd mq_rush"); 

	return 0;
}


int Mq_rush_Del_Upd(struct mq_rush_c mq_rush_c , char * reply  ){

	EXEC SQL DELETE FROM mq_rush WHERE rowid='0000';
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE mq_rush error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_rush_Clo_Upd( ){
	EXEC SQL CLOSE upd_mq_rush;

	return 0;
}

