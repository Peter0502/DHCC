int Ac_seqn_id_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ac_seqn_id.* FROM ac_seqn_id WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ac_seqn_id_upd FROM :comm;

	EXEC SQL DECLARE upd_ac_seqn_id CURSOR FOR ac_seqn_id_upd;

	EXEC SQL OPEN upd_ac_seqn_id;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ac_seqn_id_Fet_Upd( struct ac_seqn_id_c *ac_seqn_id_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ac_seqn_id INTO :ac_seqn_id_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ac_seqn_id finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ac_seqn_id_c);

	return 0;
}

int Ac_seqn_id_Upd_Upd( struct ac_seqn_id_c ac_seqn_id_c , char * reply){
	int ret;

	EXEC SQL UPDATE ac_seqn_id SET ac_id=:ac_seqn_id_c.ac_id,
			ac_seqn_id=:ac_seqn_id_c.ac_seqn_id
	WHERE rowid=:ac_seqn_id_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Ac_seqn_id_Del_Upd(struct ac_seqn_id_c ac_seqn_id_c , char * reply  ){

	EXEC SQL DELETE FROM ac_seqn_id WHERE rowid=:ac_seqn_id_c.rowid;
