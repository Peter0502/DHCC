/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ac_seqn_id_Sel(reply,ac_seqn_id_c,fmtstr,va_alist)
char    *reply;
struct  ac_seqn_id_c *ac_seqn_id_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ac_seqn_id_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ac_seqn_id_Fet_Sel( ac_seqn_id_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ac_seqn_id_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ac_seqn_id_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ac_seqn_id.* FROM ac_seqn_id WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ac_seqn_id_sel FROM :comm;
	EXEC SQL DECLARE sel_ac_seqn_id CURSOR FOR ac_seqn_id_sel;

	EXEC SQL OPEN sel_ac_seqn_id;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ac_seqn_id_Fet_Sel ( struct ac_seqn_id_c *ac_seqn_id_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ac_seqn_id INTO :ac_seqn_id_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ac_seqn_id error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ac_seqn_id finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ac_seqn_id_c);

	return 0;
}

int Ac_seqn_id_Clo_Sel (){
	EXEC SQL CLOSE sel_ac_seqn_id;
	return 0;
}

