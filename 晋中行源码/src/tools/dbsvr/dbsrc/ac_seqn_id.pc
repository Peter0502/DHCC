int Ac_seqn_id_Ins( struct ac_seqn_id_c ac_seqn_id_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO ac_seqn_id VALUES (:ac_seqn_id_c.ac_id,
			:ac_seqn_id_c.ac_seqn_id);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT ac_seqn_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
