int Cif_prdt_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_prdt_rel.* FROM cif_prdt_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_prdt_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_prdt_rel CURSOR FOR cif_prdt_rel_upd;

	EXEC SQL OPEN upd_cif_prdt_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_prdt_rel_Fet_Upd( struct cif_prdt_rel_c *cif_prdt_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_prdt_rel INTO :cif_prdt_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_prdt_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_prdt_rel_c);

	return 0;
}

int Cif_prdt_rel_Upd_Upd( struct cif_prdt_rel_c cif_prdt_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_prdt_rel SET cif_no=:cif_prdt_rel_c.cif_no,
			prdt_code=:cif_prdt_rel_c.prdt_code,
			ac_id=:cif_prdt_rel_c.ac_id,
			relation_no=:cif_prdt_rel_c.relation_no,
			relation_flag=:cif_prdt_rel_c.relation_flag,
			addr_seri=:cif_prdt_rel_c.addr_seri,
			crt_date=:cif_prdt_rel_c.crt_date,
			cls_date=:cif_prdt_rel_c.cls_date
	WHERE rowid=:cif_prdt_rel_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_prdt_rel_Del_Upd(struct cif_prdt_rel_c cif_prdt_rel_c , char * reply  ){

	EXEC SQL DELETE FROM cif_prdt_rel WHERE rowid=:cif_prdt_rel_c.rowid;
	if ( sqlca.sqlcode ){
