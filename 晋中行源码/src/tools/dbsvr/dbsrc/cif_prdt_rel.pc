/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_prdt_rel_Sel(reply,cif_prdt_rel_c,fmtstr,va_alist)
char    *reply;
struct  cif_prdt_rel_c *cif_prdt_rel_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_prdt_rel_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_prdt_rel_Fet_Sel( cif_prdt_rel_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_prdt_rel_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_prdt_rel_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_prdt_rel.* FROM cif_prdt_rel WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_prdt_rel_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_prdt_rel CURSOR FOR cif_prdt_rel_sel;

	EXEC SQL OPEN sel_cif_prdt_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_prdt_rel_Fet_Sel ( struct cif_prdt_rel_c *cif_prdt_rel_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_prdt_rel INTO :cif_prdt_rel_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_prdt_rel error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_prdt_rel finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_prdt_rel_c);

	return 0;
}

int Cif_prdt_rel_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_prdt_rel;
	return 0;
}

