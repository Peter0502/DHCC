int Auth_cond_Ins( struct auth_cond_c auth_cond_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO auth_cond VALUES (:auth_cond_c.type,
			:auth_cond_c.no,
			:auth_cond_c.tx_code,
			:auth_cond_c.seqn,
			:auth_cond_c.mo,
			:auth_cond_c.cond);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
