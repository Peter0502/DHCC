int Auth_cond_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_cond.* FROM auth_cond WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_cond_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_cond CURSOR FOR auth_cond_upd;

	EXEC SQL OPEN upd_auth_cond;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_cond_Fet_Upd( struct auth_cond_c *auth_cond_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_cond INTO :auth_cond_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_cond finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_cond_c);

	return 0;
}

int Auth_cond_Upd_Upd( struct auth_cond_c auth_cond_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_cond SET type=:auth_cond_c.type,
			no=:auth_cond_c.no,
			tx_code=:auth_cond_c.tx_code,
			seqn=:auth_cond_c.seqn,
			mo=:auth_cond_c.mo,
			cond=:auth_cond_c.cond
	WHERE rowid=:auth_cond_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Auth_cond_Del_Upd(struct auth_cond_c auth_cond_c , char * reply  ){

	EXEC SQL DELETE FROM auth_cond WHERE rowid=:auth_cond_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_cond error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_cond_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_cond;
	return 0;
}

