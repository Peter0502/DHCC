int Note_type_tmp_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,note_type_tmp.* FROM note_type_tmp WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE note_type_tmp_upd FROM :comm;

	EXEC SQL DECLARE upd_note_type_tmp CURSOR FOR note_type_tmp_upd;

	EXEC SQL OPEN upd_note_type_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Note_type_tmp_Fet_Upd( struct note_type_tmp_c *note_type_tmp_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_note_type_tmp INTO :note_type_tmp_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH note_type_tmp finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(note_type_tmp_c);

	return 0;
}

int Note_type_tmp_Upd_Upd( struct note_type_tmp_c note_type_tmp_c , char * reply){
	int ret;

	EXEC SQL UPDATE note_type_tmp SET oldtype=:note_type_tmp_c.oldtype,
			newtype=:note_type_tmp_c.newtype
	WHERE rowid=:note_type_tmp_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Note_type_tmp_Del_Upd(struct note_type_tmp_c note_type_tmp_c , char * reply  ){

	EXEC SQL DELETE FROM note_type_tmp WHERE rowid=:note_type_tmp_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Note_type_tmp_Clo_Upd( ){
	EXEC SQL CLOSE upd_note_type_tmp;
	return 0;
}

