int Note_type_tmp_Ins( struct note_type_tmp_c note_type_tmp_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO note_type_tmp VALUES (:note_type_tmp_c.oldtype,
			:note_type_tmp_c.newtype);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT note_type_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
