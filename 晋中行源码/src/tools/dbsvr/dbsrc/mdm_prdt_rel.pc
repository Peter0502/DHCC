/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mdm_prdt_rel_Sel(reply,mdm_prdt_rel_c,fmtstr,va_alist)
char    *reply;
struct  mdm_prdt_rel_c *mdm_prdt_rel_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mdm_prdt_rel_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mdm_prdt_rel_Fet_Sel( mdm_prdt_rel_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mdm_prdt_rel_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mdm_prdt_rel_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mdm_prdt_rel.* FROM mdm_prdt_rel WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mdm_prdt_rel_sel FROM :comm;
	EXEC SQL DECLARE sel_mdm_prdt_rel CURSOR FOR mdm_prdt_rel_sel;

	EXEC SQL OPEN sel_mdm_prdt_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mdm_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mdm_prdt_rel_Fet_Sel ( struct mdm_prdt_rel_c *mdm_prdt_rel_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mdm_prdt_rel INTO :mdm_prdt_rel_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mdm_prdt_rel error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mdm_prdt_rel finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mdm_prdt_rel_c);

	return 0;
}

int Mdm_prdt_rel_Clo_Sel (){
	EXEC SQL CLOSE sel_mdm_prdt_rel;
	return 0;
}

