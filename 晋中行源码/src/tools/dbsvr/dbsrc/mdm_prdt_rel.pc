/*****************************************************************************/
int Mdm_prdt_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mdm_prdt_rel.* FROM mdm_prdt_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mdm_prdt_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_mdm_prdt_rel CURSOR FOR mdm_prdt_rel_upd;

	EXEC SQL OPEN upd_mdm_prdt_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mdm_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mdm_prdt_rel_Fet_Upd( struct mdm_prdt_rel_c *mdm_prdt_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mdm_prdt_rel INTO :mdm_prdt_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mdm_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mdm_prdt_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mdm_prdt_rel_c);

	return 0;
}

int Mdm_prdt_rel_Upd_Upd( struct mdm_prdt_rel_c mdm_prdt_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE mdm_prdt_rel SET prdt_no=:mdm_prdt_rel_c.prdt_no,
			mdm_code1=:mdm_prdt_rel_c.mdm_code1,
			mdm_code2=:mdm_prdt_rel_c.mdm_code2,
			mdm_code3=:mdm_prdt_rel_c.mdm_code3
	WHERE rowid=:mdm_prdt_rel_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mdm_prdt_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


