#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"mdm_prdt_rel_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mdm_prdt_rel_con ( struct mdm_prdt_rel_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mdm_prdt_rel_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->prdt_no);
	zip_tail(ps->mdm_code1);
	zip_tail(ps->mdm_code2);
	zip_tail(ps->mdm_code3);
}
