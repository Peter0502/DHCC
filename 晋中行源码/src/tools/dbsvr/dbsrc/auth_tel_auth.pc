int Auth_tel_auth_Fet_Upd( struct auth_tel_auth_c *auth_tel_auth_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_tel_auth INTO :auth_tel_auth_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
