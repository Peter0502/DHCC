		sprintf(acErrMsg,"FETCH auth_tel_auth finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_tel_auth_c);

	return 0;
}

