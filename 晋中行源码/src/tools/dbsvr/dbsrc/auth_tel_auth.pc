/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Auth_tel_auth_Sel(reply,auth_tel_auth_c,fmtstr,va_alist)
char    *reply;
struct  auth_tel_auth_c *auth_tel_auth_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Auth_tel_auth_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Auth_tel_auth_Fet_Sel( auth_tel_auth_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Auth_tel_auth_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Auth_tel_auth_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,auth_tel_auth.* FROM auth_tel_auth WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_tel_auth_sel FROM :comm;
	EXEC SQL DECLARE sel_auth_tel_auth CURSOR FOR auth_tel_auth_sel;

	EXEC SQL OPEN sel_auth_tel_auth;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN auth_tel_auth error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_tel_auth_Fet_Sel ( struct auth_tel_auth_c *auth_tel_auth_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_auth_tel_auth INTO :auth_tel_auth_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch auth_tel_auth error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch auth_tel_auth finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(auth_tel_auth_c);

	return 0;
}

int Auth_tel_auth_Clo_Sel (){
	EXEC SQL CLOSE sel_auth_tel_auth;
	return 0;
}

