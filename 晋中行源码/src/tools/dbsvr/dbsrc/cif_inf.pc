#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"cif_inf_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int cif_inf_con ( struct cif_inf_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct cif_inf_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->name);
	zip_tail(ps->sts);
	zip_tail(ps->br_no);
}
int Cif_inf_Debug(struct cif_inf_c *ps){
	vtcp_log("cif_inf_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("cif_inf_c.cif_no=[%ld]\n",ps->cif_no);
	vtcp_log("cif_inf_c.name=[%s]\n",ps->name);
	vtcp_log("cif_inf_c.crt_date=[%ld]\n",ps->crt_date);
	vtcp_log("cif_inf_c.sts=[%s]\n",ps->sts);
	vtcp_log("cif_inf_c.br_no=[%s]\n",ps->br_no);
	return(0);
}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_inf_Sel(reply,cif_inf_c,fmtstr,va_alist)
char    *reply;
struct  cif_inf_c *cif_inf_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_inf_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_inf_Fet_Sel( cif_inf_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_inf_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_inf_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_inf.* FROM cif_inf WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_inf_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_inf CURSOR FOR cif_inf_sel;

	EXEC SQL OPEN sel_cif_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_inf_Fet_Sel ( struct cif_inf_c *cif_inf_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_inf INTO :cif_inf_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_inf error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_inf finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_inf_c);

	return 0;
}

int Cif_inf_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_inf;
	return 0;
}

