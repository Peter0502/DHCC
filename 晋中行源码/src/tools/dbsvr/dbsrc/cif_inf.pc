int Cif_inf_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_inf.* FROM cif_inf WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_inf_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_inf CURSOR FOR cif_inf_upd;

	EXEC SQL OPEN upd_cif_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_inf_Fet_Upd( struct cif_inf_c *cif_inf_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_inf INTO :cif_inf_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_inf finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_inf_c);

	return 0;
}

int Cif_inf_Upd_Upd( struct cif_inf_c cif_inf_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_inf SET cif_no=:cif_inf_c.cif_no,
			name=:cif_inf_c.name,
			crt_date=:cif_inf_c.crt_date,
			sts=:cif_inf_c.sts,
			br_no=:cif_inf_c.br_no
	WHERE rowid=:cif_inf_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_inf_Del_Upd(struct cif_inf_c cif_inf_c , char * reply  ){

	EXEC SQL DELETE FROM cif_inf WHERE rowid=:cif_inf_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_inf_Clo_Upd( ){
	EXEC SQL CLOSE upd_cif_inf;
	return 0;
}

/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Cif_inf_Ins( struct cif_inf_c cif_inf_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO cif_inf VALUES (:cif_inf_c.cif_no,
			:cif_inf_c.name,
			:cif_inf_c.crt_date,
			:cif_inf_c.sts,
			:cif_inf_c.br_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT cif_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
