/*****************************************************************************/
int Card_reg_Sel1(reply,card_reg_c,fmtstr,va_alist)
char    *reply;
struct  card_reg_c *card_reg_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Card_reg_Dec_Sel1( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Card_reg_Fet_Sel1( card_reg_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Card_reg_Clo_Sel1();

	return 0;
}



char *key_strcat(char *,char *);
int Card_reg_Dec_Sel1 ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,card_reg.* FROM card_reg WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE card_reg_sel1 FROM :comm;
	EXEC SQL DECLARE sel_card_reg1 CURSOR FOR card_reg_sel1;

	EXEC SQL OPEN sel_card_reg1;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN card_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Card_reg_Fet_Sel1 ( struct card_reg_c *card_reg_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_card_reg1 INTO :card_reg_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch card_reg error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch card_reg finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
