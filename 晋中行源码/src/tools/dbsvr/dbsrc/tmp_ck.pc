#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"tmp_ck_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int tmp_ck_con ( struct tmp_ck_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct tmp_ck_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->acct_no);
	zip_tail(ps->flag);
	zip_tail(ps->ac_no);
}
