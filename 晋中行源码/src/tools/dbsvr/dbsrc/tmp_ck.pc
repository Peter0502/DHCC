	return(0);
}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Tmp_ck_Sel(reply,tmp_ck_c,fmtstr,va_alist)
char    *reply;
struct  tmp_ck_c *tmp_ck_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tmp_ck_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tmp_ck_Fet_Sel( tmp_ck_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tmp_ck_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tmp_ck_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tmp_ck.* FROM tmp_ck WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tmp_ck_sel FROM :comm;
	EXEC SQL DECLARE sel_tmp_ck CURSOR FOR tmp_ck_sel;

	EXEC SQL OPEN sel_tmp_ck;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tmp_ck error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_ck_Fet_Sel ( struct tmp_ck_c *tmp_ck_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tmp_ck INTO :tmp_ck_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tmp_ck error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tmp_ck finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tmp_ck_c);

	return 0;
}

int Tmp_ck_Clo_Sel (){
	EXEC SQL CLOSE sel_tmp_ck;
	return 0;
}

