	int ret;

	EXEC SQL UPDATE tmp_ck SET acct_no=:tmp_ck_c.acct_no,
			accu_amt=:tmp_ck_c.accu_amt,
			flag=:tmp_ck_c.flag,
			ac_no=:tmp_ck_c.ac_no,
			lx=:tmp_ck_c.lx,
			ac_id=:tmp_ck_c.ac_id,
			ac_sdeqn=:tmp_ck_c.ac_sdeqn
	WHERE rowid=:tmp_ck_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tmp_ck error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


