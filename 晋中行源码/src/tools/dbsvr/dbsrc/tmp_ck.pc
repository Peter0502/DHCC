int Tmp_ck_Ins( struct tmp_ck_c tmp_ck_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tmp_ck VALUES (:tmp_ck_c.acct_no,
			:tmp_ck_c.accu_amt,
			:tmp_ck_c.flag,
			:tmp_ck_c.ac_no,
			:tmp_ck_c.lx,
			:tmp_ck_c.ac_id,
			:tmp_ck_c.ac_sdeqn);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_ck error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_ck error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
