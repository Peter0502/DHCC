#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int auth_def_con ( struct auth_def_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct auth_def_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->tx_code);
	zip_tail(ps->cond_code);
	zip_tail(ps->lvl);
	zip_tail(ps->chk_ind);
	zip_tail(ps->flow_code);
	zip_tail(ps->mo);
}
