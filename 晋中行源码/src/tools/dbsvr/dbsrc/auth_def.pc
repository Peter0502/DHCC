int Auth_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_def.* FROM auth_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_def_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_def CURSOR FOR auth_def_upd;

	EXEC SQL OPEN upd_auth_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_def_Fet_Upd( struct auth_def_c *auth_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_def INTO :auth_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_def_c);

	return 0;
}

int Auth_def_Upd_Upd( struct auth_def_c auth_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_def SET tx_code=:auth_def_c.tx_code,
			seqn=:auth_def_c.seqn,
			cond_code=:auth_def_c.cond_code,
			lvl=:auth_def_c.lvl,
			chk_ind=:auth_def_c.chk_ind,
			flow_code=:auth_def_c.flow_code,
			mo=:auth_def_c.mo
	WHERE rowid=:auth_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Auth_def_Del_Upd(struct auth_def_c auth_def_c , char * reply  ){

	EXEC SQL DELETE FROM auth_def WHERE rowid=:auth_def_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_def_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_def;
	return 0;
}

