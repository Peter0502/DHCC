int Auth_def_Ins( struct auth_def_c auth_def_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO auth_def VALUES (:auth_def_c.tx_code,
			:auth_def_c.seqn,
			:auth_def_c.cond_code,
			:auth_def_c.lvl,
			:auth_def_c.chk_ind,
			:auth_def_c.flow_code,
			:auth_def_c.mo);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT auth_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
