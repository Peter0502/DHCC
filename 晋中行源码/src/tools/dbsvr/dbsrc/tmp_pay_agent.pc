/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Tmp_pay_agent_Sel(reply,tmp_pay_agent_c,fmtstr,va_alist)
char    *reply;
struct  tmp_pay_agent_c *tmp_pay_agent_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tmp_pay_agent_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tmp_pay_agent_Fet_Sel( tmp_pay_agent_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tmp_pay_agent_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tmp_pay_agent_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tmp_pay_agent.* FROM tmp_pay_agent WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tmp_pay_agent_sel FROM :comm;
	EXEC SQL DECLARE sel_tmp_pay_agent CURSOR FOR tmp_pay_agent_sel;

	EXEC SQL OPEN sel_tmp_pay_agent;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tmp_pay_agent error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_pay_agent_Fet_Sel ( struct tmp_pay_agent_c *tmp_pay_agent_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tmp_pay_agent INTO :tmp_pay_agent_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tmp_pay_agent error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tmp_pay_agent finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tmp_pay_agent_c);

	return 0;
}

int Tmp_pay_agent_Clo_Sel (){
	EXEC SQL CLOSE sel_tmp_pay_agent;
	return 0;
}

