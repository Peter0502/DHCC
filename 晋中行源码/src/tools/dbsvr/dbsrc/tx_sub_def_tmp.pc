int Tx_sub_def_tmp_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,tx_sub_def_tmp.* FROM tx_sub_def_tmp WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_sub_def_tmp_upd FROM :comm;

	EXEC SQL DECLARE upd_tx_sub_def_tmp CURSOR FOR tx_sub_def_tmp_upd;

	EXEC SQL OPEN upd_tx_sub_def_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tx_sub_def_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_sub_def_tmp_Fet_Upd( struct tx_sub_def_tmp_c *tx_sub_def_tmp_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tx_sub_def_tmp INTO :tx_sub_def_tmp_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tx_sub_def_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tx_sub_def_tmp finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tx_sub_def_tmp_c);

	return 0;
}

