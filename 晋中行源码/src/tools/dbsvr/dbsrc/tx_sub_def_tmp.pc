/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Tx_sub_def_tmp_Sel(reply,tx_sub_def_tmp_c,fmtstr,va_alist)
char    *reply;
struct  tx_sub_def_tmp_c *tx_sub_def_tmp_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tx_sub_def_tmp_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tx_sub_def_tmp_Fet_Sel( tx_sub_def_tmp_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tx_sub_def_tmp_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tx_sub_def_tmp_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tx_sub_def_tmp.* FROM tx_sub_def_tmp WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_sub_def_tmp_sel FROM :comm;
	EXEC SQL DECLARE sel_tx_sub_def_tmp CURSOR FOR tx_sub_def_tmp_sel;

	EXEC SQL OPEN sel_tx_sub_def_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tx_sub_def_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_sub_def_tmp_Fet_Sel ( struct tx_sub_def_tmp_c *tx_sub_def_tmp_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tx_sub_def_tmp INTO :tx_sub_def_tmp_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tx_sub_def_tmp error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tx_sub_def_tmp finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tx_sub_def_tmp_c);

	return 0;
}

int Tx_sub_def_tmp_Clo_Sel (){
	EXEC SQL CLOSE sel_tx_sub_def_tmp;
	return 0;
}

