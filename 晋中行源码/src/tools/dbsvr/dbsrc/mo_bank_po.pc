/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mo_bank_po_Sel(reply,mo_bank_po_c,fmtstr,va_alist)
char    *reply;
struct  mo_bank_po_c *mo_bank_po_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mo_bank_po_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mo_bank_po_Fet_Sel( mo_bank_po_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mo_bank_po_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mo_bank_po_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mo_bank_po.* FROM mo_bank_po WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_bank_po_sel FROM :comm;
	EXEC SQL DECLARE sel_mo_bank_po CURSOR FOR mo_bank_po_sel;

	EXEC SQL OPEN sel_mo_bank_po;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mo_bank_po error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_bank_po_Fet_Sel ( struct mo_bank_po_c *mo_bank_po_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mo_bank_po INTO :mo_bank_po_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mo_bank_po error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mo_bank_po finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mo_bank_po_c);

	return 0;
}

int Mo_bank_po_Clo_Sel (){
	EXEC SQL CLOSE sel_mo_bank_po;
	return 0;
}

