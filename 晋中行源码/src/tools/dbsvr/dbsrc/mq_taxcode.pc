int Mq_taxcode_Ins( struct mq_taxcode_c mq_taxcode_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mq_taxcode VALUES (:mq_taxcode_c.taxorg_code,
			:mq_taxcode_c.taxorg_name,
			:mq_taxcode_c.taxorg_type,
			:mq_taxcode_c.ofnode_code,
			:mq_taxcode_c.effect_date);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mq_taxcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mq_taxcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" ins mq_taxcode"); 

	return 0;
}
