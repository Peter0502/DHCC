#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"mq_taxcode_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mq_taxcode_con ( struct mq_taxcode_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mq_taxcode_c* ps){
	zip_tail(ps->taxorg_code);
	zip_tail(ps->taxorg_name);
	zip_tail(ps->taxorg_type);
	zip_tail(ps->ofnode_code);
}
