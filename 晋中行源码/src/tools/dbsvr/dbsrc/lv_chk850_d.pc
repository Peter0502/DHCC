int Lv_chk850_d_Sel(reply,lv_chk850_d_c,fmtstr,va_alist)
char    *reply;
struct  lv_chk850_d_c *lv_chk850_d_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Lv_chk850_d_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Lv_chk850_d_Fet_Sel( lv_chk850_d_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Lv_chk850_d_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Lv_chk850_d_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,lv_chk850_d.* FROM lv_chk850_d WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE lv_chk850_d_sel FROM :comm;
	EXEC SQL DECLARE sel_lv_chk850_d CURSOR FOR lv_chk850_d_sel;

	EXEC SQL OPEN sel_lv_chk850_d;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN lv_chk850_d error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Lv_chk850_d_Fet_Sel ( struct lv_chk850_d_c *lv_chk850_d_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_lv_chk850_d INTO :lv_chk850_d_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch lv_chk850_d error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch lv_chk850_d finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(lv_chk850_d_c);

	return 0;
}

int Lv_chk850_d_Clo_Sel (){
	EXEC SQL CLOSE sel_lv_chk850_d;
	return 0;
}

