/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ln_mst_bk_Sel(reply,ln_mst_bk_c,fmtstr,va_alist)
char    *reply;
struct  ln_mst_bk_c *ln_mst_bk_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ln_mst_bk_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ln_mst_bk_Fet_Sel( ln_mst_bk_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ln_mst_bk_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ln_mst_bk_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ln_mst_bk.* FROM ln_mst_bk WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ln_mst_bk_sel FROM :comm;
	EXEC SQL DECLARE sel_ln_mst_bk CURSOR FOR ln_mst_bk_sel;

	EXEC SQL OPEN sel_ln_mst_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ln_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ln_mst_bk_Fet_Sel ( struct ln_mst_bk_c *ln_mst_bk_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ln_mst_bk INTO :ln_mst_bk_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ln_mst_bk error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ln_mst_bk finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ln_mst_bk_c);

	return 0;
}

int Ln_mst_bk_Clo_Sel (){
	EXEC SQL CLOSE sel_ln_mst_bk;
	return 0;
}

