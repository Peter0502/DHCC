}

int Auth_item_Fet_Upd( struct auth_item_c *auth_item_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_item INTO :auth_item_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_item finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_item_c);

	return 0;
}

int Auth_item_Upd_Upd( struct auth_item_c auth_item_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_item SET tx_code=:auth_item_c.tx_code,
			seqn=:auth_item_c.seqn,
			code_ac=:auth_item_c.code_ac,
			code_seqn=:auth_item_c.code_seqn,
			code_amt=:auth_item_c.code_amt,
			code_dc=:auth_item_c.code_dc,
			code_ct=:auth_item_c.code_ct,
			code_xx=:auth_item_c.code_xx,
			code_yy=:auth_item_c.code_yy,
			code_zz=:auth_item_c.code_zz,
			ind=:auth_item_c.ind,
