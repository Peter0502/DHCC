int Auth_item_Ins( struct auth_item_c auth_item_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO auth_item VALUES (:auth_item_c.tx_code,
			:auth_item_c.seqn,
			:auth_item_c.code_ac,
			:auth_item_c.code_seqn,
			:auth_item_c.code_amt,
			:auth_item_c.code_dc,
			:auth_item_c.code_ct,
			:auth_item_c.code_xx,
			:auth_item_c.code_yy,
			:auth_item_c.code_zz,
			:auth_item_c.ind,
			:auth_item_c.mo);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT auth_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT auth_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
