int Qury_reg_Ins( struct qury_reg_c qury_reg_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO qury_reg VALUES (:qury_reg_c.tx_br_no,
			:qury_reg_c.tx_tel,
			:qury_reg_c.tx_date,
			:qury_reg_c.tx_code,
			:qury_reg_c.tx_time,
			:qury_reg_c.wherelist,
			:qury_reg_c.filler);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT qury_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT qury_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
