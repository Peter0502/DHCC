#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int qury_reg_con ( struct qury_reg_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct qury_reg_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->tx_br_no);
	zip_tail(ps->tx_tel);
	zip_tail(ps->tx_code);
	zip_tail(ps->wherelist);
	zip_tail(ps->filler);
}
