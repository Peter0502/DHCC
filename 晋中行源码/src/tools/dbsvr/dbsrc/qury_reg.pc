int Qury_reg_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,qury_reg.* FROM qury_reg WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE qury_reg_upd FROM :comm;

	EXEC SQL DECLARE upd_qury_reg CURSOR FOR qury_reg_upd;

	EXEC SQL OPEN upd_qury_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open qury_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Qury_reg_Fet_Upd( struct qury_reg_c *qury_reg_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_qury_reg INTO :qury_reg_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd qury_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH qury_reg finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(qury_reg_c);

	return 0;
}

int Qury_reg_Upd_Upd( struct qury_reg_c qury_reg_c , char * reply){
	int ret;

	EXEC SQL UPDATE qury_reg SET tx_br_no=:qury_reg_c.tx_br_no,
			tx_tel=:qury_reg_c.tx_tel,
			tx_date=:qury_reg_c.tx_date,
			tx_code=:qury_reg_c.tx_code,
			tx_time=:qury_reg_c.tx_time,
			wherelist=:qury_reg_c.wherelist,
			filler=:qury_reg_c.filler
	WHERE rowid=:qury_reg_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE qury_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Qury_reg_Del_Upd(struct qury_reg_c qury_reg_c , char * reply  ){

	EXEC SQL DELETE FROM qury_reg WHERE rowid=:qury_reg_c.rowid;
