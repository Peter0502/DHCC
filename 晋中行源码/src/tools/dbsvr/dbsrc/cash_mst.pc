			:cash_mst_c.t1000,
			:cash_mst_c.t500,
			:cash_mst_c.t200,
			:cash_mst_c.t100,
			:cash_mst_c.t50,
			:cash_mst_c.t20,
			:cash_mst_c.t10,
			:cash_mst_c.t5,
			:cash_mst_c.t2,
			:cash_mst_c.t1,
			:cash_mst_c.sts);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cash_mst error %d",sqlca.sqlcode);
