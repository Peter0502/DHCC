EXEC SQL INCLUDE SQLCA;
#include"mq_ctl_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mq_ctl_con ( struct mq_ctl_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mq_ctl_c* ps){
	zip_tail(ps->src);
	zip_tail(ps->des);
	zip_tail(ps->login_sts);
	zip_tail(ps->work_sts);
	zip_tail(ps->stoprun_reason);
}
