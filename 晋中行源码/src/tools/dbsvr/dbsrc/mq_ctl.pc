int Mq_ctl_Ins( struct mq_ctl_c mq_ctl_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mq_ctl VALUES (:mq_ctl_c.src,
			:mq_ctl_c.des,
			:mq_ctl_c.workdate,
			:mq_ctl_c.login_sts,
			:mq_ctl_c.work_sts,
			:mq_ctl_c.stop_date,
			:mq_ctl_c.stop_time,
			:mq_ctl_c.back_date,
			:mq_ctl_c.back_time,
			:mq_ctl_c.stoprun_reason);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" ins mq_ctl"); 

	return 0;
}
