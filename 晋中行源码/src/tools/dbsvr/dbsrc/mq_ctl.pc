int Mq_ctl_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT mq_ctl.* FROM mq_ctl WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_ctl_upd FROM :comm;

	EXEC SQL DECLARE upd_mq_ctl CURSOR FOR mq_ctl_upd;

	EXEC SQL OPEN upd_mq_ctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_ctl_Fet_Upd( struct mq_ctl_c *mq_ctl_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mq_ctl INTO :mq_ctl_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mq_ctl finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mq_ctl_c);

	return 0;
}

int Mq_ctl_Upd_Upd( struct mq_ctl_c mq_ctl_c , char * reply){
	int ret;

	EXEC SQL UPDATE mq_ctl SET src=:mq_ctl_c.src,
			des=:mq_ctl_c.des,
			workdate=:mq_ctl_c.workdate,
			login_sts=:mq_ctl_c.login_sts,
			work_sts=:mq_ctl_c.work_sts,
			stop_date=:mq_ctl_c.stop_date,
			stop_time=:mq_ctl_c.stop_time,
			back_date=:mq_ctl_c.back_date,
			back_time=:mq_ctl_c.back_time,
			stoprun_reason=:mq_ctl_c.stoprun_reason
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd mq_ctl"); 

	return 0;
}


int Mq_ctl_Del_Upd(struct mq_ctl_c mq_ctl_c , char * reply  ){

	EXEC SQL DELETE FROM mq_ctl WHERE rowid='0000';
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE mq_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_ctl_Clo_Upd( ){
	EXEC SQL CLOSE upd_mq_ctl;

	return 0;
}

