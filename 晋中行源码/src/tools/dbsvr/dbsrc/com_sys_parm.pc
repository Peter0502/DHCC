#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"com_sys_parm_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int com_sys_parm_con ( struct com_sys_parm_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct com_sys_parm_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->real_time_ind);
	zip_tail(ps->sys_ind);
}
