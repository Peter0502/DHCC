	vtcp_log("com_sys_parm_c.sys_date=[%ld]\n",ps->sys_date);
	vtcp_log("com_sys_parm_c.real_time_ind=[%s]\n",ps->real_time_ind);
	vtcp_log("com_sys_parm_c.sys_ind=[%s]\n",ps->sys_ind);
