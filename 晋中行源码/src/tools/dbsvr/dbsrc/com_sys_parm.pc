}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Com_sys_parm_Sel(reply,com_sys_parm_c,fmtstr,va_alist)
char    *reply;
struct  com_sys_parm_c *com_sys_parm_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Com_sys_parm_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Com_sys_parm_Fet_Sel( com_sys_parm_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Com_sys_parm_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Com_sys_parm_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,com_sys_parm.* FROM com_sys_parm WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_sys_parm_sel FROM :comm;
	EXEC SQL DECLARE sel_com_sys_parm CURSOR FOR com_sys_parm_sel;

	EXEC SQL OPEN sel_com_sys_parm;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN com_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_sys_parm_Fet_Sel ( struct com_sys_parm_c *com_sys_parm_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_com_sys_parm INTO :com_sys_parm_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch com_sys_parm error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch com_sys_parm finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(com_sys_parm_c);

	return 0;
}

int Com_sys_parm_Clo_Sel (){
	EXEC SQL CLOSE sel_com_sys_parm;
	return 0;
}

