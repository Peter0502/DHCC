int Com_sys_parm_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,com_sys_parm.* FROM com_sys_parm WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_sys_parm_upd FROM :comm;

	EXEC SQL DECLARE upd_com_sys_parm CURSOR FOR com_sys_parm_upd;

	EXEC SQL OPEN upd_com_sys_parm;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open com_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_sys_parm_Fet_Upd( struct com_sys_parm_c *com_sys_parm_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_com_sys_parm INTO :com_sys_parm_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd com_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH com_sys_parm finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(com_sys_parm_c);

	return 0;
}

int Com_sys_parm_Upd_Upd( struct com_sys_parm_c com_sys_parm_c , char * reply){
	int ret;

	EXEC SQL UPDATE com_sys_parm SET sys_sts=:com_sys_parm_c.sys_sts,
			lst_date=:com_sys_parm_c.lst_date,
			sys_date=:com_sys_parm_c.sys_date,
			real_time_ind=:com_sys_parm_c.real_time_ind,
			sys_ind=:com_sys_parm_c.sys_ind
	WHERE rowid=:com_sys_parm_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE com_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Com_sys_parm_Del_Upd(struct com_sys_parm_c com_sys_parm_c , char * reply  ){

	EXEC SQL DELETE FROM com_sys_parm WHERE rowid=:com_sys_parm_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE com_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

