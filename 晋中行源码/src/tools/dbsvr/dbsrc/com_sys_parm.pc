	int ret;

	EXEC SQL INSERT INTO com_sys_parm VALUES (:com_sys_parm_c.sys_sts,
			:com_sys_parm_c.lst_date,
			:com_sys_parm_c.sys_date,
			:com_sys_parm_c.real_time_ind,
			:com_sys_parm_c.sys_ind);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT com_sys_parm error %d",sqlca.sqlcode);
