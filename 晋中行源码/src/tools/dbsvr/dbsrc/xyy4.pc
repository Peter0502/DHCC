/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Xyy4_Sel(reply,xyy4_c,fmtstr,va_alist)
char    *reply;
struct  xyy4_c *xyy4_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Xyy4_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Xyy4_Fet_Sel( xyy4_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Xyy4_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Xyy4_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,xyy4.* FROM xyy4 WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE xyy4_sel FROM :comm;
	EXEC SQL DECLARE sel_xyy4 CURSOR FOR xyy4_sel;

	EXEC SQL OPEN sel_xyy4;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN xyy4 error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Xyy4_Fet_Sel ( struct xyy4_c *xyy4_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_xyy4 INTO :xyy4_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch xyy4 error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch xyy4 finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(xyy4_c);

	return 0;
}

int Xyy4_Clo_Sel (){
	EXEC SQL CLOSE sel_xyy4;
	return 0;
}

