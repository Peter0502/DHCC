int Tx_def_Sel(reply,tx_def_c,fmtstr,va_alist)
char    *reply;
struct  tx_def_c *tx_def_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tx_def_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tx_def_Fet_Sel( tx_def_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tx_def_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tx_def_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tx_def.* FROM tx_def WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_def_sel FROM :comm;
	EXEC SQL DECLARE sel_tx_def CURSOR FOR tx_def_sel;

	EXEC SQL OPEN sel_tx_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_def_Fet_Sel ( struct tx_def_c *tx_def_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tx_def INTO :tx_def_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tx_def error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tx_def finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tx_def_c);

	return 0;
}

int Tx_def_Clo_Sel (){
	EXEC SQL CLOSE sel_tx_def;
	return 0;
}

