int Tx_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,tx_def.* FROM tx_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tx_def_upd FROM :comm;

	EXEC SQL DECLARE upd_tx_def CURSOR FOR tx_def_upd;

	EXEC SQL OPEN upd_tx_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tx_def_Fet_Upd( struct tx_def_c *tx_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tx_def INTO :tx_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tx_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tx_def_c);

	return 0;
}

int Tx_def_Upd_Upd( struct tx_def_c tx_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE tx_def SET tx_code=:tx_def_c.tx_code,
			tx_name=:tx_def_c.tx_name,
			bit_map=:tx_def_c.bit_map,
			log_ind=:tx_def_c.log_ind,
			tx_type=:tx_def_c.tx_type,
			wrk_sts=:tx_def_c.wrk_sts
	WHERE rowid=:tx_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


