int Tx_def_Ins( struct tx_def_c tx_def_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tx_def VALUES (:tx_def_c.tx_code,
			:tx_def_c.tx_name,
			:tx_def_c.bit_map,
			:tx_def_c.log_ind,
			:tx_def_c.tx_type,
			:tx_def_c.wrk_sts);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tx_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
