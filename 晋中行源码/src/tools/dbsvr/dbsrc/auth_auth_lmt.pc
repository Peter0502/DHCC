int Auth_auth_lmt_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_auth_lmt.* FROM auth_auth_lmt WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_auth_lmt_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_auth_lmt CURSOR FOR auth_auth_lmt_upd;

	EXEC SQL OPEN upd_auth_auth_lmt;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_auth_lmt error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_auth_lmt_Fet_Upd( struct auth_auth_lmt_c *auth_auth_lmt_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_auth_lmt INTO :auth_auth_lmt_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_auth_lmt error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_auth_lmt finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_auth_lmt_c);

	return 0;
}

int Auth_auth_lmt_Upd_Upd( struct auth_auth_lmt_c auth_auth_lmt_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_auth_lmt SET auth_code=:auth_auth_lmt_c.auth_code,
			min_amt=:auth_auth_lmt_c.min_amt,
			max_amt=:auth_auth_lmt_c.max_amt,
			mo=:auth_auth_lmt_c.mo
	WHERE rowid=:auth_auth_lmt_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_auth_lmt error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Auth_auth_lmt_Del_Upd(struct auth_auth_lmt_c auth_auth_lmt_c , char * reply  ){

	EXEC SQL DELETE FROM auth_auth_lmt WHERE rowid=:auth_auth_lmt_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_auth_lmt error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_auth_lmt_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_auth_lmt;
	return 0;
}

