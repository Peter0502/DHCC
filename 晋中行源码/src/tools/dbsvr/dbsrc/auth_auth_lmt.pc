#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"auth_auth_lmt_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int auth_auth_lmt_con ( struct auth_auth_lmt_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct auth_auth_lmt_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->auth_code);
	zip_tail(ps->mo);
}
int Auth_auth_lmt_Debug(struct auth_auth_lmt_c *ps){
	vtcp_log("auth_auth_lmt_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("auth_auth_lmt_c.auth_code=[%s]\n",ps->auth_code);
	vtcp_log("auth_auth_lmt_c.min_amt=[%f]\n",ps->min_amt);
	vtcp_log("auth_auth_lmt_c.max_amt=[%f]\n",ps->max_amt);
	vtcp_log("auth_auth_lmt_c.mo=[%s]\n",ps->mo);
