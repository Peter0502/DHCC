}

int Mo_def_agio_Fet_Upd( struct mo_def_agio_c *mo_def_agio_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_def_agio INTO :mo_def_agio_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
