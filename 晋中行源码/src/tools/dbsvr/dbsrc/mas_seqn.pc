#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"mas_seqn_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mas_seqn_con ( struct mas_seqn_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mas_seqn_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->name);
}
int Mas_seqn_Debug(struct mas_seqn_c *ps){
	vtcp_log("mas_seqn_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("mas_seqn_c.seqn=[%ld]\n",ps->seqn);
