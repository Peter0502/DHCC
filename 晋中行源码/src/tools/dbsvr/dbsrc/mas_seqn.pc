int Mas_seqn_Ins( struct mas_seqn_c mas_seqn_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mas_seqn VALUES (:mas_seqn_c.seqn,
			:mas_seqn_c.name);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mas_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mas_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
