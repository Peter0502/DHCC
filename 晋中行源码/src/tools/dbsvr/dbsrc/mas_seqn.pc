int Mas_seqn_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mas_seqn.* FROM mas_seqn WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mas_seqn_upd FROM :comm;

	EXEC SQL DECLARE upd_mas_seqn CURSOR FOR mas_seqn_upd;

	EXEC SQL OPEN upd_mas_seqn;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mas_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mas_seqn_Fet_Upd( struct mas_seqn_c *mas_seqn_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mas_seqn INTO :mas_seqn_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mas_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mas_seqn finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mas_seqn_c);

	return 0;
}

int Mas_seqn_Upd_Upd( struct mas_seqn_c mas_seqn_c , char * reply){
	int ret;

	EXEC SQL UPDATE mas_seqn SET seqn=:mas_seqn_c.seqn,
			name=:mas_seqn_c.name
	WHERE rowid=:mas_seqn_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
