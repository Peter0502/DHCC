}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mas_seqn_Sel(reply,mas_seqn_c,fmtstr,va_alist)
char    *reply;
struct  mas_seqn_c *mas_seqn_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mas_seqn_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mas_seqn_Fet_Sel( mas_seqn_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mas_seqn_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mas_seqn_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mas_seqn.* FROM mas_seqn WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mas_seqn_sel FROM :comm;
	EXEC SQL DECLARE sel_mas_seqn CURSOR FOR mas_seqn_sel;

	EXEC SQL OPEN sel_mas_seqn;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mas_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mas_seqn_Fet_Sel ( struct mas_seqn_c *mas_seqn_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mas_seqn INTO :mas_seqn_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mas_seqn error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mas_seqn finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mas_seqn_c);

	return 0;
}

int Mas_seqn_Clo_Sel (){
	EXEC SQL CLOSE sel_mas_seqn;
	return 0;
}

