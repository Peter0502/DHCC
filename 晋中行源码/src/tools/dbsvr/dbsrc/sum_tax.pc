int Sum_tax_Ins( struct sum_tax_c sum_tax_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO sum_tax VALUES (:sum_tax_c.date,
			:sum_tax_c.br_no,
			:sum_tax_c.prdt_no,
			:sum_tax_c.cls_amt,
			:sum_tax_c.intst,
			:sum_tax_c.tax,
			:sum_tax_c.cnt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT sum_tax error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT sum_tax error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
