		return CM_SQLCODE;
	}

	return 0;
}

int Sum_tax_Fet_Upd( struct sum_tax_c *sum_tax_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_sum_tax INTO :sum_tax_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd sum_tax error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH sum_tax finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(sum_tax_c);

	return 0;
}

int Sum_tax_Upd_Upd( struct sum_tax_c sum_tax_c , char * reply){
	int ret;

	EXEC SQL UPDATE sum_tax SET "date"=:sum_tax_c.date,
			br_no=:sum_tax_c.br_no,
			prdt_no=:sum_tax_c.prdt_no,
			cls_amt=:sum_tax_c.cls_amt,
			intst=:sum_tax_c.intst,
			tax=:sum_tax_c.tax,
