#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"sum_tax_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int sum_tax_con ( struct sum_tax_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct sum_tax_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->br_no);
	zip_tail(ps->prdt_no);
}
