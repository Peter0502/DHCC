	return 0;
}

int Auth_seqn_Upd_Upd( struct auth_seqn_c auth_seqn_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_seqn SET seqn=:auth_seqn_c.seqn
	WHERE rowid=:auth_seqn_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
