int Tmp_table_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,tmp_table.* FROM tmp_table WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tmp_table_upd FROM :comm;

	EXEC SQL DECLARE upd_tmp_table CURSOR FOR tmp_table_upd;

	EXEC SQL OPEN upd_tmp_table;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tmp_table error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_table_Fet_Upd( struct tmp_table_c *tmp_table_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tmp_table INTO :tmp_table_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tmp_table error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tmp_table finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tmp_table_c);

	return 0;
}

int Tmp_table_Upd_Upd( struct tmp_table_c tmp_table_c , char * reply){
	int ret;

	EXEC SQL UPDATE tmp_table SET ac_id=:tmp_table_c.ac_id,
			ac_seqn=:tmp_table_c.ac_seqn,
			bz=:tmp_table_c.bz
	WHERE rowid=:tmp_table_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tmp_table error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


