int Tmp_table_Ins( struct tmp_table_c tmp_table_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tmp_table VALUES (:tmp_table_c.ac_id,
			:tmp_table_c.ac_seqn,
			:tmp_table_c.bz);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_table error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_table error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
