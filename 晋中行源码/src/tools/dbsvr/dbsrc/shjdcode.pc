		sprintf(acErrMsg,"FETCH shjdcode finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(shjdcode_c);

	return 0;
}

int Shjdcode_Upd_Upd( struct shjdcode_c shjdcode_c , char * reply){
	int ret;

	EXEC SQL UPDATE shjdcode SET trancode=:shjdcode_c.trancode,
			tranname=:shjdcode_c.tranname,
			txcode=:shjdcode_c.txcode,
			text=:shjdcode_c.text
	WHERE rowid=:shjdcode_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE shjdcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


