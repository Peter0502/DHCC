}

int Shjdcode_Fet_Upd( struct shjdcode_c *shjdcode_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_shjdcode INTO :shjdcode_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd shjdcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
