/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Shjdcode_Ins( struct shjdcode_c shjdcode_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO shjdcode VALUES (:shjdcode_c.trancode,
			:shjdcode_c.tranname,
			:shjdcode_c.txcode,
			:shjdcode_c.text);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT shjdcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT shjdcode error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
