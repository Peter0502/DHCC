int Mo_jl_bal_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_jl_bal.* FROM mo_jl_bal WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_jl_bal_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_jl_bal CURSOR FOR mo_jl_bal_upd;

	EXEC SQL OPEN upd_mo_jl_bal;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_jl_bal error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_jl_bal_Fet_Upd( struct mo_jl_bal_c *mo_jl_bal_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_jl_bal INTO :mo_jl_bal_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_jl_bal error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_jl_bal finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_jl_bal_c);

	return 0;
}

int Mo_jl_bal_Upd_Upd( struct mo_jl_bal_c mo_jl_bal_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_jl_bal SET ac_id=:mo_jl_bal_c.ac_id,
			ac_seqn=:mo_jl_bal_c.ac_seqn,
			beg_date=:mo_jl_bal_c.beg_date,
			end_date=:mo_jl_bal_c.end_date,
			bal=:mo_jl_bal_c.bal
	WHERE rowid=:mo_jl_bal_c.rowid;
