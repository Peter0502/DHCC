int Com_rate_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,com_rate.* FROM com_rate WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_rate_upd FROM :comm;

	EXEC SQL DECLARE upd_com_rate CURSOR FOR com_rate_upd;

	EXEC SQL OPEN upd_com_rate;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open com_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_rate_Fet_Upd( struct com_rate_c *com_rate_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_com_rate INTO :com_rate_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd com_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH com_rate finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(com_rate_c);

	return 0;
}

int Com_rate_Upd_Upd( struct com_rate_c com_rate_c , char * reply){
	int ret;

	EXEC SQL UPDATE com_rate SET cur_no=:com_rate_c.cur_no,
			rate_code=:com_rate_c.rate_code,
			name=:com_rate_c.name,
			beg_date=:com_rate_c.beg_date,
			end_date=:com_rate_c.end_date,
			rate_val=:com_rate_c.rate_val,
			rate_type=:com_rate_c.rate_type,
			rate_unit=:com_rate_c.rate_unit,
			fl_rate_code=:com_rate_c.fl_rate_code,
			ref_rate_code=:com_rate_c.ref_rate_code,
			ref_ratio=:com_rate_c.ref_ratio
	WHERE rowid=:com_rate_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE com_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


