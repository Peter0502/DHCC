int Com_rate_Ins( struct com_rate_c com_rate_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO com_rate VALUES (:com_rate_c.cur_no,
			:com_rate_c.rate_code,
			:com_rate_c.name,
			:com_rate_c.beg_date,
			:com_rate_c.end_date,
			:com_rate_c.rate_val,
			:com_rate_c.rate_type,
			:com_rate_c.rate_unit,
			:com_rate_c.fl_rate_code,
			:com_rate_c.ref_rate_code,
			:com_rate_c.ref_ratio);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT com_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT com_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
