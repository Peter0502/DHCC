/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Tel_save_inf_Sel(reply,tel_save_inf_c,fmtstr,va_alist)
char    *reply;
struct  tel_save_inf_c *tel_save_inf_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Tel_save_inf_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Tel_save_inf_Fet_Sel( tel_save_inf_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Tel_save_inf_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Tel_save_inf_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,tel_save_inf.* FROM tel_save_inf WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE tel_save_inf_sel FROM :comm;
	EXEC SQL DECLARE sel_tel_save_inf CURSOR FOR tel_save_inf_sel;

	EXEC SQL OPEN sel_tel_save_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN tel_save_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tel_save_inf_Fet_Sel ( struct tel_save_inf_c *tel_save_inf_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_tel_save_inf INTO :tel_save_inf_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch tel_save_inf error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch tel_save_inf finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(tel_save_inf_c);

	return 0;
}

int Tel_save_inf_Clo_Sel (){
	EXEC SQL CLOSE sel_tel_save_inf;
	return 0;
}

