int Changetab_Ins( struct changetab_c changetab_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO changetab VALUES (:changetab_c.change_no,
			:changetab_c.br_no,
			:changetab_c.br_name,
			:changetab_c.address,
			:changetab_c.phone,
			:changetab_c.postcode,
			:changetab_c.fax,
			:changetab_c.telegrap,
			:changetab_c.stat);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
