/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Changetab_Sel(reply,changetab_c,fmtstr,va_alist)
char    *reply;
struct  changetab_c *changetab_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Changetab_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Changetab_Fet_Sel( changetab_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Changetab_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Changetab_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,changetab.* FROM changetab WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE changetab_sel FROM :comm;
	EXEC SQL DECLARE sel_changetab CURSOR FOR changetab_sel;

	EXEC SQL OPEN sel_changetab;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Changetab_Fet_Sel ( struct changetab_c *changetab_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_changetab INTO :changetab_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch changetab error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch changetab finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(changetab_c);

	return 0;
}

int Changetab_Clo_Sel (){
	EXEC SQL CLOSE sel_changetab;
	return 0;
}

