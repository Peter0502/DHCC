int Changetab_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,changetab.* FROM changetab WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE changetab_upd FROM :comm;

	EXEC SQL DECLARE upd_changetab CURSOR FOR changetab_upd;

	EXEC SQL OPEN upd_changetab;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Changetab_Fet_Upd( struct changetab_c *changetab_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_changetab INTO :changetab_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH changetab finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(changetab_c);

	return 0;
}

int Changetab_Upd_Upd( struct changetab_c changetab_c , char * reply){
	int ret;

	EXEC SQL UPDATE changetab SET change_no=:changetab_c.change_no,
			br_no=:changetab_c.br_no,
			br_name=:changetab_c.br_name,
			address=:changetab_c.address,
			phone=:changetab_c.phone,
			postcode=:changetab_c.postcode,
			fax=:changetab_c.fax,
			telegrap=:changetab_c.telegrap,
			stat=:changetab_c.stat
	WHERE rowid=:changetab_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE changetab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


