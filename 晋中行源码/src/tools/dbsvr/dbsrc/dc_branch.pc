#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int dc_branch_con ( struct dc_branch_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct dc_branch_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->br_no);
	zip_tail(ps->br_name);
	zip_tail(ps->cash_ac_ind);
	zip_tail(ps->br_lvl);
	zip_tail(ps->up_br_no);
}
