int Dc_branch_Ins( struct dc_branch_c dc_branch_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO dc_branch VALUES (:dc_branch_c.br_no,
			:dc_branch_c.br_name,
			:dc_branch_c.cash_ac_ind,
			:dc_branch_c.br_lvl,
			:dc_branch_c.up_br_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT dc_branch error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT dc_branch error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
