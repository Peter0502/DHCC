int Ac_ac_rel_Ins( struct ac_ac_rel_c ac_ac_rel_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO ac_ac_rel VALUES (:ac_ac_rel_c.ac_id,
			:ac_ac_rel_c.ac_id1);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
