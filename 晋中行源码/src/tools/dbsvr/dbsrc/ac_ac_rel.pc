/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ac_ac_rel_Sel(reply,ac_ac_rel_c,fmtstr,va_alist)
char    *reply;
struct  ac_ac_rel_c *ac_ac_rel_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ac_ac_rel_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ac_ac_rel_Fet_Sel( ac_ac_rel_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ac_ac_rel_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ac_ac_rel_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ac_ac_rel.* FROM ac_ac_rel WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ac_ac_rel_sel FROM :comm;
	EXEC SQL DECLARE sel_ac_ac_rel CURSOR FOR ac_ac_rel_sel;

	EXEC SQL OPEN sel_ac_ac_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ac_ac_rel_Fet_Sel ( struct ac_ac_rel_c *ac_ac_rel_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ac_ac_rel INTO :ac_ac_rel_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ac_ac_rel error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ac_ac_rel finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ac_ac_rel_c);

	return 0;
}

int Ac_ac_rel_Clo_Sel (){
	EXEC SQL CLOSE sel_ac_ac_rel;
	return 0;
}

/*****************************************************************************/
/****                              修改函数部分                           ****/
/*****************************************************************************/
int Ac_ac_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ac_ac_rel.* FROM ac_ac_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ac_ac_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_ac_ac_rel CURSOR FOR ac_ac_rel_upd;

	EXEC SQL OPEN upd_ac_ac_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ac_ac_rel_Fet_Upd( struct ac_ac_rel_c *ac_ac_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ac_ac_rel INTO :ac_ac_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ac_ac_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ac_ac_rel_c);

	return 0;
}

int Ac_ac_rel_Upd_Upd( struct ac_ac_rel_c ac_ac_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE ac_ac_rel SET ac_id=:ac_ac_rel_c.ac_id,
			ac_id1=:ac_ac_rel_c.ac_id1
	WHERE rowid=:ac_ac_rel_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Ac_ac_rel_Del_Upd(struct ac_ac_rel_c ac_ac_rel_c , char * reply  ){

	EXEC SQL DELETE FROM ac_ac_rel WHERE rowid=:ac_ac_rel_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE ac_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ac_ac_rel_Clo_Upd( ){
	EXEC SQL CLOSE upd_ac_ac_rel;
	return 0;
}

