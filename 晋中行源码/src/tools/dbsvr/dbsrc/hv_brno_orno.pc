/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Hv_brno_orno_Sel(reply,hv_brno_orno_c,fmtstr,va_alist)
char    *reply;
struct  hv_brno_orno_c *hv_brno_orno_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Hv_brno_orno_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Hv_brno_orno_Fet_Sel( hv_brno_orno_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Hv_brno_orno_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Hv_brno_orno_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,hv_brno_orno.* FROM hv_brno_orno WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE hv_brno_orno_sel FROM :comm;
	EXEC SQL DECLARE sel_hv_brno_orno CURSOR FOR hv_brno_orno_sel;

	EXEC SQL OPEN sel_hv_brno_orno;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN hv_brno_orno error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Hv_brno_orno_Fet_Sel ( struct hv_brno_orno_c *hv_brno_orno_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_hv_brno_orno INTO :hv_brno_orno_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch hv_brno_orno error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch hv_brno_orno finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(hv_brno_orno_c);

	return 0;
}

int Hv_brno_orno_Clo_Sel (){
	EXEC SQL CLOSE sel_hv_brno_orno;
	return 0;
}

