/****                              修改函数部分                           ****/
/*****************************************************************************/
int Td_jycx_tmp_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,td_jycx_tmp.* FROM td_jycx_tmp WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_jycx_tmp_upd FROM :comm;

	EXEC SQL DECLARE upd_td_jycx_tmp CURSOR FOR td_jycx_tmp_upd;

	EXEC SQL OPEN upd_td_jycx_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open td_jycx_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_jycx_tmp_Fet_Upd( struct td_jycx_tmp_c *td_jycx_tmp_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_td_jycx_tmp INTO :td_jycx_tmp_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd td_jycx_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH td_jycx_tmp finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(td_jycx_tmp_c);

	return 0;
}

int Td_jycx_tmp_Upd_Upd( struct td_jycx_tmp_c td_jycx_tmp_c , char * reply){
	int ret;

	EXEC SQL UPDATE td_jycx_tmp SET ac_no=:td_jycx_tmp_c.ac_no
	WHERE rowid=:td_jycx_tmp_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE td_jycx_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Td_jycx_tmp_Del_Upd(struct td_jycx_tmp_c td_jycx_tmp_c , char * reply  ){

	EXEC SQL DELETE FROM td_jycx_tmp WHERE rowid=:td_jycx_tmp_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE td_jycx_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

