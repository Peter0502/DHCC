/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Td_jycx_tmp_Sel(reply,td_jycx_tmp_c,fmtstr,va_alist)
char    *reply;
struct  td_jycx_tmp_c *td_jycx_tmp_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Td_jycx_tmp_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Td_jycx_tmp_Fet_Sel( td_jycx_tmp_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Td_jycx_tmp_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Td_jycx_tmp_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,td_jycx_tmp.* FROM td_jycx_tmp WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_jycx_tmp_sel FROM :comm;
	EXEC SQL DECLARE sel_td_jycx_tmp CURSOR FOR td_jycx_tmp_sel;

	EXEC SQL OPEN sel_td_jycx_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN td_jycx_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_jycx_tmp_Fet_Sel ( struct td_jycx_tmp_c *td_jycx_tmp_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_td_jycx_tmp INTO :td_jycx_tmp_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch td_jycx_tmp error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch td_jycx_tmp finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(td_jycx_tmp_c);

	return 0;
}

int Td_jycx_tmp_Clo_Sel (){
	EXEC SQL CLOSE sel_td_jycx_tmp;
	return 0;
}

