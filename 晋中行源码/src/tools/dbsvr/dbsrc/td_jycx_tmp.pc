#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"td_jycx_tmp_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int td_jycx_tmp_con ( struct td_jycx_tmp_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct td_jycx_tmp_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->ac_no);
}
