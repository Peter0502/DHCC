#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"mbfe_sys_ctl_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mbfe_sys_ctl_con ( struct mbfe_sys_ctl_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mbfe_sys_ctl_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->par_code);
	zip_tail(ps->par_name);
	zip_tail(ps->par_value);
	zip_tail(ps->par_desc);
}
int Mbfe_sys_ctl_Debug(struct mbfe_sys_ctl_c *ps){
	vtcp_log("mbfe_sys_ctl_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("mbfe_sys_ctl_c.par_code=[%s]\n",ps->par_code);
	vtcp_log("mbfe_sys_ctl_c.par_name=[%s]\n",ps->par_name);
	vtcp_log("mbfe_sys_ctl_c.par_value=[%s]\n",ps->par_value);
	vtcp_log("mbfe_sys_ctl_c.par_desc=[%s]\n",ps->par_desc);
