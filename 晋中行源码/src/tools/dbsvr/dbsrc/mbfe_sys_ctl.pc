int Mbfe_sys_ctl_Ins( struct mbfe_sys_ctl_c mbfe_sys_ctl_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mbfe_sys_ctl VALUES (:mbfe_sys_ctl_c.par_code,
			:mbfe_sys_ctl_c.par_name,
			:mbfe_sys_ctl_c.par_value,
			:mbfe_sys_ctl_c.par_desc);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mbfe_sys_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mbfe_sys_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
