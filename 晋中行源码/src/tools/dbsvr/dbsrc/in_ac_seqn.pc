int In_ac_seqn_Ins( struct in_ac_seqn_c in_ac_seqn_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO in_ac_seqn VALUES (:in_ac_seqn_c.app_cod,
			:in_ac_seqn_c.seque);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
