int In_ac_seqn_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,in_ac_seqn.* FROM in_ac_seqn WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE in_ac_seqn_upd FROM :comm;

	EXEC SQL DECLARE upd_in_ac_seqn CURSOR FOR in_ac_seqn_upd;

	EXEC SQL OPEN upd_in_ac_seqn;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int In_ac_seqn_Fet_Upd( struct in_ac_seqn_c *in_ac_seqn_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_in_ac_seqn INTO :in_ac_seqn_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH in_ac_seqn finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(in_ac_seqn_c);

	return 0;
}

int In_ac_seqn_Upd_Upd( struct in_ac_seqn_c in_ac_seqn_c , char * reply){
	int ret;

	EXEC SQL UPDATE in_ac_seqn SET app_cod=:in_ac_seqn_c.app_cod,
			seque=:in_ac_seqn_c.seque
	WHERE rowid=:in_ac_seqn_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int In_ac_seqn_Del_Upd(struct in_ac_seqn_c in_ac_seqn_c , char * reply  ){

	EXEC SQL DELETE FROM in_ac_seqn WHERE rowid=:in_ac_seqn_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE in_ac_seqn error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int In_ac_seqn_Clo_Upd( ){
	EXEC SQL CLOSE upd_in_ac_seqn;
	return 0;
}

