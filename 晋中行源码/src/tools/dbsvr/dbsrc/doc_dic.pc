EXEC SQL INCLUDE SQLCA;
#include"doc_dic_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int doc_dic_con ( struct doc_dic_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct doc_dic_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->tab);
	zip_tail(ps->fied);
	zip_tail(ps->type);
	zip_tail(ps->dic_code);
	zip_tail(ps->mo);
	zip_tail(ps->list);
}
