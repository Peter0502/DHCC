int Doc_dic_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,doc_dic.* FROM doc_dic WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE doc_dic_upd FROM :comm;

	EXEC SQL DECLARE upd_doc_dic CURSOR FOR doc_dic_upd;

	EXEC SQL OPEN upd_doc_dic;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Doc_dic_Fet_Upd( struct doc_dic_c *doc_dic_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_doc_dic INTO :doc_dic_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH doc_dic finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(doc_dic_c);

	return 0;
}

int Doc_dic_Upd_Upd( struct doc_dic_c doc_dic_c , char * reply){
	int ret;

	EXEC SQL UPDATE doc_dic SET seqn=:doc_dic_c.seqn,
			tab=:doc_dic_c.tab,
			fied=:doc_dic_c.fied,
			type=:doc_dic_c.type,
			leng=:doc_dic_c.leng,
			dec=:doc_dic_c.dec,
			dic_code=:doc_dic_c.dic_code,
			mo=:doc_dic_c.mo,
			list=:doc_dic_c.list
	WHERE rowid=:doc_dic_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Doc_dic_Del_Upd(struct doc_dic_c doc_dic_c , char * reply  ){

	EXEC SQL DELETE FROM doc_dic WHERE rowid=:doc_dic_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Doc_dic_Clo_Upd( ){
	EXEC SQL CLOSE upd_doc_dic;
	return 0;
}

/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Doc_dic_Ins( struct doc_dic_c doc_dic_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO doc_dic VALUES (:doc_dic_c.seqn,
			:doc_dic_c.tab,
			:doc_dic_c.fied,
			:doc_dic_c.type,
			:doc_dic_c.leng,
			:doc_dic_c.dec,
			:doc_dic_c.dic_code,
			:doc_dic_c.mo,
			:doc_dic_c.list);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
