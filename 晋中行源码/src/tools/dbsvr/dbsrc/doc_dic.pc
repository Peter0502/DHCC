/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Doc_dic_Sel(reply,doc_dic_c,fmtstr,va_alist)
char    *reply;
struct  doc_dic_c *doc_dic_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Doc_dic_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Doc_dic_Fet_Sel( doc_dic_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Doc_dic_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Doc_dic_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,doc_dic.* FROM doc_dic WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE doc_dic_sel FROM :comm;
	EXEC SQL DECLARE sel_doc_dic CURSOR FOR doc_dic_sel;

	EXEC SQL OPEN sel_doc_dic;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN doc_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Doc_dic_Fet_Sel ( struct doc_dic_c *doc_dic_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_doc_dic INTO :doc_dic_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch doc_dic error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch doc_dic finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(doc_dic_c);

	return 0;
}

int Doc_dic_Clo_Sel (){
	EXEC SQL CLOSE sel_doc_dic;
	return 0;
}

