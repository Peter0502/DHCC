/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Subbr_deposit_Sel(reply,subbr_deposit_c,fmtstr,va_alist)
char    *reply;
struct  subbr_deposit_c *subbr_deposit_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Subbr_deposit_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Subbr_deposit_Fet_Sel( subbr_deposit_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Subbr_deposit_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Subbr_deposit_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,subbr_deposit.* FROM subbr_deposit WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE subbr_deposit_sel FROM :comm;
	EXEC SQL DECLARE sel_subbr_deposit CURSOR FOR subbr_deposit_sel;

	EXEC SQL OPEN sel_subbr_deposit;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN subbr_deposit error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Subbr_deposit_Fet_Sel ( struct subbr_deposit_c *subbr_deposit_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_subbr_deposit INTO :subbr_deposit_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch subbr_deposit error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch subbr_deposit finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(subbr_deposit_c);

	return 0;
}

int Subbr_deposit_Clo_Sel (){
	EXEC SQL CLOSE sel_subbr_deposit;
	return 0;
}

