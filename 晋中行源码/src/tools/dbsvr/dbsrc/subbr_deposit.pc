	EXEC SQL INSERT INTO subbr_deposit VALUES (:subbr_deposit_c.up_br_no,
			:subbr_deposit_c.opn_br_no,
			:subbr_deposit_c.ex_year,
			:subbr_deposit_c.ex_month,
			:subbr_deposit_c.ac_id,
			:subbr_deposit_c.ac_seqn,
			:subbr_deposit_c.monthb_bal,
			:subbr_deposit_c.bal,
			:subbr_deposit_c.mon_increment,
			:subbr_deposit_c.month_addbal,
			:subbr_deposit_c.season_addbal,
			:subbr_deposit_c.year_addbal,
			:subbr_deposit_c.draw_deposit,
			:subbr_deposit_c.client_mgr);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT subbr_deposit error %d",sqlca.sqlcode);
