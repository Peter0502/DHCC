int Cupsinsctl_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cupsinsctl.* FROM cupsinsctl WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cupsinsctl_upd FROM :comm;

	EXEC SQL DECLARE upd_cupsinsctl CURSOR FOR cupsinsctl_upd;

	EXEC SQL OPEN upd_cupsinsctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cupsinsctl_Fet_Upd( struct cupsinsctl_c *cupsinsctl_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cupsinsctl INTO :cupsinsctl_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cupsinsctl finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cupsinsctl_c);

	return 0;
}

int Cupsinsctl_Upd_Upd( struct cupsinsctl_c cupsinsctl_c , char * reply){
	int ret;

	EXEC SQL UPDATE cupsinsctl SET trseq=:cupsinsctl_c.trseq,
			brtype=:cupsinsctl_c.brtype,
			lstinsday=:cupsinsctl_c.lstinsday,
			name1=:cupsinsctl_c.name1,
			name2=:cupsinsctl_c.name2,
			name3=:cupsinsctl_c.name3,
			tabname=:cupsinsctl_c.tabname
	WHERE rowid=:cupsinsctl_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


