/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cupsinsctl_Sel(reply,cupsinsctl_c,fmtstr,va_alist)
char    *reply;
struct  cupsinsctl_c *cupsinsctl_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cupsinsctl_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cupsinsctl_Fet_Sel( cupsinsctl_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cupsinsctl_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cupsinsctl_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cupsinsctl.* FROM cupsinsctl WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cupsinsctl_sel FROM :comm;
	EXEC SQL DECLARE sel_cupsinsctl CURSOR FOR cupsinsctl_sel;

	EXEC SQL OPEN sel_cupsinsctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cupsinsctl_Fet_Sel ( struct cupsinsctl_c *cupsinsctl_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cupsinsctl INTO :cupsinsctl_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cupsinsctl error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cupsinsctl finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cupsinsctl_c);

	return 0;
}

int Cupsinsctl_Clo_Sel (){
	EXEC SQL CLOSE sel_cupsinsctl;
	return 0;
}

