int Cupsinsctl_Ins( struct cupsinsctl_c cupsinsctl_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO cupsinsctl VALUES (:cupsinsctl_c.trseq,
			:cupsinsctl_c.brtype,
			:cupsinsctl_c.lstinsday,
			:cupsinsctl_c.name1,
			:cupsinsctl_c.name2,
			:cupsinsctl_c.name3,
			:cupsinsctl_c.tabname);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT cupsinsctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
