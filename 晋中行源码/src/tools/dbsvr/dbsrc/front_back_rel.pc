int Front_back_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,front_back_rel.* FROM front_back_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE front_back_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_front_back_rel CURSOR FOR front_back_rel_upd;

	EXEC SQL OPEN upd_front_back_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open front_back_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Front_back_rel_Fet_Upd( struct front_back_rel_c *front_back_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_front_back_rel INTO :front_back_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd front_back_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH front_back_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(front_back_rel_c);

	return 0;
}

int Front_back_rel_Upd_Upd( struct front_back_rel_c front_back_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE front_back_rel SET f_txcode=:front_back_rel_c.f_txcode,
			b_txcode=:front_back_rel_c.b_txcode,
			tx_name=:front_back_rel_c.tx_name,
