int auth_tel_tx_def_con ( struct auth_tel_tx_def_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct auth_tel_tx_def_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->tel);
	zip_tail(ps->br_no);
	zip_tail(ps->tx_code);
	zip_tail(ps->auth_code);
}
