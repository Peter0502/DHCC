}

int Auth_tel_tx_def_Fet_Upd( struct auth_tel_tx_def_c *auth_tel_tx_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_tel_tx_def INTO :auth_tel_tx_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_tel_tx_def error %d",sqlca.sqlcode);
