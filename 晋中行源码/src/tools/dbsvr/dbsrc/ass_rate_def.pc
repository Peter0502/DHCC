int Ass_rate_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ass_rate_def.* FROM ass_rate_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ass_rate_def_upd FROM :comm;

	EXEC SQL DECLARE upd_ass_rate_def CURSOR FOR ass_rate_def_upd;

	EXEC SQL OPEN upd_ass_rate_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ass_rate_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ass_rate_def_Fet_Upd( struct ass_rate_def_c *ass_rate_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ass_rate_def INTO :ass_rate_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ass_rate_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ass_rate_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ass_rate_def_c);

	return 0;
}

int Ass_rate_def_Upd_Upd( struct ass_rate_def_c ass_rate_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE ass_rate_def SET rate_code=:ass_rate_def_c.rate_code,
			seqn=:ass_rate_def_c.seqn,
			cond_ind=:ass_rate_def_c.cond_ind,
			min_bal=:ass_rate_def_c.min_bal,
			max_bal=:ass_rate_def_c.max_bal,
			rate=:ass_rate_def_c.rate
	WHERE rowid=:ass_rate_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ass_rate_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Ass_rate_def_Del_Upd(struct ass_rate_def_c ass_rate_def_c , char * reply  ){

	EXEC SQL DELETE FROM ass_rate_def WHERE rowid=:ass_rate_def_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE ass_rate_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ass_rate_def_Clo_Upd( ){
	EXEC SQL CLOSE upd_ass_rate_def;
	return 0;
}

