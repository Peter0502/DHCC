int Data_dic_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,data_dic.* FROM data_dic WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE data_dic_upd FROM :comm;

	EXEC SQL DECLARE upd_data_dic CURSOR FOR data_dic_upd;

	EXEC SQL OPEN upd_data_dic;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Data_dic_Fet_Upd( struct data_dic_c *data_dic_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_data_dic INTO :data_dic_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH data_dic finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(data_dic_c);

	return 0;
}

int Data_dic_Upd_Upd( struct data_dic_c data_dic_c , char * reply){
	int ret;

	EXEC SQL UPDATE data_dic SET data_code=:data_dic_c.data_code,
			data_type=:data_dic_c.data_type,
			data_leng=:data_dic_c.data_leng,
			data_dec=:data_dic_c.data_dec,
			data_name=:data_dic_c.data_name,
			bit_seq=:data_dic_c.bit_seq
	WHERE rowid=:data_dic_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Data_dic_Del_Upd(struct data_dic_c data_dic_c , char * reply  ){

	EXEC SQL DELETE FROM data_dic WHERE rowid=:data_dic_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Data_dic_Clo_Upd( ){
	EXEC SQL CLOSE upd_data_dic;
	return 0;
}

/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Data_dic_Ins( struct data_dic_c data_dic_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO data_dic VALUES (:data_dic_c.data_code,
			:data_dic_c.data_type,
			:data_dic_c.data_leng,
			:data_dic_c.data_dec,
			:data_dic_c.data_name,
			:data_dic_c.bit_seq);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT data_dic error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
