/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int In_mst_hst_Sel(reply,in_mst_hst_c,fmtstr,va_alist)
char    *reply;
struct  in_mst_hst_c *in_mst_hst_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = In_mst_hst_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = In_mst_hst_Fet_Sel( in_mst_hst_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	In_mst_hst_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int In_mst_hst_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,in_mst_hst.* FROM in_mst_hst WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE in_mst_hst_sel FROM :comm;
	EXEC SQL DECLARE sel_in_mst_hst CURSOR FOR in_mst_hst_sel;

	EXEC SQL OPEN sel_in_mst_hst;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN in_mst_hst error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int In_mst_hst_Fet_Sel ( struct in_mst_hst_c *in_mst_hst_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_in_mst_hst INTO :in_mst_hst_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch in_mst_hst error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch in_mst_hst finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(in_mst_hst_c);

	return 0;
}

int In_mst_hst_Clo_Sel (){
	EXEC SQL CLOSE sel_in_mst_hst;
	return 0;
}

