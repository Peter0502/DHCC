	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,in_mst_hst.* FROM in_mst_hst WHERE  ");
	key_strcat(comm,wherelist);
