	int ret;

	EXEC SQL INSERT INTO bal_ciftype VALUES (:bal_ciftype_c.opn_br_no,
			:bal_ciftype_c.bal,
			:bal_ciftype_c.ys_bal,
			:bal_ciftype_c.prdt_no,
			:bal_ciftype_c.cif_type);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT bal_ciftype error %d",sqlca.sqlcode);
