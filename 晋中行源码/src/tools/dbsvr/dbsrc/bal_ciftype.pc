/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Bal_ciftype_Sel(reply,bal_ciftype_c,fmtstr,va_alist)
char    *reply;
struct  bal_ciftype_c *bal_ciftype_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Bal_ciftype_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Bal_ciftype_Fet_Sel( bal_ciftype_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Bal_ciftype_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Bal_ciftype_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,bal_ciftype.* FROM bal_ciftype WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE bal_ciftype_sel FROM :comm;
	EXEC SQL DECLARE sel_bal_ciftype CURSOR FOR bal_ciftype_sel;

	EXEC SQL OPEN sel_bal_ciftype;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN bal_ciftype error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Bal_ciftype_Fet_Sel ( struct bal_ciftype_c *bal_ciftype_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_bal_ciftype INTO :bal_ciftype_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch bal_ciftype error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch bal_ciftype finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(bal_ciftype_c);

	return 0;
}

int Bal_ciftype_Clo_Sel (){
	EXEC SQL CLOSE sel_bal_ciftype;
	return 0;
}

