int Auth_tx_list_Ins( struct auth_tx_list_c auth_tx_list_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO auth_tx_list VALUES (:auth_tx_list_c.tx_type,
			:auth_tx_list_c.menu_code);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
