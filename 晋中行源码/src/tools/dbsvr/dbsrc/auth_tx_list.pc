int Auth_tx_list_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_tx_list.* FROM auth_tx_list WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_tx_list_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_tx_list CURSOR FOR auth_tx_list_upd;

	EXEC SQL OPEN upd_auth_tx_list;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_tx_list_Fet_Upd( struct auth_tx_list_c *auth_tx_list_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_tx_list INTO :auth_tx_list_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_tx_list finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_tx_list_c);

	return 0;
}

int Auth_tx_list_Upd_Upd( struct auth_tx_list_c auth_tx_list_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_tx_list SET tx_type=:auth_tx_list_c.tx_type,
			menu_code=:auth_tx_list_c.menu_code
	WHERE rowid=:auth_tx_list_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Auth_tx_list_Del_Upd(struct auth_tx_list_c auth_tx_list_c , char * reply  ){

	EXEC SQL DELETE FROM auth_tx_list WHERE rowid=:auth_tx_list_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_tx_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_tx_list_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_tx_list;
	return 0;
}

