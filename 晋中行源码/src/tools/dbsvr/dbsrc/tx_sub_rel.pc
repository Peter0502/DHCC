/****                              增加函数部分                           ****/
/*****************************************************************************/
int Tx_sub_rel_Ins( struct tx_sub_rel_c tx_sub_rel_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tx_sub_rel VALUES (:tx_sub_rel_c.tx_code,
			:tx_sub_rel_c.in_sub_tx,
			:tx_sub_rel_c.out_ind,
			:tx_sub_rel_c.out_data,
			:tx_sub_rel_c.in_data,
			:tx_sub_rel_c.memo);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tx_sub_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tx_sub_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
