	EXEC SQL INSERT INTO mo_pun_acc VALUES (:mo_pun_acc_c.ac_id,
			:mo_pun_acc_c.ac_seqn,
			:mo_pun_acc_c.sts,
			:mo_pun_acc_c.beg_date,
			:mo_pun_acc_c.end_date,
			:mo_pun_acc_c.beg_tel,
			:mo_pun_acc_c.beg_br_no,
			:mo_pun_acc_c.end_tel,
			:mo_pun_acc_c.end_br_no,
			:mo_pun_acc_c.brf);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mo_pun_acc error %d",sqlca.sqlcode);
