int Mo_pun_acc_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_pun_acc.* FROM mo_pun_acc WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_pun_acc_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_pun_acc CURSOR FOR mo_pun_acc_upd;

	EXEC SQL OPEN upd_mo_pun_acc;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_pun_acc error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_pun_acc_Fet_Upd( struct mo_pun_acc_c *mo_pun_acc_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_pun_acc INTO :mo_pun_acc_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_pun_acc error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_pun_acc finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_pun_acc_c);

	return 0;
}

int Mo_pun_acc_Upd_Upd( struct mo_pun_acc_c mo_pun_acc_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_pun_acc SET ac_id=:mo_pun_acc_c.ac_id,
			ac_seqn=:mo_pun_acc_c.ac_seqn,
			sts=:mo_pun_acc_c.sts,
			beg_date=:mo_pun_acc_c.beg_date,
			end_date=:mo_pun_acc_c.end_date,
			beg_tel=:mo_pun_acc_c.beg_tel,
			beg_br_no=:mo_pun_acc_c.beg_br_no,
