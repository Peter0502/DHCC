int Mo_def_rate_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_def_rate.* FROM mo_def_rate WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_def_rate_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_def_rate CURSOR FOR mo_def_rate_upd;

	EXEC SQL OPEN upd_mo_def_rate;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_def_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_def_rate_Fet_Upd( struct mo_def_rate_c *mo_def_rate_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_def_rate INTO :mo_def_rate_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_def_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_def_rate finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_def_rate_c);

	return 0;
}

int Mo_def_rate_Upd_Upd( struct mo_def_rate_c mo_def_rate_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_def_rate SET chrg_code=:mo_def_rate_c.chrg_code,
			chrg_type=:mo_def_rate_c.chrg_type,
			min_amt=:mo_def_rate_c.min_amt,
			max_amt=:mo_def_rate_c.max_amt,
			chrg_rate_no=:mo_def_rate_c.chrg_rate_no
	WHERE rowid=:mo_def_rate_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mo_def_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Mo_def_rate_Del_Upd(struct mo_def_rate_c mo_def_rate_c , char * reply  ){

	EXEC SQL DELETE FROM mo_def_rate WHERE rowid=:mo_def_rate_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE mo_def_rate error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_def_rate_Clo_Upd( ){
	EXEC SQL CLOSE upd_mo_def_rate;
	return 0;
}

