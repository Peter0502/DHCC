	EXEC SQL INSERT INTO mo_fax VALUES (:mo_fax_c.tx_date,
			:mo_fax_c.tx_br_no,
			:mo_fax_c.fax_no,
			:mo_fax_c.requ_name,
			:mo_fax_c.payer_br_no,
			:mo_fax_c.payer_br_name,
			:mo_fax_c.payer_ac_id,
			:mo_fax_c.payer_ac_seqn,
			:mo_fax_c.payer_name,
			:mo_fax_c.payee_br_no,
			:mo_fax_c.payee_br_name,
			:mo_fax_c.payee_ac_no,
			:mo_fax_c.payee_name,
			:mo_fax_c.fax_amt,
			:mo_fax_c.fax_sts,
			:mo_fax_c.pr_cnt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mo_fax error %d",sqlca.sqlcode);
