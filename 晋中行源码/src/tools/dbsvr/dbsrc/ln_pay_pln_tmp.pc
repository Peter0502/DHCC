int Ln_pay_pln_tmp_Sel(reply,ln_pay_pln_tmp_c,fmtstr,va_alist)
char    *reply;
struct  ln_pay_pln_tmp_c *ln_pay_pln_tmp_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ln_pay_pln_tmp_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ln_pay_pln_tmp_Fet_Sel( ln_pay_pln_tmp_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ln_pay_pln_tmp_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ln_pay_pln_tmp_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ln_pay_pln_tmp.* FROM ln_pay_pln_tmp WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ln_pay_pln_tmp_sel FROM :comm;
	EXEC SQL DECLARE sel_ln_pay_pln_tmp CURSOR FOR ln_pay_pln_tmp_sel;

	EXEC SQL OPEN sel_ln_pay_pln_tmp;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ln_pay_pln_tmp error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ln_pay_pln_tmp_Fet_Sel ( struct ln_pay_pln_tmp_c *ln_pay_pln_tmp_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ln_pay_pln_tmp INTO :ln_pay_pln_tmp_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ln_pay_pln_tmp error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ln_pay_pln_tmp finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ln_pay_pln_tmp_c);

	return 0;
}

int Ln_pay_pln_tmp_Clo_Sel (){
	EXEC SQL CLOSE sel_ln_pay_pln_tmp;
	return 0;
}

