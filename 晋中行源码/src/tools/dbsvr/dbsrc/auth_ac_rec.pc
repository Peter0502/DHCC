#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int auth_ac_rec_con ( struct auth_ac_rec_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct auth_ac_rec_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->dc);
	zip_tail(ps->ct);
	zip_tail(ps->ind);
	zip_tail(ps->tx_code);
}
