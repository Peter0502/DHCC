int Auth_ac_rec_Ins( struct auth_ac_rec_c auth_ac_rec_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO auth_ac_rec VALUES (:auth_ac_rec_c.ac_id,
			:auth_ac_rec_c.ac_seqn,
			:auth_ac_rec_c.dc,
			:auth_ac_rec_c.ct,
			:auth_ac_rec_c.ind,
			:auth_ac_rec_c.tx_code,
			:auth_ac_rec_c.amt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT auth_ac_rec error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT auth_ac_rec error %d",sqlca.sqlcode);
