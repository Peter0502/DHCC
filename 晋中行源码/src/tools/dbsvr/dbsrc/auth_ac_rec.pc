/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Auth_ac_rec_Sel(reply,auth_ac_rec_c,fmtstr,va_alist)
char    *reply;
struct  auth_ac_rec_c *auth_ac_rec_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Auth_ac_rec_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Auth_ac_rec_Fet_Sel( auth_ac_rec_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Auth_ac_rec_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Auth_ac_rec_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,auth_ac_rec.* FROM auth_ac_rec WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_ac_rec_sel FROM :comm;
	EXEC SQL DECLARE sel_auth_ac_rec CURSOR FOR auth_ac_rec_sel;

	EXEC SQL OPEN sel_auth_ac_rec;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN auth_ac_rec error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_ac_rec_Fet_Sel ( struct auth_ac_rec_c *auth_ac_rec_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_auth_ac_rec INTO :auth_ac_rec_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch auth_ac_rec error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch auth_ac_rec finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(auth_ac_rec_c);

	return 0;
}

int Auth_ac_rec_Clo_Sel (){
	EXEC SQL CLOSE sel_auth_ac_rec;
	return 0;
}

