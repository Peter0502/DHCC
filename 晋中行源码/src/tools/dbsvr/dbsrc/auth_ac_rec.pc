int Auth_ac_rec_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_ac_rec.* FROM auth_ac_rec WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_ac_rec_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_ac_rec CURSOR FOR auth_ac_rec_upd;

	EXEC SQL OPEN upd_auth_ac_rec;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_ac_rec error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_ac_rec_Fet_Upd( struct auth_ac_rec_c *auth_ac_rec_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_ac_rec INTO :auth_ac_rec_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_ac_rec error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_ac_rec finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_ac_rec_c);

	return 0;
}

int Auth_ac_rec_Upd_Upd( struct auth_ac_rec_c auth_ac_rec_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_ac_rec SET ac_id=:auth_ac_rec_c.ac_id,
			ac_seqn=:auth_ac_rec_c.ac_seqn,
			dc=:auth_ac_rec_c.dc,
			ct=:auth_ac_rec_c.ct,
			ind=:auth_ac_rec_c.ind,
			tx_code=:auth_ac_rec_c.tx_code,
			amt=:auth_ac_rec_c.amt
	WHERE rowid=:auth_ac_rec_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_ac_rec error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


