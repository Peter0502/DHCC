int Mo_dep_reg_Ins( struct mo_dep_reg_c mo_dep_reg_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mo_dep_reg VALUES (:mo_dep_reg_c.pact_no,
			:mo_dep_reg_c.po_no,
			:mo_dep_reg_c.po_amt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mo_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mo_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
