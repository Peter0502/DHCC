/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Hv_errormr_Sel(reply,hv_errormr_c,fmtstr,va_alist)
char    *reply;
struct  hv_errormr_c *hv_errormr_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Hv_errormr_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Hv_errormr_Fet_Sel( hv_errormr_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Hv_errormr_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Hv_errormr_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,hv_errormr.* FROM hv_errormr WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE hv_errormr_sel FROM :comm;
	EXEC SQL DECLARE sel_hv_errormr CURSOR FOR hv_errormr_sel;

	EXEC SQL OPEN sel_hv_errormr;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN hv_errormr error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Hv_errormr_Fet_Sel ( struct hv_errormr_c *hv_errormr_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_hv_errormr INTO :hv_errormr_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch hv_errormr error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch hv_errormr finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(hv_errormr_c);

	return 0;
}

int Hv_errormr_Clo_Sel (){
	EXEC SQL CLOSE sel_hv_errormr;
	return 0;
}

