int Mo_merge_ac_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_merge_ac.* FROM mo_merge_ac WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_merge_ac_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_merge_ac CURSOR FOR mo_merge_ac_upd;

	EXEC SQL OPEN upd_mo_merge_ac;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_merge_ac error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_merge_ac_Fet_Upd( struct mo_merge_ac_c *mo_merge_ac_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_merge_ac INTO :mo_merge_ac_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_merge_ac error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_merge_ac finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_merge_ac_c);

	return 0;
}

