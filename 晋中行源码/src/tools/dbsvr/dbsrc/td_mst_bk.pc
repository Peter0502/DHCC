/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Td_mst_bk_Sel(reply,td_mst_bk_c,fmtstr,va_alist)
char    *reply;
struct  td_mst_bk_c *td_mst_bk_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Td_mst_bk_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Td_mst_bk_Fet_Sel( td_mst_bk_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Td_mst_bk_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Td_mst_bk_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,td_mst_bk.* FROM td_mst_bk WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_mst_bk_sel FROM :comm;
	EXEC SQL DECLARE sel_td_mst_bk CURSOR FOR td_mst_bk_sel;

	EXEC SQL OPEN sel_td_mst_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN td_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_mst_bk_Fet_Sel ( struct td_mst_bk_c *td_mst_bk_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_td_mst_bk INTO :td_mst_bk_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch td_mst_bk error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch td_mst_bk finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(td_mst_bk_c);

	return 0;
}

int Td_mst_bk_Clo_Sel (){
	EXEC SQL CLOSE sel_td_mst_bk;
	return 0;
}

