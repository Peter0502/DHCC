int Td_mst_bk_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,td_mst_bk.* FROM td_mst_bk WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_mst_bk_upd FROM :comm;

	EXEC SQL DECLARE upd_td_mst_bk CURSOR FOR td_mst_bk_upd;

	EXEC SQL OPEN upd_td_mst_bk;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open td_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_mst_bk_Fet_Upd( struct td_mst_bk_c *td_mst_bk_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_td_mst_bk INTO :td_mst_bk_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd td_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH td_mst_bk finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(td_mst_bk_c);

	return 0;
}

int Td_mst_bk_Upd_Upd( struct td_mst_bk_c td_mst_bk_c , char * reply){
	int ret;

	EXEC SQL UPDATE td_mst_bk SET opn_br_no=:td_mst_bk_c.opn_br_no,
			prdt_no=:td_mst_bk_c.prdt_no,
			bal=:td_mst_bk_c.bal,
			cnt=:td_mst_bk_c.cnt
	WHERE rowid=:td_mst_bk_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE td_mst_bk error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


