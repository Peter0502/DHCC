int Chnl_tradelimit_Fet_Upd( struct chnl_tradelimit_c *chnl_tradelimit_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_chnl_tradelimit INTO :chnl_tradelimit_c;
