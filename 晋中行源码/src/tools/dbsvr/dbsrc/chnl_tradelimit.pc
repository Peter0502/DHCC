/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Chnl_tradelimit_Sel(reply,chnl_tradelimit_c,fmtstr,va_alist)
char    *reply;
struct  chnl_tradelimit_c *chnl_tradelimit_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Chnl_tradelimit_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Chnl_tradelimit_Fet_Sel( chnl_tradelimit_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Chnl_tradelimit_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Chnl_tradelimit_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,chnl_tradelimit.* FROM chnl_tradelimit WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE chnl_tradelimit_sel FROM :comm;
	EXEC SQL DECLARE sel_chnl_tradelimit CURSOR FOR chnl_tradelimit_sel;

	EXEC SQL OPEN sel_chnl_tradelimit;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN chnl_tradelimit error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Chnl_tradelimit_Fet_Sel ( struct chnl_tradelimit_c *chnl_tradelimit_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_chnl_tradelimit INTO :chnl_tradelimit_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch chnl_tradelimit error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch chnl_tradelimit finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(chnl_tradelimit_c);

	return 0;
}

int Chnl_tradelimit_Clo_Sel (){
	EXEC SQL CLOSE sel_chnl_tradelimit;
	return 0;
}

