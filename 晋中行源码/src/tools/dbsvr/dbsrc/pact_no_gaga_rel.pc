int Pact_no_gaga_rel_Sel(reply,pact_no_gaga_rel_c,fmtstr,va_alist)
char    *reply;
struct  pact_no_gaga_rel_c *pact_no_gaga_rel_c;
char    *fmtstr;
va_dcl
{
	int   ret;
