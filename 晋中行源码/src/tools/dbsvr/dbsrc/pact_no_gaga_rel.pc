	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Pact_no_gaga_rel_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Pact_no_gaga_rel_Fet_Sel( pact_no_gaga_rel_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Pact_no_gaga_rel_Clo_Sel();

