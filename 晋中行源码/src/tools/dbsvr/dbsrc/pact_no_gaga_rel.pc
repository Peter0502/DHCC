char *key_strcat(char *,char *);
int Pact_no_gaga_rel_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,pact_no_gaga_rel.* FROM pact_no_gaga_rel WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE pact_no_gaga_rel_sel FROM :comm;
	EXEC SQL DECLARE sel_pact_no_gaga_rel CURSOR FOR pact_no_gaga_rel_sel;

	EXEC SQL OPEN sel_pact_no_gaga_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN pact_no_gaga_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Pact_no_gaga_rel_Fet_Sel ( struct pact_no_gaga_rel_c *pact_no_gaga_rel_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_pact_no_gaga_rel INTO :pact_no_gaga_rel_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch pact_no_gaga_rel error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch pact_no_gaga_rel finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(pact_no_gaga_rel_c);

	return 0;
}

int Pact_no_gaga_rel_Clo_Sel (){
	EXEC SQL CLOSE sel_pact_no_gaga_rel;
	return 0;
}

