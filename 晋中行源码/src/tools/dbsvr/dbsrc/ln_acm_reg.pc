}

int Ln_acm_reg_Fet_Upd( struct ln_acm_reg_c *ln_acm_reg_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ln_acm_reg INTO :ln_acm_reg_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
