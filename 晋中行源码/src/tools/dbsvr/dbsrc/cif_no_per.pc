		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_no_per_Del_Upd(struct cif_no_per_c cif_no_per_c , char * reply  ){

	EXEC SQL DELETE FROM cif_no_per WHERE rowid=:cif_no_per_c.rowid;
