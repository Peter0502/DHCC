int Cif_no_per_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_no_per.* FROM cif_no_per WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_no_per_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_no_per CURSOR FOR cif_no_per_upd;

	EXEC SQL OPEN upd_cif_no_per;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_no_per error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_no_per_Fet_Upd( struct cif_no_per_c *cif_no_per_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_no_per INTO :cif_no_per_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_no_per error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_no_per finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_no_per_c);

	return 0;
}

int Cif_no_per_Upd_Upd( struct cif_no_per_c cif_no_per_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_no_per SET cif_no=:cif_no_per_c.cif_no
	WHERE rowid=:cif_no_per_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
