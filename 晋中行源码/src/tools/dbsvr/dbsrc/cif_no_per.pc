/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_no_per_Sel(reply,cif_no_per_c,fmtstr,va_alist)
char    *reply;
struct  cif_no_per_c *cif_no_per_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_no_per_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_no_per_Fet_Sel( cif_no_per_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_no_per_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_no_per_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_no_per.* FROM cif_no_per WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_no_per_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_no_per CURSOR FOR cif_no_per_sel;

	EXEC SQL OPEN sel_cif_no_per;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_no_per error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_no_per_Fet_Sel ( struct cif_no_per_c *cif_no_per_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_no_per INTO :cif_no_per_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_no_per error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_no_per finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_no_per_c);

	return 0;
}

int Cif_no_per_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_no_per;
	return 0;
}

