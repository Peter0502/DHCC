int Mo_po_co_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_po_co.* FROM mo_po_co WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_po_co_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_po_co CURSOR FOR mo_po_co_upd;

	EXEC SQL OPEN upd_mo_po_co;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_po_co_Fet_Upd( struct mo_po_co_c *mo_po_co_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_po_co INTO :mo_po_co_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_po_co finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_po_co_c);

	return 0;
}

int Mo_po_co_Upd_Upd( struct mo_po_co_c mo_po_co_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_po_co SET pact_no=:mo_po_co_c.pact_no,
			contract_no=:mo_po_co_c.contract_no
	WHERE rowid=:mo_po_co_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Mo_po_co_Del_Upd(struct mo_po_co_c mo_po_co_c , char * reply  ){

	EXEC SQL DELETE FROM mo_po_co WHERE rowid=:mo_po_co_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_po_co_Clo_Upd( ){
	EXEC SQL CLOSE upd_mo_po_co;
	return 0;
}

