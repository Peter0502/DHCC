int Mo_po_co_Ins( struct mo_po_co_c mo_po_co_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mo_po_co VALUES (:mo_po_co_c.pact_no,
			:mo_po_co_c.contract_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mo_po_co error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
