/*****************************************************************************/
/****                              查询函数部分                           ****/
/*****************************************************************************/
int Mq_batch_head_Sel(reply,mq_batch_head_c,fmtstr,va_alist)
char    *reply;
struct  mq_batch_head_c *mq_batch_head_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mq_batch_head_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mq_batch_head_Fet_Sel( mq_batch_head_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mq_batch_head_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mq_batch_head_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT mq_batch_head.* FROM mq_batch_head WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_batch_head_sel FROM :comm;
	EXEC SQL DECLARE sel_mq_batch_head CURSOR FOR mq_batch_head_sel;

	EXEC SQL OPEN sel_mq_batch_head;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mq_batch_head error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_batch_head_Fet_Sel ( struct mq_batch_head_c *mq_batch_head_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mq_batch_head INTO :mq_batch_head_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mq_batch_head error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mq_batch_head finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mq_batch_head_c);

	return 0;
}

int Mq_batch_head_Clo_Sel (){
	EXEC SQL CLOSE sel_mq_batch_head;
}

