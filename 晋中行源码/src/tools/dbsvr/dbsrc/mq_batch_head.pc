int Mq_batch_head_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT mq_batch_head.* FROM mq_batch_head WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_batch_head_upd FROM :comm;

	EXEC SQL DECLARE upd_mq_batch_head CURSOR FOR mq_batch_head_upd;

	EXEC SQL OPEN upd_mq_batch_head;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mq_batch_head error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_batch_head_Fet_Upd( struct mq_batch_head_c *mq_batch_head_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mq_batch_head INTO :mq_batch_head_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mq_batch_head error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mq_batch_head finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mq_batch_head_c);

	return 0;
}

int Mq_batch_head_Upd_Upd( struct mq_batch_head_c mq_batch_head_c , char * reply){
	int ret;

	EXEC SQL UPDATE mq_batch_head SET msg_id=:mq_batch_head_c.msg_id,
			paybk_code=:mq_batch_head_c.paybk_code,
			payee_bankno=:mq_batch_head_c.payee_bankno,
			chk_date=:mq_batch_head_c.chk_date,
			chk_ord=:mq_batch_head_c.chk_ord,
			chk_type=:mq_batch_head_c.chk_type,
			all_num=:mq_batch_head_c.all_num,
			all_amt=:mq_batch_head_c.all_amt,
			child_num=:mq_batch_head_c.child_num,
			pack_ord=:mq_batch_head_c.pack_ord,
			pack_num=:mq_batch_head_c.pack_num,
			pack_amt=:mq_batch_head_c.pack_amt
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mq_batch_head error %d",sqlca.sqlcode);
