int Mo_coll_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_coll.* FROM mo_coll WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_coll_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_coll CURSOR FOR mo_coll_upd;

	EXEC SQL OPEN upd_mo_coll;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_coll error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_coll_Fet_Upd( struct mo_coll_c *mo_coll_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_coll INTO :mo_coll_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_coll error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_coll finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_coll_c);

	return 0;
}

int Mo_coll_Upd_Upd( struct mo_coll_c mo_coll_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_coll SET ac_no=:mo_coll_c.ac_no,
			name=:mo_coll_c.name,
			pick_date=:mo_coll_c.pick_date,
			pick_br_no=:mo_coll_c.pick_br_no,
			pick_tel=:mo_coll_c.pick_tel,
			ret_date=:mo_coll_c.ret_date,
			ret_br_no=:mo_coll_c.ret_br_no,
			sts=:mo_coll_c.sts,
			wrk_date=:mo_coll_c.wrk_date,
			trance_no=:mo_coll_c.trance_no,
			ret_tel=:mo_coll_c.ret_tel
	WHERE rowid=:mo_coll_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mo_coll error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


