int Mo_coll_Ins( struct mo_coll_c mo_coll_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mo_coll VALUES (:mo_coll_c.ac_no,
			:mo_coll_c.name,
			:mo_coll_c.pick_date,
			:mo_coll_c.pick_br_no,
			:mo_coll_c.pick_tel,
			:mo_coll_c.ret_date,
			:mo_coll_c.ret_br_no,
			:mo_coll_c.sts,
			:mo_coll_c.wrk_date,
			:mo_coll_c.trance_no,
			:mo_coll_c.ret_tel);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mo_coll error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mo_coll error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
