/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Clt_id_seq_Sel(reply,clt_id_seq_c,fmtstr,va_alist)
char    *reply;
struct  clt_id_seq_c *clt_id_seq_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Clt_id_seq_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Clt_id_seq_Fet_Sel( clt_id_seq_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Clt_id_seq_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Clt_id_seq_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,clt_id_seq.* FROM clt_id_seq WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE clt_id_seq_sel FROM :comm;
	EXEC SQL DECLARE sel_clt_id_seq CURSOR FOR clt_id_seq_sel;

	EXEC SQL OPEN sel_clt_id_seq;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN clt_id_seq error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Clt_id_seq_Fet_Sel ( struct clt_id_seq_c *clt_id_seq_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_clt_id_seq INTO :clt_id_seq_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch clt_id_seq error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch clt_id_seq finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(clt_id_seq_c);

	return 0;
}

int Clt_id_seq_Clo_Sel (){
	EXEC SQL CLOSE sel_clt_id_seq;
	return 0;
}

