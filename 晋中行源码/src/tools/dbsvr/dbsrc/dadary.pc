/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Dadary_Sel(reply,dadary_c,fmtstr,va_alist)
char    *reply;
struct  dadary_c *dadary_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Dadary_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Dadary_Fet_Sel( dadary_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Dadary_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Dadary_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,dadary.* FROM dadary WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE dadary_sel FROM :comm;
	EXEC SQL DECLARE sel_dadary CURSOR FOR dadary_sel;

	EXEC SQL OPEN sel_dadary;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN dadary error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Dadary_Fet_Sel ( struct dadary_c *dadary_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_dadary INTO :dadary_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch dadary error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch dadary finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(dadary_c);

	return 0;
}

int Dadary_Clo_Sel (){
	EXEC SQL CLOSE sel_dadary;
	return 0;
}

