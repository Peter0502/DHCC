#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"dadary_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int dadary_con ( struct dadary_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct dadary_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->ac_no);
}
