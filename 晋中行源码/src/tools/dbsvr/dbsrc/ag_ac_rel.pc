	EXEC SQL DELETE FROM ag_ac_rel WHERE rowid=:ag_ac_rel_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ag_ac_rel_Clo_Upd( ){
	EXEC SQL CLOSE upd_ag_ac_rel;
	return 0;
}

