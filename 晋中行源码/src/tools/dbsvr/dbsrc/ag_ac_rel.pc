int Ag_ac_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ag_ac_rel.* FROM ag_ac_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ag_ac_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_ag_ac_rel CURSOR FOR ag_ac_rel_upd;

	EXEC SQL OPEN upd_ag_ac_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ag_ac_rel_Fet_Upd( struct ag_ac_rel_c *ag_ac_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ag_ac_rel INTO :ag_ac_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ag_ac_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ag_ac_rel_c);

	return 0;
}

int Ag_ac_rel_Upd_Upd( struct ag_ac_rel_c ag_ac_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE ag_ac_rel SET main_ac_id=:ag_ac_rel_c.main_ac_id,
			app_ac_id=:ag_ac_rel_c.app_ac_id,
			sts=:ag_ac_rel_c.sts,
			filler1=:ag_ac_rel_c.filler1,
			filler2=:ag_ac_rel_c.filler2
	WHERE rowid=:ag_ac_rel_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


