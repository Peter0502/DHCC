}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ag_ac_rel_Sel(reply,ag_ac_rel_c,fmtstr,va_alist)
char    *reply;
struct  ag_ac_rel_c *ag_ac_rel_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ag_ac_rel_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ag_ac_rel_Fet_Sel( ag_ac_rel_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ag_ac_rel_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ag_ac_rel_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ag_ac_rel.* FROM ag_ac_rel WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ag_ac_rel_sel FROM :comm;
	EXEC SQL DECLARE sel_ag_ac_rel CURSOR FOR ag_ac_rel_sel;

	EXEC SQL OPEN sel_ag_ac_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ag_ac_rel_Fet_Sel ( struct ag_ac_rel_c *ag_ac_rel_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ag_ac_rel INTO :ag_ac_rel_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ag_ac_rel error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ag_ac_rel finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ag_ac_rel_c);

	return 0;
}

int Ag_ac_rel_Clo_Sel (){
	EXEC SQL CLOSE sel_ag_ac_rel;
	return 0;
}

