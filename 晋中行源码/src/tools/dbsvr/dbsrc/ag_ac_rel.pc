int Ag_ac_rel_Ins( struct ag_ac_rel_c ag_ac_rel_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO ag_ac_rel VALUES (:ag_ac_rel_c.main_ac_id,
			:ag_ac_rel_c.app_ac_id,
			:ag_ac_rel_c.sts,
			:ag_ac_rel_c.filler1,
			:ag_ac_rel_c.filler2);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT ag_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
