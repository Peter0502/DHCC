#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"ag_ac_rel_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int ag_ac_rel_con ( struct ag_ac_rel_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct ag_ac_rel_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->sts);
	zip_tail(ps->filler1);
	zip_tail(ps->filler2);
}
