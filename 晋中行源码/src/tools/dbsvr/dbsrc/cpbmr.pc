	EXEC SQL INSERT INTO cpbmr VALUES ();
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cpbmr error %d",sqlca.sqlcode);
		WRITEMSG
