	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open tmp_hold error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Tmp_hold_Fet_Upd( struct tmp_hold_c *tmp_hold_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_tmp_hold INTO :tmp_hold_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd tmp_hold error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH tmp_hold finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(tmp_hold_c);

	return 0;
}

int Tmp_hold_Upd_Upd( struct tmp_hold_c tmp_hold_c , char * reply){
	int ret;

	EXEC SQL UPDATE tmp_hold SET ac_id=:tmp_hold_c.ac_id,
			ac_seqn=:tmp_hold_c.ac_seqn,
			acct_no=:tmp_hold_c.acct_no,
			bal=:tmp_hold_c.bal
	WHERE rowid=:tmp_hold_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tmp_hold error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


