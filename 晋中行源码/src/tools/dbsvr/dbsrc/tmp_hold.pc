/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Tmp_hold_Ins( struct tmp_hold_c tmp_hold_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tmp_hold VALUES (:tmp_hold_c.ac_id,
			:tmp_hold_c.ac_seqn,
			:tmp_hold_c.acct_no,
			:tmp_hold_c.bal);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_hold error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tmp_hold error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
