/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mo_spec_intst_Sel(reply,mo_spec_intst_c,fmtstr,va_alist)
char    *reply;
struct  mo_spec_intst_c *mo_spec_intst_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mo_spec_intst_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mo_spec_intst_Fet_Sel( mo_spec_intst_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mo_spec_intst_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mo_spec_intst_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mo_spec_intst.* FROM mo_spec_intst WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_spec_intst_sel FROM :comm;
	EXEC SQL DECLARE sel_mo_spec_intst CURSOR FOR mo_spec_intst_sel;

	EXEC SQL OPEN sel_mo_spec_intst;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mo_spec_intst error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_spec_intst_Fet_Sel ( struct mo_spec_intst_c *mo_spec_intst_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mo_spec_intst INTO :mo_spec_intst_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mo_spec_intst error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mo_spec_intst finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mo_spec_intst_c);

	return 0;
}

int Mo_spec_intst_Clo_Sel (){
	EXEC SQL CLOSE sel_mo_spec_intst;
	return 0;
}

