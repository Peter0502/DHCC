int Od_drt_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,od_drt_def.* FROM od_drt_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE od_drt_def_upd FROM :comm;

	EXEC SQL DECLARE upd_od_drt_def CURSOR FOR od_drt_def_upd;

	EXEC SQL OPEN upd_od_drt_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Od_drt_def_Fet_Upd( struct od_drt_def_c *od_drt_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_od_drt_def INTO :od_drt_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH od_drt_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(od_drt_def_c);

	return 0;
}

int Od_drt_def_Upd_Upd( struct od_drt_def_c od_drt_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE od_drt_def SET prdt_code=:od_drt_def_c.prdt_code,
			tx_code=:od_drt_def_c.tx_code,
			drt_ratio=:od_drt_def_c.drt_ratio
	WHERE rowid=:od_drt_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Od_drt_def_Del_Upd(struct od_drt_def_c od_drt_def_c , char * reply  ){

	EXEC SQL DELETE FROM od_drt_def WHERE rowid=:od_drt_def_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Od_drt_def_Clo_Upd( ){
	EXEC SQL CLOSE upd_od_drt_def;
	return 0;
}

