#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"od_drt_def_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int od_drt_def_con ( struct od_drt_def_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct od_drt_def_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->prdt_code);
	zip_tail(ps->tx_code);
}
int Od_drt_def_Debug(struct od_drt_def_c *ps){
	vtcp_log("od_drt_def_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("od_drt_def_c.prdt_code=[%s]\n",ps->prdt_code);
	vtcp_log("od_drt_def_c.tx_code=[%s]\n",ps->tx_code);
	vtcp_log("od_drt_def_c.drt_ratio=[%f]\n",ps->drt_ratio);
	return(0);
}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Od_drt_def_Sel(reply,od_drt_def_c,fmtstr,va_alist)
char    *reply;
struct  od_drt_def_c *od_drt_def_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Od_drt_def_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Od_drt_def_Fet_Sel( od_drt_def_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Od_drt_def_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Od_drt_def_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,od_drt_def.* FROM od_drt_def WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE od_drt_def_sel FROM :comm;
	EXEC SQL DECLARE sel_od_drt_def CURSOR FOR od_drt_def_sel;

	EXEC SQL OPEN sel_od_drt_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Od_drt_def_Fet_Sel ( struct od_drt_def_c *od_drt_def_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_od_drt_def INTO :od_drt_def_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch od_drt_def error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch od_drt_def finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(od_drt_def_c);

	return 0;
}

int Od_drt_def_Clo_Sel (){
	EXEC SQL CLOSE sel_od_drt_def;
	return 0;
}

