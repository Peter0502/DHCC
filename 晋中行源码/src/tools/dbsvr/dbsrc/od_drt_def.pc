int Od_drt_def_Ins( struct od_drt_def_c od_drt_def_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO od_drt_def VALUES (:od_drt_def_c.prdt_code,
			:od_drt_def_c.tx_code,
			:od_drt_def_c.drt_ratio);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT od_drt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
