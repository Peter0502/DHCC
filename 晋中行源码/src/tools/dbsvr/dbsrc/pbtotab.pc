/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Pbtotab_Sel(reply,pbtotab_c,fmtstr,va_alist)
char    *reply;
struct  pbtotab_c *pbtotab_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Pbtotab_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Pbtotab_Fet_Sel( pbtotab_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Pbtotab_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Pbtotab_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,pbtotab.* FROM pbtotab WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE pbtotab_sel FROM :comm;
	EXEC SQL DECLARE sel_pbtotab CURSOR FOR pbtotab_sel;

	EXEC SQL OPEN sel_pbtotab;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN pbtotab error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Pbtotab_Fet_Sel ( struct pbtotab_c *pbtotab_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_pbtotab INTO :pbtotab_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch pbtotab error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch pbtotab finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(pbtotab_c);

	return 0;
}

int Pbtotab_Clo_Sel (){
	EXEC SQL CLOSE sel_pbtotab;
	return 0;
}

