	int ret;

	EXEC SQL INSERT INTO mob_sys_parm VALUES (:mob_sys_parm_c.jmbz,
			:mob_sys_parm_c.jmsj);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
