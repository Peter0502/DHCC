int Mob_sys_parm_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mob_sys_parm.* FROM mob_sys_parm WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mob_sys_parm_upd FROM :comm;

	EXEC SQL DECLARE upd_mob_sys_parm CURSOR FOR mob_sys_parm_upd;

	EXEC SQL OPEN upd_mob_sys_parm;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mob_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mob_sys_parm_Fet_Upd( struct mob_sys_parm_c *mob_sys_parm_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mob_sys_parm INTO :mob_sys_parm_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mob_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mob_sys_parm finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mob_sys_parm_c);

	return 0;
}

int Mob_sys_parm_Upd_Upd( struct mob_sys_parm_c mob_sys_parm_c , char * reply){
	int ret;

	EXEC SQL UPDATE mob_sys_parm SET jmbz=:mob_sys_parm_c.jmbz,
			jmsj=:mob_sys_parm_c.jmsj
	WHERE rowid=:mob_sys_parm_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mob_sys_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Mob_sys_parm_Del_Upd(struct mob_sys_parm_c mob_sys_parm_c , char * reply  ){

	EXEC SQL DELETE FROM mob_sys_parm WHERE rowid=:mob_sys_parm_c.rowid;
