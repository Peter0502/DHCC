/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Td_list_Sel(reply,td_list_c,fmtstr,va_alist)
char    *reply;
struct  td_list_c *td_list_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Td_list_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Td_list_Fet_Sel( td_list_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Td_list_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Td_list_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,td_list.* FROM td_list WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_list_sel FROM :comm;
	EXEC SQL DECLARE sel_td_list CURSOR FOR td_list_sel;

	EXEC SQL OPEN sel_td_list;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN td_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_list_Fet_Sel ( struct td_list_c *td_list_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_td_list INTO :td_list_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch td_list error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch td_list finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(td_list_c);

	return 0;
}

int Td_list_Clo_Sel (){
	EXEC SQL CLOSE sel_td_list;
	return 0;
}

