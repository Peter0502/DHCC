int Td_list_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,td_list.* FROM td_list WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE td_list_upd FROM :comm;

	EXEC SQL DECLARE upd_td_list CURSOR FOR td_list_upd;

	EXEC SQL OPEN upd_td_list;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open td_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Td_list_Fet_Upd( struct td_list_c *td_list_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_td_list INTO :td_list_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd td_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH td_list finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(td_list_c);

	return 0;
}

int Td_list_Upd_Upd( struct td_list_c td_list_c , char * reply){
	int ret;

	EXEC SQL UPDATE td_list SET ac_id=:td_list_c.ac_id,
			ac_seqn=:td_list_c.ac_seqn,
			opn_br_no=:td_list_c.opn_br_no,
