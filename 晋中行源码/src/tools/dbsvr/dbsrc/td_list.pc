int Td_list_Ins( struct td_list_c td_list_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO td_list VALUES (:td_list_c.ac_id,
			:td_list_c.ac_seqn,
			:td_list_c.opn_br_no,
			:td_list_c.tx_date,
			:td_list_c.td_rate,
			:td_list_c.dd_rate,
			:td_list_c.flag,
			:td_list_c.td_day,
			:td_list_c.dd_day,
			:td_list_c.td_lx,
			:td_list_c.dd_lx,
			:td_list_c.td_amt,
			:td_list_c.dd_amt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT td_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT td_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
