			td_rate=:td_list_c.td_rate,
			dd_rate=:td_list_c.dd_rate,
			flag=:td_list_c.flag,
			td_day=:td_list_c.td_day,
			dd_day=:td_list_c.dd_day,
			td_lx=:td_list_c.td_lx,
			dd_lx=:td_list_c.dd_lx,
			td_amt=:td_list_c.td_amt,
