int Com_acc_br_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,com_acc_br.* FROM com_acc_br WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_acc_br_upd FROM :comm;

	EXEC SQL DECLARE upd_com_acc_br CURSOR FOR com_acc_br_upd;

	EXEC SQL OPEN upd_com_acc_br;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_acc_br_Fet_Upd( struct com_acc_br_c *com_acc_br_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_com_acc_br INTO :com_acc_br_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH com_acc_br finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(com_acc_br_c);

	return 0;
}

int Com_acc_br_Upd_Upd( struct com_acc_br_c com_acc_br_c , char * reply){
	int ret;

	EXEC SQL UPDATE com_acc_br SET acc_hrt=:com_acc_br_c.acc_hrt,
			br_no=:com_acc_br_c.br_no,
			ind=:com_acc_br_c.ind
	WHERE rowid=:com_acc_br_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Com_acc_br_Del_Upd(struct com_acc_br_c com_acc_br_c , char * reply  ){

	EXEC SQL DELETE FROM com_acc_br WHERE rowid=:com_acc_br_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_acc_br_Clo_Upd( ){
	EXEC SQL CLOSE upd_com_acc_br;
	return 0;
}

