int Com_acc_br_Ins( struct com_acc_br_c com_acc_br_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO com_acc_br VALUES (:com_acc_br_c.acc_hrt,
			:com_acc_br_c.br_no,
			:com_acc_br_c.ind);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT com_acc_br error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
