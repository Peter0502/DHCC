#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"com_acc_br_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int com_acc_br_con ( struct com_acc_br_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct com_acc_br_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->acc_hrt);
	zip_tail(ps->br_no);
	zip_tail(ps->ind);
}
