/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ass_cp_stat_Sel(reply,ass_cp_stat_c,fmtstr,va_alist)
char    *reply;
struct  ass_cp_stat_c *ass_cp_stat_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ass_cp_stat_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ass_cp_stat_Fet_Sel( ass_cp_stat_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ass_cp_stat_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ass_cp_stat_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ass_cp_stat.* FROM ass_cp_stat WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ass_cp_stat_sel FROM :comm;
	EXEC SQL DECLARE sel_ass_cp_stat CURSOR FOR ass_cp_stat_sel;

	EXEC SQL OPEN sel_ass_cp_stat;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ass_cp_stat error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ass_cp_stat_Fet_Sel ( struct ass_cp_stat_c *ass_cp_stat_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ass_cp_stat INTO :ass_cp_stat_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ass_cp_stat error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ass_cp_stat finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ass_cp_stat_c);

	return 0;
}

int Ass_cp_stat_Clo_Sel (){
	EXEC SQL CLOSE sel_ass_cp_stat;
	return 0;
}

