int Ass_cp_stat_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ass_cp_stat.* FROM ass_cp_stat WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ass_cp_stat_upd FROM :comm;

	EXEC SQL DECLARE upd_ass_cp_stat CURSOR FOR ass_cp_stat_upd;

	EXEC SQL OPEN upd_ass_cp_stat;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ass_cp_stat error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ass_cp_stat_Fet_Upd( struct ass_cp_stat_c *ass_cp_stat_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ass_cp_stat INTO :ass_cp_stat_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ass_cp_stat error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ass_cp_stat finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ass_cp_stat_c);

	return 0;
}

