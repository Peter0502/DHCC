/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_family_inf_Sel(reply,cif_family_inf_c,fmtstr,va_alist)
char    *reply;
struct  cif_family_inf_c *cif_family_inf_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_family_inf_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_family_inf_Fet_Sel( cif_family_inf_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_family_inf_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_family_inf_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_family_inf.* FROM cif_family_inf WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_family_inf_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_family_inf CURSOR FOR cif_family_inf_sel;

	EXEC SQL OPEN sel_cif_family_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_family_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_family_inf_Fet_Sel ( struct cif_family_inf_c *cif_family_inf_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_family_inf INTO :cif_family_inf_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_family_inf error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_family_inf finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_family_inf_c);

	return 0;
}

int Cif_family_inf_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_family_inf;
	return 0;
}

