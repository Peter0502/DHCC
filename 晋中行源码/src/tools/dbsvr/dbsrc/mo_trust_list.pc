int Mo_trust_list_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_trust_list.* FROM mo_trust_list WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_trust_list_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_trust_list CURSOR FOR mo_trust_list_upd;

	EXEC SQL OPEN upd_mo_trust_list;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_trust_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_trust_list_Fet_Upd( struct mo_trust_list_c *mo_trust_list_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_trust_list INTO :mo_trust_list_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_trust_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_trust_list finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_trust_list_c);

	return 0;
}

int Mo_trust_list_Upd_Upd( struct mo_trust_list_c mo_trust_list_c , char * reply){
	int ret;

	EXEC SQL UPDATE mo_trust_list SET turst_pay_no=:mo_trust_list_c.turst_pay_no,
			payer_seqn=:mo_trust_list_c.payer_seqn,
			payer_ac_no=:mo_trust_list_c.payer_ac_no,
			payer_name=:mo_trust_list_c.payer_name,
			payer_br_no=:mo_trust_list_c.payer_br_no,
			payer_br_name=:mo_trust_list_c.payer_br_name,
			amt=:mo_trust_list_c.amt,
			sts=:mo_trust_list_c.sts
	WHERE rowid=:mo_trust_list_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mo_trust_list error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


