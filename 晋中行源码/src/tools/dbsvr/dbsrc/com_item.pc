int Com_item_Ins( struct com_item_c com_item_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO com_item VALUES (:com_item_c.acc_no,
			:com_item_c.acc_hrt,
			:com_item_c.app_cod,
			:com_item_c.up_acc_hrt,
			:com_item_c.acc_name,
			:com_item_c.sub_acc_yn,
			:com_item_c.acc_lvl,
			:com_item_c.dc_ind,
			:com_item_c.roll_ind,
			:com_item_c.acc_knd,
			:com_item_c.fore_ind,
			:com_item_c.equa_ind,
			:com_item_c.amt_dc_ind,
			:com_item_c.od_ind,
			:com_item_c.in_ind,
			:com_item_c.scope);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT com_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT com_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
