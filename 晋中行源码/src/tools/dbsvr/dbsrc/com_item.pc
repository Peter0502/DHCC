int Com_item_Sel(reply,com_item_c,fmtstr,va_alist)
char    *reply;
struct  com_item_c *com_item_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Com_item_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Com_item_Fet_Sel( com_item_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Com_item_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Com_item_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,com_item.* FROM com_item WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_item_sel FROM :comm;
	EXEC SQL DECLARE sel_com_item CURSOR FOR com_item_sel;

	EXEC SQL OPEN sel_com_item;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN com_item error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_item_Fet_Sel ( struct com_item_c *com_item_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_com_item INTO :com_item_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch com_item error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch com_item finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(com_item_c);

	return 0;
}

int Com_item_Clo_Sel (){
	EXEC SQL CLOSE sel_com_item;
	return 0;
}

