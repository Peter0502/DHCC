int New_old_accno_Sel(reply,new_old_accno_c,fmtstr,va_alist)
char    *reply;
struct  new_old_accno_c *new_old_accno_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = New_old_accno_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = New_old_accno_Fet_Sel( new_old_accno_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	New_old_accno_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int New_old_accno_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,new_old_accno.* FROM new_old_accno WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE new_old_accno_sel FROM :comm;
	EXEC SQL DECLARE sel_new_old_accno CURSOR FOR new_old_accno_sel;

	EXEC SQL OPEN sel_new_old_accno;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN new_old_accno error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int New_old_accno_Fet_Sel ( struct new_old_accno_c *new_old_accno_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_new_old_accno INTO :new_old_accno_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch new_old_accno error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch new_old_accno finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(new_old_accno_c);

	return 0;
}

int New_old_accno_Clo_Sel (){
	EXEC SQL CLOSE sel_new_old_accno;
	return 0;
}

