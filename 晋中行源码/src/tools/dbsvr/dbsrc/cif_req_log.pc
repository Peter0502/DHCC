int Cif_req_log_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,cif_req_log.* FROM cif_req_log WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_req_log_upd FROM :comm;

	EXEC SQL DECLARE upd_cif_req_log CURSOR FOR cif_req_log_upd;

	EXEC SQL OPEN upd_cif_req_log;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cif_req_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_req_log_Fet_Upd( struct cif_req_log_c *cif_req_log_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cif_req_log INTO :cif_req_log_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cif_req_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cif_req_log finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cif_req_log_c);

	return 0;
}

int Cif_req_log_Upd_Upd( struct cif_req_log_c cif_req_log_c , char * reply){
	int ret;

	EXEC SQL UPDATE cif_req_log SET advice_type=:cif_req_log_c.advice_type,
			cif_no=:cif_req_log_c.cif_no,
			seq_no=:cif_req_log_c.seq_no,
			req_clerk_no=:cif_req_log_c.req_clerk_no,
			request=:cif_req_log_c.request,
			"date"=:cif_req_log_c.date,
			clerk_no=:cif_req_log_c.clerk_no
	WHERE rowid=:cif_req_log_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cif_req_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Cif_req_log_Del_Upd(struct cif_req_log_c cif_req_log_c , char * reply  ){

	EXEC SQL DELETE FROM cif_req_log WHERE rowid=:cif_req_log_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE cif_req_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
