}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_req_log_Sel(reply,cif_req_log_c,fmtstr,va_alist)
char    *reply;
struct  cif_req_log_c *cif_req_log_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_req_log_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_req_log_Fet_Sel( cif_req_log_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_req_log_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_req_log_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_req_log.* FROM cif_req_log WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_req_log_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_req_log CURSOR FOR cif_req_log_sel;

	EXEC SQL OPEN sel_cif_req_log;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_req_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_req_log_Fet_Sel ( struct cif_req_log_c *cif_req_log_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_req_log INTO :cif_req_log_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_req_log error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_req_log finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_req_log_c);

	return 0;
}

int Cif_req_log_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_req_log;
	return 0;
}

