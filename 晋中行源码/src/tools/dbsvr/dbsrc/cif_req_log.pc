	EXEC SQL INSERT INTO cif_req_log VALUES (:cif_req_log_c.advice_type,
			:cif_req_log_c.cif_no,
			:cif_req_log_c.seq_no,
			:cif_req_log_c.req_clerk_no,
			:cif_req_log_c.request,
			:cif_req_log_c.date,
			:cif_req_log_c.clerk_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT cif_req_log error %d",sqlca.sqlcode);
