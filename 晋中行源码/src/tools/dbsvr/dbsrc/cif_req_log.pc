int cif_req_log_con ( struct cif_req_log_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct cif_req_log_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->advice_type);
	zip_tail(ps->req_clerk_no);
	zip_tail(ps->request);
	zip_tail(ps->clerk_no);
}
