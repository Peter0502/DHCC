int Agent_info_Ins( struct agent_info_c agent_info_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO agent_info VALUES (:agent_info_c.ac_no,
			:agent_info_c.type,
			:agent_info_c.tx_date,
			:agent_info_c.br_no,
			:agent_info_c.tel,
			:agent_info_c.agent_name,
			:agent_info_c.id_type,
			:agent_info_c.id_no,
			:agent_info_c.phone);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT agent_info error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT agent_info error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
