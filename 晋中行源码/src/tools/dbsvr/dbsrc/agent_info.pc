}

int Agent_info_Fet_Upd( struct agent_info_c *agent_info_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_agent_info INTO :agent_info_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd agent_info error %d",sqlca.sqlcode);
		WRITEMSG
