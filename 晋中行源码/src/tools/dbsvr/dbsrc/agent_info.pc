/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Agent_info_Sel(reply,agent_info_c,fmtstr,va_alist)
char    *reply;
struct  agent_info_c *agent_info_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Agent_info_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Agent_info_Fet_Sel( agent_info_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Agent_info_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Agent_info_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,agent_info.* FROM agent_info WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE agent_info_sel FROM :comm;
	EXEC SQL DECLARE sel_agent_info CURSOR FOR agent_info_sel;

	EXEC SQL OPEN sel_agent_info;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN agent_info error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Agent_info_Fet_Sel ( struct agent_info_c *agent_info_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_agent_info INTO :agent_info_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch agent_info error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch agent_info finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(agent_info_c);

	return 0;
}

int Agent_info_Clo_Sel (){
	EXEC SQL CLOSE sel_agent_info;
	return 0;
}

