int Trace_parm_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,trace_parm.* FROM trace_parm WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE trace_parm_upd FROM :comm;

	EXEC SQL DECLARE upd_trace_parm CURSOR FOR trace_parm_upd;

	EXEC SQL OPEN upd_trace_parm;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Trace_parm_Fet_Upd( struct trace_parm_c *trace_parm_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_trace_parm INTO :trace_parm_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH trace_parm finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(trace_parm_c);

	return 0;
}

int Trace_parm_Upd_Upd( struct trace_parm_c trace_parm_c , char * reply){
	int ret;

	EXEC SQL UPDATE trace_parm SET tx_code=:trace_parm_c.tx_code,
			seq_no=:trace_parm_c.seq_no,
			field=:trace_parm_c.field,
			field_type=:trace_parm_c.field_type,
			field_name=:trace_parm_c.field_name,
			list=:trace_parm_c.list
	WHERE rowid=:trace_parm_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Trace_parm_Del_Upd(struct trace_parm_c trace_parm_c , char * reply  ){

	EXEC SQL DELETE FROM trace_parm WHERE rowid=:trace_parm_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Trace_parm_Clo_Upd( ){
	EXEC SQL CLOSE upd_trace_parm;
	return 0;
}

