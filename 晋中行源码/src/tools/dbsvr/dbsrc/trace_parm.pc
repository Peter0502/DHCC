int Trace_parm_Ins( struct trace_parm_c trace_parm_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO trace_parm VALUES (:trace_parm_c.tx_code,
			:trace_parm_c.seq_no,
			:trace_parm_c.field,
			:trace_parm_c.field_type,
			:trace_parm_c.field_name,
			:trace_parm_c.list);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT trace_parm error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
