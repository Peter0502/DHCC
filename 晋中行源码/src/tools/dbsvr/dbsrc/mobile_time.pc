}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mobile_time_Sel(reply,mobile_time_c,fmtstr,va_alist)
char    *reply;
struct  mobile_time_c *mobile_time_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mobile_time_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mobile_time_Fet_Sel( mobile_time_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mobile_time_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mobile_time_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mobile_time.* FROM mobile_time WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mobile_time_sel FROM :comm;
	EXEC SQL DECLARE sel_mobile_time CURSOR FOR mobile_time_sel;

	EXEC SQL OPEN sel_mobile_time;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mobile_time error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mobile_time_Fet_Sel ( struct mobile_time_c *mobile_time_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mobile_time INTO :mobile_time_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mobile_time error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mobile_time finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mobile_time_c);

	return 0;
}

int Mobile_time_Clo_Sel (){
	EXEC SQL CLOSE sel_mobile_time;
	return 0;
}

