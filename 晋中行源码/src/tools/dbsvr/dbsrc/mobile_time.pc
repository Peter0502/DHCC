#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"mobile_time_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int mobile_time_con ( struct mobile_time_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct mobile_time_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->name);
	zip_tail(ps->time);
}
