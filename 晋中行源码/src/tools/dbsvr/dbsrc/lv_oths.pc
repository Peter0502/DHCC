int Lv_oths_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,lv_oths.* FROM lv_oths WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE lv_oths_upd FROM :comm;

	EXEC SQL DECLARE upd_lv_oths CURSOR FOR lv_oths_upd;

	EXEC SQL OPEN upd_lv_oths;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Lv_oths_Fet_Upd( struct lv_oths_c *lv_oths_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_lv_oths INTO :lv_oths_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH lv_oths finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(lv_oths_c);

	return 0;
}

int Lv_oths_Upd_Upd( struct lv_oths_c lv_oths_c , char * reply){
	int ret;

	EXEC SQL UPDATE lv_oths SET wtday=:lv_oths_c.wtday,
			orbrno=:lv_oths_c.orbrno,
			cmtno=:lv_oths_c.cmtno,
			orderno=:lv_oths_c.orderno,
			stat=:lv_oths_c.stat,
			lwflg=:lv_oths_c.lwflg,
			brno=:lv_oths_c.brno,
			content=:lv_oths_c.content,
			filler=:lv_oths_c.filler
	WHERE rowid=:lv_oths_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


