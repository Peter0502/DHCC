int Lv_oths_Ins( struct lv_oths_c lv_oths_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO lv_oths VALUES (:lv_oths_c.wtday,
			:lv_oths_c.orbrno,
			:lv_oths_c.cmtno,
			:lv_oths_c.orderno,
			:lv_oths_c.stat,
			:lv_oths_c.lwflg,
			:lv_oths_c.brno,
			:lv_oths_c.content,
			:lv_oths_c.filler);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
