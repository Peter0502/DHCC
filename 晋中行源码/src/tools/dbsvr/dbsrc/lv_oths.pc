/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Lv_oths_Sel(reply,lv_oths_c,fmtstr,va_alist)
char    *reply;
struct  lv_oths_c *lv_oths_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Lv_oths_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Lv_oths_Fet_Sel( lv_oths_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Lv_oths_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Lv_oths_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,lv_oths.* FROM lv_oths WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE lv_oths_sel FROM :comm;
	EXEC SQL DECLARE sel_lv_oths CURSOR FOR lv_oths_sel;

	EXEC SQL OPEN sel_lv_oths;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN lv_oths error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Lv_oths_Fet_Sel ( struct lv_oths_c *lv_oths_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_lv_oths INTO :lv_oths_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch lv_oths error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch lv_oths finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(lv_oths_c);

	return 0;
}

int Lv_oths_Clo_Sel (){
	EXEC SQL CLOSE sel_lv_oths;
	return 0;
}

