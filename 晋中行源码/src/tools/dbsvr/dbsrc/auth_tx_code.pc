int Auth_tx_code_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT auth_tx_code.* FROM auth_tx_code WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_tx_code_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_tx_code CURSOR FOR auth_tx_code_upd;

	EXEC SQL OPEN upd_auth_tx_code;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_tx_code error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_tx_code_Fet_Upd( struct auth_tx_code_c *auth_tx_code_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_tx_code INTO :auth_tx_code_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_tx_code error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_tx_code finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_tx_code_c);

	return 0;
}

int Auth_tx_code_Upd_Upd( struct auth_tx_code_c auth_tx_code_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_tx_code SET name=:auth_tx_code_c.name
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_tx_code error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd auth_tx_code"); 

	return 0;
}


int Auth_tx_code_Del_Upd(struct auth_tx_code_c auth_tx_code_c , char * reply  ){

	EXEC SQL DELETE FROM auth_tx_code WHERE rowid='0000';
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_tx_code error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_tx_code_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_tx_code;

	return 0;
}

