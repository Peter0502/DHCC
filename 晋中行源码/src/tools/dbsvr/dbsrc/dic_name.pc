int Dic_name_Fet_Upd( struct dic_name_c *dic_name_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_dic_name INTO :dic_name_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd dic_name error %d",sqlca.sqlcode);
