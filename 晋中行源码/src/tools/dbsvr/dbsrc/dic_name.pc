		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(dic_name_c);

	return 0;
}

int Dic_name_Upd_Upd( struct dic_name_c dic_name_c , char * reply){
	int ret;

	EXEC SQL UPDATE dic_name SET pr_code=:dic_name_c.pr_code,
			pr_name=:dic_name_c.pr_name,
			pr_int=:dic_name_c.pr_int
	WHERE rowid=:dic_name_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE dic_name error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


