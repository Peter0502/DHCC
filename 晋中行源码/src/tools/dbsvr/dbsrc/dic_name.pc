/*****************************************************************************/
/****                              增加函数部分                           ****/
/*****************************************************************************/
int Dic_name_Ins( struct dic_name_c dic_name_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO dic_name VALUES (:dic_name_c.pr_code,
			:dic_name_c.pr_name,
			:dic_name_c.pr_int);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT dic_name error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT dic_name error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
