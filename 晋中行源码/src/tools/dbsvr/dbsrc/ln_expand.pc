int Ln_expand_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ln_expand.* FROM ln_expand WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ln_expand_upd FROM :comm;

	EXEC SQL DECLARE upd_ln_expand CURSOR FOR ln_expand_upd;

	EXEC SQL OPEN upd_ln_expand;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ln_expand_Fet_Upd( struct ln_expand_c *ln_expand_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ln_expand INTO :ln_expand_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ln_expand finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ln_expand_c);

	return 0;
}

int Ln_expand_Upd_Upd( struct ln_expand_c ln_expand_c , char * reply){
	int ret;

	EXEC SQL UPDATE ln_expand SET ac_id=:ln_expand_c.ac_id,
			ac_seqn=:ln_expand_c.ac_seqn,
			exp_br_no=:ln_expand_c.exp_br_no,
			exp_amt=:ln_expand_c.exp_amt,
			exp_rate=:ln_expand_c.exp_rate,
			exp_beg_date=:ln_expand_c.exp_beg_date,
			exp_mtr_date=:ln_expand_c.exp_mtr_date,
			exp_ind=:ln_expand_c.exp_ind,
			rgst_date=:ln_expand_c.rgst_date,
			tel=:ln_expand_c.tel,
			trace_no=:ln_expand_c.trace_no
	WHERE rowid=:ln_expand_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
