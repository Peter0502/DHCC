/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Ln_expand_Sel(reply,ln_expand_c,fmtstr,va_alist)
char    *reply;
struct  ln_expand_c *ln_expand_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Ln_expand_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Ln_expand_Fet_Sel( ln_expand_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Ln_expand_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Ln_expand_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,ln_expand.* FROM ln_expand WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ln_expand_sel FROM :comm;
	EXEC SQL DECLARE sel_ln_expand CURSOR FOR ln_expand_sel;

	EXEC SQL OPEN sel_ln_expand;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ln_expand_Fet_Sel ( struct ln_expand_c *ln_expand_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_ln_expand INTO :ln_expand_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch ln_expand error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch ln_expand finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(ln_expand_c);

	return 0;
}

int Ln_expand_Clo_Sel (){
	EXEC SQL CLOSE sel_ln_expand;
	return 0;
}

