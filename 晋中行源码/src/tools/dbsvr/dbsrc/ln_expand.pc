int Ln_expand_Ins( struct ln_expand_c ln_expand_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO ln_expand VALUES (:ln_expand_c.ac_id,
			:ln_expand_c.ac_seqn,
			:ln_expand_c.exp_br_no,
			:ln_expand_c.exp_amt,
			:ln_expand_c.exp_rate,
			:ln_expand_c.exp_beg_date,
			:ln_expand_c.exp_mtr_date,
			:ln_expand_c.exp_ind,
			:ln_expand_c.rgst_date,
			:ln_expand_c.tel,
			:ln_expand_c.trace_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT ln_expand error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
