int Tel_control_Upd_Upd( struct tel_control_c tel_control_c , char * reply){
	int ret;

	EXEC SQL UPDATE tel_control SET br_no=:tel_control_c.br_no,
			tel=:tel_control_c.tel,
			tx_date=:tel_control_c.tx_date,
			sts=:tel_control_c.sts,
			msg=:tel_control_c.msg
	WHERE rowid=:tel_control_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE tel_control error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


