int Tel_control_Ins( struct tel_control_c tel_control_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO tel_control VALUES (:tel_control_c.br_no,
			:tel_control_c.tel,
			:tel_control_c.tx_date,
			:tel_control_c.sts,
			:tel_control_c.msg);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT tel_control error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT tel_control error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
