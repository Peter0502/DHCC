int Auth_lmt_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,auth_lmt_def.* FROM auth_lmt_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE auth_lmt_def_upd FROM :comm;

	EXEC SQL DECLARE upd_auth_lmt_def CURSOR FOR auth_lmt_def_upd;

	EXEC SQL OPEN upd_auth_lmt_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open auth_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_lmt_def_Fet_Upd( struct auth_lmt_def_c *auth_lmt_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_auth_lmt_def INTO :auth_lmt_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd auth_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH auth_lmt_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(auth_lmt_def_c);

	return 0;
}

int Auth_lmt_def_Upd_Upd( struct auth_lmt_def_c auth_lmt_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE auth_lmt_def SET lmt_code=:auth_lmt_def_c.lmt_code,
			brf=:auth_lmt_def_c.brf,
			min=:auth_lmt_def_c.min,
			max=:auth_lmt_def_c.max
	WHERE rowid=:auth_lmt_def_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE auth_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Auth_lmt_def_Del_Upd(struct auth_lmt_def_c auth_lmt_def_c , char * reply  ){

	EXEC SQL DELETE FROM auth_lmt_def WHERE rowid=:auth_lmt_def_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE auth_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Auth_lmt_def_Clo_Upd( ){
	EXEC SQL CLOSE upd_auth_lmt_def;
	return 0;
}

