int Sysmenuitems_Ins( struct sysmenuitems_c sysmenuitems_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO sysmenuitems VALUES (:sysmenuitems_c.imenuname,
			:sysmenuitems_c.itemnum,
			:sysmenuitems_c.mtext,
			:sysmenuitems_c.mtype,
			:sysmenuitems_c.progname);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT sysmenuitems error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT sysmenuitems error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
