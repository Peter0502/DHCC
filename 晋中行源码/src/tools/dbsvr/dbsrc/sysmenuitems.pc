/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Sysmenuitems_Sel(reply,sysmenuitems_c,fmtstr,va_alist)
char    *reply;
struct  sysmenuitems_c *sysmenuitems_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Sysmenuitems_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Sysmenuitems_Fet_Sel( sysmenuitems_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Sysmenuitems_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Sysmenuitems_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,sysmenuitems.* FROM sysmenuitems WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE sysmenuitems_sel FROM :comm;
	EXEC SQL DECLARE sel_sysmenuitems CURSOR FOR sysmenuitems_sel;

	EXEC SQL OPEN sel_sysmenuitems;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN sysmenuitems error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Sysmenuitems_Fet_Sel ( struct sysmenuitems_c *sysmenuitems_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_sysmenuitems INTO :sysmenuitems_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch sysmenuitems error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch sysmenuitems finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(sysmenuitems_c);

	return 0;
}

int Sysmenuitems_Clo_Sel (){
	EXEC SQL CLOSE sel_sysmenuitems;
	return 0;
}

