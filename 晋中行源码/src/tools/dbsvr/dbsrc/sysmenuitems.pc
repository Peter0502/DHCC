EXEC SQL INCLUDE SQLCA;
#include"sysmenuitems_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int sysmenuitems_con ( struct sysmenuitems_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct sysmenuitems_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->imenuname);
	zip_tail(ps->mtext);
	zip_tail(ps->mtype);
	zip_tail(ps->progname);
}
