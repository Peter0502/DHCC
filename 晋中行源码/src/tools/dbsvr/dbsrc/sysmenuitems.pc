	return 0;
}

int Sysmenuitems_Upd_Upd( struct sysmenuitems_c sysmenuitems_c , char * reply){
	int ret;

	EXEC SQL UPDATE sysmenuitems SET imenuname=:sysmenuitems_c.imenuname,
			itemnum=:sysmenuitems_c.itemnum,
			mtext=:sysmenuitems_c.mtext,
			mtype=:sysmenuitems_c.mtype,
