#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"impa_dep_reg_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int impa_dep_reg_con ( struct impa_dep_reg_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct impa_dep_reg_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->pact_no);
	zip_tail(ps->mort_ac_no);
}
