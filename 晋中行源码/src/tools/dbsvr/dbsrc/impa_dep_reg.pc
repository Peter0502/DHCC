	vtcp_log("impa_dep_reg_c.rowid=[%s]\n",ps->rowid);
	vtcp_log("impa_dep_reg_c.pact_no=[%s]\n",ps->pact_no);
	vtcp_log("impa_dep_reg_c.mort_ac_no=[%s]\n",ps->mort_ac_no);
	vtcp_log("impa_dep_reg_c.mort_amt=[%f]\n",ps->mort_amt);
