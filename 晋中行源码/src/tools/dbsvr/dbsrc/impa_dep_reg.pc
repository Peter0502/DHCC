int Impa_dep_reg_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,impa_dep_reg.* FROM impa_dep_reg WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE impa_dep_reg_upd FROM :comm;

	EXEC SQL DECLARE upd_impa_dep_reg CURSOR FOR impa_dep_reg_upd;

	EXEC SQL OPEN upd_impa_dep_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Impa_dep_reg_Fet_Upd( struct impa_dep_reg_c *impa_dep_reg_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_impa_dep_reg INTO :impa_dep_reg_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH impa_dep_reg finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(impa_dep_reg_c);

	return 0;
}

int Impa_dep_reg_Upd_Upd( struct impa_dep_reg_c impa_dep_reg_c , char * reply){
	int ret;

	EXEC SQL UPDATE impa_dep_reg SET pact_no=:impa_dep_reg_c.pact_no,
			mort_ac_no=:impa_dep_reg_c.mort_ac_no,
			mort_amt=:impa_dep_reg_c.mort_amt
	WHERE rowid=:impa_dep_reg_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


int Impa_dep_reg_Del_Upd(struct impa_dep_reg_c impa_dep_reg_c , char * reply  ){

	EXEC SQL DELETE FROM impa_dep_reg WHERE rowid=:impa_dep_reg_c.rowid;
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Impa_dep_reg_Clo_Upd( ){
	EXEC SQL CLOSE upd_impa_dep_reg;
	return 0;
}

