int Impa_dep_reg_Ins( struct impa_dep_reg_c impa_dep_reg_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO impa_dep_reg VALUES (:impa_dep_reg_c.pact_no,
			:impa_dep_reg_c.mort_ac_no,
			:impa_dep_reg_c.mort_amt);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
