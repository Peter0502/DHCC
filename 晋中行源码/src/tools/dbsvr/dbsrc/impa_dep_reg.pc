}
/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Impa_dep_reg_Sel(reply,impa_dep_reg_c,fmtstr,va_alist)
char    *reply;
struct  impa_dep_reg_c *impa_dep_reg_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Impa_dep_reg_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Impa_dep_reg_Fet_Sel( impa_dep_reg_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Impa_dep_reg_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Impa_dep_reg_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,impa_dep_reg.* FROM impa_dep_reg WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE impa_dep_reg_sel FROM :comm;
	EXEC SQL DECLARE sel_impa_dep_reg CURSOR FOR impa_dep_reg_sel;

	EXEC SQL OPEN sel_impa_dep_reg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN impa_dep_reg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Impa_dep_reg_Fet_Sel ( struct impa_dep_reg_c *impa_dep_reg_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_impa_dep_reg INTO :impa_dep_reg_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch impa_dep_reg error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch impa_dep_reg finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(impa_dep_reg_c);

	return 0;
}

int Impa_dep_reg_Clo_Sel (){
	EXEC SQL CLOSE sel_impa_dep_reg;
	return 0;
}

