	EXEC SQL PREPARE mbfe_hang_upd FROM :comm;

	EXEC SQL DECLARE upd_mbfe_hang CURSOR FOR mbfe_hang_upd;

	EXEC SQL OPEN upd_mbfe_hang;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
