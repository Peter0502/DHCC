int Mbfe_hang_Ins( struct mbfe_hang_c mbfe_hang_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mbfe_hang VALUES (:mbfe_hang_c.consigndate,
			:mbfe_hang_c.txssno,
			:mbfe_hang_c.odficode,
			:mbfe_hang_c.hang_reply);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
