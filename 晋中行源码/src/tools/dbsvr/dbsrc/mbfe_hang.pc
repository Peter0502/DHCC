	}

	return 0;
}

int Mbfe_hang_Fet_Upd( struct mbfe_hang_c *mbfe_hang_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mbfe_hang INTO :mbfe_hang_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mbfe_hang finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mbfe_hang_c);

	return 0;
}

int Mbfe_hang_Upd_Upd( struct mbfe_hang_c mbfe_hang_c , char * reply){
	int ret;

	EXEC SQL UPDATE mbfe_hang SET consigndate=:mbfe_hang_c.consigndate,
			txssno=:mbfe_hang_c.txssno,
			odficode=:mbfe_hang_c.odficode,
			hang_reply=:mbfe_hang_c.hang_reply
	WHERE rowid=:mbfe_hang_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


