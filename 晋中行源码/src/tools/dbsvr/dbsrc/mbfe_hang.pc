/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mbfe_hang_Sel(reply,mbfe_hang_c,fmtstr,va_alist)
char    *reply;
struct  mbfe_hang_c *mbfe_hang_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mbfe_hang_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mbfe_hang_Fet_Sel( mbfe_hang_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mbfe_hang_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mbfe_hang_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mbfe_hang.* FROM mbfe_hang WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mbfe_hang_sel FROM :comm;
	EXEC SQL DECLARE sel_mbfe_hang CURSOR FOR mbfe_hang_sel;

	EXEC SQL OPEN sel_mbfe_hang;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mbfe_hang error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mbfe_hang_Fet_Sel ( struct mbfe_hang_c *mbfe_hang_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mbfe_hang INTO :mbfe_hang_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mbfe_hang error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mbfe_hang finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mbfe_hang_c);

	return 0;
}

int Mbfe_hang_Clo_Sel (){
	EXEC SQL CLOSE sel_mbfe_hang;
	return 0;
}

