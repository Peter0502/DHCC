/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Mo_cli_log_Sel(reply,mo_cli_log_c,fmtstr,va_alist)
char    *reply;
struct  mo_cli_log_c *mo_cli_log_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Mo_cli_log_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Mo_cli_log_Fet_Sel( mo_cli_log_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Mo_cli_log_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Mo_cli_log_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,mo_cli_log.* FROM mo_cli_log WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_cli_log_sel FROM :comm;
	EXEC SQL DECLARE sel_mo_cli_log CURSOR FOR mo_cli_log_sel;

	EXEC SQL OPEN sel_mo_cli_log;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN mo_cli_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mo_cli_log_Fet_Sel ( struct mo_cli_log_c *mo_cli_log_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_mo_cli_log INTO :mo_cli_log_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch mo_cli_log error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch mo_cli_log finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(mo_cli_log_c);

	return 0;
}

int Mo_cli_log_Clo_Sel (){
	EXEC SQL CLOSE sel_mo_cli_log;
	return 0;
}

