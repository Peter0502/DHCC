	}

	return 0;
}

int Mo_cli_log_Fet_Upd( struct mo_cli_log_c *mo_cli_log_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mo_cli_log INTO :mo_cli_log_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mo_cli_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mo_cli_log finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mo_cli_log_c);

	return 0;
}

