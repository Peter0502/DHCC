int Mo_cli_log_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,mo_cli_log.* FROM mo_cli_log WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mo_cli_log_upd FROM :comm;

	EXEC SQL DECLARE upd_mo_cli_log CURSOR FOR mo_cli_log_upd;

	EXEC SQL OPEN upd_mo_cli_log;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mo_cli_log error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
