int Acc_ac_id_Ins( struct acc_ac_id_c acc_ac_id_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO acc_ac_id VALUES (:acc_ac_id_c.acc_no,
			:acc_ac_id_c.data_code,
			:acc_ac_id_c.ac_id);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT acc_ac_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT acc_ac_id error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" ins acc_ac_id"); 

	return 0;
}
