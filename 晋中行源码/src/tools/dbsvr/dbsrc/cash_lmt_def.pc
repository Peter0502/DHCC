/*****************************************************************************/
/****                              查询函数部分                           ****/
/*****************************************************************************/
int Cash_lmt_def_Sel(reply,cash_lmt_def_c,fmtstr,va_alist)
char    *reply;
struct  cash_lmt_def_c *cash_lmt_def_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cash_lmt_def_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cash_lmt_def_Fet_Sel( cash_lmt_def_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cash_lmt_def_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cash_lmt_def_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT cash_lmt_def.* FROM cash_lmt_def WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cash_lmt_def_sel FROM :comm;
	EXEC SQL DECLARE sel_cash_lmt_def CURSOR FOR cash_lmt_def_sel;

	EXEC SQL OPEN sel_cash_lmt_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cash_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cash_lmt_def_Fet_Sel ( struct cash_lmt_def_c *cash_lmt_def_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cash_lmt_def INTO :cash_lmt_def_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cash_lmt_def error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cash_lmt_def finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cash_lmt_def_c);

	return 0;
}

int Cash_lmt_def_Clo_Sel (){
	EXEC SQL CLOSE sel_cash_lmt_def;
}

