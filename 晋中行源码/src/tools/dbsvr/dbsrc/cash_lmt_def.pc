int Cash_lmt_def_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT cash_lmt_def.* FROM cash_lmt_def WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cash_lmt_def_upd FROM :comm;

	EXEC SQL DECLARE upd_cash_lmt_def CURSOR FOR cash_lmt_def_upd;

	EXEC SQL OPEN upd_cash_lmt_def;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open cash_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cash_lmt_def_Fet_Upd( struct cash_lmt_def_c *cash_lmt_def_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_cash_lmt_def INTO :cash_lmt_def_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd cash_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH cash_lmt_def finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(cash_lmt_def_c);

	return 0;
}

int Cash_lmt_def_Upd_Upd( struct cash_lmt_def_c cash_lmt_def_c , char * reply){
	int ret;

	EXEC SQL UPDATE cash_lmt_def SET br_no=:cash_lmt_def_c.br_no,
			lmt_amt=:cash_lmt_def_c.lmt_amt
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE cash_lmt_def error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd cash_lmt_def"); 

	return 0;
}


