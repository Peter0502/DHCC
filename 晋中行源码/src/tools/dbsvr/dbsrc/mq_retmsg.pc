int Mq_retmsg_Ins( struct mq_retmsg_c mq_retmsg_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO mq_retmsg VALUES (:mq_retmsg_c.addword_no,
			:mq_retmsg_c.addwork);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" ins mq_retmsg"); 

	return 0;
}
