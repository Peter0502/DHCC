int Mq_retmsg_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT mq_retmsg.* FROM mq_retmsg WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_retmsg_upd FROM :comm;

	EXEC SQL DECLARE upd_mq_retmsg CURSOR FOR mq_retmsg_upd;

	EXEC SQL OPEN upd_mq_retmsg;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_retmsg_Fet_Upd( struct mq_retmsg_c *mq_retmsg_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mq_retmsg INTO :mq_retmsg_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mq_retmsg finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mq_retmsg_c);

	return 0;
}

int Mq_retmsg_Upd_Upd( struct mq_retmsg_c mq_retmsg_c , char * reply){
	int ret;

	EXEC SQL UPDATE mq_retmsg SET addword_no=:mq_retmsg_c.addword_no,
			addwork=:mq_retmsg_c.addwork
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd mq_retmsg"); 

	return 0;
}


int Mq_retmsg_Del_Upd(struct mq_retmsg_c mq_retmsg_c , char * reply  ){

	EXEC SQL DELETE FROM mq_retmsg WHERE rowid='0000';
	if ( sqlca.sqlcode ){
		sprintf(acErrMsg,"DELETE mq_retmsg error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_retmsg_Clo_Upd( ){
	EXEC SQL CLOSE upd_mq_retmsg;

	return 0;
}

