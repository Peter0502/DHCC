#include<stdio.h>
#include<varargs.h>

#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"wjdeview_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int wjdeview_con ( struct wjdeview_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct wjdeview_c* ps){
	zip_tail(ps->rowid);
}
