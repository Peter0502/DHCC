#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int ag_qs_gd_rel_con ( struct ag_qs_gd_rel_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct ag_qs_gd_rel_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->qs_ac_no);
	zip_tail(ps->gd_ac_no);
	zip_tail(ps->unit_no);
	zip_tail(ps->tel);
}
