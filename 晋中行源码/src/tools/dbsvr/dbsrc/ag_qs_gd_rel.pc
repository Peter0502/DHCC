int Ag_qs_gd_rel_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,ag_qs_gd_rel.* FROM ag_qs_gd_rel WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE ag_qs_gd_rel_upd FROM :comm;

	EXEC SQL DECLARE upd_ag_qs_gd_rel CURSOR FOR ag_qs_gd_rel_upd;

	EXEC SQL OPEN upd_ag_qs_gd_rel;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open ag_qs_gd_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Ag_qs_gd_rel_Fet_Upd( struct ag_qs_gd_rel_c *ag_qs_gd_rel_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_ag_qs_gd_rel INTO :ag_qs_gd_rel_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd ag_qs_gd_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH ag_qs_gd_rel finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(ag_qs_gd_rel_c);

	return 0;
}

int Ag_qs_gd_rel_Upd_Upd( struct ag_qs_gd_rel_c ag_qs_gd_rel_c , char * reply){
	int ret;

	EXEC SQL UPDATE ag_qs_gd_rel SET qs_ac_no=:ag_qs_gd_rel_c.qs_ac_no,
			gd_ac_no=:ag_qs_gd_rel_c.gd_ac_no,
			unit_no=:ag_qs_gd_rel_c.unit_no,
			clr_date=:ag_qs_gd_rel_c.clr_date,
			tel=:ag_qs_gd_rel_c.tel
	WHERE rowid=:ag_qs_gd_rel_c.rowid;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE ag_qs_gd_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}

	return 0;
}


