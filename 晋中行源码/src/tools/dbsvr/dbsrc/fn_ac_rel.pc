int Fn_ac_rel_Ins( struct fn_ac_rel_c fn_ac_rel_c , char * reply ){
	int ret;

	EXEC SQL INSERT INTO fn_ac_rel VALUES (:fn_ac_rel_c.prdt_code,
			:fn_ac_rel_c.qs_ac_no,
			:fn_ac_rel_c.wt_ac_no,
			:fn_ac_rel_c.sy_ac_no);
	if ( sqlca.sqlcode && sqlca.sqlcode != -1 )
	{
		sprintf(acErrMsg,"INSERT fn_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D107" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == -1 )
	{
		sprintf(acErrMsg,"INSERT fn_ac_rel error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D108" );
		return CM_SQLCODE;
	}

	return 0;
}
