#define EXTERN
#include"global.h"


EXEC SQL INCLUDE SQLCA;
#include"fn_ac_rel_c.h"
#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)

int fn_ac_rel_con ( struct fn_ac_rel_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct fn_ac_rel_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->prdt_code);
	zip_tail(ps->qs_ac_no);
	zip_tail(ps->wt_ac_no);
	zip_tail(ps->sy_ac_no);
}
