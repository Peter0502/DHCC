/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Com_rollback_ctl_Sel(reply,com_rollback_ctl_c,fmtstr,va_alist)
char    *reply;
struct  com_rollback_ctl_c *com_rollback_ctl_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Com_rollback_ctl_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Com_rollback_ctl_Fet_Sel( com_rollback_ctl_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Com_rollback_ctl_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Com_rollback_ctl_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,com_rollback_ctl.* FROM com_rollback_ctl WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_rollback_ctl_sel FROM :comm;
	EXEC SQL DECLARE sel_com_rollback_ctl CURSOR FOR com_rollback_ctl_sel;

	EXEC SQL OPEN sel_com_rollback_ctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN com_rollback_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_rollback_ctl_Fet_Sel ( struct com_rollback_ctl_c *com_rollback_ctl_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_com_rollback_ctl INTO :com_rollback_ctl_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch com_rollback_ctl error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch com_rollback_ctl finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(com_rollback_ctl_c);

	return 0;
}

int Com_rollback_ctl_Clo_Sel (){
	EXEC SQL CLOSE sel_com_rollback_ctl;
	return 0;
}

