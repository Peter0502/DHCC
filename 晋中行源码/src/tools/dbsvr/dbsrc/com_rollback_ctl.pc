int Com_rollback_ctl_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT rowid,com_rollback_ctl.* FROM com_rollback_ctl WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE com_rollback_ctl_upd FROM :comm;

	EXEC SQL DECLARE upd_com_rollback_ctl CURSOR FOR com_rollback_ctl_upd;

	EXEC SQL OPEN upd_com_rollback_ctl;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open com_rollback_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Com_rollback_ctl_Fet_Upd( struct com_rollback_ctl_c *com_rollback_ctl_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_com_rollback_ctl INTO :com_rollback_ctl_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd com_rollback_ctl error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH com_rollback_ctl finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(com_rollback_ctl_c);

	return 0;
}

int Com_rollback_ctl_Upd_Upd( struct com_rollback_ctl_c com_rollback_ctl_c , char * reply){
	int ret;

	EXEC SQL UPDATE com_rollback_ctl SET sub_tx_code=:com_rollback_ctl_c.sub_tx_code,
			reg_ind=:com_rollback_ctl_c.reg_ind,
			memo=:com_rollback_ctl_c.memo
	WHERE rowid=:com_rollback_ctl_c.rowid;
