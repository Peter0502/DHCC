/*****************************************************************************/
/****                              查询函数部分[]                       ****/
/*****************************************************************************/
int Cif_crdt_inf_Sel(reply,cif_crdt_inf_c,fmtstr,va_alist)
char    *reply;
struct  cif_crdt_inf_c *cif_crdt_inf_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Cif_crdt_inf_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Cif_crdt_inf_Fet_Sel( cif_crdt_inf_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Cif_crdt_inf_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Cif_crdt_inf_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,cif_crdt_inf.* FROM cif_crdt_inf WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE cif_crdt_inf_sel FROM :comm;
	EXEC SQL DECLARE sel_cif_crdt_inf CURSOR FOR cif_crdt_inf_sel;

	EXEC SQL OPEN sel_cif_crdt_inf;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN cif_crdt_inf error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Cif_crdt_inf_Fet_Sel ( struct cif_crdt_inf_c *cif_crdt_inf_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH sel_cif_crdt_inf INTO :cif_crdt_inf_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch cif_crdt_inf error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch cif_crdt_inf finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(cif_crdt_inf_c);

	return 0;
}

int Cif_crdt_inf_Clo_Sel (){
	EXEC SQL CLOSE sel_cif_crdt_inf;
	return 0;
}

