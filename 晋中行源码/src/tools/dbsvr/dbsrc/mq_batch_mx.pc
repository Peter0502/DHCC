int Mq_batch_mx_Dec_Upd( reply , fmtstr , va_alist)
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm , "SELECT mq_batch_mx.* FROM mq_batch_mx WHERE  ");
	key_strcat(comm,wherelist);
	strcat(comm," FOR UPDATE WAIT 5 ");
	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE mq_batch_mx_upd FROM :comm;

	EXEC SQL DECLARE upd_mq_batch_mx CURSOR FOR mq_batch_mx_upd;

	EXEC SQL OPEN upd_mq_batch_mx;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"open mq_batch_mx error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Mq_batch_mx_Fet_Upd( struct mq_batch_mx_c *mq_batch_mx_c , char * reply ){
	int   ret;

	EXEC SQL FETCH upd_mq_batch_mx INTO :mq_batch_mx_c;
	if ( sqlca.sqlcode && sqlca.sqlcode!=1403 ){
		sprintf(acErrMsg,"FETCH upd mq_batch_mx error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D103" );
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"FETCH mq_batch_mx finished %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D104" );
		return CM_SQLCODE;
	}
	zip_struct(mq_batch_mx_c);

	return 0;
}

int Mq_batch_mx_Upd_Upd( struct mq_batch_mx_c mq_batch_mx_c , char * reply){
	int ret;

	EXEC SQL UPDATE mq_batch_mx SET chk_date=:mq_batch_mx_c.chk_date,
			chk_ord=:mq_batch_mx_c.chk_ord,
			pack_ord=:mq_batch_mx_c.pack_ord,
			pay_acct=:mq_batch_mx_c.pay_acct,
			taxorg_code=:mq_batch_mx_c.taxorg_code,
			entrust_date=:mq_batch_mx_c.entrust_date,
			pack_no=:mq_batch_mx_c.pack_no,
			tra_no=:mq_batch_mx_c.tra_no,
			tra_amt=:mq_batch_mx_c.tra_amt,
			flag=:mq_batch_mx_c.flag
	WHERE 1=0;
	if ( sqlca.sqlcode && sqlca.sqlcode != 1403 ){
		sprintf(acErrMsg,"UPDATE mq_batch_mx error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D105" );
		return CM_SQLCODE;
	}
	pub_reg_rec_file(" upd mq_batch_mx"); 

	return 0;
}


