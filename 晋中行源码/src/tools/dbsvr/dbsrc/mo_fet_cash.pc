	EXEC SQL INSERT INTO mo_fet_cash VALUES (:mo_fet_cash_c.ac_id,
			:mo_fet_cash_c.ac_seqn,
			:mo_fet_cash_c.tx_date,
			:mo_fet_cash_c.amt,
