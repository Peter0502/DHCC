#define CM_SQLCODE sqlca.sqlcode==1403?100:(sqlca.sqlcode==-1?-239:sqlca.sqlcode)
static char str[3000];
static char mystery[17];

int dd_mst_con ( struct dd_mst_c , char *);
int put_mystery( char *, char * );
char *zip_tail(char *s);
static void zip_struct(struct dd_mst_c* ps){
	zip_tail(ps->rowid);
	zip_tail(ps->opn_br_no);
	zip_tail(ps->prdt_no);
	zip_tail(ps->cash_ind);
	zip_tail(ps->sttl_type);
	zip_tail(ps->ac_type);
	zip_tail(ps->intst_type);
	zip_tail(ps->odtt_ind);
	zip_tail(ps->ac_sts);
	zip_tail(ps->hold_sts);
	zip_tail(ps->od_ind);
	zip_tail(ps->draw_pwd);
	zip_tail(ps->name);
	zip_tail(ps->cal_code);
	zip_tail(ps->mac);
}
/*****************************************************************************/
/****                              查询函数部分                           ****/
/*****************************************************************************/
int Eod_Dd_mst_Sel(reply,dd_mst_c,fmtstr,va_alist)
char    *reply;
struct  dd_mst_c *dd_mst_c;
char    *fmtstr;
va_dcl
{
	int   ret;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	ret = Eod_Dd_mst_Dec_Sel( reply , "%s" , wherelist );
	if (ret){
		WRITEMSG
		return ret;
	}
	ret = Eod_Dd_mst_Fet_Sel( dd_mst_c , reply );
	if (ret){
		WRITEMSG
		return ret;
	}

	Eod_Dd_mst_Clo_Sel();

	return 0;
}



char *key_strcat(char *,char *);
int Eod_Dd_mst_Dec_Sel ( reply , fmtstr , va_alist )
char    *reply;
char    *fmtstr;
va_dcl
{
	EXEC    SQL BEGIN   DECLARE SECTION;
		char    comm[1000];
	EXEC    SQL END     DECLARE SECTION;
	char    wherelist[800];

	va_list ap;
	va_start(ap);
	vsprintf ( wherelist , fmtstr , ap );
	va_end(ap);

	sprintf ( comm ," SELECT rowid,dd_mst.* FROM dd_mst WHERE  ");
	key_strcat(comm,wherelist);

	vtcp_log("%s,%d,SQLSTR=[%s]\n",__FILE__,__LINE__,comm);
	sqlca.sqlcode=0;
	EXEC SQL PREPARE eod_dd_mst_sel FROM :comm;
	if (sqlca.sqlcode){
		vtcp_log(comm);
		sprintf(acErrMsg,"prepare error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D101" );
		return CM_SQLCODE;
	}
	EXEC SQL DECLARE eod_sel_dd_mst CURSOR FOR eod_dd_mst_sel;

	EXEC SQL OPEN eod_sel_dd_mst;
	if (sqlca.sqlcode){
		sprintf(acErrMsg,"OPEN dd_mst error %d",sqlca.sqlcode);
		WRITEMSG
		strcpy ( reply , "D102" );
		return CM_SQLCODE;
	}

	return 0;
}

int Eod_Dd_mst_Fet_Sel ( struct dd_mst_c *dd_mst_c , char * reply )
{

	int   ret;

	EXEC SQL FETCH eod_sel_dd_mst INTO :dd_mst_c;
	if (sqlca.sqlcode&&sqlca.sqlcode!=1403){
		sprintf(acErrMsg,"Fetch eod_dd_mst error!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D103");
		return CM_SQLCODE;
	}
	else if ( sqlca.sqlcode == 1403 ){
		sprintf(acErrMsg,"Fetch eod_dd_mst finished!!! %d",sqlca.sqlcode);
		WRITEMSG
		strcpy (reply,"D104");
		return CM_SQLCODE;
	}
	zip_struct(dd_mst_c);

	return 0;
}

int Eod_Dd_mst_Clo_Sel (){
	EXEC SQL CLOSE eod_sel_dd_mst;
	return 0;
}

