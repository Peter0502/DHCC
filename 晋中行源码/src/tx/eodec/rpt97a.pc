		sprintf( str , fmt , s_in_parm.prdt_mo );
		break;
	case 'B': /**内部产品**/
		sprintf(fmt,"%%-%ds",bll);
		sprintf( str , fmt , s_in_mst.prdt_no );
		break;
	case 'C': /* 机构名称 */
