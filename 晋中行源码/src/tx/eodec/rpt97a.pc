			strncpy( str+i,"(",1 );
			i++;
			strncpy( str+i,s_in_mst.opn_br_no,5 );
			i=i+5;
			/*add by martin 应银监局要求对对公户增加科目号要求*/
				{
					strncpy( str+i," ",1 );/*用空格间隔可以调整*/
					i=i++;
