			sprintf(acErrMsg,"SELECT COM_BRANCH ERROR !! [%d]",ret);
			WRITEMSG
				sprintf(acErrMsg,"br_no=[%s]",s_in_mst.opn_br_no);
			WRITEMSG
				goto ErrExit;
		}
		else if (ret == 100)
