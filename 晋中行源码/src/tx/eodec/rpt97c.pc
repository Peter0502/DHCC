				}
			else
			if(v_acno_hst_log.ac_type[0]=='3' && s_mdm_ac_rel.ac_no[1]=='1' )
				{
					if ( v_acno_hst_log.dc_ind[0]=='2')
			   	 bal = v_acno_hst_log.bal+v_acno_hst_log.tx_amt ;
			    else if ( v_acno_hst_log.dc_ind[0]=='1')
			   	 bal = v_acno_hst_log.bal-v_acno_hst_log.tx_amt ;
