		sprintf( t_str , "%s" , tel );
		sprintf(fmt, "%%-%ds", bll);
		sprintf(str, fmt, t_str);
		break;
	case 'P': /* 结转余额 */
		if ( tag==1 )
		{
			if( v_acno_hst_log.ac_type[0]=='2' )/* 贷款上页反方向*/
				{
					if ( v_acno_hst_log.dc_ind[0]=='2')
