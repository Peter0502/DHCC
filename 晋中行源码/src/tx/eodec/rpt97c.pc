				strcpy(s_com_item.acc_name,"");
			}
			printf("2222222222222222\n");
			sprintf( t_str , "%s" , s_com_item.acc_name);
			sprintf(fmt,"%%-%ds",bll);
			sprintf( str , fmt , t_str );
			break;
		case 'B': /**科目**/
		printf("3333333333333333333\n");
			sprintf(fmt,"%%-%ds",bll);
			sprintf( str , fmt , s_gl_sub.acc_hrt );
			break;
		case 'C': /* 机构名称 */
