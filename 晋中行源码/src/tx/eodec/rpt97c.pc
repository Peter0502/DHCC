		sprintf( amt_tmp, "%.2lf" , bal );
		pub_rpt_flttomon( amt_tmp,amt_tmp );
		sprintf(fmt,"%%%ds",bll);
		if ( strlen(amt_tmp)>bll )
			sprintf( amt_tmp, "%.2lf" , bal );
		sprintf( str, fmt, amt_tmp );
