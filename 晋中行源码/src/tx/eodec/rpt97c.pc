		sprintf( t_str , "%ld" , date );
		sprintf(fmt,"%%-%ds",bll);
		sprintf( str , fmt , t_str );
		break;
	case 'V':/**汇总信息 摘要***/
		strcpy(brf,"金额汇总");
		pub_base_strpack(brf);
		sprintf(fmt, "%%-%ds", bll);
		sprintf(str, fmt, brf);
		break ;
	case 'W':/**汇总信息 借合计值***/
